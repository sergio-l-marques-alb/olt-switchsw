    <sub_module name="system_headers" doc="system headers">
        <defines>
                <define name="system_headers_mode_jericho"  value="0"></define>
                <define name="system_headers_mode_jericho2"  value="1"></define>
                <define name="crc_size_counted_in_pkt_len" value="2"></define>
        </defines>
        <numerics>
                <numeric name="system_headers_mode" value="1">
                <property name="system_headers_mode" method="enable">
                    <doc>
                            System headers mode that the device supports, possible values:
                            0x0 - Jericho-mode - used for Jericho/QMX/QAX/QUX mode
                            0x1 - Jericho2-mode - 0x1 used for Jericho 2 mode
                            Default: 0x1.
                    </doc>
                </property>
                </numeric>
                <numeric name="jr_mode_ftmh_lb_key_ext_mode" value="0">
                <property name="system_ftmh_load_balancing_ext_mode" method="direct_map">
                    <map name="DISABLED" value="0"></map>
                    <map name="ENABLED" value="1"></map>
                    <doc>
                            ftmh load balance key extensiton mode in JR mode:
                            DISABLED - FTMH load balancing extension is disabled
                            ENABLED -  FTMH load balancing extension is enabled.
                            Default: DISABLED
                    </doc>
                </property>
                </numeric>
                <numeric name="jr_mode_ftmh_stacking_ext_mode" value="0">
                    <property name="stacking_extension_enable" method="enable">
                        <doc>
                            ftmh stacking extension mode in JR sys-headers mode:
                            0x0 - FTMH stacking extension is disabled
                            0x1 - FTMH stacking extension is enabled
                            Defaule: DISABLED
                        </doc>
                    </property>
                </numeric>
                <numeric name="udh_base_size" value="8">
                <property name="system_headers_mode" method="direct_map">
                    <map name="1" value="8"></map>
                    <map name="0" value="0"></map>
                    <doc>
                            UDH Base header 
                            JR2 mode - Standard UDH Base with 1B
                            JR1 mode - No UDH Base header.
                            Default: JR2 mode
                    </doc>
                </property>
                </numeric>
        </numerics>
    </sub_module>

