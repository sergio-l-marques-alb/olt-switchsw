        <sub_module name="general">
        <numerics>
            <numeric name="access_only" value="0">
                <property name="custom_feature" method="suffix_enable" suffix="access_only">
                    <doc>
                        Set to boot with access only.
                    </doc>
                </property>
            </numeric>
        </numerics>
        <features>
             <feature name="data_path_hw" value="0"></feature>
             <feature name="remove_crc_bytes" value="0"></feature>
             <feature name="remove_crc_bytes_capability" value="1"></feature>
             <feature name="erpp_compensate_crc_size" value="0"></feature>
             <feature name="etpp_compensate_crc_size" value="1"></feature>
             <!-- FIXME SDK-173446 J2P:Tail editing feature changes-->
             <feature name="tail_editing_enable" value="0"></feature>
             <feature name="tail_editing_append_enable" value="1"></feature>
             <feature name="tail_editing_truncate_enable" value="1"></feature>
             <!-- FIXME SDK-173448 J2P: revisit the packet size compensation in Term block-->
             <feature name="packet_size_compensation_term" value="0"></feature>
             <feature name="current_packet_size_compensation_term" value="1"></feature>
             <!-- FIXME SDK-172107 JR2P: Ipv4/Ipv6 and L4 filters update in ERPP-->
             <feature name="erpp_filters_non_separate_enablers" value="0"></feature>
             <feature name="etppa_bypass_stamp_ftmh_enable" value="1"></feature>
             <feature name="l4_protocol_fields_config_enable" value="1"></feature>
             <feature name="da_type_map_enable" value="1"></feature>
             <feature name="ecologic_support" value="1"></feature>
             <feature name="context_selection_index_valid" value="1"></feature>
             <feature name="network_header_termination" value="1"></feature>
             <feature name="network_offset_for_system_headers_valid" value="1"></feature>
        </features>

        <defines>
        </defines>
    </sub_module>

    <sub_module name="ha">
        <numerics>
            <numeric name="warmboot_support" value="0">
                <property name="warmboot_support" method="direct_map">
                    <map name="on" value="1"></map>
                    <map name="off" value="0"></map>
                    <doc>Specify if WarmBoot is supported</doc>
                </property>
            </numeric>

            <numeric name="sw_state_max_size" value="0">
                <property name="sw_state_max_size" method="range" range_min="0" range_max="1000000000">
                    <doc>
                        Configures the size of memory to be pre allocated for sw state.
                        The valid range is 0-1000000000 (i.e up to 1TB).
                    </doc>
                </property>
            </numeric>

            <numeric name="stable_location" value="1">
                <property name="stable_location" method="range" range_min="0" range_max="5">
                    <doc>
                        Specify the stable cache option for Warm Boot operations.
                    </doc>
                </property>
            </numeric>

            <numeric name="stable_size" value="0">
                <property name="stable_size" method="range" range_min="0" range_max="1000000000">
                    <doc>
                        Specify the stable cache size in bytes used for Warm boot operations.
                    </doc>
                </property>
            </numeric>
        </numerics>

        <tables>
            <table name="stable_filename">
                <value name="val" default="NULL">
                    <property name="stable_filename" method="str" >
                        <doc>file system will be used to save the stable cache data with this filename</doc>
                    </property>
                </value>
            </table>
        </tables>
    </sub_module>

