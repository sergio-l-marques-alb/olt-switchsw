<?xml version="1.0"?>
<device_data lib="dnx_data" module="lane_map" bsl_flag="BSL_LS_BCMDNX_PORT">
    <includes>
        <include>bcm/port.h</include>
    </includes>
   
    <sub_module name="nif" doc="lane mapping attributes for NIF">

        <features>
        </features>

        <numerics>
        </numerics>

        <tables>
            <table labels="j2p_notrev" name="mapping" doc="lane to serdes mapping info for NIF ports">
                <key name="lane_id" doc="logical lane id"></key>
                <value name="serdes_rx_id" type="int" doc="Rx Serdes lane id"></value>
                <value name="serdes_tx_id" type="int" doc="Tx Serdes lane id"></value>
            </table>
        </tables>
