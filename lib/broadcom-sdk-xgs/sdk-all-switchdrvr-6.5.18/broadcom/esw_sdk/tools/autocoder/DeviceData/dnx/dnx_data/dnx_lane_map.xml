        </defines>

        <numerics>
        </numerics>
        <tables>
            <table labels="j2p_notrev" name="mapping" doc="lane to serdes mapping info for Fabric links">
                <key name="lane_id" doc="logical lane id"></key>
                <value name="serdes_rx_id" type="int" doc="Rx Serdes lane id"></value>
                <value name="serdes_tx_id" type="int" doc="Tx Serdes lane id"></value>
            </table>
        </tables>
    </sub_module>

    <sub_module name="ilkn" doc="logical lane re-mapping attributes for ILKN">

        <features>
        </features>

        <defines>
        </defines>

        <numerics>
        </numerics>
        <tables>
            <table labels="j2p_notrev" name="remapping" doc="Ilkn remapping info">
                <key name="lane_id" doc="logical lane id"></key>
                <key name="ilkn_id" doc="ILKN port ID"></key>
                <value name="ilkn_lane_id" type="int" doc="Ilkn lane ID after remapping."></value>
            </table>
        </tables>
    </sub_module>
</device_data>
