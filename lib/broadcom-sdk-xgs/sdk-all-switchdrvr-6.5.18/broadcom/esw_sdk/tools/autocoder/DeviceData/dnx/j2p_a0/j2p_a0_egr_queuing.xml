                    <entry idx="5" clock_rate_idx="0" if_id="DATA(egr_queuing,params,egr_if_eventor)" crdt_size="165" irdy_thr="132" txq_max_bytes="800" min_gap_hp="0" fqp_min_gap="0"></entry>
                    <entry idx="6" clock_rate_idx="0" speed="10000" crdt_size="66" irdy_thr="44" txq_max_bytes="32" min_gap_hp="6" fqp_min_gap="8"></entry>
