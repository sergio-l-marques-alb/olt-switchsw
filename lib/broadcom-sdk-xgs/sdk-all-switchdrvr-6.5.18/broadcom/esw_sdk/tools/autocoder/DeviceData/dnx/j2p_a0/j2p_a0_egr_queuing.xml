            <table name="if_speed_params">
                <key name="idx" size="15"></key>
                <key name="clock_rate_idx" size="1"></key>
                <value name="speed" default="-1"></value>
                <value name="if_id" default="-1"></value>
                <value name="crdt_size" default="-1"></value>
                <value name="irdy_thr" default="-1"></value>
                <value name="txq_max_bytes" default="-1"></value>
                <value name="min_gap_hp" default="-1"></value>
                <value name="fqp_min_gap" default="-1"></value>
                <entries>
                    <entry idx="0" clock_rate_idx="0" if_id="DATA(egr_queuing,params,egr_if_cpu)" crdt_size="385" irdy_thr="220" txq_max_bytes="800" min_gap_hp="0" fqp_min_gap="0"></entry>
                    <entry idx="1" clock_rate_idx="0" if_id="DATA(egr_queuing,params,egr_if_sat)" crdt_size="1562" irdy_thr="852" txq_max_bytes="800" min_gap_hp="0" fqp_min_gap="0"></entry>
                    <entry idx="2" clock_rate_idx="0" if_id="DATA(egr_queuing,params,egr_if_oam)" crdt_size="385" irdy_thr="220" txq_max_bytes="800" min_gap_hp="0" fqp_min_gap="0"></entry>
