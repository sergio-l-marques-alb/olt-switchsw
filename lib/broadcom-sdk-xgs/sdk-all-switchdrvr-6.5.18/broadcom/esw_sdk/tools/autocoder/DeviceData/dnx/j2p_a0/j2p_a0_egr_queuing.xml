                    <entry idx="8" clock_rate_idx="0" speed="40000" crdt_size="154" irdy_thr="88" txq_max_bytes="96" min_gap_hp="6" fqp_min_gap="8"></entry>
                    <entry idx="9" clock_rate_idx="0" speed="50000" crdt_size="187" irdy_thr="104" txq_max_bytes="256" min_gap_hp="4" fqp_min_gap="6"></entry>
