<?xml version="1.0"?>
<device_data lib="dnx_data" device="jr2_a0" module="fabric">
    <includes>
        <include>bcm/fabric.h</include>
        <include>bcm/port.h</include>
        <include>include/bcm_int/dnx/fabric/fabric.h</include>
        <include>soc/dnx/dnx_data/auto_generated/dnx_data_device.h</include>
        <include>shared/utilex/utilex_integer_arithmetic.h</include>
    </includes>

    <sub_module name="general">
        <features>
            <feature name="is_supported"></feature>
        </features>

        <defines>
            <define name="nof_lcplls" value="2"></define>
            <define name="fmac_clock_khz" value="892000"></define>
        </defines>

        <numerics>
            <numeric name="connect_mode" value="DNX_FABRIC_NOF_CONNECT_MODES">
                <property name="fabric_connect_mode" method="direct_map">
                    <map name="FE" value="DNX_FABRIC_CONNECT_MODE_FE"></map>
                    <map name="SINGLE_FAP" value="DNX_FABRIC_CONNECT_MODE_SINGLE_FAP"></map>
                    <map name="MESH" value="DNX_FABRIC_CONNECT_MODE_MESH"></map>
                    <doc>
                        Set the connect mode of the Fabric:
                            FE - fabric is connected to FE device.
                            SINGLE_FAP - stand-alone device.
                            MESH - fabric is connected to other FAPs directly.
                    </doc>
                </property>
            </numeric>
        </numerics>

        <tables>
            <table name="fmac_bus_size" mode="map">
                <key name="mode"></key>
                <value name="size" default="0"></value>
                <entries>
                    <entry mode="bcmPortPhyFecBaseR" size="66"></entry>
                    <entry mode="bcmPortPhyFecNone" size="66"></entry>
                    <entry mode="bcmPortPhyFecRs206" size="70"></entry>
                    <entry mode="bcmPortPhyFecRs108" size="70"></entry>
                    <entry mode="bcmPortPhyFecRs545" size="70"></entry>
                    <entry mode="bcmPortPhyFecRs304" size="70"></entry>
                </entries>
            </table>
            
            <table name="pll_phys">
                <key name="lcpll" size="DATA(fabric, general, nof_lcplls)"></key>
                <value name="min_phy_id" default="0"></value>
                <value name="max_phy_id" default="0"></value>
                <entries>
                    <entry lcpll="0" min_phy_id="0" max_phy_id="55"></entry>
                    <entry lcpll="1" min_phy_id="56" max_phy_id="111"></entry>
                </entries>
            </table>

            <table name="pm_properties">
                <key name="pm_index" size="DATA(fabric, blocks, nof_pms)"></key>
                <value name="special_pll_check" default="0"></value>
                <entries>
                    <entry pm_index="10" special_pll_check="1"></entry>
                </entries>
            </table>
          </tables>
    </sub_module>

    <sub_module name="links">

        <defines>
            <define name="nof_links" value="112"></define>
            <define name="nof_links_per_core" value="DATA(fabric,links,nof_links)/DATA(device, general, nof_cores)"></define>
            <define name="max_link_id" value="DATA(fabric,links,nof_links)-1"></define>
            <define name="max_speed" value="53125"></define>
        </defines>

        <numerics>
            <numeric name="core_mapping_mode" value="0">
                <property name="fabric_links_to_core_mapping_mode" method="direct_map">
                    <map name="SHARED" value="0"></map>
                    <doc>
                        Set Fabric link-to-core mapping mode:
                            SHARED - the links are shared between the cores.
                        Default: SHARED
                        Note: this soc property exist only for backward compatibility.
                    </doc>
                </property>
            </numeric>
        </numerics>

        <tables>
            <table name="general">
                <value name="supported_links" default="0xffffffff, 0xffffffff, 0xffffffff, 0xffff"></value>
            </table>
            <table name="polarity" init_only="1">
                <key name="link" size="DATA(fabric,links,nof_links)"></key>
                <value name="tx_polarity" default="0">
                    <property name="phy_tx_polarity_flip" method="custom">
                        <doc>
                            Flips PHY TX polarity if enabled
                            phy_tx_polarity_flip_fabric#link# = 0 / 1
                        </doc>
                    </property>
                </value>
                <value name="rx_polarity" default="0">
                    <property name="phy_rx_polarity_flip" method="custom">
                        <doc>
                            Flips PHY RX polarity if enabled
                            phy_rx_polarity_flip_fabric#link# = 0 / 1
                        </doc>
                    </property>
                </value>
            </table>
        </tables>


    </sub_module>

