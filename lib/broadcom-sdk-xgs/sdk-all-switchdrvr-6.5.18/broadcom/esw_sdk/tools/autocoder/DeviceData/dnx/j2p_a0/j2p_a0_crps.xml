<?xml version="1.0"?>
<device_data lib="dnx_data" device="j2p_a0"  module="crps">

    <sub_module name="engine">
        <features>
            <feature name="valid_address_admission_is_supported" value="1"></feature>
            <feature name="last_input_command_is_supported" value="1"></feature>
        </features>

        <defines>
            <define name="nof_engines" value="34"></define>
            <define name="nof_mid_engines" value="8"></define>
            <define name="nof_big_engines" value="18"></define>
            <define name="nof_databases" value="34"></define>
            <define  name="last_address_nof_bits" value="16"></define>
        </defines>

        <numerics>
        </numerics>

        <tables>
             <table name="engines_info" >
                <key name="engine_id" size="34"></key>
                <value name="nof_counters" default="4096"></value>
                <value name="meter_shared" default="0"></value>
                <entries>
                    <entry engine_id="0" nof_counters="4096" meter_shared="1"></entry>
                    <entry engine_id="1" nof_counters="4096" meter_shared="1"></entry>
                    <entry engine_id="2" nof_counters="4096"></entry>
                    <entry engine_id="3" nof_counters="4096"></entry>
                    <entry engine_id="4" nof_counters="4096"></entry>
                    <entry engine_id="5" nof_counters="4096"></entry>
                    <entry engine_id="6" nof_counters="4096"></entry>
                    <entry engine_id="7" nof_counters="4096"></entry>
                    <entry engine_id="8" nof_counters="8192"></entry>
                    <entry engine_id="9" nof_counters="8192"></entry>
                    <entry engine_id="10" nof_counters="8192"></entry>
                    <entry engine_id="11" nof_counters="8192"></entry>
                    <entry engine_id="12" nof_counters="8192"></entry>
                    <entry engine_id="13" nof_counters="8192"></entry>
                    <entry engine_id="14" nof_counters="8192"></entry>
                    <entry engine_id="15" nof_counters="8192"></entry>
                    <entry engine_id="16" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="17" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="18" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="19" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="20" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="21" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="22" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="23" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="24" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="25" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="26" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="27" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="28" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="29" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="30" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="31" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="32" nof_counters="16384" meter_shared="1"></entry>
                    <entry engine_id="33" nof_counters="16384" meter_shared="1"></entry>
                </entries>
            </table>
        </tables>
    </sub_module>
    <sub_module name="eviction" >
        <defines>
            <define name="seq_address_nof_bits" value="17"></define>
        </defines>        
        <features>
            <feature name="algorithmic_is_supported" value="0"></feature>
            <feature name="probabilistic_is_supported" value="1"></feature>
            <feature name="conditional_action_is_supported" value="1"></feature>
        </features>
        <tables>
            <table name="condional_action_valid">
                <key name="action_idx" size="5"></key>
                <value name="valid" default="0"></value>                
                <entries>
                    <entry action_idx="0" valid="1"></entry> <!--BCM_STAT_EVICTION_CONDIONAL_ACTION_EVICT_COUNTER-->
                    <entry action_idx="1" valid="1"></entry> <!--BCM_STAT_EVICTION_CONDIONAL_ACTION_RESET_COUNTER-->
                    <entry action_idx="2" valid="0"></entry> <!--BCM_STAT_EVICTION_CONDIONAL_ACTION_EVICT_TIME-->
                    <entry action_idx="3" valid="0"></entry> <!--BCM_STAT_EVICTION_CONDIONAL_ACTION_INTERRUPT-->
                    <entry action_idx="4" valid="1"></entry> <!--BCM_STAT_EVICTION_CONDIONAL_ACTION_NO_EVICTION-->
                </entries>              
            </table>           
        </tables>
    </sub_module>
    <sub_module name="expansion">
        <features>
        </features>

        <defines>
        </defines>

        <numerics>
        </numerics>

        <tables>
            <table name="itm_types">
                <value name="valid_types" 
                default="bcmStatExpansionTypeTmDropReason, bcmStatExpansionTypeDispositionIsDrop, bcmStatExpansionTypeTrafficClass, 
                bcmStatExpansionTypeDropPrecedenceMeterResolved, bcmStatExpansionTypeTmLastCopy, bcmStatExpansionTypeMax"></value>
                <value name="total_nof_bits" default="10"></value>               
                <value name="drop_reason_offset" default="0"></value>
                <value name="disposition_offset" default="3"></value>
                <value name="tc_offset" default="4"></value>
                <value name="final_dp_offset" default="7"></value>
                <value name="last_copy_offset" default="9"></value>
            </table>        
        </tables>
    </sub_module>   
       
</device_data>

