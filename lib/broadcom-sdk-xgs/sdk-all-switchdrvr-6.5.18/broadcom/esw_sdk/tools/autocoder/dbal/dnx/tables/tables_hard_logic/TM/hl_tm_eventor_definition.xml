<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--
    ******************************************************
    DBAL Hard-Logic Logical Tables:
    This xml contains the hard-logic logical DB for the DBAL.
    The data is loaded to the device during init process
    ******************************************************

    DB Rules:
    *********
--><AppDbCatalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../hard_logic_schema.xsd">

    <!-- Eventor Ingress PP fifo configurations -->
    <AppDB Name="EVENTOR_IPP_FIFO_CFG">
        <OneLineDescription>Control fifo of requests to eventor from ingress PP (IPP) block</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="EVENTOR_ALMOST_FULL_TH" Size="4" Type="UINT"/>
                <Field Name="EVENTOR_CODE" Size="8" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="EVENTOR_ALMOST_FULL_TH">
                    <Access>
                        <Register Name="IPPC_EVENTOR_FIFO_CFGr">
                            <HwField Name="EVENTOR_ALMOST_FULL_THf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="EVENTOR_CODE">
                   <Access>
                        <Register Name="IPPC_EVENTOR_FIFO_CFGr">
                            <HwField Name="EVENTOR_CODEf"/>
                        </Register>
                    </Access>
                </Field>                
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <!-- Control blocks requests to eventor -->
    <AppDB Name="EVENTOR_TX_HANDLER_REQ_EN">
        <OneLineDescription>Enable/disable external block requests for Eventor TX</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="TX_HANDLER_REQ_EN" Size="16" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="TX_HANDLER_REQ_EN">
                    <Access>
                        <Register Name="EVNT_TX_HANDLER_REQ_ENr">
                            <HwField Name="TX_HANDLER_REQ_ENABLEf"/>
                        </Register>
                    </Access>
                </Field>              
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <!-- Eventor Core (pipe) selection -->
    <AppDB Name="EVENTOR_PIPE_SELECTOR">
        <OneLineDescription>Select core for eventor proccessor (ingress and egress)</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Device="J2C_A0 Q2A_A0 Q2A_B0" Jira="SDK-111111" Level="PARTIALLY_FUNCTIONAL"/> <!-- The only entry that's allowed for single core devices is also the default entry (value 0), dbal tests fails for such tables -->
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field MaxValue="DNX_DATA.device.general.nof_cores-1" Name="EVENTOR_PIPE_ID" Size="DNX_DATA.device.general.core_max_nof_bits" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="EVENTOR_PIPE_ID">
                    <Access>
                        <Register Name="EVNT_PIPE_SELECTORr">
                            <HwField Name="IRE_SEL_PIPEf"/>
                        </Register>
                    </Access>
                    <Access>
                        <Register Name="EVNT_PIPE_SELECTORr">
                            <HwField Name="EPNI_SEL_PIPEf"/>
                        </Register>
                    </Access>                    
                </Field>              
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <!-- Eventor RX header size (geneated by ETPP) -->
    <AppDB Name="EVENTOR_RX_EVPCK_HEADER_STACK_SIZE">
        <OneLineDescription>Eventor RX packet header size (generated by ETPP)</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="EVENTOR_TRANSMIT_HEADER_SIZE" Size="5" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="EVENTOR_TRANSMIT_HEADER_SIZE">
                    <Access>
                        <Register Name="EVNT_RX_EVPCK_HEADER_STACK_SIZEr">
                            <HwField Name="RX_EVPCK_HEADER_SIZEf"/>
                        </Register>
                    </Access>                  
                </Field>              
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

    <!-- Eventor RX sequence check enable/disable -->
    <AppDB Name="EVENTOR_RX_SEQUENCE_CHECK">
       <OneLineDescription>Eventor RX packet sequence check enable/disable</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="SEQUENCE_CHECK_ENABLE" Size="DNX_DATA.instru.eventor.nof_contexts" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="SEQUENCE_CHECK_ENABLE">
                    <Access>
                        <Register Name="EVNT_RX_SEQ_CHECKr">
                            <HwField Name="ENABLEf"/>
                        </Register>
                    </Access>                  
                </Field>              
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <!-- Engine TX configuration -->
    <AppDB Name="EVENTOR_TX_ENG_CFG">
       <OneLineDescription>Eventor tx_engine configurations</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                 <Field Name="EVENTOR_TX_ENGINE_ID" Size="DNX_DATA.instru.eventor.nof_contexts_bits" Type="UINT"/>
            </Key>
            <Result>
                <Field Name="PADDING_ENABLE" Type="BOOL"/>
                <Field Name="CONTEXT_QUEUE_TIMEOUT" Size="32" Type="UINT"/>
                <Field Name="CONTEXT_QUEUE_MAX_SIZE" Size="11" Type="UINT"/>                     
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="PADDING_ENABLE">
                    <Access>
                        <Register Name="EVNT_TX_ENG_EVPCK_PADDING_ENABLEr">
                           <DataOffset Formula="#KEY_VALUE(EVENTOR_TX_ENGINE_ID)"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="CONTEXT_QUEUE_TIMEOUT">
                    <Access>
                        <Register Name="EVNT_TX_ENG_CONTEXT_QUEUE_TIMEOUT_VALUEr">
                           <ArrayOffset Formula="#KEY_VALUE(EVENTOR_TX_ENGINE_ID)"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="CONTEXT_QUEUE_MAX_SIZE">
                    <Access>
                        <Register Name="EVNT_TX_ENG_CONTEXT_QUEUE_MAX_SIZEr">
                           <ArrayOffset Formula="#KEY_VALUE(EVENTOR_TX_ENGINE_ID)"/>
                        </Register>
                    </Access>
                </Field>                                                  
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <!-- Context TX configuration -->
    <AppDB Name="EVENTOR_TX_CONTEXT_QUEUE_TYPE">
       <OneLineDescription>Eventor tx_context configurations</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                 <Field Name="EVENTOR_CONTEXT_ID"/>
            </Key>
            <Result>
                <Field Description="Value of 1 means standard, 0 non standard" Name="IS_STANDARD_QUEUE_TYPE" Type="BOOL"/>                       
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="IS_STANDARD_QUEUE_TYPE">
                    <Access>
                        <Register Name="EVNT_TX_ENG_CONTEXT_QUEUE_TYPEr">
                           <DataOffset Formula="#KEY_VALUE(EVENTOR_CONTEXT_ID)"/>
                        </Register>
                    </Access>
                </Field>                                                  
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>    
    
    <!-- RX to TX mapping -->
    <AppDB Name="EVENTOR_RX_CONTEXT_TO_TX_MAP" Valid="DNX_DATA.instru.eventor.is_supported">
       <OneLineDescription>Eventor Map RX context to TX</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="ENABLE_MAP" NofInstances="DNX_DATA.instru.eventor.nof_contexts" Type="BOOL"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="ENABLE_MAP">
                    <Access instanceIndex="ALL">
                        <Register Name="EVNT_RX_TO_TX_BUFFERr">
                           <DataOffset Formula="#INSTANCE_IDX()"/>
                        </Register>
                    </Access>
                </Field>                      
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <!-- TX contex is fifo dma -->
    <AppDB Name="EVENTOR_TX_CONTEXT_IS_FIFO_DMA">
       <OneLineDescription>Eventor TX context is fifo dma</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="IS_FIFO_DMA" NofInstances="DNX_DATA.instru.eventor.nof_contexts" Type="BOOL"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="IS_FIFO_DMA">
                    <Access instanceIndex="ALL">
                        <Register Name="EVNT_TX_CONTEXT_IS_FIFO_DMAr">
                           <DataOffset Formula="#INSTANCE_IDX()"/>
                        </Register>
                    </Access>
                </Field>                      
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <!-- TX context buffer threshold for polling DMA done bit (used in sbus and fifo dma) -->
    <AppDB Name="EVENTOR_TX_CONTEXT_MAIN_BUFFER_TH">
       <OneLineDescription>Eventor TX context buffer threshold for polling DMA done bit (used in sbus and fifo dma)</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="TX_BUFFER_TH" NofInstances="DNX_DATA.instru.eventor.nof_contexts" Size="9" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="TX_BUFFER_TH">
                    <Access instanceIndex="ALL">
                        <Register Name="EVNT_TX_MAIN_BUFFER_THr">
                           <DataOffset Formula="#INSTANCE_IDX()*9"/>
                        </Register>
                    </Access>
                </Field>                      
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <!-- RX context buffer threshold for polling DMA done bit (used in sbus and fifo dma) -->
    <AppDB Name="EVENTOR_RX_CONTEXT_MAIN_BUFFER_TH">
       <OneLineDescription>Eventor RX context buffer threshold for polling DMA done bit (used in sbus and fifo dma)</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="RX_BUFFER_TH" NofInstances="DNX_DATA.instru.eventor.nof_contexts" Size="9" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="RX_BUFFER_TH">
                    <Access instanceIndex="ALL">
                        <Register Name="EVNT_RX_MAIN_BUFFER_THr">
                           <DataOffset Formula="#INSTANCE_IDX()*9"/>
                        </Register>
                    </Access>
                </Field>                      
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>    
    
    <!-- Eventor tx header stack per context -->
    <AppDB Name="EVENTOR_TX_CONTEXT_EVPCK_HEADER_STACK">
       <OneLineDescription>Eventor TX context header stack</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="EVENTOR_CONTEXT_ID"/>
            </Key>
            <Result>
                <Field Description="Eventor TX context header stack size in bytes" Name="EVPCK_HEADER_STACK_SIZE" Size="5" Type="UINT"/>
                <Field Description="Eventor TX context header stack - enable bit" Name="EVPCK_HEADER_STACK_EN" Size="1" Type="UINT"/>
                <Field Description="Eventor TX context header - Number Of Samples field (applicable only when EVPCK_HEADER_STACK_EN = 0)" Name="EVPCK_HEADER_STACK_NOF_SAMPLES" Size="8" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="EVPCK_HEADER_STACK_SIZE">
                    <Access>
                        <Register Name="EVNT_EVPCK_HEADER_STACK_SIZEr">
                           <ArrayOffset Formula="#KEY_VALUE(EVENTOR_CONTEXT_ID)"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="EVPCK_HEADER_STACK_EN">
                    <Access>
                        <Register Name="EVNT_EVPCK_HEADER_STACKr">
                            <ArrayOffset Formula="#KEY_VALUE(EVENTOR_CONTEXT_ID)"/>
                            <HwField Name="EVPCK_HEADER_STACK_ENf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="EVPCK_HEADER_STACK_NOF_SAMPLES">
                    <Access>
                        <Register Name="EVNT_EVPCK_HEADER_STACKr">
                            <ArrayOffset Formula="#KEY_VALUE(EVENTOR_CONTEXT_ID)"/>
                            <HwField Name="EVPCK_HEADER_STACK_DESTINATIONf"/>
                        </Register>
                    </Access>
                </Field>                      
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <!-- Eventor header stack per builder -->
    <AppDB Name="EVENTOR_BUILDER_TRANSMIT_HEADER_STACK">
       <OneLineDescription>Eventor builder header stack</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="EVENTOR_BUILDER_ID"/>
            </Key>
            <Result>
                <Field Description="Eventor builder header stack size in bytes" Name="TRANSMIT_HEADER_STACK_SIZE" Size="7" Type="UINT"/>
                <Field Description="Eventor builder header stack data, high 512 bits (64 bytes)" Name="TRANSMIT_HEADER_STACK_DATA_HIGH" Size="512" Type="ARRAY8"/>
                <Field Description="Eventor builder header stack data, low 512 bits (64 bytes)" Name="TRANSMIT_HEADER_STACK_DATA_LOW" Size="512" Type="ARRAY8"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="TRANSMIT_HEADER_STACK_SIZE">
                    <Access>
                        <Register Name="EVNT_TRANSMIT_HEADER_STACK_SIZEr">
                           <ArrayOffset Formula="#KEY_VALUE(EVENTOR_BUILDER_ID)"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="TRANSMIT_HEADER_STACK_DATA_HIGH">
                    <Access>
                        <Register Name="EVNT_TRANSMIT_HEADER_STACKr">
                           <ArrayOffset Formula="2*#KEY_VALUE(EVENTOR_BUILDER_ID)+1"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="TRANSMIT_HEADER_STACK_DATA_LOW">
                    <Access>
                        <Register Name="EVNT_TRANSMIT_HEADER_STACKr">
                           <ArrayOffset Formula="2*#KEY_VALUE(EVENTOR_BUILDER_ID)"/>
                        </Register>
                    </Access>
                </Field>                      
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>   
    
    <!-- bank type - tx or rx -->
    <AppDB Name="EVENTOR_BANK_TYPE">
       <OneLineDescription>Eventor bank type - TX or RX</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="EVENTOR_SRAM_BANK_ID" Size="DNX_DATA.instru.eventor.sram_bank_bits" Type="UINT"/>
            </Key>
            <Result>
                <Field Name="IS_TX_BANK" Type="BOOL"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="IS_TX_BANK">
                    <Access>
                        <Register Name="EVNT_MAIN_MEM_BANK_TYPEr">
                           <ArrayOffset Formula="#KEY_VALUE(EVENTOR_SRAM_BANK_ID)"/>
                        </Register>
                    </Access>
                </Field>                      
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <!-- Eventor SRAM buffer regions per context -->
    <AppDB Name="EVENTOR_SPACE_REGIONS">
       <OneLineDescription>Eventor bank type - TX or RX</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="IS_TX_CONTEXT" Type="BOOL"/>
                <Field Name="EVENTOR_CONTEXT_ID"/>
            </Key>
            <Result>
                <Field Name="BUFFER_START_ADDR" NofInstances="2" Size="15" Type="UINT"/>
                <Field Name="BUFFER_END_ADDR" NofInstances="2" Size="15" Type="UINT"/>               
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="BUFFER_START_ADDR">
                    <Access instanceIndex="ALL">
                        <Memory Name="EVNT_SPACE_REGIONSm">
                           <ArrayOffset Formula="#KEY_VALUE(IS_TX_CONTEXT)"/>
                           <EntryOffset Formula="2*#KEY_VALUE(EVENTOR_CONTEXT_ID)+#INSTANCE_IDX()"/>
                           <HwField Name="BUFFER_START_ADDRf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="BUFFER_END_ADDR">
                    <Access instanceIndex="ALL">
                        <Memory Name="EVNT_SPACE_REGIONSm">
                           <ArrayOffset Formula="#KEY_VALUE(IS_TX_CONTEXT)"/>
                           <EntryOffset Formula="2*#KEY_VALUE(EVENTOR_CONTEXT_ID)+#INSTANCE_IDX()"/>
                           <HwField Name="BUFFER_END_ADDRf"/>
                        </Memory>
                    </Access>
                </Field>                                       
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <!-- Eventor buffer descriptors contexts address in CMIC -->
    <AppDB Name="EVENTOR_CONTEXT_PROPERITES">
       <OneLineDescription>Eventor buffer descriptors contexts address in CMIC</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="IS_TX_CONTEXT" Type="BOOL"/>
                <Field Name="EVENTOR_CONTEXT_ID"/>
            </Key>
            <Result>
                <Field Name="CMIC_BUFFER_START_ADDR" NofInstances="2" Size="32" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV INSTRU</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="CMIC_BUFFER_START_ADDR">
                    <Access instanceIndex="ALL">
                        <Memory Name="EVNT_CONTEXT_PROPERTIESm">
                           <ArrayOffset Formula="#KEY_VALUE(IS_TX_CONTEXT)"/>
                           <EntryOffset Formula="#KEY_VALUE(EVENTOR_CONTEXT_ID)"/>
                           <DataOffset Formula="#INSTANCE_IDX()*32"/>
                        </Memory>
                    </Access>
                </Field>                                   
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>          
        
</AppDbCatalog>
