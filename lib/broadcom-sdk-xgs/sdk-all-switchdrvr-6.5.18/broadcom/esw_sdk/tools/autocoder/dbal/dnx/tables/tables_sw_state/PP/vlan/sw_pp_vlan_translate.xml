        <AccessMethod>SW_STATE</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="INCOMING_TAG_STRUCTURE" Description="TAG-STARUCT HW-ID"/>
            </Key>
            <Result>
                <Field Name="BCM_TAG_FORMAT_CLASS_ID" Size="5" Type="UINT" Description="TAG-STARUCT SW-ID"/>
            </Result>
            <TableLabels>L2</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping> 
            <CoreMode Mode="SBC"/>
        </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="TAG_STRUCT_INGRESS_SW_ID_TO_HW_ID">
        <OneLineDescription>sw state table - ingress tag_struct - from SW-ID to HW-ID</OneLineDescription>
        <AccessMethod>SW_STATE</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
