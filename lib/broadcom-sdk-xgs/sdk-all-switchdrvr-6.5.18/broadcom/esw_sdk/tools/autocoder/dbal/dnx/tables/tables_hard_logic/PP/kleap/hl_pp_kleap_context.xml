    <AppDB Name="KLEAP_VTA_TCAM_HIT_INDICATION">
        <OneLineDescription>Indicate which TCAM line was hit in VTA</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <Maturity Device="JR2_A0 JR2_B0" Level="HIGH_SKIP_DEVICE"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="ROW_IDX" Size="4" Type="UINT"/>
                <Field MaxValue="7" Name="ENTRY_IDX" Size="3" Type="UINT"/>
            </Key>
            <Result>
                <Field Name="TCAM_HIT" Permission="READONLY" Type="BOOL"/>
            </Result>
            <TableLabels>FIELD</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="TCAM_HIT">
                    <Access>
                        <Memory Name="IPPF_VTACS_TCAM_HIT_INDICATIONm"> 
                            <EntryOffset Formula="#KEY_VALUE(ROW_IDX)"/>
                            <HwField Name="VTACS_HITf"/>
                            <DataOffset Formula="#KEY_VALUE(ENTRY_IDX)"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

