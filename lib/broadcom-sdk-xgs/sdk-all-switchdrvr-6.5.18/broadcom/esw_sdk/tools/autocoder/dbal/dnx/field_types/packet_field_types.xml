    <FieldType Name="VXLAN_NEXT_PROTOCOL" Size="8" Print="ENUM">
        <OneLineDescription>Pemla config parser next protocol</OneLineDescription>
        <FieldDbInterface>
            <EnumValues>
                <EnumVal Name="DEFAULT"             Description="DEFAULT"/>
                <EnumVal Name="IPV4"                Description="Next protocol after VXLAN is IPv4"/>
                <EnumVal Name="IPV6"                Description="Next protocol after VXLAN is IPv6"/>
                <EnumVal Name="ETHERNET"            Description="Next protocol after VXLAN is Ethernet"/>
                <EnumVal Name="NSH"                 Description="Next protocol after VXLAN is Network Service Header"/>
                <EnumVal Name="MPLS"                Description="Next protocol after VXLAN is MPLS"/>
                <EnumVal Name="IPT"                 Description="Next protocol after VXLAN is IPT"/>
            </EnumValues>
        </FieldDbInterface>
        <FieldToPhyDbMapping>
            <EnumValueMapping>
                <EnumVal Name="DEFAULT"             Value="0"/>
                <EnumVal Name="IPV4"                Value="1"/>
                <EnumVal Name="IPV6"                Value="2"/>
                <EnumVal Name="ETHERNET"            Value="3"/>
                <EnumVal Name="NSH"                 Value="4"/>
                <EnumVal Name="MPLS"                Value="5"/>
                <EnumVal Name="IPT"                 Value="6"/>
             </EnumValueMapping>
        </FieldToPhyDbMapping>
    </FieldType>

