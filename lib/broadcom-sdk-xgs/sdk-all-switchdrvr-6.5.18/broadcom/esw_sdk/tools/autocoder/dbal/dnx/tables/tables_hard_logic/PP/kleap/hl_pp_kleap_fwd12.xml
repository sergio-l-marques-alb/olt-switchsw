        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="KBR_SELECTOR" NofInstances="18" Type="KLEAP_STAGE_SELECTOR"/>
                <Field Name="FFC_GROUP_SELECTOR" NofInstances="5" Type="KLEAP_STAGE_SELECTOR"/>
                <Field Name="SHR_PD_SELECTOR" NofInstances="2" Type="KLEAP_STAGE_SELECTOR"/>
            </Result>
            <TableLabels>L3</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping> 
            <CoreMode Mode="SBC"/>
            <FieldMapping>
