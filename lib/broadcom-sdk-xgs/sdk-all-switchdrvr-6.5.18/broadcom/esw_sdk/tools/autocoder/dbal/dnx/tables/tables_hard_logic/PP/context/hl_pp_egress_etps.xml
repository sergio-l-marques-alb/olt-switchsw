    <AppDB Name="TERM_ETPS_TYPE_MAP" Valid="DNX_DATA.dev_init.context.context_selection_profile">
        <OneLineDescription>Map ETPS Type to Profile for Forwarding Context Selection</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="TERM_ETPS_TYPE" Type="ETPS_TYPE"/>
            </Key>
            <Result>
                <Field Description="ETPS Type Profile Value" Name="TERM_ETPS_TYPE_PROFILE" Size="6" Type="HEXA"/>
               </Result>
            <TableLabels>L3</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="TERM_ETPS_TYPE_PROFILE">
                    <Access>
                        <Memory Name="ETPPC_FORWARDING_ETPS_TYPE_PROFILEm">
                            <HwField Name="FORWARDING_ETPS_TYPE_PROFILEf"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

