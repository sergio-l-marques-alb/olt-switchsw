    <AppDB Name="SYNCE_ENABLE_CTRL">
        <OneLineDescription>This table is used for enabling the SyncE functionality.</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID"/>
            </Key>
            <Result>
                <Field Description="Reset the SyncE block" Name="RESET" Type="NIF_RESET_REVERSE_LOGIC"/>
            </Result>
            <TableLabels>J2P_NOTREV SYNCE</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping Device="JR2_A0 JR2_B0">
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="RESET">
                    <Access>
                        <KeyCondition Field="CORE_ID" Type="EQUAL_TO" Value="0"/>
                        <Register Name="ECI_SYNC_E_RSTNr">
                            <HwField Name="NIF_CORE_0_SYNC_E_RSTNf"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="CORE_ID" Type="EQUAL_TO" Value="1"/>
                        <Register Name="ECI_SYNC_E_RSTNr">
                            <HwField Name="NIF_CORE_1_SYNC_E_RSTNf"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
        <AppToPhyDbMapping Device="J2C_A0 Q2A_A0 Q2A_B0">
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="RESET">
                    <Access>
                        <Register Name="ECI_SYNC_E_RSTNr">
                            <HwField Name="NIF_SYNC_E_RSTNf"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
