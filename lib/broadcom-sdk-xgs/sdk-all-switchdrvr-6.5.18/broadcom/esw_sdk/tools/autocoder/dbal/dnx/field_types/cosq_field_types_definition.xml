     <FieldType Name="RCI_BIASING_MODE" Size="9" Print="ENUM">
        <OneLineDescription>RCI biasing mode definition</OneLineDescription>
        <FieldDbInterface>
            <EnumValues>
                <EnumVal Name="INVALID" Description="Invalid mode"/>
                <EnumVal Name="GLOBAL" Description="Global (default) biasing mode"/>
                <EnumVal Name="DEVICE_BIASING" Description="Device biasing mode"/>
                <EnumVal Name="FLOW_BIASING" Description="Flow biasing mode"/>
            </EnumValues>
        </FieldDbInterface>
        <FieldToPhyDbMapping>
            <EnumValueMapping>
                <!-- The mapping to HW is done as the following
                  bit 0: SCH_RCI_PARAMS.RCI_ENA
                  bit 1: SCH_RCI_PARAMS.DEVICE_RCI_ENA
                      2: SCH_RCI_PARAMS.SHARED_FABRIC_LINKS_MODE
                      3: SCH_RCI_PARAMS.FABRIC_RCI_DISCARD_MODE
                      4: SCH_RCI_PARAMS.USE_ONLY_LOCAL_RCI_FOR_CORE_RCI
                      5: SCH_RCI_PARAMS.RCI_UNLINK_ENA
                      6: SCH_RCI_PARAMS.RCI_UNLINK_AGGREGATE_ENA
                      7: SCH_RCI_BFC_PARAMS.RCI_TRIGGERS_FLOW_CONTROL_ENA
                      8: SCH_RCI_CREDIT_JITTER.CRDT_JTR_EN
                -->
                <EnumVal Name="INVALID" Value="0"/> 
                <EnumVal Name="GLOBAL" Value="23"/> <!-- 0x17 - 
                                                    RCI_ENA, DEVICE_RCI_ENA, SHARED_FABRIC_LINKS_MODE, (0x7) 
                                                    USE_ONLY_LOCAL_RCI_FOR_CORE_RCI (0x10) -->
                <EnumVal Name="DEVICE_BIASING" Value="287"/> <!-- 0x11f 
                                               RCI_ENA, DEVICE_RCI_ENA, SHARED_FABRIC_LINKS_MODE, FABRIC_RCI_DISCARD_MODE, (0xf)
                                               USE_ONLY_LOCAL_RCI_FOR_CORE_RCI, (0x10)
                                               CRDT_JTR_EN (0x100)
                                                             -->
                <EnumVal Name="FLOW_BIASING" Value="191"/> <!-- 0xbf -
                                                           RCI_ENA, DEVICE_RCI_ENA, SHARED_FABRIC_LINKS_MODE, FABRIC_RCI_DISCARD_MODE, (0xf)
                                                           USE_ONLY_LOCAL_RCI_FOR_CORE_RCI, RCI_UNLINK_ENA, RCI_TRIGGERS_FLOW_CONTROL_ENA (0xb0)
                                                            -->
             </EnumValueMapping>
        </FieldToPhyDbMapping>
    </FieldType>

