                <EnumVal Name="2" Description="SP1,SP2,SP3-WFQ(2)"/>
                <EnumVal Name="3" Description="SP1-WFQ(1:63),SP2 or SP1-WFQ(3),SP2"/>
                <EnumVal Name="4" Description="SP1,SP2-WFQ(1:63) or SP1,SP2-WFQ(3)"/>
