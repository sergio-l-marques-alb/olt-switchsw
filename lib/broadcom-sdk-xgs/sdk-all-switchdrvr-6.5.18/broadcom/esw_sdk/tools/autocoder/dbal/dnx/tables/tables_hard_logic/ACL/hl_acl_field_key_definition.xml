    <AppDB Name="FIELD_IPMF1_LOOKUP_ENABLER">
        <OneLineDescription>This table provides lookup enabler per conext per lookup type.</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/> 
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="FIELD_PMF_CTX_ID"/>
                <Field Name="PMF_LOOKUP_TYPE"/>
            </Key>
            <Result>
                <Field Name="ENABLE" Size="1" Type="BOOL"/>
            </Result>
            <TableLabels>FIELD</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="ENABLE">
                    <Access>
                        <KeyCondition Enum="TCAM_0" Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="4*#KEY_VALUE(FIELD_PMF_CTX_ID)"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Enum="TCAM_1" Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="4*#KEY_VALUE(FIELD_PMF_CTX_ID)+1"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Enum="TCAM_2" Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="4*#KEY_VALUE(FIELD_PMF_CTX_ID)+2"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Enum="TCAM_3" Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="4*#KEY_VALUE(FIELD_PMF_CTX_ID)+3"/>
                        </Register>
                    </Access>     
                    <Access>
                        <KeyCondition Enum="EXEM" Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="#KEY_VALUE(FIELD_PMF_CTX_ID)+256"/>
                        </Register>
                    </Access>   
                    <Access>
                        <KeyCondition Enum="DIRECT" Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="#KEY_VALUE(FIELD_PMF_CTX_ID)+320"/>
                        </Register>
                    </Access> 
                    <Access>
                        <KeyCondition Enum="STATE_TABLE" Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="#KEY_VALUE(FIELD_PMF_CTX_ID)+384"/>
                        </Register>
                    </Access>  
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

