    <AppDB Name="PRT_VIRTUAL_PORT_TCAM">
        <OneLineDescription>This is logical table of PRT port info selection - this table is built to support half mode, in full mode configuration</OneLineDescription>
        <AccessMethod>TCAM_CS</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="TCAM_BY_ID">
            <Key>
                <Field Name="PRT_KBR_INFO_HALF"/>
            </Key>
            <Result>
                <Field Name="PP_PORT"/>
                <Field Name="SOURCE_SYSTEM_PORT" Type="SYSTEM_PORT"/>
            </Result>
            <TableLabels>PP_PORT</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <TcamCsDb>PRT</TcamCsDb>
            <FieldMappingTcam>
                <Type Name="RESULT">
                    <Access>
                        <Memory Name="IPPE_VIRTUAL_PORT_ADm">
                            <DataOffset Formula="0"/>
                        </Memory>
                    </Access>
                </Type>
            </FieldMappingTcam>
        </AppToPhyDbMapping>
    </AppDB>

