    <Maturity Level="HIGH"/>
    <AppDbInterface Type="EM">
        <Key>
            <Field Name="GLOB_OUT_LIF" Size="22" MinValue="DNX_DATA.l3.rif.nof_rifs"/>
            <Field Name="CORE_ID" Size="1"/>
        </Key>
            <Result>
                <Field Name="OUT_LIF" Size="DNX_DATA.lif.out_lif.outlif_pointer_size"/>
                <Field Name="ERPP_OUT_LIF_PROFILE" Size="DNX_DATA.lif.out_lif.outlif_profile_width" Type="OUT_LIF_PROFILE" Valid="DNX_DATA.lif.out_lif.outlif_profile_width"/>
