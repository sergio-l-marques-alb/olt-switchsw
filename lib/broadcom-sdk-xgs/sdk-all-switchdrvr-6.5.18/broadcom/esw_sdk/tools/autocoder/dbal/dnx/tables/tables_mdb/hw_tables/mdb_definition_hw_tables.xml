<AppDB Name="OAM_TCAM_IDENTIFICATION_DB">
    <OneLineDescription>OAM TCAM assisted identification</OneLineDescription>
    <AccessMethod>MDB</AccessMethod>
    <Maturity Level="HIGH"/>
    <AppDbInterface Type="TCAM">
        <Key>
            <Field Name="LAYER_TYPE_CURRENT_LAYER_MINUS_1" Size="5" Type="LAYER_TYPES"/>
            <Field Name="LAYER_TYPE_CURRENT_LAYER" Size="5" Type="LAYER_TYPES"/>
            <Field Name="IN_LIF_PROFILE" Size="8"/>
            <Field Name="NOF_VALID_OAM_LIFS" Size="2" Type="UINT"/>
            <Field Name="PKT_DATA_AT_FWD_HEADER" Size="300" Type="ARRAY32"/>
        </Key>
        <Result>
            <Field Name="PACKET_IS_OAM" Size="1"/>
            <Field Name="OAM_IS_BFD" Size="1" Type="BOOL"/>
            <Field Name="OAM_OPCODE" Size="4"/>
            <Field Name="OAM_MD_LEVEL" Size="3" Type="UINT"/>
            <Field Name="OAM_YOUR_DISCR" Size="1"/>
            <Field Name="OAM_LIF_TCAM_INSTRUCTION" Size="23" Type="UINT"/>
            <Field Name="MY_CFM_MAC" Size="1"/>
            <Field Name="OAM_OFFSET" Size="8"/>
            <Field Name="OAM_STAMP_OFFSET" Size="8"/>
            <Field Name="OAM_PCP" Size="3" Type="UINT"/>
        </Result>
        <TableLabels>L2 L3 MPLS</TableLabels>
    </AppDbInterface>
    <AppToPhyDbMapping>
        <CoreMode Mode="SBC"/>
        <PhyDb>TCAM</PhyDb>
        <Stage>FWD1</Stage>
        <AppDbId Size="0" Value="0"/>
    </AppToPhyDbMapping>
</AppDB>

    <AppDB Name="EEDB_DATA_ENTRY">
        <OneLineDescription>Empty</OneLineDescription>
        <AccessMethod>MDB</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="OUT_LIF" Size="20"/>
            </Key>
            <MultipleResults>
                <Result>
                    <Field Name="RESULT_TYPE"         Size="4"    Value="DIP6"    Description="Empty"/>
                    <Field Name="DATA_ENTRY_RESERVED" />
                    <Field Name="DIP6" Type="IPV6"/>
                    <Field Name="NEXT_OUTLIF_POINTER" Type="EEDB_LL_NEXT_POINTER"/>
                </Result>
                <Result>
                    <Field Name="RESULT_TYPE"         Size="4"    Value="SFLOW_RAW_DATA"    Description="Empty"/>
                    <Field Name="DATA_ENTRY_RESERVED" />
                    <Field Name="SFLOW_DATA_EMPTY" Size="128" Type="ARRAY32"/>
                    <Field Name="NEXT_OUTLIF_POINTER" Type="EEDB_LL_NEXT_POINTER"/>
                </Result>
                <Result>
                    <Field Name="RESULT_TYPE"         Size="4"    Value="SRV6_SID"    Description="Empty"/>
                    <Field Name="DATA_ENTRY_RESERVED" />
                    <Field Name="SID" Type="IPV6"/>
                    <Field Name="NEXT_OUTLIF_POINTER" Type="EEDB_LL_NEXT_POINTER"/>
                </Result>
                <Result>
                    <Field Name="RESULT_TYPE"         Size="4"    Value="LI_VAR_SIZE"    Description="Empty"/>
                    <Field Name="HW_ENTRY_TYPE" Type="ETPP_ETPS_FORMATS" ConstValue="ETPS_DATA_VAR_SIZE_DATA"/>
                    <Field Name="LI_CONTENT_ID" />
                    <Field Name="RAW_DATA_SIZE" />
                    <Field Name="STAT_OBJECT_ID" Size="17"/>
                    <Field Name="STAT_OBJECT_CMD" Size="5"/>
                    <Field Name="EEDB_LL_EXTRA_DATA" Size="10"/>
                    <Field Name="NEXT_OUTLIF_POINTER" Type="EEDB_LL_NEXT_POINTER"/>
                </Result>
            </MultipleResults>
            <TableLabels>L3 SFLOW</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <PhyDb>EEDB2</PhyDb>
            <MultipleResultsMapping SwState="1">
                <Result LogicalValue="DIP6" Value="1"/>
                <Result LogicalValue="SFLOW_RAW_DATA" Value="2"/>
                <Result LogicalValue="SRV6_SID" Value="3"/>
                <Result LogicalValue="LI_VAR_SIZE" Value="4"/>
            </MultipleResultsMapping>
        </AppToPhyDbMapping>
    </AppDB>

