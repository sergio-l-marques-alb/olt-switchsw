        <PhyDb>GLEM1</PhyDb>
        <AppDbId Size="0" Value="0"/>
    </AppToPhyDbMapping>
</AppDB>

<AppDB Name="GLOBAL_RIF_EM">
    <OneLineDescription>Mapping Global-Out-RIF into Local-Out-RIF. 
    The tabe should have the same key size and AppToPhyDbMapping as GLOBAL_LIF_EM </OneLineDescription>
    <AccessMethod>MDB</AccessMethod>
    <Maturity Level="HIGH"/>
    <AppDbInterface Type="EM">
        <Key>
            <Field Name="GLOB_OUT_RIF" Type="UINT" Size="22" MaxValue="DNX_DATA.l3.rif.nof_rifs-1"/>
            <Field Name="CORE_ID" Size="1"/>
        </Key>
            <Result>
                <Field Name="ERPP_OUT_LIF_PROFILE" Size="4" NofInstances="4" Type="OUT_LIF_PROFILE"/>
		<Field Name="ZERO_PADDING" Size="DNX_DATA.lif.out_lif.outlif_profile_width+DNX_DATA.lif.out_lif.outlif_pointer_size-16" Description="temp W.A to make the payload size equal to GLOBAL_LIF_EM"/>
            </Result>
        <TableLabels>L2 L3 MPLS</TableLabels>
    </AppDbInterface>
    <AppToPhyDbMapping>
        <CoreMode Mode="DPC"/>
        <PhyDb>GLEM1</PhyDb>
        <AppDbId Size="0" Value="0"/>
    </AppToPhyDbMapping>
</AppDB>

