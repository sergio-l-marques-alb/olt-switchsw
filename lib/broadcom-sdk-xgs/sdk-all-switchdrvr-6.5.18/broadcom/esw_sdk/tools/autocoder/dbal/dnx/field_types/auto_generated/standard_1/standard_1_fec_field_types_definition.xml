<FieldType Name="FEC_DESTINATION_MC_RPF" Print="STRUCTURE" Size="24">
    <OneLineDescription>Typical JR2 use Cases:
                         (1)FEC KAPS result enumeration (i.e. SUPER_FEC is SUPER_FEC_NO_PROTECTION_W_2_STAT FEC)
                         (2)Multicast into MC_Group with &quot;MC SIP Based RPF&quot; (i.e. destination is MC_Group)
                            Applicable to JR1 backward compatibility as well</OneLineDescription>
    <FieldDbInterface>
        <StructFields>
            <StructField Name="MC_RPF_MODE" Size="3"/>
            <StructField Name="DESTINATION" Size="21"/>
        </StructFields>
    </FieldDbInterface>
    <FieldToPhyDbMapping/>
</FieldType>


