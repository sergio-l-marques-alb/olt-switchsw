            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="BUCKET_FILL_RATE">
                    <Access>
                        <Register Name="FMAC_LEAKY_BUCKET_CONTROL_REGISTERr">
                            <ArrayOffset Formula="#KEY_VALUE(FABRIC_LINK)%dnx_data_fabric.blocks.nof_links_in_fmac_get(unit)"/>
                            <BlockIndex Formula="#KEY_VALUE(FABRIC_LINK)/dnx_data_fabric.blocks.nof_links_in_fmac_get(unit)"/>
                            <HwField Name="BKT_FILL_RATE_Nf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="BUCKET_LINK_UP_TH">
                    <Access>
                        <Register Name="FMAC_LEAKY_BUCKET_CONTROL_REGISTERr">
                            <ArrayOffset Formula="#KEY_VALUE(FABRIC_LINK)%dnx_data_fabric.blocks.nof_links_in_fmac_get(unit)"/>
                            <BlockIndex Formula="#KEY_VALUE(FABRIC_LINK)/dnx_data_fabric.blocks.nof_links_in_fmac_get(unit)"/>
                            <HwField Name="BKT_LINK_UP_TH_Nf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="BUCKET_LINK_DOWN_TH">
                    <Access>
                        <Register Name="FMAC_LEAKY_BUCKET_CONTROL_REGISTERr">
                            <ArrayOffset Formula="#KEY_VALUE(FABRIC_LINK)%dnx_data_fabric.blocks.nof_links_in_fmac_get(unit)"/>
                            <BlockIndex Formula="#KEY_VALUE(FABRIC_LINK)/dnx_data_fabric.blocks.nof_links_in_fmac_get(unit)"/>
                            <HwField Name="BKT_LINK_DN_TH_Nf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="BUCKET_PHY_RESET_ENABLE">
                    <Access>
                        <Register Name="FMAC_LEAKY_BUCKET_CONTROL_REGISTERr">
                            <ArrayOffset Formula="#KEY_VALUE(FABRIC_LINK)%dnx_data_fabric.blocks.nof_links_in_fmac_get(unit)"/>
                            <BlockIndex Formula="#KEY_VALUE(FABRIC_LINK)/dnx_data_fabric.blocks.nof_links_in_fmac_get(unit)"/>
                            <HwField Name="SIG_DET_BKT_RST_ENA_Nf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="BUCKET_MAC_RESET_ENABLE">
                    <Access>
                        <Register Name="FMAC_LEAKY_BUCKET_CONTROL_REGISTERr">
                            <ArrayOffset Formula="#KEY_VALUE(FABRIC_LINK)%dnx_data_fabric.blocks.nof_links_in_fmac_get(unit)"/>
                            <BlockIndex Formula="#KEY_VALUE(FABRIC_LINK)/dnx_data_fabric.blocks.nof_links_in_fmac_get(unit)"/>
