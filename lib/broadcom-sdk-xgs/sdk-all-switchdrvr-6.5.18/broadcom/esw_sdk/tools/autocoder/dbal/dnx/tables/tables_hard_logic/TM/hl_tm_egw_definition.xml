    <AppDB Name="EGW_LOOKUP_CTRL">
        <OneLineDescription>This table is used for controlling the EGW lookup.</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID"/>
            </Key>
            <Result>
                <Field Description="Enables lookups to ELK" Name="ELK_LOOKUP_EN" Size="1" Type="BOOL"/>
                <Field Description="Number of clocks to issue time out (0 is disable)" Name="LOOKUP_TIMEOUT_DLY" Size="16" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV EGW</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="ELK_LOOKUP_EN">
                    <Access>
                        <Register Name="IPPD_GENERAL_CFGSr">
                            <HwField Name="ENABLE_ELK_LOOKUPf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="LOOKUP_TIMEOUT_DLY">
                    <Access>
                        <Register Name="IPPD_GENERAL_CFGSr">
                            <HwField Name="TIMEOUT_DLYf"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    <AppDB Name="EGW_TRANSMIT_ATTRIBUTES_CFG">
        <OneLineDescription>This table is used for configuring the EGW transmit attributes.</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID"/>
            </Key>
            <Result>
                <Field Description="Minimum packet size in Bytes." Name="MIN_PKT_SIZE" Size="14" Type="UINT"/>
                <Field Description="Maximum packet size in Bytes." Name="MAX_PKT_SIZE" Size="14" Type="UINT"/>
                <Field Description="EGW will not transmit FLP/CPU records if FIFO exceed this threshold." Name="FLP_TRANSMIT_THRESHOLD" Size="9" Type="UINT"/>
                <Field Description="Congestion identified when FIFO exceed this threshold." Name="CONGESTION_THRESHOLD" Size="9" Type="UINT"/>
                <Field Description="Enable ROP style packing." Name="ROP_PACKING_ENABLE" Size="1" Type="BOOL"/>
                <Field Description="If set, CPU has priority over traffic lookups." Name="CPU_RECORD_PRIO" Size="1" Type="BOOL"/>
                <Field Description="Enable terminating the packet even if not reached minimum packet size." Name="TIMER_MODE" Size="1" Type="BOOL"/>
                <Field Description="Number of cycles for timer to count untill termiate packet." Name="TIMER_DELAY" Size="9" Type="UINT"/>
                <Field Description="Enable Attaching application prefix in packet transmition." Name="TX_APP_PREFIX_EN" Size="1" Type="BOOL"/>
                <Field Description="Enable removing application prefix in packet receiving." Name="RX_APP_PREFIX_EN" Size="1" Type="BOOL"/>
                <Field Description="Channel number for transmitter." Name="TX_CHANNEL_NUM" Size="8" Type="UINT"/>
                <Field Description="Channel number for receiver." Name="RX_CHANNEL_NUM" Size="8" Type="UINT"/>
                <Field Description="If set, enable interlaken mode, to compensate Interlaken overheads in shaper." Name="ILKN_MODE" Size="1" Type="BOOL"/>
                <Field Description="Interlaken minimum burst in 32B resolution." Name="ILKN_MIN_BURST" Size="3" Type="UINT"/>
                <Field Description="Interlaken maximum burst in 32B resolution." Name="ILKN_MAX_BURST" Size="5" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV EGW</TableLabels>
        </AppDbInterface>
