                            <HwField Name="DISABLE_FABRIC_MSGSf"/>
                            <DataOffset Formula="0"/>
                        </Register>
                    </Access>
                    <Access EncodeType="BITWISE_NOT">
                        <KeyCondition Enum="SCH_CREDIT_GENERATION" Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO"/>
                        <Register Name="SCH_DVS_CONFIGr">
                            <HwField Name="FORCE_PAUSEf"/>
                            <DataOffset Formula="0"/>
                        </Register>
