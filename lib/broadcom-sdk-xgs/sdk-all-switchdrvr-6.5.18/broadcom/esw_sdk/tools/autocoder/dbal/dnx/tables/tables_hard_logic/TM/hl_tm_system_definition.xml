    <AppDB Name="SYSTEM_SYSPORT_MODPORT_MAP">
        <OneLineDescription>Map destination system port to destination fap id and FTMH PP DSP</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID"/>
                <Field Name="DST_SYS_PORT_ID" Size="15" Type="UINT"/>
            </Key>
            <Result>
                <Field Name="FAP_ID"/>
                <Field Name="FTMH_PP_DSP"/>
            </Result>
            <TableLabels>J2P_NOTREV SYSTEM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="FAP_ID">
                    <Access>
                        <Memory Name="IPS_SPMm">
                            <EntryOffset Formula="#KEY_VALUE(DST_SYS_PORT_ID)"/>
                            <HwField Name="DST_FAPf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="FTMH_PP_DSP">
                    <Access>
                        <Memory Name="IPS_SPMm">
                            <EntryOffset Formula="#KEY_VALUE(DST_SYS_PORT_ID)"/>
                            <HwField Name="PP_DSPf"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="SYSTEM_PP_DSP_MAPPING">
        <OneLineDescription>Map pp-dsp to tm, pp and system port</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID"/>
                <Field Name="PP_DSP" Type="DSP"/>
            </Key>
            <Result>
                <Field Name="TM_PORT"/>
                <Field Name="PP_PORT"/>
                <Field Name="SYSTEM_PORT"/>
            </Result>
            <TableLabels>J2P_NOTREV SYSTEM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="PP_PORT">
                    <Access>
                        <Memory Name="ERPP_PRP_PP_DSP_PTR_TABLEm">
                            <EntryOffset Formula="#KEY_VALUE(PP_DSP)"/>
                            <HwField Name="OUT_PP_PORTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ERPP_TM_PP_DSP_PTR_TABLEm">
                            <EntryOffset Formula="#KEY_VALUE(PP_DSP)"/>
                            <HwField Name="OUT_PP_PORTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ETPPA_DSP_DATA_TABLEm">
                            <EntryOffset Formula="#KEY_VALUE(PP_DSP)"/>
                            <HwField Name="OUT_PP_PORTf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="SYSTEM_PORT">
                    <Access>
                        <Memory Name="ETPPA_DSP_DATA_TABLEm">
                            <EntryOffset Formula="#KEY_VALUE(PP_DSP)"/>
                            <HwField Name="DST_SYS_PORTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ERPP_TM_PP_DSP_PTR_TABLEm">
                            <EntryOffset Formula="#KEY_VALUE(PP_DSP)"/>
                            <HwField Name="DST_SYS_PORTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ERPP_PRP_PP_DSP_PTR_TABLEm">
                            <EntryOffset Formula="#KEY_VALUE(PP_DSP)"/>
                            <HwField Name="DST_SYS_PORTf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="TM_PORT">
                    <Access>
                        <Memory Name="ETPPA_DSP_DATA_TABLEm">
                            <EntryOffset Formula="#KEY_VALUE(PP_DSP)"/>
                            <HwField Name="OUT_TM_PORTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ERPP_TM_PP_DSP_PTR_TABLEm">
                            <EntryOffset Formula="#KEY_VALUE(PP_DSP)"/>
                            <HwField Name="OUT_TM_PORTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ERPP_PRP_PP_DSP_PTR_TABLEm">
                            <EntryOffset Formula="#KEY_VALUE(PP_DSP)"/>
                            <HwField Name="OUT_TM_PORTf"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="SYSTEM_CLOS_DQCQ_MAP" Valid="DNX_DATA.fabric.general.is_supported">
        <OneLineDescription>Map a packet to DQCQ context according to FAP-ID. 
Local-0 if:
(DestDev | MASK_LOCAL_0) == (FAP_ID_LOCAL_0| MASK_LOCAL_0) 
Local-1 if:
(DestDev | MASK_LOCAL_1) == (FAP_ID_LOCAL_1| MASK_LOCAL_1)
else:
fabric   
        </OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field MaxValue="DNX_DATA.device.general.max_nof_fap_ids_per_core-1" Name="LOCAL_INDX" Size="DNX_DATA.device.general.fap_ids_per_core_bits" Type="UINT"/>
                <Field Name="CORE"/>
            
            </Key>
            <Result>
                <Field Name="FAP_ID_LOCAL" Size="11" Type="UINT"/>
                <Field Name="MASK_LOCAL" Size="11" Type="UINT" Valid="DNX_DATA.device.general.multi_core"/>
            </Result>
            <TableLabels>J2P_NOTREV SYSTEM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping Device="JR2_A0 JR2_B0">
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="FAP_ID_LOCAL">
                    <Access>
                        <Register Name="IPS_DEV_ID_TO_CONTEXT_MAPr">
                            <HwField Name="DEV_ID_TO_CONTEXT_MAPf"/>
                            <DataOffset Formula="0+32*#KEY_VALUE(CORE)*dnx_data_device.general.fap_ids_per_core_bits_get(unit)+32*#KEY_VALUE(LOCAL_INDX)"/>
                        </Register>
                    </Access>
                    <Access>
                        <Register Name="IPS_DEV_ID_TO_CONTEXT_MAPr">
                            <HwField Name="DEV_ID_TO_CONTEXT_MAPf"/>
                            <DataOffset Formula="16+32*#KEY_VALUE(CORE)*dnx_data_device.general.fap_ids_per_core_bits_get(unit)+32*#KEY_VALUE(LOCAL_INDX)"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="MASK_LOCAL">
                    <Access>
                        <Register Name="IPS_DEV_ID_MASKr">
                            <DataOffset Formula="11*#KEY_VALUE(CORE)*dnx_data_device.general.fap_ids_per_core_bits_get(unit)+11*#KEY_VALUE(LOCAL_INDX)"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
        <AppToPhyDbMapping Device="J2C_A0">
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="FAP_ID_LOCAL">
                    <Access>
                        <Register Name="IPS_DEV_ID_TO_CONTEXT_MAPr">
                            <HwField Name="DEV_ID_TO_CONTEXT_MAPf"/>
                            <DataOffset Formula="16*#KEY_VALUE(CORE)*dnx_data_device.general.fap_ids_per_core_bits_get(unit)+16*#KEY_VALUE(LOCAL_INDX)"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
