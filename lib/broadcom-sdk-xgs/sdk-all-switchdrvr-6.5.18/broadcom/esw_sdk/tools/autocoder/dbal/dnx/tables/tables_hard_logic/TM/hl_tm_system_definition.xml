    <AppDB Name="SYSTEM_MODULE_MAX_BASE_INDEX" Valid="DNX_DATA.fabric.general.is_supported">
        <OneLineDescription>Set maximum module id in the system by setting maximum base index</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Description="Maximum base index that represents the maximum FAP ID in the system [MaxFapID=(MAX_BASE_INDEX+1)*32-1]" Name="MAX_BASE_INDEX" Size="6" Type="UINT"/>
                <Field Description="Maximum base index that represents the maximum FAP ID for multicast traffic in the system [MaxMCFapID=(MAX_BASE_INDEX_MC+1)*32-1]" Name="MAX_BASE_INDEX_MC" Size="6" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV STACK</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="MAX_BASE_INDEX">
                    <Access>
                        <Register Name="RTP_MAXIMUM_BASE_INDEXr">
                            <HwField Name="MAX_BIf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="MAX_BASE_INDEX_MC">
