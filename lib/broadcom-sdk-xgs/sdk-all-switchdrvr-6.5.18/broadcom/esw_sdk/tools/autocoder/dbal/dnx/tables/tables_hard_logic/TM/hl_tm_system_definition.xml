--><AppDbCatalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../hard_logic_schema.xsd">

    <AppDB Name="SYSTEM_CFG">
        <OneLineDescription>System configuration per core</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE"/>
            </Key>
            <Result>
                <Field Name="FAP_ID" NofInstances="DNX_DATA.device.general.max_nof_fap_ids_per_core"/>
                <Field Name="FAP_INDEX" NofInstances="DNX_DATA.device.general.max_nof_fap_ids_per_core" Size="2" Type="UINT" Valid="DNX_DATA.device.general.max_nof_fap_ids_per_core-1"/>
            </Result>
            <TableLabels>J2P_NOTREV SYSTEM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping Device="JR2_A0 JR2_B0 J2P_A0">
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="FAP_ID">
                    <Access>
                        <KeyCondition Field="CORE" Type="EQUAL_TO" Value="0"/>
                        <Register Name="ECI_FAP_GLOBAL_GENERAL_CFG_2r">
                            <HwField Name="DEV_ID_0f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="CORE" Type="EQUAL_TO" Value="1"/>
                        <Register Name="ECI_FAP_GLOBAL_GENERAL_CFG_2r">
                            <HwField Name="DEV_ID_1f"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
        <AppToPhyDbMapping Device="J2C_A0">
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="FAP_ID">
                    <Access instanceIndex="0">
                        <Register Name="ECI_FAP_GLOBAL_GENERAL_CFG_2r">
                            <HwField Name="DEV_ID_0f"/>
                        </Register>
                    </Access>
                    <Access instanceIndex="1">
                        <Register Name="ECI_FAP_GLOBAL_GENERAL_CFG_2r">
                            <HwField Name="DEV_ID_1f"/>
                        </Register>
                    </Access>
                    <Access instanceIndex="2">
                        <Register Name="ECI_FAP_GLOBAL_GENERAL_CFG_4r">
                            <HwField Name="DEV_ID_2f"/>
                        </Register>
                    </Access>
                    <Access instanceIndex="3">
                        <Register Name="ECI_FAP_GLOBAL_GENERAL_CFG_4r">
                            <HwField Name="DEV_ID_3f"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FAP_INDEX">
                    <Access instanceIndex="ALL">
                        <Register Name="FDA_DEST_ID_2_PORT_EXT_CONFIGr">
                            <DataOffset Formula="2*#INSTANCE_IDX()"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
        <AppToPhyDbMapping Device="Q2A_A0 Q2A_B0">
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="FAP_ID">
                    <Access>
                        <KeyCondition Field="CORE" Type="EQUAL_TO" Value="0"/>
                        <Register Name="ECI_FAP_GLOBAL_GENERAL_CFG_2r">
                            <HwField Name="DEV_ID_0f"/>
                        </Register>
                    </Access>                    
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
