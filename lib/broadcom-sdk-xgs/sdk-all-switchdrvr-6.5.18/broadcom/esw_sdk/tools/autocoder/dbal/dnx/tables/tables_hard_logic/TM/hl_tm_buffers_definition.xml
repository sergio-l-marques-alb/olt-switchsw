    <AppDB Name="BDM_FPC_CONFIG">
        <OneLineDescription>Configuration of the BDM Free Pointer Controler</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key/>
            <Result>
                <Field Name="FPC_FIRST_PTR" NofInstances="4" Size="#log2_round_up(DNX_DATA.dram.buffers.nof_bdbs)-2" Type="UINT"/>
                <Field Name="FPC_LAST_PTR" NofInstances="4" Size="#log2_round_up(DNX_DATA.dram.buffers.nof_bdbs)-2" Type="UINT"/>
            </Result>
            <TableLabels>J2P_NOTREV ITM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping> 
                <Field Name="FPC_FIRST_PTR">
                    <Access instanceIndex="ALL">
                        <Register Name="BDM_FPC_CONTROLr">
                            <ArrayOffset Formula="#INSTANCE_IDX()"/>
                            <HwField Name="FPC_N_FIRST_PTRf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FPC_LAST_PTR">
                    <Access instanceIndex="ALL">
                        <Register Name="BDM_FPC_CONTROLr">
                            <ArrayOffset Formula="#INSTANCE_IDX()"/>
                            <HwField Name="FPC_N_LAST_PTRf"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

