    <AppDB Name="DELETE_BDB_POINTER" Valid="DNX_DATA.dram.hbm.is_delete_bdb_supported">
        <OneLineDescription>Handling of 'delete from pool' operation on BDB pointer</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID"/>
            </Key>
            <Result>
                <!-- This is an identifier of the BDB to delete (from the list of active buffers)  -->
                <Field Name="PTR_TO_DELETE" Size="#log2_round_up(DNX_DATA.dram.buffers.nof_bdbs)" Type="UINT"/>
                <!-- If, after a 'delete' attempt, this bit is set then the operation has failed, for some reason  -->
                <Field Name="DELETE_PTR_FAILED" Permission="READONLY" Type="BOOL"/>
            </Result>
            <TableLabels>J2P_NOTREV ITM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping> 
                <Field Name="PTR_TO_DELETE">
                    <Access>
                        <Register Name="BDM_FPC_DELETEr">
                            <HwField Name="FPC_DELETE_PTRf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="DELETE_PTR_FAILED">
                    <Access>
                        <Register Name="BDM_FPC_DELETEr">
                            <HwField Name="FPC_DELETE_PTR_FAILEDf"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

