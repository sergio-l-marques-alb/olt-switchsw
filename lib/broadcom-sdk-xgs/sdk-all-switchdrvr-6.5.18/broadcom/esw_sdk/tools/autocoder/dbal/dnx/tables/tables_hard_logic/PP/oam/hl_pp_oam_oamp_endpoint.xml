                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="2"/>
                        <Register Name="OAMP_ICC_MAP_REG_1r">
                            <HwField Name="ICC_MAP_IDX_2f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="3"/>
                        <Register Name="OAMP_ICC_MAP_REG_1r">
                            <HwField Name="ICC_MAP_IDX_3f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="4"/>
                        <Register Name="OAMP_ICC_MAP_REG_1r">
                            <HwField Name="ICC_MAP_IDX_4f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="5"/>
                        <Register Name="OAMP_ICC_MAP_REG_1r">
                            <HwField Name="ICC_MAP_IDX_5f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="6"/>
                        <Register Name="OAMP_ICC_MAP_REG_1r">
                            <HwField Name="ICC_MAP_IDX_6f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="7"/>
                        <Register Name="OAMP_ICC_MAP_REG_1r">
                            <HwField Name="ICC_MAP_IDX_7f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="8"/>
                        <Register Name="OAMP_ICC_MAP_REG_1r">
                            <HwField Name="ICC_MAP_IDX_8f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="9"/>
                        <Register Name="OAMP_ICC_MAP_REG_1r">
                            <HwField Name="ICC_MAP_IDX_9f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="10"/>
                        <Register Name="OAMP_ICC_MAP_REG_2r">
                            <HwField Name="ICC_MAP_IDX_10f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="11"/>
                        <Register Name="OAMP_ICC_MAP_REG_2r">
                            <HwField Name="ICC_MAP_IDX_11f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="12"/>
                        <Register Name="OAMP_ICC_MAP_REG_2r">
                            <HwField Name="ICC_MAP_IDX_12f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="13"/>
