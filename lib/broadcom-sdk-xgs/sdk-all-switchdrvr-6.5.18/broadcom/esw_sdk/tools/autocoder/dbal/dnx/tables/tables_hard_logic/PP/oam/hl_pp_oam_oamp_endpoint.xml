                            <HwField Name="OPCODE_0_RATEf"/>
                        </Memory>
                    </Access>
                    <Access instanceIndex="1">
                        <Memory Name="OAMP_MEP_PROFILEm">
                            <HwField Name="OPCODE_1_RATEf"/>
                        </Memory>
                    </Access>
                    <Access instanceIndex="2">
                        <Memory Name="OAMP_MEP_PROFILEm">
                            <HwField Name="OPCODE_2_RATEf"/>
                        </Memory>
                    </Access>
                    <Access instanceIndex="3">
                        <Memory Name="OAMP_MEP_PROFILEm">
                            <HwField Name="OPCODE_3_RATEf"/>
                        </Memory>
                    </Access>
                    <Access instanceIndex="4">
