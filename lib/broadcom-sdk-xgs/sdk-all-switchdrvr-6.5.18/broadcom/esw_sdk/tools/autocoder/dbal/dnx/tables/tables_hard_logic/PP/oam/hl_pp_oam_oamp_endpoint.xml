    <AppDB Name="OAMP_MEP_DA_MSB">
        <OneLineDescription>Use 3 bit index from OAMP MEP PROFILE to get one of 8 MAC DA's MSB 24 bits</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="LMM_DA_MSB_PROFILE"/>
            </Key>
            <Result>
                <Field Name="DA_MSB" Offset="24" Size="24" Type="L2_MAC"/>
            </Result>
            <TableLabels>OAM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="DA_MSB">
                    <Access>
                        <KeyCondition Field="LMM_DA_MSB_PROFILE" Type="EQUAL_TO" Value="0"/>
                        <Register Name="OAMP_LMM_DA_OUIr"> 
                            <HwField Name="LMM_DA_OUI_0f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="LMM_DA_MSB_PROFILE" Type="EQUAL_TO" Value="1"/>
                        <Register Name="OAMP_LMM_DA_OUIr"> 
                            <HwField Name="LMM_DA_OUI_1f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="LMM_DA_MSB_PROFILE" Type="EQUAL_TO" Value="2"/>
                        <Register Name="OAMP_LMM_DA_OUIr"> 
                            <HwField Name="LMM_DA_OUI_2f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="LMM_DA_MSB_PROFILE" Type="EQUAL_TO" Value="3"/>
                        <Register Name="OAMP_LMM_DA_OUIr"> 
                            <HwField Name="LMM_DA_OUI_3f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="LMM_DA_MSB_PROFILE" Type="EQUAL_TO" Value="4"/>
                        <Register Name="OAMP_LMM_DA_OUIr"> 
                            <HwField Name="LMM_DA_OUI_4f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="LMM_DA_MSB_PROFILE" Type="EQUAL_TO" Value="5"/>
                        <Register Name="OAMP_LMM_DA_OUIr"> 
                            <HwField Name="LMM_DA_OUI_5f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="LMM_DA_MSB_PROFILE" Type="EQUAL_TO" Value="6"/>
                        <Register Name="OAMP_LMM_DA_OUIr"> 
                            <HwField Name="LMM_DA_OUI_6f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="LMM_DA_MSB_PROFILE" Type="EQUAL_TO" Value="7"/>
                        <Register Name="OAMP_LMM_DA_OUIr"> 
                            <HwField Name="LMM_DA_OUI_7f"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

