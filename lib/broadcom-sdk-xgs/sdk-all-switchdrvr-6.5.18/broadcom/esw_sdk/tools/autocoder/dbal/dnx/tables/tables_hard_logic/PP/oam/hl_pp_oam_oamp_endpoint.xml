                 </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    <AppDB Name="OAMP_UMC_TABLE">
        <OneLineDescription>UMC table for Y1731 OAM with ICC based MAID, for packet generation and verification</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="MEP_INDEX" Size="13" Type="OAMP_MEP_DB_INDEX"/>
            </Key>
            <Result>
                <Field Description="UMC data to put in MEG ID field of Y1731 CCM PDU" Name="UMC" Size="40"/>
            </Result>
            <TableLabels>OAM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="UMC">
                    <Access>
                        <Memory Name="OAMP_UMC_TABLEm">
                            <HwField Name="UMCf"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    <AppDB Name="OAMP_MEP_PROFILE">
        <OneLineDescription>MEP profile table for Y1731 OAM for packet generation and verification</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="MEP_PROFILE_ID" Type="MEP_PROFILE"/>
            </Key>
            <Result>
                <Field Description="DMM transmission rate" Name="DMM_RATE" Type="CCM_INTERVAL"/>
                <Field Description="LMM transmission rate" Name="LMM_RATE" Type="CCM_INTERVAL"/>
                <Field Description="Profile that selects SLR measurement period" Name="MEASUREMENT_PERIOD_PROFILE" Size="3" Type="UINT"/>
                <Field Description="FALSE: single ended LM; TRUE: Dual ended LM." Name="IS_DUAL_ENDED_LM" Type="BOOL"/>
                <Field Description="FALSE: LM through LMM/LMR; TRUE: LM through SLM/SLR." Name="IS_SLM" Type="BOOL"/>
                <Field Description="FALSE: LM through statistics; TRUE: LM through Reports." Name="IS_REPORT_MODE_LM" Type="BOOL"/>
                <Field Description="FALSE: DM through statistics; TRUE: DM through Reports." Name="IS_REPORT_MODE_DM" Type="BOOL"/>
                <Field Name="RDI_GEN_METHOD"/>
                <Field Description="Offset to time stamp into DMM OAM-TS header" Name="DMM_OFFSET" Size="8" Type="UINT"/>
                <Field Description="Offset to time stamp into DMR OAM-TS header" Name="DMR_OFFSET" Size="8" Type="UINT"/>
                <Field Description="Offset to counter stamp into LMM OAM-TS header" Name="LMM_OFFSET" Size="8" Type="UINT"/>
                <Field Description="Offset to counter stamp into LMR OAM-TS header" Name="LMR_OFFSET" Size="8" Type="UINT"/>
                <Field Description="List of OAM opcode PDUs, in a bit map, to send from OAMP" Name="OPCODE_BIT_MAP" Size="8" Type="UINT"/>
                <Field Description="Transmission rate of opcode number X" Name="OPCODE_X_RATE" NofInstances="8" Type="OPCODE_RATE"/>
                <Field Description="DM measurement type and 1DM time stamp encodings if type is 1DM" Name="DM_MEASUREMENT_TYPE" Type="DM_ONE_WAY_TIMESTAMP_ENCODING"/>
                <Field Description="FALSE: MAID check is enabled in RX; TRUE: MAID check is disabled in RX." Name="DISABLE_MAID_CHECK" Type="BOOL"/>
                <Field Description="FALSE: Source IP check is enabled in RX; TRUE: Source IP check is disabled in RX." Name="DISABLE_SRC_IP_CHECK" Type="BOOL"/>
                <Field Description="FALSE: MDL check is enabled in RX; TRUE: MAID check is disabled in RX." Name="DISABLE_MDL_CHECK" Type="BOOL"/>
                <Field Description="FALSE: your-discriminator check is enabled in RX; TRUE: disabled." Name="DISABLE_YOUR_DISCR_CHECK" Type="BOOL"/>
                <Field Description="Scan count to transmit CCM" Name="CCM_COUNT" Size="19" Type="UINT"/>
                <Field Description="Scan count to transmit DMM" Name="DMM_COUNT" Size="19" Type="UINT"/>
                <Field Description="Scan count to transmit LMM" Name="LMM_COUNT" Size="19" Type="UINT"/>
                <Field Description="Scan count to transmit opcode X" Name="OPCODE_X_COUNT" NofInstances="8" Size="19" Type="UINT"/>
                <Field Description="3-bit profile index that selects the 3 most significant bytes of the MAC DA in LMM injection" Name="LMM_DA_MSB_PROFILE"/>
                <Field Description="FALSE: Disable Tx Counting; TRUE: Enable Tx Counting" Name="TX_COUNTER_ENABLE" Type="BOOL"/>
                <Field Description="FALSE: Disable Rx Counting; TRUE: Enable Rx Counting" Name="RX_COUNTER_ENABLE" Type="BOOL"/>
                <Field Description="FALSE: Opcode Mask is 000; TRUE: Opcode Mask is 111" Name="OPCODE_MSK" Type="BOOL"/>
                <Field Description="MEP ID Shift" Name="MEP_ID_SHIFT" Size="2" Type="UINT"/>
            </Result>
            <TableLabels>OAM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="DMM_RATE">
                    <Access>
                        <Memory Name="OAMP_MEP_PROFILEm">
                            <HwField Name="DMM_RATEf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="LMM_RATE">
                    <Access>
                        <Memory Name="OAMP_MEP_PROFILEm">
                            <HwField Name="LMM_RATEf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="MEASUREMENT_PERIOD_PROFILE">
                    <Access>
                        <Memory Name="OAMP_MEP_PROFILEm">
                            <HwField Name="MEASUREMENT_PERIOD_PROFILEf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="IS_DUAL_ENDED_LM">
                    <Access>
                        <Memory Name="OAMP_MEP_PROFILEm">
                            <HwField Name="PIGGYBACK_LMf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="IS_SLM">
                    <Access>
                        <Memory Name="OAMP_MEP_PROFILEm">
                            <HwField Name="SLM_LMf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="IS_REPORT_MODE_LM">
                    <Access>
                        <Memory Name="OAMP_MEP_PROFILEm">
                            <HwField Name="REPORT_MODE_LMf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="IS_REPORT_MODE_DM">
                    <Access>
