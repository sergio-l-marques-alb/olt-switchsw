    <AppDB Name="OAMP_ICC_MAP">
        <OneLineDescription>Mapping of ICC index for Y1731 OAM with ICC based MAID, for packet generation and verification</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field MaxValue="14" Name="ICC_MAP_INDEX"/>
            </Key>
            <Result>
                <Field Name="ICC_DATA"/>
            </Result>
            <TableLabels>OAM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="ICC_DATA">
                    <Access>
                        <KeyCondition Field="ICC_MAP_INDEX" Type="EQUAL_TO" Value="0"/>
                        <Register Name="OAMP_ICC_MAP_REG_1r">
                            <HwField Name="ICC_MAP_IDX_0f"/>
                        </Register>
