--><AppDbCatalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../hard_logic_schema.xsd">

    <AppDB Name="MPLS_PHP_MAP_FIRST_NIBBLE">
        <OneLineDescription>Mapping first IP nibble to nibble index</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="FIRST_NIBBLE" Size="4" Type="UINT"/>
            </Key>
            <Result>
                <Field Name="FIRST_NIBBLE_INDEX" Size="2"/> 
            </Result>
            <TableLabels>MPLS</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="FIRST_NIBBLE_INDEX">
                    <Access>
                        <Register Name="ETPPC_CFG_MAP_FIRST_NIBBLEr">
                          <DataOffset Formula="#FULL_KEY_VALUE()*2"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="MPLS_PHP_MAP_FWD_CODE">
        <OneLineDescription>Mapping nibble index with is_mc into forwarding code</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="IS_MC" Type="BOOL"/>
                <Field Name="FIRST_NIBBLE_INDEX" Size="2"/>
            </Key>
            <Result>
                <Field Name="FORWARD_CODE" Type="EGRESS_FWD_CODE"/>
            </Result>
            <TableLabels>MPLS</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="FORWARD_CODE">
                    <Access>
                        <Register Name="ETPPC_PHP_MAP_FWD_CODEr">
                            <DataOffset Formula="#KEY_VALUE(IS_MC)*4*6+#KEY_VALUE(FIRST_NIBBLE_INDEX)*6"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    <AppDB Name="INGRESS_MPLS_TERMINATION_PROFILE">
        <OneLineDescription>Ingress MPLS Termination Profile Configuration Table</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="MPLS_TERMINATION_PROFILE" Type="TERMINATION_TYPE"/>
            </Key>
            <Result>
                <Field Name="TTL_EXP_LABEL_INDEX"/>
                <Field Description="Reject TTL 0" Name="REJECT_TTL_0" Type="BOOL"/>
                <Field Description="Reject TTL 1" Name="REJECT_TTL_1" Type="BOOL"/>
                <Field Description="If valid Control Word is present" Name="HAS_CW" Type="BOOL"/>
                <Field Name="LABELS_TO_TERMINATE"/>
                <Field Description="Enables MPLS Unexpected BOS, MPLS Unexpectes No BOS actions" Name="CHECK_BOS" Type="BOOL"/>
                <Field Description="Enables BOS check" Name="EXPECT_BOS" Type="BOOL"/>
             </Result>
            <TableLabels>L3</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="TTL_EXP_LABEL_INDEX">
                    <Access>
                        <Memory GroupName="INGRESS_TERMINATION_PROFILE_TABLE">
                            <HwField Name="TTL_EXP_LABEL_INDEXf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="REJECT_TTL_0">
                    <Access>
                        <Memory GroupName="INGRESS_TERMINATION_PROFILE_TABLE">
                            <HwField Name="REJECT_TTL_0f"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="REJECT_TTL_1">
                    <Access>
                        <Memory GroupName="INGRESS_TERMINATION_PROFILE_TABLE">
                            <HwField Name="REJECT_TTL_1f"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="HAS_CW">
                    <Access>
                        <Memory GroupName="INGRESS_TERMINATION_PROFILE_TABLE">
                            <HwField Name="HAS_CWf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="LABELS_TO_TERMINATE">
                    <Access>
                        <Memory GroupName="INGRESS_TERMINATION_PROFILE_TABLE">
                            <HwField Name="LABELS_TO_TERMINATEf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="CHECK_BOS">
                    <Access>
                        <Memory GroupName="INGRESS_TERMINATION_PROFILE_TABLE">
                            <HwField Name="CHECK_BOSf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="EXPECT_BOS">
                    <Access>
                        <Memory GroupName="INGRESS_TERMINATION_PROFILE_TABLE">
                            <HwField Name="EXPECT_BOSf"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="MPLS_GENERAL_CONFIGURATION">
        <OneLineDescription>MPLS General Configuration Table</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="MPLS_ENCAP_CONTROL_WORD" Type="UINT"/>
                <Field Description="MPLS Type Pop = DBAL_FIELD_FHEI_MPLS_TYPE_POP" Name="FHEI_MPLS_TYPE_POP" Size="4" Type="UINT"/>
                <Field Description="Indicates, whether the block is ETPP or not" Name="BLOCK_IS_ETPP" Type="BOOL"/>
                <Field Name="FHEI_MPLS_POP_CMD" Type="FHEI_MPLS_COMMAND"/>                
                <Field Name="FHEI_MPLS_SWAP_CMD" Type="FHEI_MPLS_COMMAND"/>
             </Result>
            <TableLabels>MPLS</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="MPLS_ENCAP_CONTROL_WORD">
                    <Access>
                        <Register GroupName="ETPP_CFG_MPLS_CONTROL_WORD">
                        </Register>
                    </Access>
                </Field>
                <Field Name="FHEI_MPLS_TYPE_POP">
                    <Access>
                        <Register Name="ERPP_UTILITY_REGSr">
                            <HwField Name="FHEI_MPLS_TYPE_POPf"/>
                        </Register>
                    </Access>
                    <Access>
                        <Register Name="ETPPA_UTILITY_REGSr">
                            <HwField Name="FHEI_MPLS_TYPE_POPf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="BLOCK_IS_ETPP">
                    <Access>
                        <Register Name="ETPPA_UTILITY_REGSr">
                            <HwField Name="BLOCK_IS_ETPPf"/>
                        </Register>
                    </Access>
                    <Access EncodeType="BITWISE_NOT">
                        <Register Name="ERPP_UTILITY_REGSr">
                            <HwField Name="BLOCK_IS_ETPPf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FHEI_MPLS_POP_CMD">
                    <Access>
                        <Register Name="IPPD_LBP_GENERAL_CONFIG_1r">
                            <HwField Name="MPLS_POP_COMMANDf"/>
                        </Register>
                    </Access>
                    <Access>
                        <Register GroupName="EGRESS_FHEI_MPLS_COMMAND">
                            <HwField Name="FHEI_MPLS_COMMAND_POPf"/>
                        </Register>
                    </Access>
        </Field>
                <Field Name="FHEI_MPLS_SWAP_CMD">
                    <Access>
                        <Register Name="IPPD_LBP_GENERAL_CONFIG_1r">
                            <HwField Name="MPLS_SWAP_COMMANDf"/>
                        </Register>
                    </Access>
                    <Access>
                        <Register GroupName="EGRESS_FHEI_MPLS_COMMAND">
                            <HwField Name="FHEI_MPLS_COMMAND_SWAPf"/>
                        </Register>
                    </Access>
        </Field>                
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
