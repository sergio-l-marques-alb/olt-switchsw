    <AppDB Name="EGRESS_MPLS_FHEI_TO_EES_RESOLUTION">
        <OneLineDescription>MPLS Configuration for FHEI to EES Resolution</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Description="MPLS POP Result Type" Name="FHEI_MPLS_POP_TYPE" Size="DNX_DATA.mpls.general.fhei_mps_type_nof_bits" Type="UINT"/>
                <Field Description="MPLS SWAP Result Type" Name="FHEI_MPLS_SWAP_TYPE" Size="DNX_DATA.mpls.general.fhei_mps_type_nof_bits" Type="UINT"/>
                <Field Description="FHEI TTL Model Map" Name="FHEI_POP_TTL_MODEL" NofInstances="4" Type="TTL_MODE"/>
                <Field Description="FHEI QOS Model Map" Name="FHEI_POP_QOS_MODEL" NofInstances="4" Type="QOS_DP_SELECT"/>
                <Field Description="FHEI Remark Profile Map per mpls command" Name="FHEI_SWAP_REMARK" NofInstances="16" Type="REMARK_PROFILE"/>
             </Result>
            <TableLabels>MPLS</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="FHEI_MPLS_POP_TYPE">
                    <Access>
                        <Register Name="ETPPA_CFG_FHEI_MPLS_POP_TYPEr">
                             <HwField Name="CFG_FHEI_MPLS_POP_TYPEf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FHEI_MPLS_SWAP_TYPE">
                    <Access>
                        <Register Name="ETPPA_CFG_FHEI_MPLS_SWAP_TYPEr">
                             <HwField Name="CFG_FHEI_MPLS_SWAP_TYPEf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FHEI_POP_TTL_MODEL">
                    <Access instanceIndex="ALL">
                        <Register Name="ETPPA_CFG_FHEI_TTL_MODEL_MAPr">
                             <HwField Name="CFG_FHEI_TTL_MODEL_MAPf"/>
                             <DataOffset Formula="#INSTANCE_IDX()*#RESULT_SIZE(FHEI_POP_TTL_MODEL)"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FHEI_POP_QOS_MODEL">
                    <Access instanceIndex="ALL">
                        <Register Name="ETPPA_CFG_FHEI_QOS_MODEL_MAPr">
                             <HwField Name="CFG_FHEI_QOS_MODEL_MAPf"/>
                             <DataOffset Formula="#INSTANCE_IDX()*#RESULT_SIZE(FHEI_POP_QOS_MODEL)"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FHEI_SWAP_REMARK">
                    <Access instanceIndex="ALL">
                        <Register Name="ETPPA_CFG_FHEI_MPLS_SWAP_REMARK_MAPr">
                             <HwField Name="CFG_FHEI_MPLS_SWAP_REMARK_MAPf"/>
                             <DataOffset Formula="#INSTANCE_IDX()*dnx_data_headers.hw_bug.fhei_mpls_swap_remark_profile_size_get(unit)"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

