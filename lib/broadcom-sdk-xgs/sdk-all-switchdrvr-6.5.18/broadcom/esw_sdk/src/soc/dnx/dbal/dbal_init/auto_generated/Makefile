LOCALDIR = src/soc/dnx/dbal/dbal_init/auto_generated/

include ${SDK}/make/Make.config

#
# If this is compilation for SLK then optimize this library for code
# size.
#
ifeq ($(findstring SLK,$(TOOLCHAIN_BASE_DIR)),SLK)
CFGFLAGS += -Os
endif

INCDIR += -I${SDK}/include/soc/dnx

include ${SDK}/make/Make.subdirs

lib = libsoc_dnxdbal_dbalinit_autogenerated

include ${SDK}/make/Make.lib

include ${SDK}/make/Make.depend

