CFGFLAGS += -DDNX_INTERNAL

#Flags to prevent gcc 8.1.0 new warnings to appear as errors.
DEBUG_CFLAGS=-Wno-error=stringop-overflow -Wno-error=stringop-truncation -Wno-error=sizeof-pointer-memaccess -Wno-error=misleading-indentation -Wno-error=unused-const-variable -Wno-error=sizeof-array-argument

