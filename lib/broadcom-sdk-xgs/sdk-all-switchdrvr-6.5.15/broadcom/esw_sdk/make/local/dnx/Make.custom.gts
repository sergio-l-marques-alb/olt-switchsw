#ALL_DFE_CHIPS = 1
override SBX_CHIPS=

VENDOR_LIST = BROADCOM

#VALGRIND_DIR = /projects/NTSW_SW_USRS/common/tools/valgrind-3.11.0

CFGFLAGS += -D__DUNE_LINUX_BCM_CPU_PCIE__
CFGFLAGS += -D_SIMPLE_MEMORY_ALLOCATION_=9
CFGFLAGS += -DUSE_LINUX_BDE_MMAP=1
