FEATURE_LIST := CINT ATPTRANS_SOCKET L3 I2C MEM_SCAN EDITLINE CHASSIS TEST BCM_SAL_PROFILE RCPU INTR APIMODE PTP CUSTOMER MSTP DUNE_UI KBP

DEBUG_OPTIMIZE=FALSE

