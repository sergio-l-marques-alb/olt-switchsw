    <AppDB Name="EGRESS_MPLS_FHEI_TO_EES_RESOLUTION">
        <OneLineDescription>MPLS Configuration for FHEI to EES Resolution</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="FHEI_MPLS_POP_TYPE" Type="UINT" Size="6" Description="MPLS POP Result Type"/>
                <Field Name="FHEI_MPLS_SWAP_TYPE" Type="UINT" Size="6" Description="MPLS SWAP Result Type"/>
                <Field Name="FHEI_POP_TTL_MODEL" Type="TTL_MODE" NofInstances="4" Description="FHEI TTL Model Map"/>
                <Field Name="FHEI_POP_QOS_MODEL" Type="QOS_DP_SELECT" NofInstances="4"  Description="FHEI QOS Model Map"/>
                <Field Name="FHEI_SWAP_REMARK" Type="REMARK_PROFILE" NofInstances="16"  Description="FHEI Remark Profile Map per mpls command"/>
             </Result>
            <TableLabels>MPLS</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="FHEI_MPLS_POP_TYPE">
                    <Access>
                        <Register Name="ETPPA_CFG_FHEI_MPLS_POP_TYPEr">
                             <HwField Name="CFG_FHEI_MPLS_POP_TYPEf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FHEI_MPLS_SWAP_TYPE">
                    <Access>
                        <Register Name="ETPPA_CFG_FHEI_MPLS_SWAP_TYPEr">
                             <HwField Name="CFG_FHEI_MPLS_SWAP_TYPEf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FHEI_POP_TTL_MODEL">
                    <Access instanceIndex="ALL">
                        <Register Name="ETPPA_CFG_FHEI_TTL_MODEL_MAPr">
                             <HwField Name="CFG_FHEI_TTL_MODEL_MAPf"/>
                             <DataOffset Formula="INSTANCE*FHEI_POP_TTL_MODEL"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FHEI_POP_QOS_MODEL">
                    <Access instanceIndex="ALL">
                        <Register Name="ETPPA_CFG_FHEI_QOS_MODEL_MAPr">
                             <HwField Name="CFG_FHEI_QOS_MODEL_MAPf"/>
                             <DataOffset Formula="INSTANCE*FHEI_POP_QOS_MODEL"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FHEI_SWAP_REMARK">
                    <Access instanceIndex="ALL">
                        <Register Name="ETPPA_CFG_FHEI_MPLS_SWAP_REMARK_MAPr">
                             <HwField Name="CFG_FHEI_MPLS_SWAP_REMARK_MAPf"/>
                             <DataOffset Formula="INSTANCE*FHEI_SWAP_REMARK"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

