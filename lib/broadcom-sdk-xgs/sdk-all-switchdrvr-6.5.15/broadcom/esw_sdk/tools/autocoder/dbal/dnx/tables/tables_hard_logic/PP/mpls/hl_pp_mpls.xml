    <AppDB Name="MPLS_SPECIAL_LABEL_PROFILE_ATTRIBUTES">
        <OneLineDescription>Maps MPLS special label and BOS to label profile</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
