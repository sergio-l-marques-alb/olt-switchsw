<AppDbCatalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../hard_logic_schema.xsd">

    <AppDB Name="INGRESS_CONG_CGM_VOQ_OCCUPANCY">
        <OneLineDescription>Table for the occupancy level of all VOQs</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID"/>
                <Field Name="VOQ"/>
            </Key>
            <Result>
                <Field Name="SIZE_IN_WORDS" Permission="READONLY" Size="28" Type="UINT" />
                <Field Name="SIZE_IN_WORDS_SRAM" Permission="READONLY" Size="20" Type="UINT"/>
                <Field Name="BUFFERS_SIZE_SRAM" Permission="READONLY" Size="16" Type="UINT"/>
                <Field Name="NOF_PDS_SRAM" Permission="READONLY" Size="17" Type="UINT"/>
                <Field Name="SIZE_AVRG" Permission="READONLY" Size="28" Type="UINT"/>
            </Result>
            <TableLabels>ITM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping> 
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="SIZE_IN_WORDS"> 
                    <Access>
                        <Memory Name="CGM_VOQ_STATE_MEMm"> 
                            <HwField Name="WORDS_SIZEf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="SIZE_IN_WORDS_SRAM"> 
                    <Access>
                        <Memory Name="CGM_VOQ_STATE_MEMm"> 
                            <HwField Name="SRAM_WORDS_SIZEf"/>
                        </Memory>
                    </Access>                  
                </Field>
                <Field Name="BUFFERS_SIZE_SRAM"> 
                    <Access>
                        <Memory Name="CGM_VOQ_STATE_MEMm"> 
                            <HwField Name="SRAM_BUFFERS_SIZEf"/>
                        </Memory>
                    </Access>                  
                </Field>
                <Field Name="NOF_PDS_SRAM"> 
                    <Access>
                        <Memory Name="CGM_VOQ_STATE_MEMm"> 
                            <HwField Name="SRAM_PDS_SIZEf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="SIZE_AVRG"> 
                    <Access>
                        <Memory Name="CGM_VOQ_STATE_MEMm"> 
                            <HwField Name="AVRG_SIZEf"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
