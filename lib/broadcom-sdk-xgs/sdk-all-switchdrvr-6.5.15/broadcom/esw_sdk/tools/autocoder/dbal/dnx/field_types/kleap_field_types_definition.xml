    <FieldType Name="KLEAP_STAGE_SELECTOR" Size="1" Print="ENUM">
        <OneLineDescription>
         Stage A or B, select between the sub stage of a doube kleap stage.
         FWD12: FWD1/FWD2, VT23: VT2/VT3, VT45: VT4/VT5
        </OneLineDescription>
        <FieldDbInterface>
            <EnumValues>
                <EnumVal Name="STAGE_A" Description="KLEAP stage"/>
                <EnumVal Name="STAGE_B" Description="KLEAP stage"/>
            </EnumValues>
        </FieldDbInterface>
        <FieldToPhyDbMapping>
            <EnumValueMapping>
                <EnumVal Name="STAGE_A" Value="0"/>
                <EnumVal Name="STAGE_B" Value="1"/>
            </EnumValueMapping>
        </FieldToPhyDbMapping>
    </FieldType>

