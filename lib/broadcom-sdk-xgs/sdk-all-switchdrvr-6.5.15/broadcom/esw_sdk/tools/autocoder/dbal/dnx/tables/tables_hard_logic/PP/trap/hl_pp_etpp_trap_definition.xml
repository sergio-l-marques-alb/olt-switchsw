    <AppDB Name="ETPP_TRAP_SNIF_MAP">
        <OneLineDescription>Mapping from sniff action profile to snoop and mirror</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="SNIF_ACTION_PROFILE"/>
            </Key>
            <Result>
                <Field Name="SNP_ACTION_PROFILE"/>
                <Field Name="TRAP_SNP_STRENGTH"/>
                <Field Name="MRR_ACTION_PROFILE"/>
            </Result>
            <TableLabels>SNIF</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="SNP_ACTION_PROFILE">
                    <Access>
                        <Register Name="ETPPA_CFG_MAP_SNIFF_TO_SNOOP_ACTIONr">
                            <DataOffset Formula="SNIF_ACTION_PROFILE*3" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="TRAP_SNP_STRENGTH">
                    <Access>
                        <Register Name="ETPPA_CFG_MAP_SNIFF_TO_SNOOP_STRENGTHr">
                            <DataOffset Formula="SNIF_ACTION_PROFILE*3" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="MRR_ACTION_PROFILE">
                    <Access>
                        <Register Name="ETPPA_CFG_MAP_SNIFF_TO_MIRROR_ACTIONr">
                            <DataOffset Formula="SNIF_ACTION_PROFILE*3" />
                        </Register>
                    </Access>
                </Field>                
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

