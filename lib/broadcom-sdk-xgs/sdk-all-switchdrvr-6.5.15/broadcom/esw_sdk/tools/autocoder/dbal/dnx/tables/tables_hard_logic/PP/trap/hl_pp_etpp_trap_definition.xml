                <Field Name="TRAP_SNP_STRENGTH">
                    <Access>
                        <Memory GroupName="ETPP_MTU_MAP_TABLE">
                            <HwField Name="MTU_SNOOP_ACTION_STRENGTHf"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
