<?xml version="1.0" encoding="UTF-8"?>

<!--
    ******************************************************
    DBAL Pure SW Logical Tables:
    This xml contains the pure sw logical DB for the DBAL.
    The data is loaded to the device during init process
    ******************************************************

    ******************************************************
    This files describes serves to introduce Layer 
    Qualifiers fields
    ******************************************************

    DB Rules:
    *********
-->

<AppDbCatalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:noNamespaceSchemaLocation="sw_state_schema.xsd">

    <AppDB Name="ETH_LAYER_QUALIFIER">
        <OneLineDescription>Pure SW table - container to define Layer Record Field IDs</OneLineDescription>
        <AccessMethod>SW_STATE</AccessMethod>
        <Maturity Level="LOW" Jira="SDK-119629"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="ETH_IS_DA_MC"                Type="BOOL"/>
                <Field Name="ETH_IS_DA_BC"                Type="BOOL"/>
                <Field Name="ETH_1ST_TPID_EXIST"          Type="BOOL"/>
                <Field Name="ETH_1ST_TPID_INDEX"          Type="UINT"/>
                <Field Name="ETH_2ND_TPID_EXIST"          Type="BOOL"/>
                <Field Name="ETH_2ND_TPID_INDEX"          Type="UINT"/>
                <Field Name="ETH_3RD_TPID_EXIST"          Type="BOOL"/>
                <Field Name="ETH_3RD_TPID_INDEX"          Type="UINT"/>
            </Key>
            <Result><Field Name="UINT"/></Result>
            <TableLabels></TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping><CoreMode Mode="SBC"/></AppToPhyDbMapping>
    </AppDB>

