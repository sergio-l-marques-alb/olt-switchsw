    <AppDB Name="FIELD_IPMF1_LOOKUP_ENABLER">
        <OneLineDescription>This table provides lookup enabler per conext per lookup type.</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/> 
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="FIELD_PMF_CTX_ID"/>
                <Field Name="PMF_LOOKUP_TYPE"/>
            </Key>
            <Result>
                <Field Name="ENABLE" Size="1" Type="BOOL"/>
            </Result>
            <TableLabels>FIELD</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="ENABLE">
                    <Access>
                        <KeyCondition Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO" Enum="TCAM_0"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="4*FIELD_PMF_CTX_ID"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO" Enum="TCAM_1"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="4*FIELD_PMF_CTX_ID+1"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO" Enum="TCAM_2"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="4*FIELD_PMF_CTX_ID+2"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO" Enum="TCAM_3"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="4*FIELD_PMF_CTX_ID+3"/>
                        </Register>
                    </Access>     
                    <Access>
                        <KeyCondition Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO" Enum="EXEM"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="FIELD_PMF_CTX_ID+256"/>
                        </Register>
                    </Access>   
                    <Access>
                        <KeyCondition Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO" Enum="DIRECT"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="FIELD_PMF_CTX_ID+320"/>
                        </Register>
                    </Access> 
                    <Access>
                        <KeyCondition Field="PMF_LOOKUP_TYPE" Type="EQUAL_TO" Enum="STATE_TABLE"/>
                        <Register Name="IPPC_PER_CONTEXT_LOOKUP_VALID_PASS_1r">
                            <DataOffset Formula="FIELD_PMF_CTX_ID+384"/>
                        </Register>
                    </Access>  
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

