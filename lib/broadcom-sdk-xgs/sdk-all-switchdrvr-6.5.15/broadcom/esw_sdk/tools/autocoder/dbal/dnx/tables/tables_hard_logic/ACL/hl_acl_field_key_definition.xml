<AppDbCatalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../hard_logic_schema.xsd">

    <AppDB Name="FIELD_IPMF1_LAST_KEY_INFO">
        <OneLineDescription>This table provides 160 bit information per key for the last packet.</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID"/>
                <Field Name="FIELD_KEY" MaxValue="4"/>
            </Key>
            <Result>
                <Field Name="LAST_KEY" Permission="READONLY" Size="160" Type="ARRAY32"/>
            </Result>
            <TableLabels>FIELD</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="LAST_KEY">
                    <Access>
                        <KeyCondition Field="FIELD_KEY" Type="EQUAL_TO" Enum="A"/>
                        <Register Name="IPPC_DBG_LAST_KEY_Ar">
                            <HwField Name="DBG_LAST_KEY_Af"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="FIELD_KEY" Type="EQUAL_TO" Enum="B"/>
                        <Register Name="IPPC_DBG_LAST_KEY_Br">
                            <HwField Name="DBG_LAST_KEY_Bf"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="FIELD_KEY" Type="EQUAL_TO" Enum="C"/>
                        <Register Name="IPPC_DBG_LAST_KEY_Cr">
                            <HwField Name="DBG_LAST_KEY_Cf"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="FIELD_KEY" Type="EQUAL_TO" Enum="D"/>
                        <Register Name="IPPC_DBG_LAST_KEY_Dr">
                            <HwField Name="DBG_LAST_KEY_Df"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="FIELD_KEY" Type="EQUAL_TO" Enum="E"/>
                        <Register Name="IPPC_DBG_LAST_KEY_Er">
                            <HwField Name="DBG_LAST_KEY_Ef"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
