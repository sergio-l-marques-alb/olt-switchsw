        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="IN_PP_PORT" Type="PP_PORT"/>
            </Key>
            <Result>
                <Field Name="L_2_CP_TRAP_HANDLING_PROFILE"/>
