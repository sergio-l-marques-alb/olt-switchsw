    <AppDB Name="EGW_RECORD_OPCODE_MAPPING_TABLE">
        <OneLineDescription>EGW OPcode mapping table.</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID" />
                <Field Name="OPCODE" MinValue="0" MaxValue="255" Size="8" Type="UINT"/>
            </Key>
            <Result>
                <Field Name="RX_RECORD_TYPE" Size="3" Type="UINT" Description="Rx record type."/>
                <Field Name="TX_RECORD_TYPE" Size="1" Type="UINT" Description="Tx record type"/>
                <Field Name="RX_RECORD_SIZE" Size="5" Type="UINT" Description="Rx record size, byte resolution, add 1 for actual size"/>
                <Field Name="TX_RECORD_SIZE" Size="7" Type="UINT" Description="Tx record size, byte resolution, add 1 for actual size"/>
                <Field Name="CHECK_ELK_ERROR" Size="1" Type="UINT" Description="If set, check error bit at LSB of first byte of record"/>
            </Result>
            <TableLabels>EGW</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="RX_RECORD_TYPE">
                    <Access>
                        <Memory Name="IPPD_OPCODE_MAP_RXm">
                            <EntryOffset Formula="OPCODE" />
                            <HwField Name="TYPEf" />
                        </Memory>
                    </Access>
                </Field>
                <Field Name="TX_RECORD_TYPE">
                    <Access>
                        <Memory Name="IPPD_OPCODE_MAP_TXm">
                            <EntryOffset Formula="OPCODE" />
                            <HwField Name="TYPEf" />
                        </Memory>
                    </Access>
                </Field>
                <Field Name="RX_RECORD_SIZE">
                    <Access>
                        <Memory Name="IPPD_OPCODE_MAP_RXm">
                            <EntryOffset Formula="OPCODE" />
                            <HwField Name="SIZEf" />
                        </Memory>
                    </Access>
                </Field>
                <Field Name="TX_RECORD_SIZE">
                    <Access>
                        <Memory Name="IPPD_OPCODE_MAP_TXm">
                            <EntryOffset Formula="OPCODE" />
                            <HwField Name="SIZEf" />
                        </Memory>
                    </Access>
                </Field>
                <Field Name="CHECK_ELK_ERROR">
                    <Access>
                        <Memory Name="IPPD_OPCODE_MAP_RXm">
                            <EntryOffset Formula="OPCODE" />
                            <HwField Name="CHECK_ELK_ERRORf" />
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="EGW_ROP_FIFO_TABLE">
        <OneLineDescription>EGW ROP FIFO table.</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID" />
                <Field Name="FIFO_INDEX" Size="9" Type="UINT"/>
                <Field Name="ROP_TYPE" Type="ROP_PACKET_TYPE"/>
            </Key>
            <Result>
                <Field Name="ROP_DATA" Size="512" Type="ARRAY8" Description="ROP DATA"/>
            </Result>
            <TableLabels>EGW</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="ROP_DATA">
                    <Access>
                        <KeyCondition Field="ROP_TYPE" Type="EQUAL_TO" Enum="REQUEST"/>
                        <Memory Name="IPPD_FIFO_ELK_TXm">
                            <EntryOffset Formula="FIFO_INDEX" />
                            <HwField Name="DATAf" />
                            <DataOffset Formula="8"/>
                        </Memory>
                    </Access>
                    <Access>
                        <KeyCondition Field="ROP_TYPE" Type="EQUAL_TO" Enum="RESPONSE"/>
                        <Memory Name="IPPD_FIFO_ELK_RXm">
                            <EntryOffset Formula="FIFO_INDEX" />
                            <HwField Name="DATAf" />
                            <DataOffset Formula="8"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

