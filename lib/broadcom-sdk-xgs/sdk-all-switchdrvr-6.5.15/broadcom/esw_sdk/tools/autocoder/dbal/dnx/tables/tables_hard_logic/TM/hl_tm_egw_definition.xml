                <Field Name="REQUEST_PENDING" Permission="READONLY" Size="1" Type="BOOL" Description="A CPU request still waiting for ELK to return a reply."/>
                <Field Name="REQUEST_FIFO_READY" Permission="READONLY" Size="1" Type="BOOL" Description="CPU request FIFO ready for another command"/>
                <Field Name="REQUEST_FIFO_STATUS" Permission="READONLY" Size="5" Type="UINT" Description="CPU request FIFO status."/>
                <Field Name="RECORD_DATA_LSB_TRIGGER" Size="1" Permission="TRIGGER" Type="BOOL" Description="If set, writing to CpuRecordDataLsb will trigger CPU request."/>
                <Field Name="RECORD_DATA_MSB_TRIGGER" Size="1" Permission="TRIGGER" Type="BOOL" Description="If set, writing to CpuRecordDataMsb will trigger CPU request."/>
