<AppDbCatalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../hard_logic_schema.xsd">

    <AppDB Name="EGW_LOOKUP_CTRL">
        <OneLineDescription>This table is used for controlling the EGW lookup.</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID" />
            </Key>
            <Result>
                <Field Name="ELK_LOOKUP_EN" Size="1" Type="BOOL" Description="Enables lookups to ELK"/>
                <Field Name="LOOKUP_TIMEOUT_DLY" Size="16" Type="UINT" Description="Number of clocks to issue time out (0 is disable)"/>
            </Result>
            <TableLabels>EGW</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="ELK_LOOKUP_EN">
                    <Access>
                        <Register Name="IPPD_GENERAL_CFGSr">
                            <HwField Name="ENABLE_ELK_LOOKUPf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="LOOKUP_TIMEOUT_DLY">
                    <Access>
                        <Register Name="IPPD_GENERAL_CFGSr">
                            <HwField Name="TIMEOUT_DLYf"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    <AppDB Name="EGW_TRANSMIT_ATTRIBUTES_CFG">
        <OneLineDescription>This table is used for configuring the EGW transmit attributes.</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID" />
            </Key>
            <Result>
                <Field Name="MIN_PKT_SIZE" Size="14" Type="UINT" Description="Minimum packet size in Bytes."/>
                <Field Name="MAX_PKT_SIZE" Size="14" Type="UINT" Description="Maximum packet size in Bytes."/>
                <Field Name="FLP_TRANSMIT_THRESHOLD" Size="9" Type="UINT" Description="EGW will not transmit FLP/CPU records if FIFO exceed this threshold."/>
                <Field Name="CONGESTION_THRESHOLD" Size="9" Type="UINT" Description="Congestion identified when FIFO exceed this threshold."/>
                <Field Name="ROP_PACKING_ENABLE" Size="1" Type="BOOL" Description="Enable ROP style packing."/>
                <Field Name="CPU_RECORD_PRIO" Size="1" Type="BOOL" Description="If set, CPU has priority over traffic lookups."/>
                <Field Name="TIMER_MODE" Size="1" Type="BOOL" Description="Enable terminating the packet even if not reached minimum packet size."/>
                <Field Name="TIMER_DELAY" Size="9" Type="UINT" Description="Number of cycles for timer to count untill termiate packet."/>
                <Field Name="TX_APP_PREFIX_EN" Size="1" Type="BOOL" Description="Enable Attaching application prefix in packet transmition."/>
                <Field Name="RX_APP_PREFIX_EN" Size="1" Type="BOOL" Description="Enable removing application prefix in packet receiving."/>
                <Field Name="TX_CHANNEL_NUM" Size="8" Type="UINT" Description="Channel number for transmitter."/>
                <Field Name="RX_CHANNEL_NUM" Size="8" Type="UINT" Description="Channel number for receiver."/>
                <Field Name="ILKN_MODE" Size="1" Type="BOOL" Description="If set, enable interlaken mode, to compensate Interlaken overheads in shaper."/>
                <Field Name="ILKN_MIN_BURST" Size="3" Type="UINT" Description="Interlaken minimum burst in 32B resolution."/>
                <Field Name="ILKN_MAX_BURST" Size="5" Type="UINT" Description="Interlaken maximum burst in 32B resolution."/>
            </Result>
            <TableLabels>EGW</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="MIN_PKT_SIZE">
                    <Access>
                        <Register Name="IPPD_PACKET_SIZEr">
                            <HwField Name="MIN_PKT_SIZEf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="MAX_PKT_SIZE">
                    <Access>
                        <Register Name="IPPD_PACKET_SIZEr">
                            <HwField Name="MAX_PKT_SIZEf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="FLP_TRANSMIT_THRESHOLD">
                    <Access>
                        <Register Name="IPPD_TRANSMIT_CFGSr">
                            <HwField Name="FLP_TRANSMITING_THRESHOLDf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="CONGESTION_THRESHOLD">
                    <Access>
                        <Register Name="IPPD_TRANSMIT_CFGSr">
                            <HwField Name="CONGESTION_THRESHOLDf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="ROP_PACKING_ENABLE">
                    <Access>
                        <Register Name="IPPD_TRANSMIT_CFGSr">
                            <HwField Name="PACKING_ENABLEf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="CPU_RECORD_PRIO">
                    <Access>
                        <Register Name="IPPD_TRANSMIT_CFGSr">
                            <HwField Name="CPU_RECORD_PRIOf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="TIMER_MODE">
                    <Access>
                        <Register Name="IPPD_TRANSMIT_CFGSr">
                            <HwField Name="TIMER_MODEf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="TIMER_DELAY">
                    <Access>
                        <Register Name="IPPD_TRANSMIT_CFGSr">
                            <HwField Name="TIMER_DELAYf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="TX_APP_PREFIX_EN">
                    <Access>
                        <Register Name="IPPD_APP_PREFIX_CONTROLr">
                            <HwField Name="TX_APP_PREFIX_ENABLEf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="RX_APP_PREFIX_EN">
                    <Access>
                        <Register Name="IPPD_APP_PREFIX_CONTROLr">
                            <HwField Name="RX_APP_PREFIX_ENABLEf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="TX_CHANNEL_NUM">
                    <Access>
                        <Register Name="IPPD_PHY_CHANNEL_CFGSr">
                            <HwField Name="TX_CHANNEL_NUMf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="RX_CHANNEL_NUM">
                    <Access>
                        <Register Name="IPPD_PHY_CHANNEL_CFGSr">
                            <HwField Name="RX_CHANNEL_NUMf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="ILKN_MODE">
                    <Access>
                        <Register Name="IPPD_INTERLAKEN_CFGSr">
                            <HwField Name="ILKN_MODEf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="ILKN_MIN_BURST">
                    <Access>
                        <Register Name="IPPD_INTERLAKEN_CFGSr">
                            <HwField Name="ILKN_MIN_BURSTf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="ILKN_MAX_BURST">
                    <Access>
                        <Register Name="IPPD_INTERLAKEN_CFGSr">
                            <HwField Name="ILKN_MAX_BURSTf" />
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    <AppDB Name="EGW_SHAPER_CFG">
        <OneLineDescription>This table is used for configuring the EGW shapper.</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID" />
            </Key>
            <Result>
                <Field Name="SHAPER_DLY" Size="8" Type="UINT" Description="Shaper number of cycles between two egw words (32B) in transmitter."/>
                <Field Name="SHAPER_DLY_FRACTION" Size="16" Type="UINT" Description="Tunning of shaper in 1/64K cycle units."/>
                <Field Name="SHAPER_MAX_BURST" Size="9" Type="UINT" Description="Maximal number of tokens (32B) that can be accumulated."/>
                <Field Name="WORD_ALIGNMENT" Size="3" Type="UINT" Description="Alignment for word to be considered by shaper after overhead."/>
                <Field Name="PACKET_GAP" Size="5" Type="UINT" Description="Gap in Bytes between packets after alignment."/>
                <Field Name="PACKET_OVERHEAD" Size="5" Type="UINT" Description="Overhead in Bytes to be considered (per packet) by shaper."/>
            </Result>
            <TableLabels>EGW</TableLabels>
        </AppDbInterface>
