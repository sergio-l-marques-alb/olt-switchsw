                        </Register>
                    </Access>
                </Field>
                <Field Name="SHAPER_DLY_FRACTION">
                    <Access>
                        <Register Name="IPPD_SPR_DLY_CFGSr">
                            <HwField Name="SPR_DLY_FRACTIONf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="SHAPER_MAX_BURST">
                    <Access>
                        <Register Name="IPPD_SPR_DLY_CFGSr">
                            <HwField Name="SPR_MAX_BURSTf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="WORD_ALIGNMENT">
                    <Access>
                        <Register Name="IPPD_SPR_CFGSr">
                            <HwField Name="WORD_ALIGNMENTf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="PACKET_GAP">
                    <Access>
                        <Register Name="IPPD_SPR_CFGSr">
                            <HwField Name="PACKET_GAPf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="PACKET_OVERHEAD">
                    <Access>
                        <Register Name="IPPD_SPR_CFGSr">
                            <HwField Name="PACKET_OVERHEADf" />
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    <AppDB Name="EGW_CPU_RECORD_CTRL">
