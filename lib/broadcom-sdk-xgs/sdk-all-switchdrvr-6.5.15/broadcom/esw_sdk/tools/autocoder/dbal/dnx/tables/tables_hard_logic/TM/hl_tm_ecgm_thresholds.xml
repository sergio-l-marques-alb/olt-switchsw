  <!--
    **********************************************************************
    Table: ECGM_CORE_GLOBAL_THRESHOLDS
    Used in API: bcm_cosq_gport_threshold_ set / get
    Mapping of core thresholds: 
    packet descriptors - max allocated PDs and flow control threshold.
    data buffers - max allocated DBs and flow control threshold.
    **********************************************************************
-->
  <AppDB Name="ECGM_CORE_GLOBAL_THRESHOLDS">
    <OneLineDescription>This table contains core PD and DB thresholds.</OneLineDescription>
    <AccessMethod>HARD_LOGIC</AccessMethod>
    <Maturity Level="HIGH"/>

    <AppDbInterface Type="DIRECT">
      <Key>
        <Field Name="CORE_ID"/>
      </Key>
      <Result>
        <Field Name="PD_DROP" Type="ECGM_PD_THRESHOLD"/>
        <Field Name="PD_FLOW_CONTROL" Type="ECGM_PD_THRESHOLD"/>
        <Field Name="DB_DROP" Type="UINT" Size="15"/>
        <Field Name="DB_FLOW_CONTROL" Type="UINT" Size="15"/>
      </Result>
      <TableLabels>ECGM ETM FC</TableLabels>
    </AppDbInterface>

    <AppToPhyDbMapping>
      <CoreMode Mode="DPC"/>
      <FieldMapping>
        <Field Name="PD_DROP">
          <Access>
            <Register Name="ECGM_CONGESTION_MANAGEMENT_GLOBAL_PD_THRESHOLDSr">
              <HwField Name="MAXIMUM_TOTAL_PACKET_DESCRIPTORSf"/>
            </Register>
          </Access>
        </Field>
        <Field Name="PD_FLOW_CONTROL">
          <Access>
            <Register Name="ECGM_CONGESTION_MANAGEMENT_GLOBAL_FLOW_CONTROL_THRESHOLDSr">
              <HwField Name="TOTAL_PACKET_DESCRIPTORS_FLOW_CONTROLLIMITf"/>
            </Register>
          </Access>
        </Field>
        <Field Name="DB_DROP">
          <Access>
            <Register Name="ECGM_CONGESTION_MANAGEMENT_GLOBAL_DB_THRESHOLDSr">
              <HwField Name="MAXIMUM_TOTAL_DATA_BUFFERSf"/>
            </Register>
          </Access>
        </Field>
        <Field Name="DB_FLOW_CONTROL">
          <Access>
            <Register Name="ECGM_CONGESTION_MANAGEMENT_GLOBAL_FLOW_CONTROL_THRESHOLDSr">
              <HwField Name="TOTAL_DATA_BUFFERS_FLOW_CONTROL_LIMITf"/>
            </Register>
          </Access>
        </Field>
      </FieldMapping>
    </AppToPhyDbMapping>
  </AppDB>


