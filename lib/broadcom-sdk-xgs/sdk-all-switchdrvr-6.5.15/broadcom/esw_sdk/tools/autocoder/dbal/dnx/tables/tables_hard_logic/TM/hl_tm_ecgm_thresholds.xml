  <!--
    **********************************************************************
    Table: ECGM_CORE_MULTICAST_SP_THRESHOLDS
    Used in API: bcm_cosq_gport_threshold_ set / get
    Mapping of ETM resources per service pool (SP) ID:
    packet descriptors - max PDs per service pool, flow control threshold and reserved. 
    data buffers - max DBs per service pool, flow control threshold and reserved. 
    **********************************************************************
-->

  <AppDB Name="ECGM_CORE_MULTICAST_SP_THRESHOLDS">
    <OneLineDescription>This table contains multicast service pool thresholds for PD and DB.</OneLineDescription>
    <AccessMethod>HARD_LOGIC</AccessMethod>
    <Maturity Level="HIGH"/>

    <AppDbInterface Type="DIRECT">
      <Key>
        <Field Name="CORE_ID"/>
        <Field Name="SERVICE_POOL_ID"/>
      </Key>
      <Result>
        <Field Name="PD_DROP" Type="ECGM_PD_THRESHOLD"/>
        <Field Name="PD_FLOW_CONTROL" Type="ECGM_PD_THRESHOLD"/>
        <Field Name="DB_DROP" Type="UINT" Size="15"/>
        <Field Name="DB_FLOW_CONTROL" Type="UINT" Size="15"/>
        <Field Name="RESERVED_PD" Type="ECGM_PD_THRESHOLD"/>
        <Field Name="RESERVED_DB" Type="UINT" Size="15"/>
      </Result>
      <TableLabels>ECGM ETM FC</TableLabels>
    </AppDbInterface>

    <AppToPhyDbMapping>
      <CoreMode Mode="DPC"/>
      <FieldMapping>
        <Field Name="PD_DROP">
          <Access>
            <Register Name="ECGM_CONGESTION_MANAGEMENT_GLOBAL_PD_THRESHOLDSr">
              <DataOffset Formula="48+SERVICE_POOL_ID*16"/>
            </Register>
          </Access>
        </Field>
        <Field Name="PD_FLOW_CONTROL">
          <Access>
            <Register Name="ECGM_CONGESTION_MANAGEMENT_GLOBAL_FLOW_CONTROL_THRESHOLDSr">
              <DataOffset Formula="93+SERVICE_POOL_ID*16"/>
            </Register>
          </Access>
        </Field>
        <Field Name="DB_DROP">
          <Access>
            <Register Name="ECGM_CONGESTION_MANAGEMENT_GLOBAL_DB_THRESHOLDSr">
              <DataOffset Formula="45+SERVICE_POOL_ID*15"/>
            </Register>
          </Access>
        </Field>
        <Field Name="DB_FLOW_CONTROL">
          <Access>
            <Register Name="ECGM_CONGESTION_MANAGEMENT_GLOBAL_FLOW_CONTROL_THRESHOLDSr">
              <DataOffset Formula="125+SERVICE_POOL_ID*15"/>
            </Register>
          </Access>
        </Field>
        <Field Name="RESERVED_PD">
          <Access>
            <Register Name="ECGM_CONGESTION_MANAGEMENT_MULTICAST_RESERVED_RESOURCESr">
              <DataOffset Formula="SERVICE_POOL_ID*16"/>
            </Register>
          </Access>
        </Field>
        <Field Name="RESERVED_DB">
          <Access>
            <Register Name="ECGM_CONGESTION_MANAGEMENT_MULTICAST_RESERVED_RESOURCESr">
              <DataOffset Formula="32+SERVICE_POOL_ID*15"/>
            </Register>
          </Access>
        </Field>
      </FieldMapping>
    </AppToPhyDbMapping>
  </AppDB>


