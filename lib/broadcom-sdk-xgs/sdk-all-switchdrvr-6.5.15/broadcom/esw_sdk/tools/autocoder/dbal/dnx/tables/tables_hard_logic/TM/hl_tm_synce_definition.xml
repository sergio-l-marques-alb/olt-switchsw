                            <ArrayOffset Formula="SYNCE_INDEX"/>
                            <BlockIndex Formula="CORE_ID" />
                            <HwField Name="SYNC_ETH_CLOCK_DIV_Nf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="DIV_PHASE_ZERO">
                    <Access>
                        <Register Name="NMG_SYNC_ETH_CFGr">
                            <ArrayOffset Formula="SYNCE_INDEX"/>
                            <BlockIndex Formula="CORE_ID" />
                            <HwField Name="SYNC_ETH_DIVIDER_PHASE_ZERO_Nf" />
                        </Register>
                    </Access>
                </Field>
                <Field Name="DIV_PHASE_ONE">
                    <Access>
                        <Register Name="NMG_SYNC_ETH_CFGr">
                            <ArrayOffset Formula="SYNCE_INDEX"/>
                            <BlockIndex Formula="CORE_ID" />
                            <HwField Name="SYNC_ETH_DIVIDER_PHASE_ONE_Nf" />
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
