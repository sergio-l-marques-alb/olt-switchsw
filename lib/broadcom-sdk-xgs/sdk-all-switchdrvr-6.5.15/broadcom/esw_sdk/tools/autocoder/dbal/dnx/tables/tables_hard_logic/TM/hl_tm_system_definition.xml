<?xml version="1.0" encoding="UTF-8"?>

<!--
    ******************************************************
    DBAL Hard-Logic Logical Tables:
    This xml contains the hard-logic logical DB for the DBAL.
    The data is loaded to the device during init process
    ******************************************************

    DB Rules:
    *********
-->

<AppDbCatalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../hard_logic_schema.xsd">

    <AppDB Name="SYSTEM_CFG">
        <OneLineDescription>System configuration per core</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID"/>
            </Key>
            <Result>
                <Field Name="FAP_ID"/>
            </Result>
            <TableLabels>SYSTEM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="FAP_ID">
                    <Access>
                        <KeyCondition Type="EQUAL_TO" Value="0" Field="CORE_ID"/>
                        <Register Name="ECI_FAP_GLOBAL_GENERAL_CFG_2r">
                            <HwField Name="DEV_ID_0f"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Type="EQUAL_TO" Value="1" Field="CORE_ID"/>
                        <Register Name="ECI_FAP_GLOBAL_GENERAL_CFG_2r">
                            <HwField Name="DEV_ID_1f"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <AppDB Name="SYSTEM_SYSPORT_MODPORT_MAP">
        <OneLineDescription>Map destination system port to destination fap id and tm port</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID"/>
                <Field Name="DST_SYS_PORT_ID" Type="UINT" Size="15"/>
            </Key>
            <Result>
                <Field Name="FAP_ID"/>
                <Field Name="TM_PORT"/>
            </Result>
            <TableLabels>SYSTEM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="FAP_ID">
                    <Access>
                        <Memory Name="IPS_SPMm">
                            <EntryOffset Formula="DST_SYS_PORT_ID"/>
                            <HwField Name="DST_FAPf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="TM_PORT">
                    <Access>
                        <Memory Name="IPS_SPMm">
                            <EntryOffset Formula="DST_SYS_PORT_ID"/>
                            <HwField Name="PP_DSPf"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="SYSTEM_PP_DSP_MAPPING">
        <OneLineDescription>Map pp-dsp to tm, pp and system port</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID"/>
                <Field Name="PP_DSP" Type="DSP"/>
            </Key>
            <Result>
                <Field Name="TM_PORT"/>
                <Field Name="PP_PORT"/>
                <Field Name="SYSTEM_PORT"/>
            </Result>
            <TableLabels>SYSTEM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="PP_PORT">
                    <Access>
                        <Memory Name="ERPP_PRP_PP_DSP_PTR_TABLEm">
                            <EntryOffset Formula="PP_DSP"/>
                            <HwField Name="OUT_PP_PORTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ERPP_TM_PP_DSP_PTR_TABLEm">
                            <EntryOffset Formula="PP_DSP"/>
                            <HwField Name="OUT_PP_PORTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ETPPA_DSP_DATA_TABLEm">
                            <EntryOffset Formula="PP_DSP"/>
                            <HwField Name="OUT_PP_PORTf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="SYSTEM_PORT">
                    <Access>
                        <Memory Name="ETPPA_DSP_DATA_TABLEm">
                            <EntryOffset Formula="PP_DSP"/>
                            <HwField Name="DST_SYS_PORTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ERPP_TM_PP_DSP_PTR_TABLEm">
                            <EntryOffset Formula="PP_DSP"/>
                            <HwField Name="DST_SYS_PORTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ERPP_PRP_PP_DSP_PTR_TABLEm">
                            <EntryOffset Formula="PP_DSP"/>
                            <HwField Name="DST_SYS_PORTf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="TM_PORT">
                    <Access>
                        <Memory Name="ETPPA_DSP_DATA_TABLEm">
                            <EntryOffset Formula="PP_DSP"/>
                            <HwField Name="OUT_TM_PORTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ERPP_TM_PP_DSP_PTR_TABLEm">
                            <EntryOffset Formula="PP_DSP"/>
                            <HwField Name="OUT_TM_PORTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ERPP_PRP_PP_DSP_PTR_TABLEm">
                            <EntryOffset Formula="PP_DSP"/>
                            <HwField Name="OUT_TM_PORTf"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="SYSTEM_CLOS_DQCQ_MAP">
        <OneLineDescription>Map a packet to DQCQ context according to FAP-ID. 
Local-0 if:
(DestDev | MASK_LOCAL_0) == (FAP_ID_LOCAL_0| MASK_LOCAL_0) 
Local-1 if:
(DestDev | MASK_LOCAL_1) == (FAP_ID_LOCAL_1| MASK_LOCAL_1)
else:
fabric   
        </OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="FAP_ID_LOCAL_0" Type="UINT" Size="11"/>
                <Field Name="MASK_LOCAL_0" Type="UINT" Size="11"/>
                <Field Name="FAP_ID_LOCAL_1" Type="UINT" Size="11"/>
                <Field Name="MASK_LOCAL_1" Type="UINT" Size="11"/>
            </Result>
            <TableLabels>SYSTEM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="FAP_ID_LOCAL_0">
                    <Access>
                        <Register Name="IPS_DEV_ID_TO_CONTEXT_MAPr">
                            <HwField Name="DEV_ID_TO_CONTEXT_MAPf"/>
                            <DataOffset Formula="0"/>
                        </Register>
                    </Access>
                    <Access>
                        <Register Name="IPS_DEV_ID_TO_CONTEXT_MAPr">
                            <HwField Name="DEV_ID_TO_CONTEXT_MAPf"/>
                            <DataOffset Formula="16"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="MASK_LOCAL_0">
                    <Access>
                        <Register Name="IPS_DEV_ID_MASKr">
                            <HwField Name="DEV_ID_MASK_0f"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FAP_ID_LOCAL_1">
                    <Access>
                        <Register Name="IPS_DEV_ID_TO_CONTEXT_MAPr">
                            <HwField Name="DEV_ID_TO_CONTEXT_MAPf"/>
                            <DataOffset Formula="32"/>
                        </Register>
                     </Access>
                     <Access>
                        <Register Name="IPS_DEV_ID_TO_CONTEXT_MAPr">
                            <HwField Name="DEV_ID_TO_CONTEXT_MAPf"/>
                            <DataOffset Formula="48"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="MASK_LOCAL_1">
                    <Access>
                        <Register Name="IPS_DEV_ID_MASKr">
                            <HwField Name="DEV_ID_MASK_1f"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
    <AppDB Name="SYSTEM_MESH_DQCQ_MAP">
        <OneLineDescription>Map a packet to DQCQ context according to FAP-ID</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="FAP_ID_LSB" Type="UINT" Size="4" Description="4 LSBs in FAP-ID"/>
            </Key>
            <Result>
                <Field Name="SYSTEM_MESH_CONTEXT" Size="3"/>
            </Result>
            <TableLabels>SYSTEM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="SYSTEM_MESH_CONTEXT">
                    <Access>
                        <Register Name="IPS_DEV_ID_TO_CONTEXT_MAPr">
                            <HwField Name="DEV_ID_TO_CONTEXT_MAPf"/>
                            <DataOffset Formula="FAP_ID_LSB*4"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    

    <AppDB Name="SYSTEM_TRAFFIC_ENABLER">
        <OneLineDescription>Enable/disable device level traffic per stage</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="SYSTEM_TRAFFIC_STAGE"/>
            </Key>
            <Result>
                <Field Name="ENABLE" Type="BOOL"/>
            </Result>
            <TableLabels>SYSTEM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="ENABLE">
                    <Access>
                        <KeyCondition Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO" Enum="IRE"/>
                        <Register Name="IRE_DYNAMIC_CONFIGURATIONr">
                            <HwField Name="ENABLE_DATA_PATHf"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO" Enum="SPB"/>
                        <Register Name="SPB_DYNAMIC_CONFIGURATIONr">
                            <HwField Name="ENABLE_DATA_PATHf"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO" Enum="CGM"/>
                        <Register Name="CGM_ENABLERSr">
                            <HwField Name="RRP_ENf"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO" Enum="CGM"/>
                        <Register Name="CGM_ENABLERSr">
                            <HwField Name="ENQ_ENf"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO" Enum="CGM"/>
                        <Register Name="CGM_ENABLERSr">
                            <HwField Name="DEQ_TO_FABRIC_ENf"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO" Enum="CGM"/>
                        <Register Name="CGM_ENABLERSr">
                            <HwField Name="DEQ_TO_DRAM_ENf"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO" Enum="CGM"/>
                        <Register Name="CGM_ENABLERSr">
                            <HwField Name="QM_RPRT_ENf"/>
                            <DataOffset Formula="0"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO" Enum="CGM"/>
                        <Register Name="CGM_ENABLERSr">
                            <HwField Name="QM_RPRT_ENf"/>
                            <DataOffset Formula="1"/>
                        </Register>
                    </Access>
                    <Access>
                        <KeyCondition Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO" Enum="CGM"/>
                        <Register Name="CGM_ENABLERSr">
                            <HwField Name="LOW_PRIORITY_INT_ENf"/>
                        </Register>
                    </Access>
                    <Access EncodeType="BITWISE_NOT">
                        <KeyCondition Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO" Enum="EGQ"/>
                        <Register Name="RQP_GENERAL_RQP_DEBUG_CONFIGr">
                            <HwField Name="DBG_FDA_STOPf"/>
                            <DataOffset Formula="0"/>
                        </Register>
                    </Access>
                    <Access EncodeType="BITWISE_NOT">
                        <KeyCondition Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO" Enum="SCH_FSM_PROCESS"/>
                        <Register Name="SCH_SCHEDULER_CONFIGURATION_REGISTERr">
                            <HwField Name="DISABLE_FABRIC_MSGSf"/>
                            <DataOffset Formula="0"/>
                        </Register>
                    </Access>
                    <Access EncodeType="BITWISE_NOT">
                        <KeyCondition Field="SYSTEM_TRAFFIC_STAGE" Type="EQUAL_TO" Enum="SCH_CREDIT_GENERATION"/>
                        <Register Name="SCH_DVS_CONFIGr">
                            <HwField Name="FORCE_PAUSEf"/>
                            <DataOffset Formula="0"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="SYSTEM_MODULE_MAX_BASE_INDEX">
        <OneLineDescription>Set maximum module id in the system by setting maximum base index</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="MAX_BASE_INDEX" Size="6" Type="UINT" Description="Maximum base index that represents the maximum FAP ID in the system [MaxFapID=(MAX_BASE_INDEX+1)*32-1]"/>
                <Field Name="MAX_BASE_INDEX_MC" Size="6" Type="UINT" Description="Maximum base index that represents the maximum FAP ID for multicast traffic in the system [MaxMCFapID=(MAX_BASE_INDEX_MC+1)*32-1]"/>
            </Result>
            <TableLabels>STACK</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="MAX_BASE_INDEX">
                    <Access>
                        <Register Name="RTP_MAXIMUM_BASE_INDEXr">
                            <HwField Name="MAX_BIf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="MAX_BASE_INDEX_MC">
                    <Access>
                        <Register Name="RTP_MULTICAST_DISTRIBUTION_CONFIGURATIONr">
                            <HwField Name="MAX_ALRC_BASE_INDEXf"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="SYSTEM_MODULE_MC_FAP_ID_EXCLUDE">
        <OneLineDescription>Exclude specific FAP IDs from multicast list</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <!-- There is limited number of faps that can be specifically excluded -->
                <Field Name="INDEX" Size="4" Type="UINT" Description="Index of the excluded fap in the specifically excluded list"/>
            </Key>
            <Result>
                <Field Name="FAP_ID" Description="The module id to exclude"/>
                <Field Name="IS_EXCLUDED" Size="1" Type="BOOL" Description="Is fap id excluded"/>
            </Result>
            <TableLabels>STACK</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
