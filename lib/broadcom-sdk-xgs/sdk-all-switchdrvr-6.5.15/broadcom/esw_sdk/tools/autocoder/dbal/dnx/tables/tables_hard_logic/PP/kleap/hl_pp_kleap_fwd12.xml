    <AppDB Name="KLEAP_FWD12_RESOURCES_MAPPING">
        <OneLineDescription>Indicates per Context the KBR mapping in FWD stage. 0 means FWD1, 1 means FWD2</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="KBR_SELECTOR" Type="KLEAP_STAGE_SELECTOR" NofInstances="18"/>
                <Field Name="FFC_GROUP_SELECTOR" Type="KLEAP_STAGE_SELECTOR" NofInstances="5"/>
                <Field Name="SHR_PD_SELECTOR" Type="KLEAP_STAGE_SELECTOR" NofInstances="2"/>
            </Result>
            <TableLabels>L3</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping> 
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="KBR_SELECTOR">
                    <Access instanceIndex="ALL">
                        <Register Name="IPPB_FLPLE_CONFIGr"> 
                            <HwField Name="KEYS_BUILDER_STAGE_SELECTORSf"/>
                            <DataOffset Formula="INSTANCE"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="FFC_GROUP_SELECTOR">
                    <Access instanceIndex="ALL">
                        <Register Name="IPPB_FLPLE_CONFIGr"> 
                            <HwField Name="FFC_GROUP_STAGE_SELECTORf"/>
                            <DataOffset Formula="INSTANCE"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="SHR_PD_SELECTOR">
                    <Access instanceIndex="ALL">
                        <Register Name="IPPB_FLPLE_CONFIGr"> 
                            <HwField Name="SHARED_PDS_STAGE_SELECTORSf"/>
                            <DataOffset Formula="INSTANCE"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="KLEAP_FWD12_FFC_QUAD_IS_ACL">
        <OneLineDescription>Indicates per FFC Quad if it is controlled by ACL or FWD context</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CONTEXT_PROFILE" Size="6"/>
                <Field Name="FFC_QUAD_IDX" Type="UINT" Size="4" MaxValue="9"/>
            </Key>
            <Result>
                <Field Name="FFC_QUAD_IS_ACL" Type="BOOL"/>
            </Result>
            <TableLabels>L3</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping> 
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="FFC_QUAD_IS_ACL">
                    <Access>
                        <KeyCondition Field="FFC_QUAD_IDX" Type="EQUAL_TO" Value="0"/>
                        <Memory Name="IPPB_FLPLE_FFC_GROUP_0_QUAD_IS_ACL_CONTEXTm"> 
                            <EntryOffset Formula="CONTEXT_PROFILE"/>
                            <HwField Name="FFC_GROUP_0_QUAD_IS_ACL_CONTEXTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <KeyCondition Field="FFC_QUAD_IDX" Type="EQUAL_TO" Value="1"/>
                        <Memory Name="IPPB_FLPLE_FFC_GROUP_1_QUAD_IS_ACL_CONTEXTm"> 
                            <EntryOffset Formula="CONTEXT_PROFILE"/>
                            <HwField Name="FFC_GROUP_1_QUAD_IS_ACL_CONTEXTf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <KeyCondition Field="FFC_QUAD_IDX" Type="BIGGER_THAN" Value="1"/>
                        <KeyCondition Field="FFC_QUAD_IDX" Type="LOWER_THAN" Value="4"/>
                        <Memory Name="IPPB_FLPLE_FFC_GROUP_2_QUAD_IS_ACL_CONTEXTm"> 
                            <EntryOffset Formula="CONTEXT_PROFILE"/>
                            <HwField Name="FFC_GROUP_2_QUAD_IS_ACL_CONTEXTf"/>
                            <DataOffset Formula="FFC_QUAD_IDX-2"/>
                        </Memory>
                    </Access>
                    <Access>
                        <KeyCondition Field="FFC_QUAD_IDX" Type="BIGGER_THAN" Value="3"/>
                        <KeyCondition Field="FFC_QUAD_IDX" Type="LOWER_THAN" Value="8"/>
                        <Memory Name="IPPB_FLPLE_FFC_GROUP_3_QUAD_IS_ACL_CONTEXTm"> 
                            <EntryOffset Formula="CONTEXT_PROFILE"/>
                            <HwField Name="FFC_GROUP_3_QUAD_IS_ACL_CONTEXTf"/>
                            <DataOffset Formula="FFC_QUAD_IDX-4"/>
                        </Memory>
                    </Access>
                    <Access>
                        <KeyCondition Field="FFC_QUAD_IDX" Type="BIGGER_THAN" Value="7"/>
                        <Memory Name="IPPB_FLPLE_FFC_GROUP_4_QUAD_IS_ACL_CONTEXTm"> 
                            <EntryOffset Formula="CONTEXT_PROFILE"/>
                            <HwField Name="FFC_GROUP_4_QUAD_IS_ACL_CONTEXTf"/>
                            <DataOffset Formula="FFC_QUAD_IDX-8"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

