    <AppDB Name="KLEAP_FWD12_ALIGNER_MAPPING">
        <OneLineDescription>Determines the length of the KBR to be copied to Master Key, for both FWD and ACL lookups</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="KBP_KBR_IDX"/>
                <Field Name="CONTEXT_PROFILE" Size="6"/>
            </Key>
            <Result>
                <Field Name="FWD_KEY_SIZE" Type="UINT" Size="8" Description="size in bits of the MSB part of the key used for FWD lookups"/>
                <Field Name="ACL_KEY_SIZE" Type="UINT" Size="8" Description="size in bits of the MSB part of the key (after FWD_KEY_SIZE) used for ACL lookups"/>
            </Result>
            <TableLabels>L3</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="FWD_KEY_SIZE">
                    <Access>
                        <KeyCondition Field="KBP_KBR_IDX" Type="EQUAL_TO" Enum="KEY_0"/>
                        <Memory Name="IPPB_ELK_ALIGNER_MAPPING_MEMm">
                            <EntryOffset Formula="CONTEXT_PROFILE"/>
                            <HwField Name="KEY_0_SIZEf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <KeyCondition Field="KBP_KBR_IDX" Type="EQUAL_TO" Enum="KEY_1"/>
                        <Memory Name="IPPB_ELK_ALIGNER_MAPPING_MEMm">
                            <EntryOffset Formula="CONTEXT_PROFILE"/>
                            <HwField Name="KEY_1_SIZEf"/>
                        </Memory>
                    </Access>
                    <Access>
                        <KeyCondition Field="KBP_KBR_IDX" Type="EQUAL_TO" Enum="KEY_2"/>
                        <Memory Name="IPPB_ELK_ALIGNER_MAPPING_MEMm">
                            <EntryOffset Formula="CONTEXT_PROFILE"/>
                            <HwField Name="KEY_2_SIZEf"/>
