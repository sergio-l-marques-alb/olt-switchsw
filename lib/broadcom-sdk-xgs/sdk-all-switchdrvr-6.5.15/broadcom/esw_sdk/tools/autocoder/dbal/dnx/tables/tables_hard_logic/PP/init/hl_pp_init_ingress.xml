                                </Register>
                        </Access>
                </Field>
                <Field Name="ENABLE_FHEI_WITH_IN_LIF_PROFILE">
                        <Access>
                                <Register Name="IPPD_LBP_GENERAL_CONFIG_0r">
                                        <HwField Name="ENABLE_FHEI_WITH_IN_LIF_PROFILEf"/>
                                </Register>
                        </Access>
                </Field>
        </FieldMapping>
    </AppToPhyDbMapping>
</AppDB>


