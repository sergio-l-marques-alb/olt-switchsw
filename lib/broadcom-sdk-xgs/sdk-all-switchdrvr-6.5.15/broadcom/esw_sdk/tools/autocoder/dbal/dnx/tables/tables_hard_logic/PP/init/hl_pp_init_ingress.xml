<AppDB Name="INGRESS_LBP_GLOBAL">
    <OneLineDescription>Ingress LBP Global Configurations Table</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
        <Result>
            <Field Name="PPH_RESERVED_BITS_SIZE" Size="2" Type="UINT" Description="Valid values are 0-3, bit resolution"/>
            <Field Name="PPH_OUT_LIF_SIZE" Size="5" Type="UINT" Description="Valid values are 16-23, bit resolution"/>
            <Field Name="PPH_IN_LIF_SIZE" Size="5" Type="UINT" Description="Valid values are 16-22, bit resolution"/>
            <Field Name="PPH_IN_LIF_PROFILE_SIZE" Size="4" Type="UINT" Description="Valid values are 4-8, bit resolution"/>
            <Field Name="PPH_FWD_LAYER_IDX_SIZE" Size="2" Type="UINT" Description="Valid values are 1-3, bit resolution"/>
            <Field Name="PPH_FWD_LAYER_ADDITIONAL_INFO_SIZE" Size="3" Type="UINT" Description="Valid values are 0-4, bit resolution"/>
            <Field Name="PPH_FWD_DOMAIN_SIZE" Size="5" Type="UINT" Description="Valid values are 16-18, bit resolution"/>
            <Field Name="PPH_END_OF_PACKET_EDITING_SIZE" Size="2" Type="UINT" Description="Valid values are 0-3, bit resolution"/>
            <Field Name="FTMH_TM_DESTINATION_EXT_PRESENT" Type="BOOL" Description="FTMH destination extension present"/>
            <Field Name="SLB_LEARN_PAYLOAD_START_BIT" Size="6" Type="UINT" Description="Start bit on learn_payload for address to ElephantTrapCfg and SlbCfg tables"/>
            <Field Name="ELEPHANT_TRAP_LEARN_PAYLOAD_START_BIT" Size="5" Type="UINT" Description="Start bit on learn_payload for address to ElephantTrapCfg and SlbCfg tables"/>
            <Field Name="OUT_RIF_SIZE" Type="UINT" Size="15" Description="Max value of Out-RIFs"/>
            <Field Name="JR_MODE_ADD_UDH_BASE" Type="BOOL"  Description="UDH BASE present"/>
            <Field Name="JR_MODE_UDH_SIZE_COMPENSATION_ENABLE" Type="BOOL" Description="Compensate UDH size in sytem header"/>
            <Field Name="ENABLE_FHEI_WITH_IN_LIF_PROFILE" Type="BOOL" Description="enable fhei with inlif_profile"/>
        </Result>
        <TableLabels>L3</TableLabels>
    </AppDbInterface>
    <AppToPhyDbMapping>
        <CoreMode Mode="SBC"/>
        <FieldMapping>
                 <Field Name="PPH_RESERVED_BITS_SIZE">
                        <Access>
                                <Register Name="IPPD_LBP_HEADER_CONFIGr">
                                        <ArrayOffset Formula="0"/>
                                        <HwField Name="PPH_RESERVED_BITS_SIZEf"/>
                                </Register>
                        </Access>
                </Field>
                <Field Name="PPH_OUT_LIF_SIZE">
                        <Access>
                                <Register Name="IPPD_LBP_HEADER_CONFIGr">
                                        <ArrayOffset Formula="0"/>
                                        <HwField Name="PPH_OUT_LIF_SIZEf"/>
                                </Register>
                        </Access>
                </Field>
                <Field Name="PPH_IN_LIF_SIZE">
                        <Access>
                                <Register Name="IPPD_LBP_HEADER_CONFIGr">
                                        <ArrayOffset Formula="0"/>
                                        <HwField Name="PPH_IN_LIF_SIZEf"/>
                                </Register>
                        </Access>
                </Field>
                <Field Name="PPH_IN_LIF_PROFILE_SIZE">
                        <Access>
                                <Register Name="IPPD_LBP_HEADER_CONFIGr">
                                        <ArrayOffset Formula="0"/>
                                        <HwField Name="PPH_IN_LIF_PROFILE_SIZEf"/>
                                </Register>
                        </Access>
                </Field>
                <Field Name="PPH_FWD_LAYER_IDX_SIZE">
                        <Access>
                                <Register Name="IPPD_LBP_HEADER_CONFIGr">
                                        <ArrayOffset Formula="0"/>
                                        <HwField Name="PPH_FWD_LAYER_IDX_SIZEf"/>
                                </Register>
                        </Access>
                </Field>
                <Field Name="PPH_FWD_LAYER_ADDITIONAL_INFO_SIZE">
                        <Access>
                                <Register Name="IPPD_LBP_HEADER_CONFIGr">
                                        <ArrayOffset Formula="0"/>
                                        <HwField Name="PPH_FWD_LAYER_ADDITIONAL_INFO_SIZEf"/>
                                </Register>
                        </Access>
                </Field>
                <Field Name="PPH_FWD_DOMAIN_SIZE">
                        <Access>
                                <Register Name="IPPD_LBP_HEADER_CONFIGr">
                                        <ArrayOffset Formula="0"/>
                                        <HwField Name="PPH_FWD_DOMAIN_SIZEf"/>
                                </Register>
                        </Access>
                </Field>
                <Field Name="PPH_END_OF_PACKET_EDITING_SIZE">
                        <Access>
                                <Register Name="IPPD_LBP_HEADER_CONFIGr">
                                        <ArrayOffset Formula="0"/>
                                        <HwField Name="PPH_END_OF_PACKET_EDITING_SIZEf"/>
                                </Register>
                        </Access>
                </Field>
                <Field Name="FTMH_TM_DESTINATION_EXT_PRESENT">
                        <Access>
                                <Register Name="IPPD_LBP_HEADER_CONFIGr">
