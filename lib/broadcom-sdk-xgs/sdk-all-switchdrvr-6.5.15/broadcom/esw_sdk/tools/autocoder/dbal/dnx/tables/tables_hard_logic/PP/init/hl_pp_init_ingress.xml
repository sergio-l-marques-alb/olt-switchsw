    <AppDB Name="INGRESS_VTT_GLOBAL">
        <OneLineDescription>Ingress VTT Global Configurations Table</OneLineDescription>
            <AccessMethod>HARD_LOGIC</AccessMethod>
            <Maturity Level="HIGH"/>
            <AppDbInterface Type="DIRECT">
                <Key>
                </Key>
            <Result>
                <Field Name="NATIVE_ETH_STAGE_VTE" Type="BOOL" Description="Select whether to process native (inner) ETH at VTE or VTD"/>
                <Field Name="FORCE_VEC_FROM_LIF" Type="BOOL" Description="Force getting the IVEC command from the lif entry"/>
                <Field Name="VTE_IVEC_ACCESS_ENABLE" Type="BOOL" Description="Enable accessing IVEC from VTE stage"/>
                <Field Name="SECOND_VSI_LOOKUP_SEL" Type="BOOL" Description="Select whether to process second vsi lookup at stage VTD or VTE"/>
                <Field Name="SEXEM_INTERFACE_0_SEL" Type="BOOL" Description="Select SEXEM access for interface 0 between vtt stage 1 to vird My-Mac (0 means vtt)"/>
                <Field Name="SEXEM_INTERFACE_1_SEL" Type="BOOL" Description="Select SEXEM aceess for interface 1 between vtt stage 2 to vird My-Mac (0 means vtt)"/>
                <Field Name="SEXEM_INTERFACE_2_SEL" Type="BOOL" Description="Select SEXEM aceess for interface 2 between vtt stage 2 to vird My-Mac (0 means vtt)"/>
            </Result>
            <TableLabels>L3</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                    <Field Name="NATIVE_ETH_STAGE_VTE">
                        <Access>
                            <Register Name="IPPA_IP_STAGE_SELr">
                                <HwField Name="IP_STAGE_SELf"/>
                            </Register>
                        </Access>
                    </Field>
                    <Field Name="FORCE_VEC_FROM_LIF">
                        <Access>
                            <Register Name="IPPA_VTT_GENERAL_CONFIGS_0r">
                                <HwField Name="FORCE_VEC_FROM_LIFf"/>
                            </Register>
                        </Access>
                        <Access>
                            <Register Name="IPPF_VTT_GENERAL_CONFIGS_0r">
                                <HwField Name="FORCE_VEC_FROM_LIFf"/>
                            </Register>
                        </Access>
                    </Field>
                    <Field Name="VTE_IVEC_ACCESS_ENABLE">
                        <Access>
                            <Register Name="IPPA_VTB_ETHERNET_PROC_STAGE_SELr">
                                <HwField Name="IVEC_SELf"/>
                            </Register>
                        </Access>
                    </Field>
                    <Field Name="SECOND_VSI_LOOKUP_SEL">
                        <Access>
                            <Register Name="IPPA_VTB_ETHERNET_PROC_STAGE_SELr">
                                <HwField Name="SECOND_VSI_LKUP_SELf"/>
                            </Register>
                        </Access>
                    </Field>
                    <Field Name="SEXEM_INTERFACE_0_SEL">
                        <Access>
                            <Register Name="IPPF_VTT_GENERAL_CONFIGS_0r">
                                <HwField Name="VTT_SEXEM_IF_0_SELECTf"/>
                            </Register>
                        </Access>
                    </Field>
                    <Field Name="SEXEM_INTERFACE_1_SEL">
                        <Access>
                            <Register Name="IPPF_VTT_GENERAL_CONFIGS_0r">
                                <HwField Name="VTT_SEXEM_IF_1_SELECTf"/>
                            </Register>
                        </Access>
                    </Field>
                    <Field Name="SEXEM_INTERFACE_2_SEL">
                        <Access>
                            <Register Name="IPPA_VTT_GENERAL_CONFIGS_0r">
                                <HwField Name="VTT_SEXEM_IF_2_SELECTf"/>
                            </Register>
                        </Access>
                    </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="FODO_ASSIGNMENT_MODE_MAPPING">
      <OneLineDescription>Forwarding-Domain mapping table to profile</OneLineDescription>
          <AccessMethod>HARD_LOGIC</AccessMethod>
          <Maturity Level="HIGH"/>
      <AppDbInterface Type="DIRECT">
        <Key>
          <Field Name="PD_RESULT_TYPE"       Size="3"/>
          <Field Name="FODO_ASSIGNMENT_MODE" Size="2" Type="FORWARD_DOMAIN_ASSIGNMENT_MODE"/>
        </Key>
        <Result>
          <Field Name="FODO_ASSIGNMENT_PROFILE" Type="FORWARD_DOMAIN_ASSIGNMENT_MODE" Size="2" Description="currently FODO_ASSIGNMENT_MODE is 1:1 mapping to FODO_ASSIGNMENT_PROFILE"/>
        </Result>
        <TableLabels>L2 L3</TableLabels>
      </AppDbInterface>
      <AppToPhyDbMapping>
        <CoreMode Mode="SBC"/>
        <FieldMapping>
          <Field Name="FODO_ASSIGNMENT_PROFILE">
            <Access>
              <Register Name="IPPA_FWD_DOMAIN_MASK_PROFILE_MAPr">
                <ArrayOffset Formula="3"/>
                <DataOffset Formula="KEY*FODO_ASSIGNMENT_PROFILE"/>
              </Register>
            </Access>
            <Access>
              <Register Name="IPPA_FWD_DOMAIN_MASK_PROFILE_MAPr">
                <ArrayOffset Formula="4"/>
                <DataOffset Formula="KEY*FODO_ASSIGNMENT_PROFILE"/>
              </Register>
            </Access>
            <Access>
              <Register Name="IPPF_FWD_DOMAIN_MASK_PROFILE_MAPr">
                <DataOffset Formula="KEY*FODO_ASSIGNMENT_PROFILE"/>
              </Register>
            </Access>
          </Field>
        </FieldMapping>
      </AppToPhyDbMapping>
    </AppDB>

    <AppDB Name="FODO_ASSIGNMENT_PROFILE">
      <OneLineDescription>Forwarding-Domain Profile configuration table</OneLineDescription>
          <AccessMethod>HARD_LOGIC</AccessMethod>
          <Maturity Level="HIGH"/>
      <AppDbInterface Type="DIRECT">
        <Key>
          <Field Name="FODO_ASSIGNMENT_PROFILE" Type="FORWARD_DOMAIN_ASSIGNMENT_MODE" Size="2"/>
        </Key>
        <Result>
          <Field Name="FODO_BASE_MASK" Size="18" Type="UINT" Description="Mask FODO, property of the LIF"/>
          <Field Name="PD_RESULT_MASK" Size="18" Type="UINT" Description="Mask the result of SEM lookup"/>
        </Result>
        <TableLabels>L3</TableLabels>
      </AppDbInterface>
      <AppToPhyDbMapping>
        <CoreMode Mode="SBC"/>
        <FieldMapping>
          <Field Name="FODO_BASE_MASK">
            <Access>
              <Register Name="IPPA_FWD_DOMAIN_MASKSr">
                <ArrayOffset Formula="3"/>
                <DataOffset Formula="KEY*RESULT"/>
              </Register>
            </Access>
            <Access>
              <Register Name="IPPA_FWD_DOMAIN_MASKSr">
                <ArrayOffset Formula="4"/>
                <DataOffset Formula="KEY*RESULT"/>
              </Register>
            </Access>
            <Access>
              <Register Name="IPPF_FWD_DOMAIN_MASKSr">
                <DataOffset Formula="KEY*RESULT"/>
              </Register>
            </Access>
          </Field>
          <Field Name="PD_RESULT_MASK">
            <Access>
              <Register Name="IPPA_FWD_DOMAIN_MASKSr">
                <ArrayOffset Formula="3"/>
                <DataOffset Formula="KEY*RESULT+FODO_BASE_MASK"/>
              </Register>
            </Access>
            <Access>
              <Register Name="IPPA_FWD_DOMAIN_MASKSr">
                <ArrayOffset Formula="4"/>
                <DataOffset Formula="KEY*RESULT+FODO_BASE_MASK"/>
              </Register>
            </Access>
            <Access>
              <Register Name="IPPF_FWD_DOMAIN_MASKSr">
                <DataOffset Formula="KEY*RESULT+FODO_BASE_MASK"/>
              </Register>
            </Access>
          </Field>
        </FieldMapping>
      </AppToPhyDbMapping>
    </AppDB>

