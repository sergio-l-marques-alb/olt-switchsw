    <AppDB Name="LATENCY_VOQ_QUARTET_PROFILE">
        <OneLineDescription>mapping table from quartet queue (base of 4 queues) to a profile</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="QUARTET_QUEUE" Type="UINT" Size="14"/>
            </Key>
            <Result>
                <Field Name="PROFILE_ID" Type="UINT" Size="3" />
            </Result>
            <TableLabels>LATENCY</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
                <Field Name="PROFILE_ID">
                    <Access>
                        <Memory Name="ITPP_VOQ_QUARTETm">
                            <EntryOffset Formula="QUARTET_QUEUE/64"/>
                            <HwField Name="PROFILEf"/>
                            <DataOffset Formula="QUARTET_QUEUE%64*PROFILE_ID"/>
                        </Memory>
                    </Access>
                    <Access>
                        <Memory Name="ITPPD_VOQ_QUARTETm">
                            <EntryOffset Formula="QUARTET_QUEUE/64"/>
                            <HwField Name="PROFILEf"/>
                            <DataOffset Formula="QUARTET_QUEUE%64*PROFILE_ID"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>    
    
