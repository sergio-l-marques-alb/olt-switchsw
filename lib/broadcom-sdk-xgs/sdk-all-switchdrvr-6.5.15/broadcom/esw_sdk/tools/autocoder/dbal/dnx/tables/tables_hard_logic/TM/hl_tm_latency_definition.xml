    <AppDB Name="LATENCY_INGRESS_MAX_REPORT">
        <OneLineDescription>ingress latency table which holds information on the MAX ingress latency packets measured</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="CORE_ID" Size="1"/>
                <Field Name="INDEX" Type="UINT" Size="3"/>
            </Key>
            <Result>
                <Field Name="LAT_VALUE" Permission="READONLY" Type="UINT" />
                <Field Name="LAT_FLOW_ID" Permission="READONLY" Size="20" Type="UINT" />
                <Field Name="LAT_PKT_WAS_DROP" Permission="READONLY" Type="BOOL" />
                <Field Name="LAT_TRF_CLS" Permission="READONLY" Size="3" Type="UINT" />
                <Field Name="LAT_PORT_ID" Permission="READONLY" Size="8" Type="UINT" />
                <Field Name="LAT_MODID" Permission="READONLY" Size="11" Type="UINT" />
            </Result>
            <TableLabels>LATENCY</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="DPC"/>
            <FieldMapping>
                <Field Name="LAT_VALUE">
                    <Access>
                        <Memory Name="ITPP_MAX_LATENCYm">
                            <EntryOffset Formula="INDEX"/>
                            <HwField Name="LAT_VALUEf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="LAT_FLOW_ID">
                    <Access>
                        <Memory Name="ITPP_MAX_LATENCYm">
                            <EntryOffset Formula="INDEX"/>
                            <HwField Name="LAT_POINTERf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="LAT_PKT_WAS_DROP">                 
                    <Access>
                        <Memory Name="ITPP_MAX_LATENCYm">
                            <EntryOffset Formula="INDEX"/>
                            <HwField Name="LAT_PKT_WAS_DROPf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="LAT_TRF_CLS">
                    <Access>
                        <Memory Name="ITPP_MAX_LATENCYm">
                            <EntryOffset Formula="INDEX"/>
                            <HwField Name="LAT_TRF_CLSf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="LAT_PORT_ID">
                    <Access>
                        <Memory Name="ITPP_MAX_LATENCYm">
                            <EntryOffset Formula="INDEX"/>
                            <HwField Name="LAT_PORT_IDf"/>
                        </Memory>
                    </Access>
                </Field>
                <Field Name="LAT_MODID">
                    <Access>
                        <Memory Name="ITPP_MAX_LATENCYm">
                            <EntryOffset Formula="INDEX"/>
                            <HwField Name="LAT_DEST_DEVf"/>
                        </Memory>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
