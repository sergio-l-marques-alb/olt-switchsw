    <AppDB Name="OAMP_INIT_MEP_DB_BANK_CFG">
        <OneLineDescription>MEP DB scan rate configuration per bank(12)</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
                <Field Name="MEP_DB_BANK"/>
            </Key>
            <Result>
                <Field Name="SCAN_RATE_BANK" Size="1" Type="UINT" Description="'0' - normal scan rate(1.67ms), '1' -slow scan rate(3.33ms)" Valid="DNX_DATA.oam.oamp.scan_rate_bank_exist"/>
                <Field Name="NUM_IDLE_STAGES" Size="4" Type="UINT" Description="Number of txm idle stages before active"/>
                <Field Name="NUM_ACTIVE_STAGES" Size="4" Type="UINT" Description="Number of txm active stages before idle"/>
                <Field Name="NUM_CLOCKS_SCAN" Size="22" Type="UINT" Description="Number of system clocks in one MEP-DB scan(1.67ms for long and 3.33ms for short entries)"/>
                <Field Name="APPLY_JITTER" Size="1" Type="UINT"/>
            </Result>
            <TableLabels>OAM</TableLabels>
        </AppDbInterface>
        <AppToPhyDbMapping>
            <CoreMode Mode="SBC"/>
            <FieldMapping>
               <Field Name="SCAN_RATE_BANK" Device="J2B0">
                    <Access>
                        <Register Name="OAMP_TXM_BANK_SCAN_CONFIGr">
                            <ArrayOffset Formula="MEP_DB_BANK"/>
                            <HwField Name="SCAN_RATE_BANK_Nf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="NUM_IDLE_STAGES">
                    <Access>
                        <Register Name="OAMP_TXM_BANK_SCAN_CONFIGr">
                            <ArrayOffset Formula="MEP_DB_BANK"/>
                            <HwField Name="NUM_STAGES_IDLE_BANK_Nf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="NUM_ACTIVE_STAGES">
                    <Access>
                        <Register Name="OAMP_TXM_BANK_SCAN_CONFIGr">
                            <ArrayOffset Formula="MEP_DB_BANK"/>
                            <HwField Name="NUM_STAGES_ACTIVE_BANK_Nf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="NUM_CLOCKS_SCAN">
                    <Access>
                        <Register Name="OAMP_TXM_BANK_SCAN_CONFIGr">
                            <ArrayOffset Formula="MEP_DB_BANK"/>
                            <HwField Name="NUM_CLOCKS_SCAN_BANK_Nf"/>
                        </Register>
                    </Access>
                </Field>
                <Field Name="APPLY_JITTER">
                    <Access>
                        <Register Name="OAMP_TXM_BANK_SCAN_CONFIGr">
                            <ArrayOffset Formula="MEP_DB_BANK"/>
                            <HwField Name="APPLY_JITTER_BANK_Nf"/>
                        </Register>
                    </Access>
                </Field>
            </FieldMapping>
        </AppToPhyDbMapping>
    </AppDB>
    
