    <AppDB Name="OAMP_INIT_GENERAL_CFG">
        <OneLineDescription>General OAMP Configuration</OneLineDescription>
        <AccessMethod>HARD_LOGIC</AccessMethod>
        <Maturity Level="HIGH"/>
        <AppDbInterface Type="DIRECT">
            <Key>
            </Key>
            <Result>
                <Field Name="TRANSMIT_ENABLE" Size="1" Type="BOOL" Description="OAMP teransmit enable"/>
                <Field Name="SCANNER_ENABLE" Size="1" Type="BOOL" Description="OAMP scan machine enable"/>
                <Field Name="RX_2_CPU" Size="1" Type="BOOL" Description="Send all packets to CPU(debug purpose)"/>
                <Field Name="DMR_LMR_RESPONSE_ENABLE" Size="1" Type="BOOL" />
                <Field Name="DMR_LMR_RESPONSE_DROP_ENABLE" Size="1" Type="BOOL"/>
                <Field Name="GOOD_PACKET_PUNT_ENABLE" Size="1" Type="BOOL"/>
                <Field Name="REPORT_MODE"/>
                <Field Name="EVENT_FORMAT" Size="1" Type="UINT" Description="'0' - Jericho/'1' - Jericho2"/>
                <Field Name="MIN_PKT_SIZE" Size="8" Type="UINT" Description="Minimum OAMP packet size"/>
                <Field Name="NUM_OF_RMEPS" Size="DNX_DATA.oam.oamp.rmep_db_entries_nof_bits" Type="UINT"/>
                <Field Name="NUM_CLOCKS_SEC" Size="32" Type="UINT" Description="Number of system clocks in one second"/>
                <Field Name="PTCH_PP_SSP" Type="PP_PORT" Description="Value to put in PTCH2 SSP field for Down-MEP packets(OAMP port)"/>
                <Field Name="PRT_QUALIFIER" Description="Value to put in PTCH2 Opaque field(PTCH-opaque-pt-attr) for Down-MEP packets"/>
                <Field Name="OAMP_MEP_DB_FULL_ENTRY_THERSHOLD" Type="OAMP_MEP_DB_INDEX"/>
                <Field Name="OAMP_RMEP_DB_FULL_ENTRY_THERSHOLD" Type="RMEP_INDEX"/>
                <Field Name="LOC_SET_EVENT_ENABLE" Size="1" Type="BOOL" Description="Enable/Disable sending an event to CPU when LOC is set for a RMEP"/>
                <Field Name="LOC_CLEAR_EVENT_ENABLE" Size="1" Type="BOOL" Description="Enable/Disable sending an event to CPU when LOC is cleared for a RMEP"/>
                <Field Name="RDI_SET_EVENT_ENABLE" Size="1" Type="BOOL" Description="Enable/Disable sending an event to CPU when RDI is set for a RMEP"/>
                <Field Name="RDI_CLEAR_EVENT_ENABLE" Size="1" Type="BOOL" Description="Enable/Disable sending an event to CPU when RDI is cleared for a RMEP"/>
                <Field Name="TX_COUNTER_CFG_PREFIX" Size="4" Type="UINT" Description="TX Counter Prefix"/>
                <Field Name="TX_COUNTER_CFG_SHIFT" Size="5" Type="UINT" Description="TX Counter Shift"/>
                <Field Name="RX_COUNTER_CFG_PREFIX" Size="4" Type="UINT" Description="RX Counter Prefix"/>
                <Field Name="RX_COUNTER_CFG_SHIFT" Size="5" Type="UINT" Description="RX Counter Shift"/>
            </Result>
            <TableLabels>OAM</TableLabels>
