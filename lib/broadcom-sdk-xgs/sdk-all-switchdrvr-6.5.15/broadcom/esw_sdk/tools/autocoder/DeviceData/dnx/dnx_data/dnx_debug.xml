<?xml version="1.0"?>
<device_data lib="dnx_data" module="debug" bsl_flag="BSL_LS_SOC_DIAG">
    <includes>
        <include>soc/dnx/dbal/auto_generated/dbal_defines_fields.h</include>
    </includes>

    <sub_module name="mem" doc="Inforamtion per debug memory block">

        <tables>
            <table name="params" doc="Per stage properties and characteristics">
                <key name="block" doc="ASIC Block ID"></key>
                <value name="valid" type="int" doc="marks existence of debug memory for ASIC block"/>
                <value name="line_nof" type="int" doc="number of lines in debug memory"/>
                <value name="width" type="int" doc="memory width - size of lingest line"/>
            </table>
        </tables>
         
    </sub_module>

</device_data>
