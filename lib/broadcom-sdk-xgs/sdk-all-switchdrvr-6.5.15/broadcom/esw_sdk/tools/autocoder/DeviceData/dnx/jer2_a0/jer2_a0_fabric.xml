<?xml version="1.0"?>
<device_data lib="dnx_data" device="jer2_a0" module="fabric">
    <includes>
        <include>bcm/fabric.h</include>
        <include>bcm/port.h</include>
        <include>include/soc/dnx/legacy/TMC/tmc_api_fabric.h</include>
        <include>shared/utilex/utilex_integer_arithmetic.h</include>
    </includes>

    <sub_module name="general">
        <features>
            <feature name="is_supported"></feature>
        </features>

        <defines>
            <define name="nof_lcplls" value="2"></define>
            <define name="fmac_clock_khz" value="892000"></define>
        </defines>

        <numerics>
            <numeric name="connect_mode" value="SOC_DNX_FABRIC_NOF_CONNECT_MODES">
                <property name="fabric_connect_mode" method="direct_map">
                    <map name="FE" value="SOC_DNX_FABRIC_CONNECT_MODE_FE"></map>
                    <map name="SINGLE_FAP" value="SOC_DNX_FABRIC_CONNECT_MODE_SINGLE_FAP"></map>
                    <map name="MESH" value="SOC_DNX_FABRIC_CONNECT_MODE_MESH"></map>
                    <doc>
                        Set the connect mode of the Fabric:
                            FE - fabric is connected to FE device.
                            SINGLE_FAP - stand-alone device.
                            MESH - fabric is connected to other FAPs directly.
                    </doc>
                </property>
            </numeric>
        </numerics>

        <tables>
            <table name="fmac_bus_size">
                <key name="mode" size="bcmPortPhyFecCount"></key>
                <value name="size" default="0"></value>
                <entries>
                    <entry mode="bcmPortPhyFecBaseR" size="66"></entry>
                    <entry mode="bcmPortPhyFecNone" size="66"></entry>
                    <entry mode="bcmPortPhyFecRs206" size="70"></entry>
                    <entry mode="bcmPortPhyFecRs108" size="70"></entry>
                    <entry mode="bcmPortPhyFecRs545" size="70"></entry>
                    <entry mode="bcmPortPhyFecRs304" size="70"></entry>
                </entries>
            </table>
            <table name="pll_phys">
                <key name="lcpll" size="DATA(fabric, general, nof_lcplls)"></key>
                <value name="min_phy_id" default="0"></value>
                <value name="max_phy_id" default="0"></value>
                <entries>
                    <entry lcpll="0" min_phy_id="0" max_phy_id="55"></entry>
                    <entry lcpll="1" min_phy_id="56" max_phy_id="111"></entry>
                </entries>
            </table>

        </tables>
    </sub_module>

    <sub_module name="links">

        <defines>
            <define name="nof_links" value="112"></define>
            <define name="max_link_id" value="DATA(fabric,links,nof_links)-1"></define>
            <define name="max_speed" value="53125"></define>
        </defines>

        <numerics>
            <numeric name="core_mapping_mode" value="SOC_DNX_FABRIC_LINKS_TO_CORE_MAP_SHARED">
                <property name="fabric_links_to_core_mapping_mode" method="direct_map">
                    <map name="SHARED" value="SOC_DNX_FABRIC_LINKS_TO_CORE_MAP_SHARED"></map>
                    <doc>
                        Set Fabric link-to-core mapping mode:
                            SHARED - the links are shared between the cores.
                        Default: SHARED
                        Note: this soc property exist only for backward compatibility.
                    </doc>
                </property>
            </numeric>
        </numerics>

        <tables>
            <table name="general">
                <value name="supported_links" default="0xffffffff, 0xffffffff, 0xffffffff, 0xffff"></value>
            </table>
            <table name="polarity" init_only="1">
                <key name="link" size="DATA(fabric,links,nof_links)"></key>
                <value name="tx_polarity" default="0">
                    <property name="phy_tx_polarity_flip" method="custom">
                        <doc>
                            Flips PHY TX polarity if enabled
                            phy_tx_polarity_flip_fabric#link# = 0 / 1
                        </doc>
                    </property>
                </value>
                <value name="rx_polarity" default="0">
                    <property name="phy_rx_polarity_flip" method="custom">
                        <doc>
                            Flips PHY RX polarity if enabled
                            phy_rx_polarity_flip_fabric#link# = 0 / 1
                        </doc>
                    </property>
                </value>
            </table>
        </tables>


    </sub_module>

    <sub_module name="blocks">

        <features>
        </features>

        <defines>
            <define name="nof_links_in_fmac" value="4"></define>
            <define name="nof_links_in_fsrd" value="8"></define>
            <define name="nof_instances_fsrd" value="14"></define>
            <define name="nof_fmacs_in_fsrd" value="2"></define>
            <define name="nof_pms" value="DATA(fabric,blocks,nof_instances_fsrd)"></define>
            <define name="nof_links_in_pm" value="DATA(fabric,blocks,nof_links_in_fsrd)"></define>
            <define name="nof_fmacs_in_pm" value="DATA(fabric,blocks,nof_fmacs_in_fsrd)"></define>                        
        </defines>

        <numerics>
            <numeric name="nof_instances_fmac" value="28">
                <property name="diag_emulator_partial_init" method="direct_map">
                    <map name="1" value="1"></map>
                    <map name="2" value="1"></map>
                    <doc>
                    NULL
                    </doc>
                </property>
            </numeric>
        </numerics>

        <tables>
            <table name="fsrd_sbus_chain">
                <key name="fsrd_id" size="DATA(fabric, blocks, nof_instances_fsrd)"></key>
                <value name="index_in_chain" default="-1"></value>
                <entries>
                    <entry fsrd_id="0" index_in_chain="0"></entry>
                    <entry fsrd_id="1" index_in_chain="1"></entry>
                    <entry fsrd_id="2" index_in_chain="2"></entry>
                    <entry fsrd_id="3" index_in_chain="3"></entry>
                    <entry fsrd_id="4" index_in_chain="4"></entry>
                    <entry fsrd_id="5" index_in_chain="5"></entry>
                    <entry fsrd_id="6" index_in_chain="6"></entry>
                    <entry fsrd_id="7" index_in_chain="13"></entry>
                    <entry fsrd_id="8" index_in_chain="12"></entry>
                    <entry fsrd_id="9" index_in_chain="11"></entry>
                    <entry fsrd_id="10" index_in_chain="10"></entry>
                    <entry fsrd_id="11" index_in_chain="9"></entry>
                    <entry fsrd_id="12" index_in_chain="8"></entry>
                    <entry fsrd_id="13" index_in_chain="7"></entry>
                </entries>
            </table>
        </tables>

    </sub_module>

    <sub_module name="reachability">

        <features>
        </features>

        <defines>
            <define name="resolution" value="32"></define>
            <!-- 'gen_period' value is recommended by HW -->
            <define name="gen_period" value="6"></define>
            <define name="full_cycle_period_usec" value="50"></define>
            <define name="watchdog_resolution" value="4096"></define>
            <define name="watchdog_period_usec" value="100"></define>
        </defines>

        <numerics>
        </numerics>

        <tables>
        </tables>

    </sub_module>

    <sub_module name="mesh">

        <features>
            <feature name="is_supported"></feature>
        </features>

        <defines>
            <define name="max_mc_id" value="(128*1024)-1"></define>
        </defines>

        <numerics>
            <numeric name="multicast_enable" value="1">
                <property name="fabric_mesh_multicast_enable" method="custom">
                    <doc>
                        Enable / Disable MESH MC mode:
                        fabric_mesh_multicast_enable = 1 / 0 
                        Relevant only when fabric connect mode is MESH.
                        Default: 1
                    </doc>
                </property>
            </numeric>

            <!-- When MESH supports MC, one context is used for MC instead of dest device -->
            <numeric name="nof_max_dest" value="3-DATA(fabric, mesh, multicast_enable)"> </numeric>
        </numerics>

        <tables>
        </tables>

    </sub_module>


    <!-- used as an example - can be deleted when implementing jericho 2 fabric module -->
    <sub_module name="thresholds">
        
        <defines>
            <define name="default_mode" value="3"></define>
        </defines>
        
        <features>
            <feature name="mode_supported"></feature>
        </features>
        
        <tables>
            <table name="dch_default">
                <key name="severity" size="1"></key>
                <key name="mode" size="3"></key>
                <value name="gci0" default="SAL_UINT32_MAX"></value>
                <value name="gci1" default="SAL_UINT32_MAX"></value>
                <value name="gci2" default="SAL_UINT32_MAX"></value>
                <value name="rci0" default="SAL_UINT32_MAX"></value>
                <value name="rci1" default="SAL_UINT32_MAX"></value>
                <value name="rci2" default="SAL_UINT32_MAX"></value>
                
                <entries>
                    <entry severity="0" mode="0" gci0="3" gci1="100" gci2="200" rci0="3" rci1="100" rci2="200"></entry>
                    <entry severity="0" mode="1" gci0="3" gci1="100" gci2="200" rci0="3" rci1="100" rci2="200"></entry>
                    <entry severity="0" mode="2" gci0="3" gci1="100" gci2="200" rci0="3" rci1="100" rci2="200"></entry>
                </entries>
            </table>
            
        </tables>
         
    </sub_module>
    
    <sub_module name="cell">
        <defines>
            <define name="nof_priorities" value="4"></define>
            <define name="vsc256_max_size" value="256+16"></define>
            <define name="sr_cell_header_offset" value="32"></define>
        </defines>

        <numerics>
            <numeric name="default_packing_mode" value="DBAL_ENUM_FVAL_FABRIC_PCP_MODE_CONTINUOUS"></numeric>
            <numeric name="pcp_enable" value="1">
                <property name="fabric_pcp_enable" method="direct_map">
                    <map name="1" value="1"></map>
                    <doc>
                        Enable PCP:
                        fabric_pcp_enable=1
                        Default: 1
                        Note: this soc property exist only for backward compatibility.
                    </doc>
                </property>
            </numeric>
        </numerics>
    </sub_module>

    <sub_module name="pipes">
        
        <features>
        </features>
        
        <defines>
            <define name="max_nof_pipes" value="3"></define>
        </defines>

        <numerics>
            <numeric name="nof_pipes" value="1">
                <property name="fabric_num_pipes" method="range" range_min="1" range_max="3">
                    <doc>
                        Configure number of fabric pipes:
                        fabric_num_pipes= 1/2/3
                        Default: 1
                    </doc>
                </property>
            </numeric>
            
            <numeric name="multi_pipe_system" value="0">
                <property name="system_contains_multiple_pipe_device" method="enable">
                    <doc>
                        To determine if there is a multiple pipe device in the system:
                        system_contains_multiple_pipe_device = 0/1
                        Default: 0 
                    </doc>
                </property>
            </numeric>
        </numerics>
        
        <tables>
            <table name="map">
                <value name="uc" default="0, 0, 0, 0"></value>
                <value name="mc" default="0, 0, 0, 0"></value>
                <value name="name" default="SINGLE PIPE"></value>
                <value name="type" default="soc_dnxc_fabric_pipe_map_single"></value>
                <value name="min_hp_mc" default="-1"></value>
                <property name="fabric_pipe_map" method="custom">
                    <doc>
                        Assign traffic cast (unicast/multicast) and priority (0/1/2/3) to a pipe.
                        fabric_pipe_map=0/1/2
                        Assigning a specific traffic cast is done by a suffix uc/mc and a number which represents the cell priority. 
                        Not all the options supported, please find the supported options in UM
                    </doc>
                </property>
            </table>

            <table name="valid_map_config">
                <key name="config_id" size="5"></key>
                <value name="nof_pipes" default="1"></value>
                <value name="uc" default="0, 0, 0, 0"></value>
                <value name="mc" default="0, 0, 0, 0"></value>
                <value name="name" default="SINGLE PIPE"></value>
                <value name="type" default="soc_dnxc_fabric_pipe_map_single"></value>
                <value name="min_hp_mc" default="-1"></value>
                
                <entries>
                    <entry config_id="0" nof_pipes="1" uc="0, 0, 0, 0" mc="0, 0, 0, 0" min_hp_mc="-1" name="SINGLE PIPE" type="soc_dnxc_fabric_pipe_map_single"></entry>
                    <entry config_id="1" nof_pipes="2" uc="0, 0, 0, 0" mc="1, 1, 1, 1" min_hp_mc="-1" name="UC,MC" type="soc_dnxc_fabric_pipe_map_dual_uc_mc"></entry>
                    <entry config_id="2" nof_pipes="3" uc="0, 0, 0, 0" mc="2, 2, 2, 1" min_hp_mc="3" name="UC,HP MC (3),LP MC" type="soc_dnxc_fabric_pipe_map_triple_uc_hp_mc_lp_mc"></entry>
                    <entry config_id="3" nof_pipes="3" uc="0, 0, 0, 0" mc="2, 2, 1, 1" min_hp_mc="2" name="UC,HP MC (2,3),LP MC" type="soc_dnxc_fabric_pipe_map_triple_uc_hp_mc_lp_mc"></entry>
                    <entry config_id="4" nof_pipes="3" uc="0, 0, 0, 0" mc="2, 1, 1, 1" min_hp_mc="1" name="UC,HP MC (1,2,3),LP MC" type="soc_dnxc_fabric_pipe_map_triple_uc_hp_mc_lp_mc"></entry>
                </entries>
            </table>
            
        </tables>
        
    </sub_module>

    <sub_module name="debug">
        <numerics>
            <numeric name="mesh_topology_size" value="-1">
                <property name="custom_feature" method="suffix_range" range_min="-1" range_max="3" suffix="mesh_topology_size">
                    <doc>
                        To determine if there is a multiple pipe device in the system:
                        custom_feature_mesh_topology_size=-1/0/1/2/3
                        Default: -1 
                    </doc>
                </property>
            </numeric>

            <numeric name="mesh_topology_fast" value="1">
                 <property name="custom_feature" method="suffix_enable" suffix="mesh_topology_fast">
                     <doc>
                        Device is in mesh topology fast mode.
                        Used for internal validation only!
                     </doc>
                 </property>
            </numeric>
        </numerics>
    </sub_module>
    
    <sub_module name="tdm">
        <numerics>
            <numeric name="priority" value="-1"></numeric>
        </numerics>
    </sub_module>

    <sub_module name="cgm">
        <defines>
            <define name="nof_rci_levels" value="8"></define>
        </defines>
        <tables>
            <table name="access_map">
                <key name="control_type" size="bcmFabricCgmNof"></key>
                <value name="dbal_table" default="DBAL_NOF_TABLES"></value>
                <value name="dbal_key1" default="DBAL_FIELD_EMPTY"></value>
                <value name="dbal_key2" default="DBAL_FIELD_EMPTY"></value>
                <value name="dbal_result" default="DBAL_FIELD_EMPTY"></value>
                <value name="index_type" default="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_NONE"></value>
                <value name="is_valid" default="0"></value>
                <entries>
                    <!-- RCI -->
                    <entry control_type="bcmFabricCgmRciLinkPipeLevelTh" dbal_table="DBAL_TABLE_FABRIC_CGM_RCI_THRESHOLDS" dbal_key1="DBAL_FIELD_PIPE" dbal_key2="DBAL_FIELD_LEVEL" dbal_result="DBAL_FIELD_LINK_RCI_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_PIPE_LEVEL" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmRciEgressPipeLevelTh" dbal_table="DBAL_TABLE_FABRIC_CGM_RCI_THRESHOLDS" dbal_key1="DBAL_FIELD_PIPE" dbal_key2="DBAL_FIELD_LEVEL" dbal_result="DBAL_FIELD_EGRESS_RCI_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_PIPE_LEVEL" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmRciTotalEgressPipeLevelTh" dbal_table="DBAL_TABLE_FABRIC_CGM_RCI_THRESHOLDS" dbal_key1="DBAL_FIELD_PIPE" dbal_key2="DBAL_FIELD_LEVEL" dbal_result="DBAL_FIELD_TOTAL_EGRESS_RCI_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_PIPE_LEVEL" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmRciLocalLevelTh" dbal_table="DBAL_TABLE_FABRIC_CGM_RCI_LOCAL_THRESHOLDS" dbal_key1="DBAL_FIELD_LEVEL" dbal_result="DBAL_FIELD_LOCAL_RCI_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_LEVEL" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmRciTotalLocalLevelTh" dbal_table="DBAL_TABLE_FABRIC_CGM_RCI_LOCAL_THRESHOLDS" dbal_key1="DBAL_FIELD_LEVEL" dbal_result="DBAL_FIELD_TOTAL_LOCAL_RCI_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_LEVEL" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmRciLinkLevelFactor" dbal_table="DBAL_TABLE_FABRIC_CGM_RCI_SCORE_MAPPING" dbal_key1="DBAL_FIELD_LEVEL" dbal_result="DBAL_FIELD_LINK_RCI_FACTOR" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_LEVEL" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmRciEgressLevelFactor" dbal_table="DBAL_TABLE_FABRIC_CGM_RCI_SCORE_MAPPING" dbal_key1="DBAL_FIELD_LEVEL" dbal_result="DBAL_FIELD_EGRESS_RCI_SCORE" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_LEVEL" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmRciHighSeverityMinLinksParam" dbal_table="DBAL_TABLE_FABRIC_CGM_RCI_HIGH_SEVERITY" dbal_result="DBAL_FIELD_MIN_NOF_LINKS" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_NONE" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmRciHighSeverityFactor" dbal_table="DBAL_TABLE_FABRIC_CGM_RCI_HIGH_SEVERITY" dbal_result="DBAL_FIELD_SCORE" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_NONE" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmRciCoreLevelMappingTh" dbal_table="DBAL_TABLE_FABRIC_CGM_RCI_SCORE_TO_LEVEL_THRESHOLDS" dbal_key1="DBAL_FIELD_LEVEL" dbal_result="DBAL_FIELD_CORE_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_LEVEL" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmRciDeviceLevelMappingTh" dbal_table="DBAL_TABLE_FABRIC_CGM_RCI_SCORE_TO_LEVEL_THRESHOLDS" dbal_key1="DBAL_FIELD_LEVEL" dbal_result="DBAL_FIELD_DEVICE_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_LEVEL" is_valid="1"></entry>
                    <!-- GCI -->
                    <entry control_type="bcmFabricCgmGciLeakyBucketEn" dbal_table="DBAL_TABLE_FABRIC_CGM_GCI_LEAKY_BUCKET_FC_ENABLE" dbal_result="DBAL_FIELD_LEAKY_BUCKET_ENABLE" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_NONE" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmGciLeakyBucketSizeLeakyBucketTh" dbal_table="DBAL_TABLE_FABRIC_CGM_GCI_LEAKY_BUCKET_CONFIG" dbal_key1="DBAL_FIELD_LEAKY_BUCKET_ID" dbal_result="DBAL_FIELD_MAX_VALUE" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_LEAKY_BUCKET" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmGciLeakyBucketFCLeakyBucketTh" dbal_table="DBAL_TABLE_FABRIC_CGM_GCI_LEAKY_BUCKET_CONFIG" dbal_key1="DBAL_FIELD_LEAKY_BUCKET_ID" dbal_result="DBAL_FIELD_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_LEAKY_BUCKET" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmGciBackoffEn" dbal_table="DBAL_TABLE_FABRIC_CGM_GCI_BACKOFF_FC_ENABLE" dbal_result="DBAL_FIELD_BACKOFF_ENABLE" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_NONE" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmGciBackoffLevelTh" dbal_table="DBAL_TABLE_FABRIC_CGM_GCI_BACKOFF_CONGESTION_SEVERITY_THRESHOLDS" dbal_key1="DBAL_FIELD_LEVEL_SEPARATE" dbal_result="DBAL_FIELD_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_LEVEL_SEPARATE" is_valid="1"></entry>
                    <!-- Slow Start -->
                    <entry control_type="bcmFabricCgmSlowStartEn" dbal_table="DBAL_TABLE_FABRIC_CGM_SLOW_START_ENABLE" dbal_key1="DBAL_FIELD_FMC_SHAPER_ID" dbal_result="DBAL_FIELD_ENABLE" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_FMC_SHAPER" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmSlowStartRateShaperPhaseParam" dbal_table="DBAL_TABLE_FABRIC_CGM_SLOW_START_CONFIG" dbal_key1="DBAL_FIELD_FMC_SHAPER_ID" dbal_key2="DBAL_FIELD_PHASE" dbal_result="DBAL_FIELD_RATE_PERCENT" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_FMC_SLOW_START_PHASE" is_valid="1"></entry>
                    <!-- LLFC -->
                    <entry control_type="bcmFabricCgmLlfcPipeTh" dbal_table="DBAL_TABLE_FABRIC_CGM_LLFC_THRESHOLDS" dbal_key1="DBAL_FIELD_PIPE" dbal_result="DBAL_FIELD_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_PIPE" is_valid="1"></entry>
                    <!-- Drop -->
                    <entry control_type="bcmFabricCgmDropFabricClassTh" dbal_table="DBAL_TABLE_FABRIC_CGM_EGRESS_FIFO_DROP_THRESHOLDS" dbal_key1="DBAL_FIELD_CAST" dbal_key2="DBAL_FIELD_FABRIC_PRIORITY" dbal_result="DBAL_FIELD_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_CAST_PRIORITY" is_valid="1"></entry>
                    <entry control_type="bcmFabricCgmDropMeshMcPriorityTh" dbal_table="DBAL_TABLE_FABRIC_CGM_MESH_MC_FIFO_DROP_THRESHOLDS" dbal_key1="DBAL_FIELD_FABRIC_PRIORITY" dbal_result="DBAL_FIELD_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_PRIORITY" is_valid="1"></entry>
                    <!-- Flow Control -->
                    <entry control_type="bcmFabricCgmFcIngressTh" dbal_table="DBAL_TABLE_FABRIC_CGM_FC_INGRESS_THRESHOLDS" dbal_result="DBAL_FIELD_LINK_FIFO_SIZE_THRESHOLD" is_valid="1"></entry>
                    <!-- Dynamic WFQ -->
                    <entry control_type="bcmFabricCgmEgressDynamicWfqTh" dbal_table="DBAL_TABLE_FABRIC_CGM_EGRESS_DYNAMIC_WFQ" dbal_result="DBAL_FIELD_THRESHOLD" index_type="DNX_FABRIC_CGM_CONTROL_INDEX_TYPE_NONE" is_valid="1"></entry>
                </entries>
            </table>
        </tables>
    </sub_module>

    <sub_module name="ilkn" doc="data about ILKN over fabric">
        <tables>
            <table name="bypass_info">
                <key name="pm_id" size="DATA(fabric,blocks,nof_pms)"></key>
                <value name="links" default="0">
                    <property name="use_fabric_links_for_ilkn_nif" method="suffix_only_range" range_min="0" range_max="0xff" suffix="bmp_fab_oct">
                        <doc>
                           bitmap of fabrics link in bypass mode per PM (0x00 - 0xff) 
                        </doc>
                    </property>
                 </value>
            </table>
        </tables>        
    </sub_module>

    <sub_module name="dbal">
        
        <features>
        </features>
        
        <defines>
        </defines>

        <numerics>
            <!-- Number of bits needed for FABRIC_PRIORITY field -->
            <!-- utilex_msb_bit_on returns the msb of nof_priorities-1 in 0 based, so we need to add 1 -->
            <numeric name="priority_bits_nof" value="utilex_msb_bit_on(DATA(fabric,cell,nof_priorities)-1)+1"></numeric>
