        <numerics>
            <numeric name="worth" value="4096">
                <property name="credit_size" method="range" range_min="1" range_max="8191">
                   <doc>
                       Set local credit worth. Recommended value is 4096 Bytes. 
                       Credit worth has significant implications on the scheduled traffic. 
                       Consult with Broadcom applications engineer before changing the credit size value.
                       Supported range [1-8191]
                       Syntax:
                       credit_size=#credit size in bytes#
                       Default:
                       4096
                   </doc>
                </property>
            </numeric>
            <numeric name="fabric_delay" value="300">
                <property name="fabric_connect_mode" method="direct_map">
                    <map name="FE" value="1000"></map>
                    <map name="SINGLE_FAP" value="300"></map>
                    <map name="MESH" value="300"></map>
                    <doc>
                        See fabric module
                    </doc>
                </property>
            </numeric>
            
            <numeric name="fabric_multicast_delay" value="200"> </numeric>
                        
        </numerics>

        <tables>
