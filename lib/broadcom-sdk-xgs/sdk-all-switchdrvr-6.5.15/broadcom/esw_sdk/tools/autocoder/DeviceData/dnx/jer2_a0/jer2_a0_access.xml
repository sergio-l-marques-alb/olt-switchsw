        <numerics>
            <numeric name="ext_divisor" value="-1">
                <property name="rate_ext_mdio_divisor" method="range" range_min="0" range_max="255">
                    <doc>
                        Configure external MDIO clock rate divisor
                        ext_mdio_divisor= 0~255
                        Default: -1
                    </doc>
                </property>
            </numeric>
            <numeric name="int_divisor" value="-1">
                <property name="rate_int_mdio_divisor" method="range" range_min="0" range_max="255">
                    <doc>
                        Configure internal MDIO clock rate divisor
                        int_mdio_divisor= 0~255
                        Default: -1
                    </doc>
                </property>
            </numeric>
            <numeric name="out_delay" value="-1">
                <property name="mdio_output_delay" method="range" range_min="0" range_max="255">
                    <doc>
                        Configure number of clock delay between the rising edge of MDC and the starting data of MDIO
                        mdio_output_delay= 0~255
                        Default: -1
                    </doc>
                </property>
            </numeric>
        </numerics>

