<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >


<FastPathXML>
    <webxml version="1.0" name="DNSServerConfig" title="DNS Server Configuration">
        <group level="ReadWriteUser" id="1" heading=" ">
