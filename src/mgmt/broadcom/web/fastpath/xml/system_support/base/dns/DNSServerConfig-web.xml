            </node>
            <node id="2">
                <button scope="page" type="buttonSubmit" value="Add" obj-name="baseResCtlDnsName_Status" help="Submits the form on this page to the Switch" id="1" name="Submit">
                    <action type="button" disable="1_3" enable="1_1_1"/>
                </button>
            </node>
            <repeat present="vtable" edit="inline" heading="DNS Server List" id="3">
                <element properties="filter,iterate" obj-name="baseResCtlDnsName_ServerIP" id="1" name="DNS Server Address"/>
                <element properties="" obj-name="baseResCtlDnsName_Preference" id="2" name="Precedence"/>
		            <element control="checkbox-neg" properties="edit" obj-name="baseResCtlDnsName_Status" id="3" name="Remove">
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="no"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="no"/>
                </element>
            </repeat>
            <node id="4">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh">
                    <action type="button" disable="1_3"/>
                </button>
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="2" name="Delete">
                    <action type="button" disable="1_1_1|1_2_1" enable="1_3"/>
                </button>
            </node>
        </group>
        <group level="ReadOnlyUser" id="2" heading="  ">
            <node id="1">
                <message>DNS Server List</message>
            </node>
            <repeat present="vtable" edit="no" heading="  " id="2">
                <element properties="filter,iterate" obj-name="baseResCtlDnsName_ServerIP" id="1" name="DNS Server Address"/>
                <element properties="" obj-name="baseResCtlDnsName_Preference" id="2" name="Precedence"/>
            </repeat>
            <node id="3">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>
            </node>
        </group>
    </webxml>
</FastPathXML>