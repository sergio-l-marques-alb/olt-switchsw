#!/bin/sh

export MODULE=fastpath-cxo2t4
export PLATFORMS="CXO2T4-MX"

export APPLICATION=fp

echo "Compiling fastpath for CXO2T4..."
make -C ../fastpath -f swdrvr-cxo2t4.make
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Copying binaries to apps..."
make -C ../fastpath -f swdrvr-cxo2t4.make install
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Done... Going to prepare tarball."
echo

. ./fastpath.build $1
