#!/bin/sh

export MODULE=fastpath-ta48ge
export PLATFORMS="TA48GE"

export APPLICATION=fp

echo "Compiling fastpath for TA48GE..."
make -C ../fastpath -f ta48ge.make
echo "Copying binaries to apps..."
make -C ../fastpath -f ta48ge.make install
echo "Done... Going to prepare tarball."
echo

. ./fastpath.build $1
