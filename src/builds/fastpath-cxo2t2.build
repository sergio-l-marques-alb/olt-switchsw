#!/bin/sh

export MODULE=fastpath-cxo2t2
export PLATFORMS="CXO2T2"

export APPLICATION=fp

echo "Compiling fastpath for TA48GE..."
make -C ../fastpath -f swdrvr-cxo2t2.make
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Copying binaries to apps..."
make -C ../fastpath -f swdrvr-cxo2t2.make install
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Done... Going to prepare tarball."
echo

. ./fastpath.build $1
