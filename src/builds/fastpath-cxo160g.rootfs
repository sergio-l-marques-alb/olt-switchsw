#!/bin/sh

export MODULE=fastpath-cxo160g
export PLATFORM=CXO160G

export APPLICATION=fp

echo "Compiling CXO160G..."
make -C ../fastpath -f cxo160g.make
echo "Copying binaries to apps..."
make -C ../fastpath -f cxo160g.make install
echo "Done... Going to prepare tarball."
echo

. ./fastpath.rootfs $1
