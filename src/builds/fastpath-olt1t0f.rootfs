#!/bin/sh

export MODULE=fastpath-olt1t0f
export PLATFORM=CXOLT1T0-F

export APPLICATION=fp

echo "Compiling OLT1T0..."
make -C ../fastpath -f olt1t0f.make
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Copying binaries to apps..."
make -C ../fastpath -f olt1t0f.make install
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Done... Going to prepare tarball."
echo

. ./fastpath.rootfs $1
