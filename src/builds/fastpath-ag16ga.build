#!/bin/sh

export MODULE=swdrv-ag16ga
export PLATFORMS="AG16GA"
export FILESYSTEM=
export APPLICATION=fp

echo "Compiling fastpath for TG16G..."
make -C ../fastpath -f swdrvr-ag16ga.make
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Copying binaries to apps..."
make -C ../fastpath -f swdrvr-ag16ga.make install
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Done... Going to prepare tarball."
echo

. ./swdrv.build $1

