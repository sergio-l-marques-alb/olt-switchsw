#!/bin/sh

export MODULE=fastpath-olt2t4
export PLATFORMS="cxo2t4 tt08sxg ta12xg"

export APPLICATION=fp

#echo "Updating SVN..."
#svn update

echo "Compiling fastpath for OLT1T3..."
make -C ../fastpath -f swdrvr-olt2t4.make all
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Copying binaries to apps..."
make -C ../fastpath -f swdrvr-olt2t4.make install
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Done... Going to prepare tarball."
echo

. ./fastpath.build $1
