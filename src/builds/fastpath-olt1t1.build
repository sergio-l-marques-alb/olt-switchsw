#!/bin/sh

export MODULE=fastpath-olt1t3
export PLATFORMS="CXO160G TG16G TA48GE"

export APPLICATION=fp

#echo "Updating SVN..."
#svn update

echo "Compiling fastpath for OLT1T1..."
make -C ../fastpath -f olt1t1.make all
echo "Copying binaries to apps..."
make -C ../fastpath -f olt1t1.make install
echo "Done... Going to prepare tarball."
echo

. ./fastpath.build $1

