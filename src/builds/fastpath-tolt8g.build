#!/bin/sh

export MODULE=fastpath-tolt8g
export PLATFORMS="TOLT8G"

export APPLICATION=fp

echo "Compiling fastpath for TOLT8G..."
make -C ../fastpath -f tolt8g.make
echo "Copying binaries to apps..."
make -C ../fastpath -f tolt8g.make install
echo "Done... Going to prepare tarball."
echo

. ./fastpath.build $1
