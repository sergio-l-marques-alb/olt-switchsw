#!/bin/sh

export MODULE=fastpath-olt1t0
export PLATFORMS="OLT1T0"

export APPLICATION=fp

#echo "Updating SVN..."
#svn update

echo "Compiling fastpath for OLT1T0..."
make -C ../fastpath -f olt1t0.make all
echo "Copying binaries to apps..."
make -C ../fastpath -f olt1t0.make install
echo "Done... Going to prepare tarball."
echo

. ./fastpath.build $1

