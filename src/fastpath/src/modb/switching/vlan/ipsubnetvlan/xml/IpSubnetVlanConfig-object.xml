<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM "object.dtd">
<FastPathXML>
<objectxml name="Switching" version="1.0" >
<module name="IpSubnetVlanConfig" key-method="individual" >
<key-combo list="IPAddress,SubnetMask,VlanId"/>
<object name="IPAddress"  >
<core  type="user" xtype="formatipv4_t" key="yes" access="read-write" descr="This object specifies the IP address"/>
<ui  name="IP Address" style="inputLine">
 <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.19.1.1.1"/> 
</ui></object>
<object name="SubnetMask" >
<core  type="user" xtype="formatipv4_t" key="yes" access="read-write" descr="This object specifies the subnet."/>
<ui  name="Subnet Mask" style="inputLine">
 <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.19.1.1.2"/> 
</ui></object>
<object name="VlanId" >
<core  type="uint" key="yes" min="L7_DOT1Q_MIN_VLAN_ID" max="L7_DOT1Q_MAX_VLAN_ID" access="read-write" descr="This object specifies the VLAN that is associated to the IP address and subnet mask."/>
<ui  name="Vlan Id" style="inputLine">
 <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.19.1.1.3"/> 
</ui></object>
<object name="RowStatus" depends="IPAddress,SubnetMask,VlanId" >
<core  type="row-status" xtype="L7_ROW_STATUS_t" access="read-write" descr="This instance is used to configure, Subnet to VLAN association. Supported values: Add - used to create a new entry delete- removes the entry"/>
<ui  name="Subnet-VLAN Row Status" style="inputLine">
 <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.19.1.1.4"/> 
</ui></object>
</module>
</objectxml>
</FastPathXML>
