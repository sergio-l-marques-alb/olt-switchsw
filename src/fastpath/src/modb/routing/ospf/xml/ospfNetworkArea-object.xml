<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM "object.dtd">
<FastPathXML>
<objectxml name="routing" version="1.0" >
<module name="ospfNetworkArea">
<key-combo list="ipAddr,wildcardMask,areaId"/>
<object name="ipAddr"  >
<core  type="ipv4" key="yes" access="read-write" descr="Network address for creating/deleting a network area entry. All IP interfaces with matching prefix address would be configured with the network area entry's area id."/>
<ui  name="Network Address" style="inputLine" />
</object>
<object name="wildcardMask"  >
<core  type="ipv4" key="yes" access="read-write" descr="WildcardMask for creating/deleting a network area entry."/>
<ui  name="Wild Card Mask" style="inputLine" />
</object>
<object name="areaId"  >
<core  type="uint" key="yes" access="read-write" descr="Area associated with the network area entry"/>
<ui  name="Area Id" style="inputLine" />
</object>
<object name="RowStatus" depends="ipAddr,wildcardMask,areaId" >
<core type="row-status" xtype= "L7_ROW_STATUS_t" access="read-write" descr="This variable displays the status of the entry."/>
<ui  name="Row Status" style="inputLine" />
</object>
</module>
</objectxml>
</FastPathXML>
