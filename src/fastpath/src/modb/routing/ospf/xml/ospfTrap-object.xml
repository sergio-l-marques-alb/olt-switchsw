<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM "object.dtd">
<FastPathXML>
<objectxml name="routing" version="1.0" >
<module name="ospfTrapFlag"  >
<object name="TrapFlag"  >
<core  type="enum" xtype="enable_t" access="read-only" descr="Allows a user to determine configured ospf trap flags"/>
<ui  name="Trap Flag" style="plainText" />
</object>
<object name="TrapMode"  >
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to determine whether the specified trap is enabled or disabled"/>
<ui  name="Trap Mode" style="choiceNselect1" />
</object>
</module>
<module name="ospfv2GranularTraps">
<!-- ALL OSPFv2 Traps -->
<object name="TrapAll"  >
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable all the ospfv2 Traps"/>
<ui  name="All Traps" style="choiceNselect1" />
</object>
<!-- Error type OSPFv2 Traps -->
<object name="TrapErrosAll"  >
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable all the ospfv2 Error Traps"/>
<ui  name="All Error Traps" style="choiceNselect1" />
</object>
<object name="TrapErrosAuthFail">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable Authentication Failure Trap"/>
<ui  name="Authentication Failure Trap" style="choiceNselect1" />
</object>
<object name="TrapBadPkt">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable the bad-packet Trap"/>
<ui  name="Bad Packet Trap" style="choiceNselect1" />
</object>
<object name="TrapConfgError">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable the Configuration Error Trap"/>
<ui  name="Configuration Error Trap" style="choiceNselect1" />
</object>
<object name="TrapVirtAuthFail">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable the Virtual Authentication Failure Trap"/>
<ui  name="Virtual Authentication Failure Trap" style="choiceNselect1" />
</object>
<object name="TrapVirtBadPkt">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable the Virtual Bad Packet Trap"/>
<ui  name="Virtual Bad Packet Trap" style="choiceNselect1" />
</object>
<object name="TrapVirtCfgError">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable the Virtual Config Error Trap"/>
<ui  name="Virtual Configuration Error Trap" style="choiceNselect1" />
</object>

<!-- If-rx type OSPFv2 Traps -->
<object name="TrapIfRxPkt">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable If-Rx pkt Trap"/>
<ui  name="Interface Rx Packet Trap" style="choiceNselect1" />
</object>

<!-- LSA type OSPFv2 Traps -->
<object name="TrapLSAAll"  >
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable all the ospfv2 LSA Traps"/>
<ui  name="All LSA Traps" style="choiceNselect1" />
</object>
<object name="TrapLSAMaxAge">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable LSA Max Age Trap"/>
<ui  name="LSA Maximum Age Trap" style="choiceNselect1" />
</object>
<object name="TrapLSAOriginate">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable LSA Originate Trap"/>
<ui  name="LSA Originate Trap" style="choiceNselect1" />
</object>
<!-- Overflow type OSPFv2 Traps -->
<object name="TrapOverflowAll"  >
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable all the ospfv2 Overflow Traps"/>
<ui  name="All Overflow Traps" style="choiceNselect1" />
</object>
<object name="TrapLsdbOverFlow">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable LSDB Overflow Trap"/>
<ui  name="LSDB Overflow Trap" style="choiceNselect1" />
</object>
