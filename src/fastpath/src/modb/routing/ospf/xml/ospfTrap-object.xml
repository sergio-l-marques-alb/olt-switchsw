<object name="TrapStateChngAll"  >
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable all the ospfv2 State Change Traps"/>
<ui  name="All State Change Traps" style="choiceNselect1" />
</object>
<object name="TrapIfStateChng">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable Interface State Change Trap"/>
<ui  name="Interface State Change Trap" style="choiceNselect1" />
</object>
<object name="TrapNbrStateChng">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable Neighbor State Change Trap"/>
<ui  name="Neighbor State Change Trap" style="choiceNselect1" />
</object>
<object name="TrapVirtIfStateChng">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable Virtual Interface State Change Trap"/>
<ui  name="Virtual Interface State Change Trap" style="choiceNselect1" />
</object>
<object name="TrapVirtNbrStateChng">
<core  type="enum" xtype="enable_t" access="read-write" descr="Allows a user to enable/disable Virtual Neighbor State Change Trap"/>
<ui  name="Virtual Neighbor State Change Trap" style="choiceNselect1" />
</object>

</module>
</objectxml>
</FastPathXML>
