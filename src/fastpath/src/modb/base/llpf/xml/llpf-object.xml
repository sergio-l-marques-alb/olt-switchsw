<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM  "object.dtd" >

<!-- Object definitions for LLPF component -->

<FastPathXML>
    <objectxml version="1.0" name="base">
        <module name="llpf">
            <object name="ifIndex" compile_if="IF_STACKING_PACKAGE_PRESENT">
               <core type="user" xtype="usp_t" key="yes" access="read-write" descr="valid LLPF Interface Number"/>
               <ui name="Unit/Slot/Port" style="plainText">
                   <snmp asn-id=" "/>
               </ui>
             </object>

            <object name="ifIndex" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT">
               <core type="user" xtype="usp_t" key="yes" access="read-write" descr="valid LLPF Interface Number"/>
               <ui name="Slot/Port" style="plainText">
                   <snmp asn-id=" "/>
               </ui>
             </object>

             <object name="protocolType" depends="ifIndex">
              <core type="enum" xtype="L7_LLPF_BLOCK_TYPE_t" key="yes" access="read-write" descr=" The type of LLPF Protocol that needs to be configured for blocking or un blocking."/>
              <ui name="Protocol Type" style="plainText">
                <snmp asn-id=" "/>
              </ui>
             </object>

           <object name="BlockMode" depends="ifIndex,protocolType">
                <core type="enum" xtype="TrueFalse_t" descr="LLPF Protocol Filter Blocking Mode" access="read-write"/>
                <ui style="inputLine" name="Block Mode">
                    <snmp asn-id=" "/>
                </ui>
            </object>

<!-- This object is only for a summary page -->
           <object name="BlockModeSummary" depends="ifIndex">
                <core type="string" access="read-only" descr="LLPF Protocol Filter Blocking Mode"/>
                <ui style="plainText" name="Block Mode">
                    <snmp asn-id=" "/>
                </ui>
            </object>

        </module>

 </objectxml>
</FastPathXML>
