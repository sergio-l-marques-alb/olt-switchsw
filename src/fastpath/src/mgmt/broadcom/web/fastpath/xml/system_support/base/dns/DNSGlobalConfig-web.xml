<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >


<FastPathXML>
    <webxml version="1.0" name="DNSGlobalConfig" title="DNS Global Configuration">
        <group level="ReadWriteUser" id="1" heading=" ">
            <node id="1">
                <element control="combo" obj-name="basednsResCtl_AdminMode" id="1" name="Admin Mode"/>
            </node>
            <node id="2">
                <element arghelp="Def_Domain_Name" maxLength="255" obj-name="basednsResCtl_DefDomainName" id="1" name="Default Domain Name" validation="validate_hostname_allownull"/>
            </node>
            <node id="3">
                <element arghelp="HELP_FORMAT_FP" maxLength="3" obj-name="basednsResCtl_RequestRetransmits" id="1" name="Retry Number"/>
            </node>
            <node id="4">
                <element arghelp="HELP_FORMAT_FP_UNITS_SECS" maxLength="4" obj-name="basednsResCtl_RequestTimeout" id="1" name="Response Timeout (secs)"/>
            </node>
            <repeat present="vtable" edit="inline" heading="Default Domain List" id="5">
                <element properties="filter,iterate" obj-name="baseResCtlDomain_ListName" id="1" name="Domain List"/>
		            <element control="checkbox-neg" properties="edit" obj-name="baseResCtlDomain_Status" id="2" name="Remove">
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="no"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="no"/>
	              </element> 
            </repeat>
            <node id="6">
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="1" name="Submit">
                    <action type="button" enable="1_1|1_2|1_3|1_4" disable="1_5"/>
                </button>
                <button scope="page" type="buttonSubmit" level="ReadWriteUser" target-page="DNSDomainListConfig.html" help="Submits the form on this page to the Switch" id="2" name="Create">
                    <action type="button" disable="1_6_1|1_6_3|1_1|1_3|1_5|1_2|1_4"/>
                </button>
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="3" name="Delete">
                    <action type="button" disable="1_1|1_2|1_3|1_4" enable="1_5"/>
                </button>
                 <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="4" name="Refresh">
                     <action type="button" disable="1_5"/>
                 </button>
                  
            </node>
        </group>
        <group level="ReadOnlyUser" id="2" heading="  ">
            <node id="1">
                <element control="label" properties="read-only" obj-name="basednsResCtl_AdminMode" id="1" name="AdminMode"/>
            </node>
            <node id="2">
                <element control="label" properties="read-only" obj-name="basednsResCtl_DefDomainName" id="1" name="DefDomainName"/>
            </node>
            <node id="3">
                <element control="label" properties="read-only" obj-name="basednsResCtl_RequestRetransmits" id="1" name="RequestRetransmits"/>
            </node>
            <node id="4">
                <element control="label" properties="read-only" obj-name="basednsResCtl_RequestTimeout" id="1" name="RequestTimeout"/>
            </node>
            <node id="5">
                <message>Default Domain List</message>
            </node>
            <repeat present="vtable" edit="no" heading="  " id="6">
                <element properties="filter,iterate" obj-name="baseResCtlDomain_ListName" id="1" name="Domain List"/>
            </repeat>
            <node id="7">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>
            </node>
        </group>
    </webxml>
</FastPathXML>
