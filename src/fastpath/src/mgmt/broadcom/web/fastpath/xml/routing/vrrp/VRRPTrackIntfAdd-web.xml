<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="VRRPTrackIntfAdd" title="VRRP Interface Tracking" java-script="VRRPTrackIntfAdd_copy">
        <group id="1" heading=" ">

           <node id="7">
                <element control="label" properties="filter,iterate" obj-name="routingvrrpOperationEntry_ifIndex" id="1" />
            </node>
            <node id="8">
                <element control="label" properties="filter,iterate" obj-name="routingvrrpOperationEntry_VrId" id="1" name="Virtual Router ID"/>
            </node>  
        
            <node id="1">
                <element control="combo"  properties="hidden" cpval="routingvrrpOperationEntry_ifIndex" obj-name="routingVrrpTrackIntfTable_ifIndex" id="1" />
            </node>
            
            <node id="2">
                <element control="combo"  properties="hidden" cpval="routingvrrpOperationEntry_VrId" obj-name="routingVrrpTrackIntfTable_VrrpOperVrId" id="1" name="VRID"/>
            </node>

            <node id="3" compile_if="IF_STACKING_PACKAGE_PRESENT">
                <element control="combo" properties="list" obj-name="routingVrrpTrackIntfTable_VrrpTrackIntfPortList" id="1" name="Track Unit/Slot/Port"/>
            </node>

            <node id="3" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT">
                <element control="combo" properties="list" obj-name="routingVrrpTrackIntfTable_VrrpTrackIntfPortList" id="1" name="Track Slot/Port"/>
            </node>

            <node id="4">
                <element arghelp="HELP_FORMAT_FP" control="textbox"  value="10" properties="write-only" maxLength="10" obj-name="routingVrrpTrackIntfTable_VrrpTrackIfPrioDec" id="1" name="Priority Decrement"/>
            </node>

            <node id="6">
                <element control="textbox"  value="" properties="write-only,hidden" obj-name="routingVrrpTrackIntfTable_VrrpTrackIntf" id="1" name="VRID"/>
            </node>

            <node id="5">
            
                <button scope="page" type="buttonSubmit" value="Add" target-page="VRRPTrackIntf.html" obj-name="routingVrrpTrackIntfTable_VrrpTrackIfStatus" help="Refreshes the form on this page from the Switch" id="1" name="Submit">
                <action type="button" disable="1_3_1|1_5_2"/>
                </button>
 
                <button scope="page" type="buttonClear" target-page="VRRPTrackIntf.html" help="Refreshes the form on this page from the Switch" id="2" name="Cancel">
                <action type="button" disable="1_3_1|1_4_1|1_5_1|1_6_1"/>
                </button>
                
            </node>
        </group>
        
    </webxml>
</FastPathXML>
