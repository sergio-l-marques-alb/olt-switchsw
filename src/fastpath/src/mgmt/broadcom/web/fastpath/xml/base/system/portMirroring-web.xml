<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM "web.dtd">
<FastPathXML>

  <webxml name="PortMirroring" title="Port Multiple Port Mirroring" version="1.0">
    <group heading="" id="1" level="ReadWriteUser">
      <node id="1">
        <element id="1" obj-name="basePortMirroring_PortMirrorSessionID" properties="filter" name="Session ID">
           <action type="default" hide="1_4_1"/>
           <action type="value" value="" />
        </element>
      </node>
      <node id="2">
        <element id="1" control="combo" obj-name="basePortMirroring_PortMirrorAdminMode" name="Mode"/>
      </node>
      <node id="3">
        <element id="1" control="combo" obj-name="basePortMirroring_PortMirrorDestinationPort" properties="list" name="Destination Port"/>
      </node>
       <node id="4">
        <element control="label" obj-name="basePortMirroring_PortMirrorIsConfigured" id="1" name="">
                   <action type="enum"  enable="2_2_2" show="2_2_2"/>
                   <action type="enum"  disable="2_2_2" hide="2_2_2"/>
        </element>
      </node>
        <repeat present="vtable" edit="no" heading="" id="5">
          <element properties="filter,iterate" obj-name="basePortMirroring_PortMirrorDirectionKey" id="1" name="Direction"/>
          <element control="label" properties="collate" obj-name="basePortMirroring_PortMirrorSourcePortConfigured" id="2" name="Source Port(s)"/>
        </repeat>
    </group>
    <group heading="" id="2" level="ReadWriteUser">
      <node id="2">
        <button scope="page" type="buttonSubmit" name="Add Source Port" help="" id="1" target-page="portMirroringConfigAddSourcePort.html">
            <action type="button" discard="no" disable="1_2|1_3|2_2_2|2_2_3|2_2_4"/>
        </button>
        <button scope="page" type="buttonSubmit" name="Remove Source Port" help="" id="2" target-page="portMirroringConfigRemoveSourcePort.html">
            <action type="button" discard="no" disable="1_2|1_3|2_2_1|2_2_3|2_2_4"/>
        </button>
        <button scope="page" type="buttonSubmit" name="Submit" help="" id="3">
            <action type="button" discard="no" disable="2_2_1|2_2_2|2_2_4"/>
        </button>
        <button scope="page" type="buttonSubmit" obj-name="basePortMirroring_PortMirrorRemoveSession" name="Delete" value="1" id="4" help="">
            <action type="button" discard="no" disable="1_2|1_3|2_2_1|2_2_2|2_2_3"/>
        </button>
      </node>
    </group>
    <group id="3" heading="Port Mirror Summary" level="ReadOnlyUser">
      <node id="1">
          <element control="combo" properties="filter" obj-name="basePortMirroring_PortMirrorSessionID" name="Session ID"  id="1"/>
      </node>
      <node id="2">
          <element control="combo" properties="read-only,list" obj-name="basePortMirroring_PortMirrorDestinationPort" name="Destination Port" id="2"/>
      </node>
      <node id="3">
          <element control="label" obj-name="basePortMirroring_PortMirrorAdminMode" name="Mode" id="1"/>
      </node>
      <repeat present="vtable" edit="no" heading="" id="4">
          <element properties="filter,iterate" obj-name="basePortMirroring_PortMirrorDirectionKey" id="5" name="Direction"/>
          <element control="label" properties="collate" obj-name="basePortMirroring_PortMirrorSourcePortConfigured" id="6" name="Source Port(s)"/>
      </repeat>
    </group>
    <group id="4" heading="" level="ReadOnlyUser">
      <node id="1">
       <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>
      </node>
    </group>

  </webxml>


</FastPathXML>
