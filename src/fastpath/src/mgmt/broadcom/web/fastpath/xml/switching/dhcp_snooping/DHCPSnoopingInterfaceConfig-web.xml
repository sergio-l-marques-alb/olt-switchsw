<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="DHCPSnoopingInterfaceConfig" title="DHCP Snooping Interface Configuration" java-script="DHCPSnoopingInterfaceConfig">
        <group level="ReadWriteUser" id="1" heading=" ">
            <node id="1">
                <element control="combo" properties="filter,iterate" obj-name="SwitchingDHCPSnooping_Interface" id="1" name="Interface"/>
            </node>
            <node id="2">
		<element control="combo" obj-name="SwitchingDHCPSnooping_TrustedState" id="1" name="Trust State">
		   <action type="enum" web_name="Disable"/>
                   <action type="enum" web_name="Enable"/>		   
		</element>	    
            </node>
            <node id="3">
		    <element control="combo" obj-name="SwitchingDHCPSnooping_LogInvalidPackets" id="1" name="Logging Invalid Packets">
			    <action type="enum" web_name="Disable"/>
			    <action type="enum" web_name="Enable"/>
		   </element>    
            </node>
            <node id="5">
                <element arghelp="HELP_DHCPSNOOPING_INTERFACE_RATELIMIT" validation="validate_dhcpsnooping_rate_limit" maxLength="3" obj-name="SwitchingDHCPSnooping_RateLimit" id="1" name="Rate Limit">
                <action type="value" value="-1" setval_ids="1_5_4" setval_args="L7_TRUE"/>
                </element>

                <element control="textbox" obj-name="xLibObjects_dummy" properties="write-only" value="-1" id="6" name="Rate Limit"/>
                <element control="checkbox" uienum="TrueFalse_t" properties="write-only" value="" obj-name="xLibObjects_dummy" id="4" name="No Limit">
                    <action type="enum" enable="1_5_3" disable="1_5_1|1_5_6|1_4_1|1_4_2" hide="1_4_1|1_5_1|1_5_3" show="1_5_6|1_4_2"/>
                    <action type="enum" enable="1_5_1|1_4_1" disable="1_5_3|1_5_6|1_4_2" hide="1_4_2|1_5_6|1_5_3" show="1_5_1|1_4_1"/>
                </element>
                <element control="textbox" obj-name="SwitchingDHCPSnooping_NoLimit" properties="write-only" value="-1" id="3" name="Rate Limit"/>

            </node>
            <node id="4">
                <element arghelp="HELP_FORMAT_FP_UNITS_SECONDS" maxLength="2" obj-name="SwitchingDHCPSnooping_BurstInterval" id="1" name="Burst Interval"/>
                <element obj-name="SwitchingDHCPSnooping_BurstInterval" id="2" name="Burst Interval"/>
            </node>
            <node id="6">
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="1" name="Submit"/>
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="2" name="Refresh"/>
            </node>
        </group>
        <group level="ReadOnlyUser" id="2" heading="  ">
            <node id="1">
                <element control="combo" properties="filter,iterate" obj-name="SwitchingDHCPSnooping_Interface" id="1" name="Interface"/>
            </node>
            <node id="2">
		    <element control="label" properties="read-only" obj-name="SwitchingDHCPSnooping_TrustedState" id="1" name="Trust State">
	               <action type="enum" web_name="Disable"/>
                       <action type="enum" web_name="Enable"/>		    
		   </element>
            </node>
            <node id="3">
		    <element control="label" properties="read-only" obj-name="SwitchingDHCPSnooping_LogInvalidPackets" id="1" name="Logging  Invalid Packets">
			<action type="enum" web_name="Disable"/>
                        <action type="enum" web_name="Enable"/>
	            </element>		    
            </node>
            <node id="4">
                <element control="label" arghelp="HELP_DHCPSNOOPING_INTERFACE_RATELIMIT" properties="read-only" obj-name="SwitchingDHCPSnooping_RateLimit" id="1" name="Rate Limit"/>
            </node>
            <node id="5">
                <element control="label" arghelp="HELP_FORMAT_FP" properties="read-only" obj-name="SwitchingDHCPSnooping_BurstInterval" id="1" name="Burst Interval"/>
            </node>
            <node id="6">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>
            </node>
        </group>
    </webxml>
</FastPathXML>
