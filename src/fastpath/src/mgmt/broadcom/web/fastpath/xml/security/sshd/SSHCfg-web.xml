<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="SSHCfg" title="Secure Shell Configuration">
        <group id="1" heading=" " level="ReadWriteUser">
            <node id="1">
                <element control="combo" obj-name="securitySSHConfig_AdminMode" id="1" name="Admin Mode">
                    <action type="enum" discard="no" hide="1_10_1|1_7_1|1_11_1" setval_args="L7_ENABLE" setval_ids="1_11_1"/>
                    <action type="enum" discard="no" hide="1_10_1|1_7_1|1_11_1" setval_args="L7_DISABLE" setval_ids="1_11_1"/>
                </element>            
            </node>
            <node id="11">
                <element control="combo" uienum="enable_t" obj-name="xLibObjects_dummy" id="1" name="Enable/Disable Items">
                    <action type="enum" hide="2_1_2|2_1_3|2_1_4" disable="2_1_2|2_1_3|2_1_4" invoke="onLoad"/>
                    <action type="enum" show="2_1_2|2_1_3|2_1_4" enable="2_1_2|2_1_3|2_1_4" invoke="onLoad"/>
                </element>
            </node>

            <node id="2">
                <element control="combo" obj-name="securitySSHConfig_SSHv1Mode" id="1" name="SSH Version 1"/>
            </node>
            <node id="3">
                <element control="combo" obj-name="securitySSHConfig_SSHv2Mode" id="1" name="SSH Version 2"/>
            </node>
            <node id="4">
                <element obj-name="securitySSHConfig_SessionsCount" id="1" name="SSH Connections Currently in Use"/>
            </node>
            <node id="5">
                <element maxLength="1" arghelp="HELP_FORMAT_FP" obj-name="securitySSHConfig_MaxSessionsCount" id="1" name="Maximum number of SSH Sessions Allowed"/>
            </node>
            <node id="6">
                <element maxLength="3" arghelp="HELP_FORMAT_FP" obj-name="securitySSHConfig_SessionTimeout" id="1" name="SSH Session Timeout (minutes) "/>
            </node>
            <node id="7">
                <element control="label" obj-name="securitySSHConfig_KeysPresent" id="1" name="KeysPresent">
                    <action type="enum" discard="no" enable="1_8_5|1_8_4" disable="1_8_3|1_8_2" hide="1_8_2|1_8_3" show="1_8_4|1_8_5"/>
                    <action type="enum" discard="no" enable="1_8_3|1_8_2" disable="1_8_5|1_8_4" hide="1_8_4|1_8_5" show="1_8_2|1_8_3"/>
                    <action type="enum" discard="no" enable="1_8_3|1_8_5|1_8_2|1_8_4" show="1_8_4|1_8_5|1_8_2|1_8_3"/>
                    <action type="enum" discard="no" disable="1_8_2|1_8_3|1_8_4|1_8_5" hide="1_8_4|1_8_5|1_8_2|1_8_3"/>
                </element>
            </node>
            <node id="8">
                <element control="label" obj-name="xLibObjects_dummy" id="1" name="Keys Present"/>

                <element properties="noname" obj-name="securitySSHConfig_DSAKeyPresent" id="4" name="">
                    <action type="value" discard="no" value="None" hide="1_8_5"/>
                    <action type="default" discard="no"/>
                </element>

                <button scope="node" type="buttonSubmit" value="Delete" obj-name="securitySSHConfig_DSAKeyControl" help="Submits the form on this page to the Switch" id="5" name="Delete DSA Key">
                     <action type="button" disable="2_1_1|2_1_2|2_1_3|2_1_4|1_11_1|1_8_3"/>           
                </button> 
                <element properties="noname" obj-name="securitySSHConfig_RSAKeyPresent" id="2" name="">
                    <action type="value" discard="no" value="None" hide="1_8_3"/>
                    <action type="default" discard="no"/>
                </element>

                <button scope="node" type="buttonSubmit" value="Delete" obj-name="securitySSHConfig_RSAKeyControl" help="Executes the associated actions if any." id="3" name="Delete RSA Key">
                       <action type="button" disable="2_1_1|2_1_2|2_1_3|2_1_4|1_11_1|1_8_5"/>           
                </button>
            </node>

            <node id="9">
                <element obj-name="securitySSHConfig_GenerationStatus" id="1" name="Key Generation Status ">
                   <action type="enum" discard="no" />
                    <action type="enum" discard="no" />
                    <action type="enum" discard="no" />
                    <action type="enum" discard="no" web_name="No key generation in progress"/>
                </element>
            </node>

            <node id="10">
                <element control="combo" obj-name="securitySSHConfig_GenerationStatus" id="1" name="GenerationStatus">
                    <action type="enum" discard="no" hide="2_1_2|2_1_3|2_1_4" show="2_1_1|2_1_5"/>
                    <action type="enum" discard="no" hide="2_1_2|2_1_3|2_1_4" show="2_1_1|2_1_5"/>
                    <action type="enum" discard="no" hide="2_1_2|2_1_3|2_1_4" show="2_1_1|2_1_5"/>
                    <action type="enum" discard="no" show="2_1_1|2_1_5" />
                </element>
            </node>
        </group>
        <group id="2" heading=" " level="ReadWriteUser">
            <node id="1">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>

                <button scope="page" type="buttonSubmit" target-page="DownloadFile.html" help="Submits the form on this page to the Switch" id="2" name="Download Host Keys">
                   <action type="button" disable="2_1_1|2_1_3|2_1_4|2_1_5|1_11_1|1_8_3|1_8_5"/>
                </button>
                <button scope="page" type="buttonSubmit" value="Generate" obj-name="securitySSHConfig_RSAKeyControl" help="Submits the form on this page to the Switch" id="4" name="Generate RSA Key">
                    <action type="button" disable="2_1_1|2_1_2|2_1_3|2_1_5|1_11_1|1_8_3|1_8_5"/>
                </button>

                <button scope="page" type="buttonSubmit" value="Generate" obj-name="securitySSHConfig_DSAKeyControl" help="Submits the form on this page to the Switch" id="3" name="Generate DSA Key">
                    <action type="button" disable="2_1_1|2_1_2|2_1_4|2_1_5|1_11_1|1_8_3|1_8_5"/>
                </button>
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="5" name="Submit">
                   <action type="button" disable="2_1_1|2_1_2|2_1_3|2_1_4|1_11_1|1_8_3|1_8_5"/>           
                </button> 
           </node>
        </group>
         <group id="3" heading=" " level="ReadOnlyUser">
            <node id="1">
                <element control="textbox" properties="read-only" obj-name="securitySSHConfig_AdminMode" id="1" name="Admin Mode" />
            </node>
            <node id="2">
                <element control="textbox" properties="read-only" obj-name="securitySSHConfig_SSHv1Mode" id="1" name="SSH Version 1"/>
            </node>
            <node id="3">
                <element control="textbox" properties="read-only" obj-name="securitySSHConfig_SSHv2Mode" id="1" name="SSH Version 2"/>
            </node>
            <node id="4">
                <element control="textbox" properties="read-only" obj-name="securitySSHConfig_SessionsCount" id="1" name="SSH Connections Currently in Use"/>
            </node>
            <node id="5">
                <element control="textbox" properties="read-only" obj-name="securitySSHConfig_MaxSessionsCount" id="1" name="Maximum number of SSH Sessions Allowed"/>
            </node>
            <node id="6">
                <element control="textbox" properties="read-only" obj-name="securitySSHConfig_SessionTimeout" id="1" name="SSH Session Timeout (minutes) "/>
            </node>
            <node id="7">
                <element control="textbox" properties="read-only" obj-name="securitySSHConfig_KeysPresent" id="1" name="KeysPresent" />
            </node>
            <node id="9">
                <element control="textbox" properties="read-only" obj-name="securitySSHConfig_GenerationStatus" id="1" name="Key Generation Status "/>
            </node>
            <node id="10">
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="1" name="Refresh"/>
            </node>
        </group>
    </webxml>
</FastPathXML>
