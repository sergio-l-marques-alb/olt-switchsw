<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="sFlowRecvrConfig" title="sFlow Receiver Configuration">
        <group level="ReadWriteUser"  id="1" heading=" ">
            <node id="1">
                <element control="combo" properties="filter,iterate" obj-name="basesFlowRcvrTable_RcvrIndex" id="1" name="Receiver Index"/>
            </node>
            <node id="2">
                <element control="textbox" maxLength="127" arghelp="HELP_FORMAT_NAME127" obj-name="basesFlowRcvrTable_RcvrOwner" id="1" name="Receiver Owner String"/>
            </node>
            <node id="3">
                <element arghelp="HELP_FORMAT_FP_UNITS_SECS" maxLength="10" control="textbox" obj-name="basesFlowRcvrTable_RcvrTimeout" id="1" name="Receiver Timeout"/>
            </node>
            <node id="4">
                <element arghelp="HELP_FORMAT_FP" maxLength="4" control="textbox" obj-name="basesFlowRcvrTable_RcvrMaximumDatagramSize" id="1" name="Receiver Maximum Datagram Size"/>
            </node>
            <node id="5">
                <element control="textbox" maxLength="15" obj-name="basesFlowRcvrTable_RcvrAddress" id="1" name="Receiver Address"/>
            </node>
            <node id="6">
                <element arghelp="HELP_FORMAT_FP" maxLength="5" control="textbox" obj-name="basesFlowRcvrTable_RcvrPort" id="1" name="Receiver Port"/>
            </node>
            <node id="7">
                <element control="label" properties="read-only" obj-name="basesFlowRcvrTable_RcvrDatagramVersion" id="1" name="Receiver Datagram Version"/>
            </node>
           </group>
           <group id="2" heading=" ">
            <repeat present="vtable" edit="no" heading=" " id="1">
                <element properties="filter,iterate" obj-name="basesFlowRcvrTable_RcvrIndex" id="1" name="Receiver Index"/>
