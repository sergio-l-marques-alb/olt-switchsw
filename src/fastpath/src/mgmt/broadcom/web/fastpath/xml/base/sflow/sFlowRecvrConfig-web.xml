                <element control="label" properties="edit,read-only" obj-name="basesFlowRcvrTable_RcvrTimeout" id="3" name="Receiver Timeout"/>
                <element control="label" properties="edit,read-only" obj-name="basesFlowRcvrTable_RcvrMaximumDatagramSize" id="4" name="Receiver Maximum Datagram Size"/>
                <element control="label" properties="edit,read-only" obj-name="basesFlowRcvrTable_RcvrAddress" id="5" name="Receiver Address"/>
                <element control="label" properties="edit,read-only" obj-name="basesFlowRcvrTable_RcvrPort" id="6" name="Receiver Port"/>
                <element control="label" properties="edit,read-only" obj-name="basesFlowRcvrTable_RcvrDatagramVersion" id="7" name="Receiver Datagram Version"/>
            </repeat>
            <node id="2">
                <button scope="page" level="ReadWriteUser" type="buttonSubmit" help="Submits the form on this page to the Switch" id="1" name="Submit"/>
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="2" name="Refresh"/>
            </node>
        </group>
    </webxml>
</FastPathXML>
