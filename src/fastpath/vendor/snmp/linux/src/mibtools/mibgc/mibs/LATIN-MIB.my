--
--
-- The latinEventTable.
--
--

latinEventTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF LatinEventEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION "For all ports, this table specifies all events (the actions) 
                 the LATIN agent should run when a data record matches a 
                 regular expression."
::= { latinEvent 2 }

latinEventEntry OBJECT-TYPE
    SYNTAX     LatinEventEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION "Specifies a single event."
    INDEX { latinPortIndex, latinEventIndex }
::= { latinEventTable 1 }

LatinEventEntry ::= SEQUENCE {
    latinEventIndex      Integer32,
    latinEventCategoryDescr DisplayString,
    latinEventCategoryOwner DisplayString,
    latinEventType       Integer32,
    latinEventLogTableBucketsRequested Integer32,
    latinEventLogTableTimeOutTime Integer32,
    latinEventLogFileName DisplayString,
    latinEventTrapChain  INTEGER,
    latinEventAutoResponse OctetString,
    latinEventAutoResponsePort Integer32,
    latinEventScriptName OctetString,
    latinEventDependsOn  Integer32,
    latinEventActivated  INTEGER,
    latinEventLifetimeType INTEGER,
    latinEventLifetime   Integer32,
    latinEventStorageType StorageType,
    latinEventStatus     RowStatus,
    latinEventSoftTrap OctetString,
    latinEventSoftTrapInstances OctetString
}

latinEventIndex OBJECT-TYPE
    SYNTAX     Integer32 
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION "Provides a unique numerical index for rows in this table."
::= { latinEventEntry 1 }

latinEventCategoryDescr OBJECT-TYPE
    SYNTAX     DisplayString 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "Lets you describe the significance of the event. 

                 Example:  An unrecoverable communication error occurred. 

                 The LATIN agent does not restrict the value of the 
                 latinEventCategoryDescr display string."
::= { latinEventEntry 2 }

latinEventCategoryOwner OBJECT-TYPE
    SYNTAX     DisplayString 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "Lets you specify who created this event."
::= { latinEventEntry 3 }

latinEventType OBJECT-TYPE
    SYNTAX     Integer32 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "Specifies the action:

                  1 - save the record in an in-memory table.
                  2 - save the record in a file on non-volatile storage.
                  4 - send a trap.
                  8 - send a automatic response string out the serial port.
                 16 - run a script.

                 You can add these values together to produce combinations
                 of actions.  For example:

                 latinEventType 1 + latinEventType 4 = latinEventType 5

                 This would save the record to an in-memory table AND send 
                 a trap."
::= { latinEventEntry 4 }

latinEventLogTableBucketsRequested OBJECT-TYPE
    SYNTAX     Integer32 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "As an event, a LATIN agent can log the incoming data records
                 to an in-memory table.  

                 This MIB object specifies the desired number of data records
                 stored in the latinLogDataTable for this event.

                 The LATIN agent does not restrict the value of this object,
                 but the agent may have practical limits, such as the memory 
                 size of the machine on which the agent runs."
::= { latinEventEntry 5 }

latinEventLogTableTimeOutTime OBJECT-TYPE
    SYNTAX     Integer32 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "As an event, a LATIN agent can log the incoming data records
                 to an in-memory table.  

                 Specifies a minimum time interval, in seconds, that the LATIN
                 agent retains a data record in the latinLogDataTable.

                 The LATIN agent does not restrict the value of the 
                 latinEventLogTableTimeOutTime Integer32.  If it is set to
                 zero, the LATIN agent does not expire the records based on
                 age."
::= { latinEventEntry 6 }

latinEventLogFileName OBJECT-TYPE
    SYNTAX     DisplayString 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "As an event, a LATIN agent can log the incoming data records
                 to a file on an external storage device.

                 The latinEventLogFileName object specifies the name of the
                 log file.

                 The LATIN agent does not restrict the value of the 
                 latinEventLogFileName display string."
::= { latinEventEntry 7 }

latinEventTrapChain OBJECT-TYPE
    SYNTAX     INTEGER{
                   none(1),
                   previous(2),
                   current(3),
                   both(4)
                   } 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "Does nothing"
::= { latinEventEntry 8 }

latinEventAutoResponse OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "As an event, a LATIN agent can send an automatic response to
                 a serial-port device.  

                 The latinEventAutoResponse specifies the octet string to send 
                 as an automatic response.  

                 The LATIN agent does not restrict the value of the 
                 latinEventAutoResponse octet string."
::= { latinEventEntry 9 }

latinEventAutoResponsePort OBJECT-TYPE
    SYNTAX     Integer32 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "As an event, a LATIN agent can send an automatic response to
                 a serial-port device.  

                 The latinEventAutoResponsePort specifies which serial port
                 the automatic response should be sent.  

                 The LATIN agent restricts the value of this object to values
                 between 0 and the highest numbered serial port.  A value
                 of 0 means send the response to the port the record came 
                 in on (latinPortIndex)."
::= { latinEventEntry 10 }

latinEventScriptName OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "As an event, a LATIN agent can run a script (program) to do
                 additional processing or actions.

                 The latinEventScriptName object specifies the script name 
                 the LATIN agent should run.  

                 The LATIN agent does not restrict the value of the 
                 latinEventScriptName octet string."
::= { latinEventEntry 11 }

latinEventDependsOn OBJECT-TYPE
    SYNTAX     Integer32 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION " "
::= { latinEventEntry 12 }

latinEventActivated OBJECT-TYPE
    SYNTAX     INTEGER{
                   activated(1),
                   deActivated(2),
                   deActivating(3)
                   } 
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION " "
::= { latinEventEntry 13 }

latinEventLifetimeType OBJECT-TYPE
    SYNTAX     INTEGER{centiSeconds(1),numRecords(2)} 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION " "
::= { latinEventEntry 14 }

latinEventLifetime OBJECT-TYPE
    SYNTAX     Integer32 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION " "
::= { latinEventEntry 15 }

latinEventStorageType OBJECT-TYPE
    SYNTAX     StorageType 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "Does nothing."
::= { latinEventEntry 16 }

latinEventStatus OBJECT-TYPE
    SYNTAX     RowStatus 
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "This is the row status for this table."
::= { latinEventEntry 17 }

latinEventSoftTrap OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "This object specifies a Soft Trap (as defined in the
                 SOFTTRAP-MIB) to be sent when an entry in the
                 latinEventTable is used to generate a trap.  If the value
                 of this object is a zero-length OctetString, then the
                 agent generates a latinEventTrap as specified elsewhere
                 in this MIB.  Otherwise, a Soft Trap is generated.  The
                 contents of this object must be either an ASCII string
                 specifying the textual name of the soft trap to be
                 generated, or an ASCII string representing an integer
                 value which specifies the index of the soft trap to
                 be generated.

                 The latinEventSoftTrapInstances object is used to
                 determine a set of instances to be used when generating
                 a soft trap."
::= { latinEventEntry 18 }

latinEventSoftTrapInstances OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-create
    STATUS     current
    DESCRIPTION "This object specifies a set of instances to be used
                 when generating a soft trap.  It must be an ASCII
                 string of the following format:

                     INST INST INST . . .

                 Each INST field (of which there may be zero, one, or more)
                 must each contain an OID fragment of the following form:

                     sid.sid.sid

                 Each sid is an integer value.  These instances will be
                 used as the instance list for generating the Soft Trap.
                 This list will be prefixed with the instances of the
                 entries in the following tables which are associated
                 with the trap:
                     - latinRecordHistoryTable
                     - latinFilterTable
                     - latinFilterHistoryTable
                     - latinEventTable
                     - latinLogTable

                 For example, if the string is '1.3 1.4', then
                 the generated Soft Trap  will use an instance list
                 which looks something like this:
                     1.7 1.1 1.5 1.1 1.1.5 1.3 1.4
                 The first five instances in the list mean that the
                 data was received on port 1, was placed
                 in the seventh entry in the latinRecordHistoryTable,
                 matched the first filter in the latinFilterTable, was
                 placed in the fifth entry in the latinFilterHistoryTable,
                 caused the first entry of the latinEventTable to be used
                 for generating events, and was logged in the fifth entry
                 in the latinLogTable."
::= { latinEventEntry 19 }

