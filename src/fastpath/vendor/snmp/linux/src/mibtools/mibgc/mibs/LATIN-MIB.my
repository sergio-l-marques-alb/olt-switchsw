latinLogEntry OBJECT-TYPE
    SYNTAX     LatinLogEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION "Contains a single data record and its associated information."
    INDEX {latinPortIndex, latinEventIndex, latinLogIndex }
::= { latinLogTable 1 }

LatinLogEntry ::= SEQUENCE {
    latinLogIndex        Integer32,
    latinLogData         OctetString,
    latinLogTime         OctetString,
    latinLogTimeStamp    TimeStamp
}

latinLogIndex OBJECT-TYPE
    SYNTAX     Integer32 
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION "Provides a unique numerical index for rows in this table."
::= { latinLogEntry 1 }

latinLogData OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION "Contains the logged data record."
::= { latinLogEntry 2 }

latinLogTime OBJECT-TYPE
    SYNTAX     OCTET STRING
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION "Specifies the time the incoming record arrived.  The LATIN 
                 agent records this time stamp in the format provided by the 
                 operating system under which the LATIN agent runs."
::= { latinLogEntry 3 }

latinLogTimeStamp OBJECT-TYPE
    SYNTAX     TimeStamp 
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION "Specifies the time the incoming record arrived.  The LATIN 
                 agent records this time stamp in sysUpTime format."
::= { latinLogEntry 4 }


--
--
--  LATIN traps.
--
--

latinTraps OBJECT IDENTIFIER ::= { latin 5 }
 
latinMoreDataTrap NOTIFICATION-TYPE
    STATUS     current
    DESCRIPTION "When a LATIN agent receives new data records from a character
                 stream, it can send SNMP traps to alert an SNMP management
                 application.  This object defines the LATIN more data trap."
::= { latinTraps 1 }

latinEventTrap NOTIFICATION-TYPE
    STATUS     current
    DESCRIPTION "As an event, a LATIN agent can send traps to an SNMP
                 management application.  This object defines the event trap."
::= { latinTraps 2 }

latinPortNoCharsTrap NOTIFICATION-TYPE
    STATUS     current
    DESCRIPTION "The LATIN agent sends this trap if it does not receive
                 characters from the incoming datastream within the time
                 period specified by the latinPortNoCharsTrapPeriod.  This
                 helps a management application identify a possible 
                 failure of the character stream device."
::= { latinTraps 3 }

latinPortModemStatusChangeTrap NOTIFICATION-TYPE
    STATUS     current
    DESCRIPTION "The LATIN agent sends this trap if the modem status input
                 signals change.  This helps a management application 
                 identify a possible failure of the character stream device."
::= { latinTraps 4 }


END
