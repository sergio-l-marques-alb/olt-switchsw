#!/bin/sh

export MODULE=fastpath-cxo640g
export PLATFORMS="CXO640G-MX"

export APPLICATION=fp

echo "Compiling fastpath for CXO640G..."
make -C ../fastpath -f swdrvr-cxo640g.make
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Copying binaries to apps..."
make -C ../fastpath -f swdrvr-cxo640g.make install
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Done... Going to prepare tarball."
echo

. ./fastpath.build $1
