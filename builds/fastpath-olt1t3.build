#!/bin/sh

export MODULE=fastpath-olt1t3
export PLATFORMS="CXO640G-MX TG16G TG16GF TT04SXG TA48GE"

export APPLICATION=fp

#echo "Updating SVN..."
#svn update

echo "Compiling fastpath for OLT1T3..."
make -C ../fastpath -f olt1t3.make all
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Copying binaries to apps..."
make -C ../fastpath -f olt1t3.make install
if [ $? -ne 0 ]; then
 echo ERROR!!!
 exit 1;
fi
echo "Done... Going to prepare tarball."
echo

. ./fastpath.build $1
