<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="RadiusServCfg" title="RADIUS Server Configuration">
        <group level="ReadWriteUser" id="1" heading=" ">
            <node id="1">
                <element control="combo" properties="filter,iterate" obj-name="securityRadius_RadiusServerAddress" id="1" name="RADIUS Server Host Address">
                    <action type="extn" value="Add" show="1_2_1|1_10_1|1_11_1" enable="1_2_1|1_10_1|1_11_1" hide="1_4_1|1_5_1|1_5_2|1_6_1|1_7_1|1_8_1|1_9_1|1_11_2|1_11_3|1_11_4" disable="1_4_1|1_5_1|1_5_2|1_6_1|1_7_1|1_8_1|1_9_1|1_11_2|1_11_3|1_11_4" setval_ids="1_10_1|1_2_1" setval_args="Default_RADIUS_Server_Name|valMapEmptyString"/>
                    <action type="default" show="1_4_1|1_5_1|1_5_2|1_6_1|1_7_1|1_8_1|1_9_1|1_11_2|1_11_3|1_11_4" enable="1_4_1|1_5_1|1_5_2|1_6_1|1_7_1|1_8_1|1_9_1|1_11_2|1_11_3|1_11_4" hide="1_2_1|1_11_1" disable="1_2_1|1_11_1"/>
                </element>
            </node>
            <node id="2">
                <element maxLength="255" control="textbox" obj-name="securityRadius_RadiusServerAddress" id="1" name="RADIUS Server Host Address"/>
            </node>
            <node id="3">
                <element properties="hidden" obj-name="securityRadius_RadiusServerIndex" id="1" name="RADIUS Server Index"/>
            </node>
            <node id="4">
                <element maxLength="10" obj-name="securityRadius_RadiusServerPort" arghelp="HELP_FORMAT_FP" id="1" name="Port"/>
            </node>
            <node id="5">
		            <element maxLength="16" control="textbox-passwd" obj-name="securityRadius_RadiusServerSecret" id="1" name="Secret"/>
		            <element control="checkbox" uienum="TrueFalse_t" properties="write-only" value="L7_FALSE" obj-name="xLibObjects_dummy" id="2" name="Apply">
                    <action type="enum" discard="no" enable="1_5_1"/>
                    <action type="enum" discard="no" disable="1_5_1"/>
                </element>
            </node>
            <node id="6">
		            <element control="combo" obj-name="securityRadius_RadiusServerPrimaryMode" id="1" name="Primary Server">
		                <action type="enum" web_name="Yes"/>
		                <action type="enum" web_name="No"/>
	             	</element>
            </node>
            <node id="7">
                <element control="combo" obj-name="securityRadius_RadiusServerMsgAuth" id="1" name="Message Authenticator"/>
            </node>
            <node id="8">
		            <element control="label" uienum="TrueFalse_t" obj-name="securityRadius_IsRadiusServerSharedSecretConfiguredSet" id="1" name="Secret Configured">
		                <action type="enum" web_name="Yes"/>
		                <action type="enum" web_name="No"/>
		            </element>
	          </node>
            <node id="9">
                <element obj-name="securityRadius_RadiusServerIsCurrentAuthServer" id="1" name="Current">
 		                <action type="enum" web_name="Yes"/>
		                <action type="enum" web_name="No"/>
		            </element>
            </node>	    
            <node id="10">
                <element control="textbox" maxLength="32" arghelp="HELP_FORMAT_NAME31" obj-name="securityRadius_RadiusServerName" id="1" name="RADIUS Server Name" validation="validate_alphanum_underscore_dash_space32"/>
            </node>
            <node id="11">
		            <button scope="page" type="buttonSubmit" value="Add" obj-name="securityRadius_RadiusServerStatus" help="Submits the form on this page to the Switch" id="1" name="Submit"/>
		            <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="2" name="Submit">   
		               <action type="button" disable="1_11_3"/>	
		            </button>
                <button scope="page" type="buttonSubmit" value="Delete" obj-name="securityRadius_RadiusServerStatus" help="Submits the form on this page to the Switch" id="3" name="Remove"/>
		            <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="4" name="Refresh"/>
            </node>
        </group>
    </webxml>
</FastPathXML>