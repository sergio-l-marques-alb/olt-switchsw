<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml java-script="dual_image_status" version="1.0" name="dual_image_status" title="Dual Image Status">
        <group id="1" heading=" ">
            <repeat present="vtable" edit="no" heading=" " id="1">
                <element properties="filter,iterate" obj-name="basesysImage_sysUnitIndex" id="1" name="Unit"/>
                <element obj-name="basesysImage_sysImage1Version" id="2" name="Image1 Ver"/>
                <element obj-name="basesysImage_sysImage2Version" id="3" name="Image2 Ver"/>
                <element obj-name="basesysImage_sysActiveImageName" id="4" name="Current-active"/>
                <element obj-name="basesysImage_sysActivatedImageName" id="5" name="Next-active"/>
            </repeat>
            <node id="2">
                <element control="textarea" properties="read-only" obj-name="basesysImage_sysImage1Descr" id="1" name="Image1 Description"/>
            </node>
            <node id="4">
                <element control="textarea" properties="read-only" obj-name="basesysImage_sysImage2Descr" id="1" name="Image2 Description"/>
            </node>
            <node id="5">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>
            </node>
        </group>
    </webxml>
</FastPathXML>
