srcdir = .
FP_LSRC = $(srcdir)/..
LIB     = $(srcdir)/../../lib
ROOT    = $(srcdir)/../..

include	$(FP_BASE)/src/l7tools/build/make.cfg

HEADER_NAME = include

include	$(ROOT)/config.$(CONFIG)/config.mak

OBJECTS     += ews_tel ews_cli cli_lvl7
SOURCES     += ews_tel.c ews_cli.c cli_lvl7.c

XCFLAGS     += -g -I$(FP_LSRC)/server -I../../../util -I../../vxworks
XCFLAGS     += -I$(FP_BASE)/src/l7public/common
XCFLAGS     += -I$(FP_BASE)/src/l7public/api
XCFLAGS     += -I$(FP_BASE)/src/l7public/porting
XCFLAGS     += -I$(FP_LSRC)/include
XCFLAGS     += -I$(FP_BASE)/bsp/$(L7_CUSTOMER)/$(L7_PLATFORM)/$(L7_TARGETOS)/$(HEADER_NAME)
XCFLAGS     += -I$(FP_BASE)/bsp/$(L7_CUSTOMER)/$(L7_PLATFORM)/common/$(HEADER_NAME)
XCFLAGS     += -I../../../cli/base/include
XCFLAGS     += -I../../../cli/routing/include/

TARGETA = $(OD)/ews_telnet.a

default: _objdir $(TARGETA)

include	$(LIB)/rules.mak

include $(SOURCES:.c=.d)
