 <snmp asn-id=".1.3.6.1.2.1.82.1.6.1.5"/> 
</ui></object>
<object name="mgmdRouterCacheExpiryTime" depends="mgmdRouterCacheAddressType,mgmdRouterCacheIfIndex,mgmdRouterCacheAddress">
<core type="user" xtype="timeStampHHMMSS_t" min="0" access="read-only" descr="The minimum amount of time remaining before this entry will be aged out.  The value must always be greater than 0"/>
