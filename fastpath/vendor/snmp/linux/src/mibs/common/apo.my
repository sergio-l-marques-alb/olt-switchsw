-- SNMP Advanced Protocol Operations (APO) Consortium MIB

APO-CONSORTIUM-MIB DEFINITIONS ::= BEGIN

IMPORTS
   MODULE-IDENTITY, enterprises
      FROM SNMPv2-SMI;

--
-- Top level assignment for the APO Consortium
--
apoConsortium MODULE-IDENTITY
    LAST-UPDATED "200410150000Z"
    ORGANIZATION "APO Consortium"
    CONTACT-INFO "Postal:  SNMP Research
                           3001 Kimberlin Heights Rd.
                           Knoxville, TN 37920-9716
                           USA
 
                  Tel:     +1 865 573 1434
                  E-mail:  info@snmp.com"
    DESCRIPTION  "Top level assigment for the APO Consortium"
    ::= { enterprises 21734 } --  IANA assigned


--
-- OID assignments for APO MIB modules
--
apoModules OBJECT IDENTIFIER ::= { apoConsortium 1 }

  apoCapabilitiesMIB     OBJECT IDENTIFIER ::= { apoModules 1 }
  apoAggregateMIB        OBJECT IDENTIFIER ::= { apoModules 2 }
  apoTargetParamsMIB     OBJECT IDENTIFIER ::= { apoModules 3 }

--
-- OID assigments for APO error codes
--
apoErrorCodes          OBJECT IDENTIFIER ::= { apoConsortium 2 }

  apoNoError             OBJECT IDENTIFIER ::= { apoErrorCodes 0 }
  apoTooBig              OBJECT IDENTIFIER ::= { apoErrorCodes 1 }
  apoNoSuchName          OBJECT IDENTIFIER ::= { apoErrorCodes 2 }
  apoBadValue            OBJECT IDENTIFIER ::= { apoErrorCodes 3 }
  apoReadOnly            OBJECT IDENTIFIER ::= { apoErrorCodes 4 }
  apoGenErr              OBJECT IDENTIFIER ::= { apoErrorCodes 5 }
  apoNoAccess            OBJECT IDENTIFIER ::= { apoErrorCodes 6 }
  apoWrongType           OBJECT IDENTIFIER ::= { apoErrorCodes 7 }
  apoWrongLength         OBJECT IDENTIFIER ::= { apoErrorCodes 8 }
  apoWrongEncoding       OBJECT IDENTIFIER ::= { apoErrorCodes 9 }
  apoWrongValue          OBJECT IDENTIFIER ::= { apoErrorCodes 10 }
  apoNoCreation          OBJECT IDENTIFIER ::= { apoErrorCodes 11 }
  apoInconsistentValue   OBJECT IDENTIFIER ::= { apoErrorCodes 12 }
  apoRapourceUnavailable OBJECT IDENTIFIER ::= { apoErrorCodes 13 }
  apoCommitFailed        OBJECT IDENTIFIER ::= { apoErrorCodes 14 }
  apoUndoFailed          OBJECT IDENTIFIER ::= { apoErrorCodes 15 }
  apoAuthorizationError  OBJECT IDENTIFIER ::= { apoErrorCodes 16 }
  apoNotWritable         OBJECT IDENTIFIER ::= { apoErrorCodes 17 }
  apoInconsistentName    OBJECT IDENTIFIER ::= { apoErrorCodes 18 }


END

