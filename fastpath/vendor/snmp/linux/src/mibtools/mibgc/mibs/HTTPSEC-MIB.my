        FROM USEC-MIB
    RowStatus, TestAndIncr, StorageType, RowPointer
        FROM SNMPv2-TC
    HttpTCPIPAddress
        FROM SR-TC
    TransportLabel, AuthName
        FROM V2ADMIN-MIB
    MODULE-COMPLIANCE, OBJECT-GROUP
        FROM SNMPv2-CONF
    srExperimentalMIBs, srExperimentalDomains
        FROM SNMP-RESEARCH-MIB;
-- MIB in the right place these will "go away" when we put this

httpSecurityMIB MODULE-IDENTITY
    LAST-UPDATED "9610021700"
    ORGANIZATION "SNMP Research International"
    CONTACT-INFO "
		  Postal Address:

                  SNMP Research, Inc
                  3001 Kimberlin Heights Rd.
                  Knoxville, TN 37920-9716
                  USA

                  Tel:     +1 865 573 1434
                  Fax:     +1 865 573 9197
                  E-mail:  info@snmp.com
