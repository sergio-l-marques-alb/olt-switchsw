+  {
+    fp->start = config_bcm_array;
+    fp->current = config_bcm_array;
+    fp->end = fp->start + strlen((char *)config_bcm_array);
+  }
+  else
+  {
+    free(fp);
+    return NULL;
+  }
+
+  return fp;
+}
+
+void mem_fclose(MEM_FILE_t *fp)
+{
+  if (fp != NULL)
+  {
+    free(fp);
+  }
+}
+
+char *mem_fgets(char *s, int size, MEM_FILE_t *fp)
+{
+  int i;
+
+  for(i = 0; i < (size - 1); i++)
+  {
+    /* check for EOF */
+    if (fp->current >= fp->end)
+    {
+      if (i == 0)
+      {
+        return 0;
+      }
+      else
+      {
+        break;
+      }
+    }
+
+    s[i] = fp->current[0];
+
+    /* check for newline */
+    if (fp->current[0] == '\n')
+    {
+      fp->current++;
+      i++;
+      break;
+    }
+
+    fp->current++;
+  }
+
+  s[i] = 0;
+
+  return s;
+}
