+user_libs:
+	$(MAKE) -C $(SDK)/systems/linux/user/fastpath_common -f Makefile.fastpath CFLAGS="$(CFLAGS)"
+	$(MAKE) -C $(SDK)/systems/bde/linux/user CFLAGS="$(CFLAGS)"
+	$(MAKE) -C $(SDK)/src CFLAGS="$(ADD_TO_CFLAGS)"
