+KERN_BLDROOT=$(SDK)/build/$(BLDCONFIG)/$(kernel-override)$(bldroot_suffix)
+else
+KERN_BLDROOT=$(SDK)/build/$(kernel-override)$(bldroot_suffix)
+endif
+endif # end LVL7_FIXUP
+endif
