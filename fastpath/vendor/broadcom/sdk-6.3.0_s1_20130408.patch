+
+$(BLDDIR)/version.c: $(BLDDIR)/bdecreate.o $(BCM_LIBS_BLD)
+	$(PERL) $(SDK)/tools/version.pl > $@
+$(BLDDIR)/version.o: $(BLDDIR)/version.c
