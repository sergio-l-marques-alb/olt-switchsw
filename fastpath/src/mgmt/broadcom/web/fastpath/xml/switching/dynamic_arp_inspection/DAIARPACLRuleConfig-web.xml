<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="DAIARPACLRuleConfig" title="Dynamic ARP Inspection ARP ACL Rule Configuration">
        <group level="ReadWriteUser" id="1" heading=" ">
            <node id="1">
                <element control="combo" properties="filter,iterate" obj-name="SwitchingARPAclRule_ARPACL" id="1" name="ARP ACL Name">
                <action type="value" value="valMapEmptyString" hide="1_1|1_2|1_3|1_4|1_5|1_6|1_7" show="1_8"/>
                  <action type="default" hide="1_8"/>
                 </element>
            </node>
            <node id="2">
                <message>Add New ARP ACL Rule</message>
            </node>
            <node id="3">
                <element properties="write-only" value="" maxLength="15" obj-name="SwitchingARPAclRule_RuleIP" id="1" name="Sender IP Address"/>
            </node>
            <node id="4">
                <element properties="write-only" value="" maxLength="17" obj-name="SwitchingARPAclRule_RuleMAC" id="1" name="Sender MAC Address"/>
            </node>
            <node id="5">
                <button scope="page" type="buttonSubmit" value="Add" obj-name="SwitchingARPAclRule_ROWSTATUS" help="Submits the form on this page to the Switch" id="1" name="Add">
                   <action type="button" disable="1_7_1"/>
                </button>
            </node>
            <repeat present="vtable" edit="inline" heading="List of ARP ACL Rules" id="6">
                <element properties="iterate" obj-name="SwitchingARPAclRule_RuleIP" id="2" name="Sender IP Address"/>
                <element properties="iterate" obj-name="SwitchingARPAclRule_RuleMAC" id="3" name="Sender MAC Address"/>
                <element control="checkbox-neg" obj-name="SwitchingARPAclRule_ROWSTATUS" id="4" name="Remove">
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="no"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="no"/>
                </element>
            </repeat>
            <node id="7">
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="1" name="Delete">
                   <action type="button" disable="1_3_1|1_4_1|1_5_1"/>
                </button>
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="2" name="Refresh"/>
            </node>
            <node id="8" clazz="unavailableattributes">
            <message>No ARP ACLs Found</message>
            </node>
        </group>
        <group level="ReadOnlyUser" id="2" heading="  ">
            <node id="1">
                <element control="combo" properties="filter,iterate" obj-name="SwitchingARPAclRule_ARPACL" id="1" name="ARP ACL Name">
                <action type="value" value="valMapEmptyString" hide="2_1|2_2|2_3" show="2_4"/>
                  <action type="default" hide="2_4"/>
                  </element>
            </node>
            <repeat present="vtable" edit="no" heading="List of ARP ACL Rules" id="2">
                <element properties="iterate" obj-name="SwitchingARPAclRule_RuleIP" id="1" name="Sender IP Address"/>
                <element properties="iterate" obj-name="SwitchingARPAclRule_RuleMAC" id="2" name="Sender MAC Address"/>
            </repeat>
            <node id="3">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>
            </node>
            <node id="4" clazz="unavailableattributes">
            <message>No ARP ACLs Found</message>
            </node>
        </group>
    </webxml>
</FastPathXML>