            <repeat present="vtable" edit="no" heading=" " id="2">
			    <element properties="filter,iterate" obj-name="SwitchingPortSecurityStaticEntries_StaticMACAddress" id="2" name="MAC Address"/>
			    <element properties="filter,iterate" obj-name="SwitchingPortSecurityStaticEntries_StaticVLANId" id="1" name="VLAN ID"/>			    
