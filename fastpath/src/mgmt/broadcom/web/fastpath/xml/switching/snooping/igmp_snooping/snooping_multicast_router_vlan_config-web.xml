<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="snooping_multicast_router_vlan_config" title="IGMP Snooping Multicast Router VLAN Configuration ">
        <group level="ReadWriteUser" id="1" heading=" ">
            <node id="1">
                <element control="combo" properties="filter,iterate" obj-name="SwitchingSnoopingIntfGroup_Interface" id="1" name="Interface"/>
            </node>
            <node id="2">
                <element control="combo" properties="write-only,hidden" value="IGMPSnooping" obj-name="SwitchingSnoopingIntfGroup_SnoopingProtocol" id="1" name="Snooping Protocol">
                 <action type="enum" enable="1_3_1|1_4_1|1_5_1" show="1_3_1|1_4_1|1_5_1"/>
                 <action type="enum" disable="1_3_1|1_4_1|1_5_1" hide="1_3_1|1_4_1|1_5_1"/>
                </element>
            </node>
            <node id="3">
                <element control="textbox" arghelp="HELP_FORMAT_FP" maxLength="4" properties="filter,iterate" obj-name="SwitchingSnoopingIntfGroup_SnoopingIntfVlanID" id="1" name="VLAN ID"/>
            </node>
            <node id="4">
                <element control="combo" obj-name="SwitchingSnoopingIntfGroup_intfMulticastRouterVlanMode" id="1" name="Multicast Router"/>
            </node>
            <node id="5">
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="1" name="Submit"/>
            </node>
        </group>
    </webxml>
</FastPathXML>
