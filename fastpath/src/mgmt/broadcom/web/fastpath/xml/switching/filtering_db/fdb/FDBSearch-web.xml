<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="FDBSearch" title="Forwarding Database Search" java-script="fdb_search">
        <group id="1" heading=" ">
            <node id="1">
                <element properties="filter,iterate,hidden" obj-name="baseSlotInfo_UnitNumber" id="1" name="Management Unit"/>
            </node>
            <node id="2">
                <element properties="filter,iterate" control="combo" obj-name="SwitchingFDB_FdbFilter" id="1" name="Filter"/>
            </node>
            <node id="3">
                <element control="textbox" properties="write-only" value="" obj-name="xLibObjects_dummy" id="1" name="MAC Address" maxLength="23"/>
                <button scope="node" type="buttonNone" help="Executes the associated actions if any." id="2" name="Search"/>
            </node>
            <repeat present="plain" edit="no" heading=" " id="4">
                <element control="label" properties="iterate" obj-name="SwitchingFDB_FdbMac" id="1" name="MAC Address"/>
                <element control="label" obj-name="SwitchingFDB_FdbIntIfNum" id="2" name="Source Unit/Slot/Port(s)" compile_if="IF_STACKING_PACKAGE_PRESENT"/>
                <element control="label" obj-name="SwitchingFDB_FdbIntIfNum" id="2" name="Source Slot/Port(s)" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT"/>
				<element obj-name="SwitchingFDB_FdbIfIndex" id="3" name="Interface Index"/>
                <element control="label" obj-name="SwitchingFDB_FdbEntryType" id="4" name="Status"/>
            </repeat>
            <node id="5">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>
            </node>
        </group>
    </webxml>
</FastPathXML>
