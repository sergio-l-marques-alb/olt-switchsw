<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="DNSHostIPMapping" title="DNS HostName IP Mapping Summary">
        <group id="1" heading=" ">
            <repeat present="vtable" edit="inline" heading="DNS Static Entries" id="1">
                <element properties="filter,iterate" obj-name="basednsResCtlStatic_HostName" id="1" name="Host Name"/>
                <element properties="filter,iterate" obj-name="basednsResCtlStatic_IPAddress" id="2" name="Inet Address"/>
		            <element level="ReadWriteUser" control="checkbox-neg" obj-name="basednsResCtlStatic_Status" id="3" name="Remove Static">
              	    <action type="enum" discard="yes"/>
                    <action type="enum" discard="no"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="no"/>
	              </element>		
            </repeat>
            <node id="2" clazz="buttonalign">
		           <button scope="group" type="buttonSubmit" level="ReadWriteUser" target-page="IPHostMapConfig.html" help="Submits the form on this page to the Switch" id="1" name="Add Static Entry">
		               <action type="button" disable="1_1|2_3|2_4_1|2_4_2|2_4_3" hide="1_1|2_3|2_4_1|2_4_2|2_4_3" />	     
	            </button>		    
            </node>
        </group>
        <group id="2" heading=" ">
            <repeat present="vtable" edit="inline" heading="DNS Dynamic Entries" id="3">
                <element properties="filter,iterate,hidden" obj-name="baseDnsResCache_RRIndex" id="1"/>
                <element properties="filter,iterate,hidden" obj-name="baseDnsResCache_RRName" id="2"/>
                <element properties="filter,iterate,hidden" obj-name="baseDnsResCache_RRClass" id="3"/>
                <element obj-name="baseDnsResCache_RRDisplayName" id="4" name="Host Name"/>
                <element obj-name="baseDnsResCache_RRTTL" id="5" name="Total"/>
                <element obj-name="baseDnsResCache_RRElapsedTTL" id="6" name="Elapsed"/>
                <element properties="filter,iterate" obj-name="baseDnsResCache_RRType" id="7" name="Type"/>
                <element obj-name="baseDnsResCache_RRData" id="8" name="Addresses"/>
		            <element level="ReadWriteUser" control="checkbox-neg" obj-name="baseDnsResCache_RRStatus" id="9" name="Remove Dynamic">
		                <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="no"/>
                    <action type="enum" discard="yes"/>
                    <action type="enum" discard="no"/>
	              </element>		
            </repeat>
            <node id="4">
                <button scope="page" type="buttonSubmit" level="ReadWriteUser" help="Submits the form on this page to the Switch" id="1" name="Submit"/>
                <button scope="page" type="buttonSubmit" level="ReadWriteUser" value="Enable" obj-name="baseDnsResCache_FlushStatus" help="Submits the form on this page to the Switch" id="2" name="Clear Dynamic Entries">
               <action type="button" disable="1_1"/>
               </button>
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="3" name="Refresh">
                    <action type="button" disable="1_1|2_3"/>
                </button>  
            </node>
        </group>
    </webxml>
</FastPathXML>
