<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
  <webxml version="1.0" name="class_vlanid" title="DiffServ Class Rule Match VLAN" java-script="match_vlan" help-file="help_diffserv_class.html">
    <group id="1" heading=" ">
      <node id="1">
        <element properties="hidden" obj-name="qosDiffServClassRule_ClassIndex" id="1" name="ClassIndex"/>
      </node>
      <node id="2">
        <element properties="hidden" obj-name="qosDiffServClassRule_ClassRuleIndex" id="1" name="ClassRuleIndex"/>
      </node>
      <node id="3">
        <element obj-name="qosDiffServClassRule_Name" id="1" name="Class Name" control="label"/>
      </node>
      <node id="4">
        <element obj-name="qosDiffServClassRule_Type" id="1" name="Class Type" control="label"/>
      </node>
      <node id="5">
        <element obj-name="qosDiffServClassRule_ProtoType" id="1" name="Class Layer 3 Protocol" control="label"/>
      </node>
      <node id="10">          
        <element obj-name="qosDiffServClassRule_MatchEntryType" id="1" name="Entry type" control="textbox" properties="hidden,write-only" value="VLAN"/>
        <element control="combo" properties="hidden" obj-name="qosDiffServClassRule_MatchExcludeFlag" id="2" name="Exclude" value="False" run_if="DIFFSERV_CLASS_MATCH_EXCLUDE_FEATURE"/>
      </node> 
      <node id="6">
        <element obj-name="qosDiffServClassRule_MatchVlanIdStart" id="1" name="VLAN Identifier" maxLength="4" arghelp="vlanrange"/>
        <element obj-name="qosDiffServClassRule_MatchVlanIdEnd" id="2" name="VLAN Identifier End" properties="hidden"/>
      </node>
      <node id="8"> 
        <element control="checkbox" uienum="TrueFalse_t" obj-name="xLibObjects_dummy" id="1" name="Exclude" run_if="DIFFSERV_CLASS_MATCH_EXCLUDE_FEATURE">
          <action type="enum" discard="no" setval_args="TRUTH_VALUE_TRUE" setval_ids="1_10_2"/>
          <action type="enum" discard="no" setval_args="TRUTH_VALUE_FALSE" setval_ids="1_10_2"/>
        </element>
      </node>
      <node id="7">
        <button scope="page" type="buttonSubmit" value="Add" obj-name="qosDiffServClassRule_RowStatus" help="Submits the form on this page to the Switch" id="1" name="Submit" target-page="DiffservClassConfig.html">
          <action type="button" disable="1_3_1|1_4_1|1_5_1"/>
        </button>
        <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="2" name="Cancel" target-page="DiffservClassConfig.html"/>
      </node>
    </group>
  </webxml>
</FastPathXML>