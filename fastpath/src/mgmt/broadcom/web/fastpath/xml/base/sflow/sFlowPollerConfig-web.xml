<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="sFlowPollerConfig" title="sFlow Poller Configuration">
        <group level="ReadWriteUser" id="1" heading=" ">
            <node id="1">
                <element control="combo" properties="filter,list" obj-name="basesFlowCpTable_CpDataSource" id="1" name="Poller DataSource"/>
            </node>
            <node id="2">
                <element properties="filter,iterate,hidden" obj-name="basesFlowCpTable_CpInstance" id="1" name="CpInstance"/>
            </node>
            <node id="3">
                <element arghelp="SFLOW_RCVR_HELP" maxLength="1" control="textbox" obj-name="basesFlowCpTable_CpReceiver" id="1" name="Receiver Index"/>
            </node>
            <node id="4">
                <element arghelp="HELP_FORMAT_FP_UNITS_SECS" maxLength="5" control="textbox" obj-name="basesFlowCpTable_CpInterval" id="1" name="Poller Interval"/>
            </node>
            <node id="5">
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="1" name="Submit"/>
            </node>
        </group>
	<group id="2" heading=" ">
            <repeat present="vtable" edit="no" heading=" " id="1">
                <element properties="filter,iterate" obj-name="basesFlowCpTable_CpDataSource" id="1" name="Poller DataSource"/>
                <element properties="filter,iterate,hidden" obj-name="basesFlowCpTable_CpInstance" id="2"/>
                <element control="label" obj-name="basesFlowCpTable_CpReceiver" id="3" name="Receiver Index"/>
                <element control="label" obj-name="basesFlowCpTable_CpInterval" id="4" name="Poller Interval"/>
            </repeat>
	    <node id="2">
              <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>
            </node>
        </group>
    </webxml>
</FastPathXML>