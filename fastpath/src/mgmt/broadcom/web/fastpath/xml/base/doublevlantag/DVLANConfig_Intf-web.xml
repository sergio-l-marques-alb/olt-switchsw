<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="DVLANConfig_Intf" title="DVLAN Interface Configuration">
        <group level="ReadWriteUser" id="1" heading=" ">
            <node id="1">
                <element control="combo" properties="filter,iterate" obj-name="baseDVLANTunneling_Interface" id="1" name="Interface">
		                <action type="extn" value="All" enable="1_5_1|1_4_1|1_7_1|1_12_1" 
                                                    disable="1_2_1|1_6_1|1_10_1|1_9_1|1_9_2"
                                                    hide="1_2_1|1_6_1|1_5_1|1_10_1|1_9_1|1_11_1|1_8_2|1_8_3" 
                                                    show="1_4_1|1_7_1|1_12_1|1_8_1" setval_ids="1_11_1" setval_args="L7_ENABLE"/>
            		    <action type="default" enable="1_2_1|1_6_1|1_10_1|1_9_1|1_9_2" 
                                           disable="1_5_1|1_4_1|1_7_1|1_12_1"
                                           hide="1_5_1|1_4_1|1_7_1|1_11_1|1_12_1|1_8_1"
                                           show="1_2_1|1_6_1|1_10_1|1_8_2|1_8_3" setval_ids="1_11_1" setval_args="L7_DISABLE"/>
                </element>
            </node>
            <node id="2">
                <element control="combo" obj-name="baseDVLANTunneling_Mode" id="1" name="Interface Mode"/>
            </node>
            <node id="12">
                <element control="combo" obj-name="baseDVLANTunneling_GlobalMode" id="1" value="Enable" name="Interface Mode"/>
            </node>
            <node id="4">
		            <element run_if="DVLANTAG_ETHERTYPE_PER_INTERFACE_FEATURE" control="combo" uienum="dvlanEthertype_t" obj-name="xLibObjects_dummy" id="1" name="Interface EtherType">
	                   <action type="enum" hide="1_5_1" setval_ids="1_5_1" setval_args="DVLAN_Ethertype_802Q"/>
                     <action type="enum" hide="1_5_1" setval_ids="1_5_1" setval_args="DVLAN_Ethertype_vman"/>
                     <action type="enum" show="1_5_1" setval_ids="1_5_1" setval_args="valMapEmptyString"/> 
               </element>
            </node>   
            <node id="11">
                <element control="combo" obj-name="xLibObjects_dummy" uienum="enable_t" id="1">
		    <action type="enum" />
		    <action type="enum" hide="1_5_1"/>
                </element>
            </node>
            <node id="5">
                <element control="textbox" value="" arghelp="HELP_FORMAT_ETHERTYPE" obj-name="baseDVLANTunneling_AllIntfEtherType" id="1" run_if="DVLANTAG_ETHERTYPE_PER_INTERFACE_FEATURE" name="Custom Value" maxLength="5"/>
            </node>            
            <node id="10">
		            <element run_if="DVLANTAG_ETHERTYPE_PER_INTERFACE_FEATURE" control="combo" uienum="dvlanEthertype_t" obj-name="xLibObjects_dummy" id="1" name="Interface EtherType">
	                   <action type="enum" hide="1_9_1" enable="1_9_1" setval_ids="1_9_1" setval_args="DVLAN_Ethertype_802Q"/>
                     <action type="enum" hide="1_9_1" enable="1_9_1" setval_ids="1_9_1" setval_args="DVLAN_Ethertype_vman"/>
                     <action type="enum" show="1_9_1" enable="1_9_1" setval_ids="1_9_1" setval_args="valMapEmptyString"/> 
               </element>
            </node>   
            <node id="9">
                <element control="textbox" arghelp="HELP_FORMAT_ETHERTYPE" obj-name="baseDVLANTunneling_EtherType" id="1" run_if="DVLANTAG_ETHERTYPE_PER_INTERFACE_FEATURE" name="Custom Value" maxLength="5"/>
            </node>            
            <node id="6">
                <element arghelp="HELP_FORMAT_FP" obj-name="baseDVLANTunneling_CustID" id="1" name="Customer ID" run_if="DVLANTAG_CUSTID_FEATURE" maxLength="4"/>
            </node>
            <node id="7">
                <element value="" arghelp="HELP_FORMAT_FP" obj-name="baseDVLANTunneling_GlobalCustID" id="1" name="Customer ID" run_if="DVLANTAG_CUSTID_FEATURE" maxLength="4"/>
            </node>
            <node id="8">
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="1" name="Submit" run_if="DVLANTAG_ETHERTYPE_PER_INTERFACE_FEATURE"/>
                <button scope="page" type="buttonSubmit" value="Add" obj-name="baseDVLANTunneling_PortDVlanTagRowStatus" help="Submits the form on this page to the Switch" id="3" name="Submit" run_if="DVLANTAG_ETHERTYPE_PER_INTERFACE_FEATURE"/>
                <button scope="page" type="buttonSubmit" value="Delete" obj-name="baseDVLANTunneling_PortDVlanTagRowStatus" help="Submits the form on this page to the Switch" id="2" name="Remove" run_if="DVLANTAG_ETHERTYPE_PER_INTERFACE_FEATURE">
                </button>
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="4" name="Submit" run_if="NO_DVLANTAG_ETHERTYPE_PER_INTERFACE_FEATURE"/>
            </node>
        </group>
        <group level="ReadOnlyUser" id="2" heading=" ">
            <node id="1">
                <element control="combo" properties="filter,iterate" obj-name="baseDVLANTunneling_Interface" id="1" name="Interface">
                </element>
            </node>
            <node id="2">
                <element control="label" obj-name="baseDVLANTunneling_Mode" id="1" name="Interface Mode"/>
            </node>
            <node id="3">
                <element control="label" obj-name="baseDVLANTunneling_EtherType" id="1" run_if="DVLANTAG_ETHERTYPE_PER_INTERFACE_FEATURE" name="Custom Value"/>
            </node>
            <node id="4">
                <element control="label" obj-name="baseDVLANTunneling_CustID" id="1" name="Customer ID" run_if="DVLANTAG_CUSTID_FEATURE"/>
            </node>
            <node id="5">
                <button scope="page" type="buttonClear" help="Refreshes the form" id="1" name="Refresh"/>
            </node>
        </group>



    </webxml>
</FastPathXML>
