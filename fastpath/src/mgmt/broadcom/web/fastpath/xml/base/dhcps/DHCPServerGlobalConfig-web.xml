<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 

<FastPathXML>
    <webxml version="1.0" name="DHCPServerGlobalConfig" title="DHCP Server Global Configuration">
       <group level="ReadWriteUser" id="1" heading="">
            <node id="1">
                <element control="combo" obj-name="baseDhcpServerGlobal_DhcpServerAdminMode" id="1" name="Admin Mode"/>
            </node>
            <node id="2">
                <element arghelp="HELP_FORMAT_EIGHT" maxLength="2" obj-name="baseDhcpServerStats_DhcpServerPingPktNos" id="1" name="Ping Packet Count"/>
            </node>
            <node id="3">
                <element control="combo" obj-name="baseDhcpServerAddressConflict_DhcpServerAddressConflictLogging" id="1" name="Conflict Logging Mode"/>
            </node>
            <node id="4">
                <element control="combo" obj-name="baseDhcpServerGlobal_DhcpServerBootpAutomatic" id="1" name="Bootp Automatic Mode"/>
            </node>
            <node id="5">
                <message properties="write-only">Add Excluded Addresses</message> 
            </node>
            <node id="7">
                <element control="textbox" value="0.0.0.0" maxLength="15" obj-name="baseDhcpServerExcluded_RangeStartIpAddress" id="1" name="From"/>
            </node>
            <node id="8">
                <element control="textbox" value="0.0.0.0" maxLength="15" obj-name="baseDhcpServerExcluded_RangeEndIpAddress" id="1" name="To" arghelp="valmapDhcpSrvrNote"/>
            </node>
            <node id="11">
                <button scope="page" type="buttonSubmit" value="Add" obj-name="baseDhcpServerExcluded_Status" help="Submits the form on this page to the Switch" id="1" name="Submit">
                    <action type="button" disable="1_10|1_21_1" enable="1_1_1|1_2_1|1_3_1|1_4_1|1_7_1|1_8_1"/>
                </button>
            </node>
            <repeat present="vtable" edit="inline" heading="Delete Excluded Addresses" id="10">
                <element properties="filter,iterate,hidden" obj-name="baseDhcpServerExcluded_RangeIndex" id="1"/>
                <element control="checkbox-neg" obj-name="baseDhcpServerExcluded_Status" id="2" name="Delete">
                    <action type="enum" discard="yes" />
                    <action type="enum" discard="no" />
                    <action type="enum" discard="yes" />
                    <action type="enum" discard="yes" />
                    <action type="enum" discard="yes" />
                    <action type="enum" discard="yes" />
                    <action type="enum" discard="no" />
                </element>
                <element obj-name="baseDhcpServerExcluded_StartIpAddress" id="3" name="From"/>
                <element obj-name="baseDhcpServerExcluded_EndIpAddress" id="4" name="To"/>
            </repeat>
            <node id="21">
               <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="1" name="Delete">
                  <action type="button" disable="1_11_1|1_1_1|1_2_1|1_3_1|1_4_1|1_7_1|1_8_1" />
               </button>
            </node>
        </group>

        <group level="ReadOnlyUser" id="2" heading="">
            <node id="1">
               <element control="label" properties="read-only" obj-name="baseDhcpServerGlobal_DhcpServerAdminMode" id="1" name="Admin Mode"/>
            </node>
            <node id="2">
               <element control="label" properties="read-only" obj-name="baseDhcpServerStats_DhcpServerPingPktNos" id="1" name="Ping Packet Count"/>
            </node>
            <node id="3">
               <element control="label" properties="read-only" obj-name="baseDhcpServerAddressConflict_DhcpServerAddressConflictLogging" id="1" name="Conflict Logging Mode"/>
            </node>
            <node id="4">
               <element control="label" properties="read-only" obj-name="baseDhcpServerGlobal_DhcpServerBootpAutomatic" id="1" name="Bootp Automatic Mode"/>
            </node>
            <repeat present="vtable" edit="no" heading="Excluded Addresses" id="5">
               <element properties="filter,iterate,hidden" obj-name="baseDhcpServerExcluded_RangeIndex" id="1"/>
               <element  obj-name="baseDhcpServerExcluded_StartIpAddress" id="2" name="From"/>
               <element  obj-name="baseDhcpServerExcluded_EndIpAddress" id="3" name="To"/>
            </repeat>
        </group>
    </webxml>
</FastPathXML>
