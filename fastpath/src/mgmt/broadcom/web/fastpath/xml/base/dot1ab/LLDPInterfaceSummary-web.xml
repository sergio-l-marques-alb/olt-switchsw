<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="LLDPInterfaceSummary" title="LLDP Interface Summary">
        <group id="1" heading=" ">
            <node id="3">
             <message> TLV Codes: 0- Port Description,   1- System Name, 2- System Description, 3- System Capabilities </message>
            </node>
            <repeat present="vtable" edit="no" heading=" " id="1">
                <element properties="filter,iterate" obj-name="SwitchingLLDPPortConfig_PortNum" id="1" name="Interface"/>
                <element properties="" obj-name="SwitchingLLDPPortConfig_LinkStatus" id="2" name="Link Status"/>
                <element control="label" obj-name="SwitchingLLDPPortConfig_LldpXPortTransmitMode" id="3" name="Transmit"/>
                <element control="label" obj-name="SwitchingLLDPPortConfig_LldpPortReceiveMode" id="4" name="Receive"/>
