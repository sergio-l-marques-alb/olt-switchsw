<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >

<FastPathXML>
    <webxml version="1.0" name="TimeRangeConfig" title="Time Range Configuration">
        <group level="ReadWriteUser" id="1" heading=" ">
        <!-- key object Time Range Index in hidden mode -->           
            <node id="1">
                <element properties="hidden" obj-name="baseTimeRange_timeRangeIndex" id="1" name="Time Range ID"/>
            </node>
            <node id="6">
                <element control="label" properties="read-only,hidden" obj-name="baseTimeRange_timeRangeTableMaxSizeStatus" id="1" name="Time Range Max Size Status">
                   <action invoke="both" type="enum" discard="no" web_name="True"  enable="1_2_1"  show="1_2_1" disable="1_5_1" hide="1_5_1"/>
                   <action invoke="both" type="enum" discard="no" web_name="False" enable="1_5_1"  show="1_5_1" disable="1_2_1" hide="1_2_1"/>
                </element>
            </node>
            <node id="2">
                <element control="combo"  obj-name="baseTimeRange_timeRangeName" properties="filter" id="1" name="Time Range">
                    <action type="extn" value="Create New Time Range" disable="1_4_2" hide="1_4_2" enable="1_3_1|1_4_1" show="1_3_1|1_4_1" setval_args="valMapEmptyString" setval_ids="1_3_1" position="0"/>
                    <action type="default" hide="1_3_1|1_4_1" disable="1_3_1|1_4_1" enable="1_4_2" show="1_4_2"/>
                </element>
            </node>
            <node id="5">
                <element control="combo"  obj-name="baseTimeRange_timeRangeName" properties="filter" id="1" name="Time Range">
                    <action type="default" hide="1_3_1|1_4_1" disable="1_3_1|1_4_1" enable="1_4_2" show="1_4_2"/>
                </element>
            </node>
            <node id="3">
                <element control="textbox" maxLength="31" obj-name="baseTimeRange_timeRangeName" id="1" name="Time Range Name" arghelp= "HELP_FORMAT_NAME31" validation="validate_name31" />
            </node>

            <node id="4">
                <button scope="group" type="buttonSubmit" level="ReadWriteUser" value="Add" obj-name="baseTimeRange_timeRangeRowStatus" help="Submits the form on this page to the Switch" id="1" name="Submit"/>                
               <button scope="group" type="buttonSubmit" level="ReadWriteUser" value="Delete" obj-name="baseTimeRange_timeRangeRowStatus" help="on Click submits the form and deletes the selected TimeRange" id="2" name="Delete">
               </button> 
            </node>
        </group>

        <group id="2" heading=" ">
            <node id="1">
                <element control="label" properties="noname,write-only" obj-name="xLibObjects_dummy" id="1" name="Blank Row" value=""/>
            </node>
            <node id="2">
                <element control="label" clazz="rowdataheader" value="Table" properties="noname,write-only" obj-name="xLibObjects_dummy" id="1"/>
                <element control="label" clazz="rowdataheader" value="Current Number / Maximum Number" properties="noname,write-only" obj-name="xLibObjects_dummy" id="2"/>
            </node>
            <node id="3">
                <element control="label" clazz="rowdataattributes"  obj-name="baseTimeRange_timeRangeTableSizeAndMax" id="1" name="Time Ranges"/>
            </node>
        </group>
    </webxml>
</FastPathXML>
