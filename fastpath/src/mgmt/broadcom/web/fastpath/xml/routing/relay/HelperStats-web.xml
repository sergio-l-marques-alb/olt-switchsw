<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="HelperStats" title="IP Helper Statistics">
        <group id="1" heading=" ">
            <node id="1">
                <element control="label" properties="read-only" obj-name="routingswitchHelperStatistics_NumOfDhcpClientPktsRx" id="1" name="DHCP Client Messages Received"/>
            </node>
            <node id="2">
                <element control="label" properties="read-only" obj-name="routingswitchHelperStatistics_NumOfDhcpClientPktsRelayed" id="1" name="DHCP Client Messages Relayed"/>
            </node>
            <node id="3">
                <element control="label" properties="read-only" obj-name="routingswitchHelperStatistics_NumOfDhcpServerPktsRx" id="1" name="DHCP Server Messages Received"/>
            </node>
            <node id="4">
                <element control="label" properties="read-only" obj-name="routingswitchHelperStatistics_NumOfDhcpServerPktsRelayed" id="1" name="DHCP Server Messages Relayed"/>
            </node>
            <node id="5">
                <element control="label" properties="read-only" obj-name="routingswitchHelperStatistics_NumOfUdpClientPktsRx" id="1" name="UDP Client Messages Received"/>
            </node>
            <node id="6">
                <element obj-name="routingswitchHelperStatistics_NumOfUdpClientPktsRelayed" id="1" name="UDP Client Messages Relayed"/>
            </node>
            <node id="7">
                <element obj-name="routingswitchHelperStatistics_NumOfDhcpMaxHopCountPktsRx" id="1" name="DHCP Client Messages with hops greater than Max"/>
            </node>
            <node id="8">
                <element obj-name="routingswitchHelperStatistics_NumOfDhcpTooEarlyPktsRx" id="1" name="DHCP Pkts Received too early"/>
            </node>
            <node id="9">
                <element obj-name="routingswitchHelperStatistics_NumOfDhcpSpoofedGiaddrPktsRx" id="1" name="Received DHCP Client message with giaddr as our own"/>
            </node>
            <node id="10">
                <element obj-name="routingswitchHelperStatistics_NumOfUdpTtlExpiredPktsRx" id="1" name="UDP TTL Expired Pkts Received"/>
            </node>
            <node id="11">
                <element obj-name="routingswitchHelperStatistics_NumOfUdpDisardPkts" id="1" name="UDP Pkts Discarded"/>
            </node>
        </group>
        <group id="2" heading=" ">
            <node id="1">
                <button scope="page" level="ReadWriteUser" type="buttonSubmit" help="Submits the form on this page to the Switch" id="1" name="Clear Statistics" obj-name="routingswitchHelperStatistics_ResetHelperStats" value="1" />
            </node>
           <node id="2">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>
            </node>
        </group>
        
    </webxml>
</FastPathXML>
