srcdir	:= .
FP_SRC  := $(srcdir)/..
ROOT	:= $(FP_SRC)/..
LIB     := $(ROOT)/lib

CONFIG = CONFIG_MAKE_VARIABLE_MUST_BE_SET

include $(ROOT)/config.$(CONFIG)/config.mak

OBJECTS  += ew_uncmp
SOURCES  += ew_uncmp.c

INCLUDES += ew_cmp_i.h
TARGETA  += $(OD)/compress.a


default: _objdir $(TARGETA)


include	$(LIB)/rules.mak

include depend.mak

uncmp_only: _objdir .FORCE
	$(CC) -o $(OD)/ew_uncmp.o $(CFLAGS) $(XCFLAGS) $(IFLAGS) $(XIFLAGS) -c ew_uncmp.c
	$(AR) $(ARFLAGS) $(OD)/compress.a $(OD)/ew_uncmp.o
	$(RANLIB) $(OD)/compress.a
