L7_RC_t ospfv3GroupMembershipLsaDecode(L7_char8 *, L7_char8 *lsaPayload, outputFuncPtr, void*, L7_uint32);
L7_RC_t ospfv3Type7LsaDecode(L7_char8 *, outputFuncPtr, void *, L7_uint32);
L7_RC_t ospfv3LinkLsaDecode(L7_char8 *, L7_char8 *lsaPayload, outputFuncPtr, void *, L7_uint32);
