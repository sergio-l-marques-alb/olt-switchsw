L7_RC_t ospfv3RouterLsaIfDecode(L7_char8 *, outputFuncPtr, void *);
L7_RC_t ospfv3NetworkLsaDecode(L7_char8 *, L7_char8 *lsaPayload, outputFuncPtr, void *, L7_uint32);
