L7_RC_t ospfv3LsaPrefixOptionsDecode(L7_char8 *, outputFuncPtr, void *);
L7_RC_t ospfv3IntraAreaPrefixLsaDecode(L7_char8 *, L7_char8 *lsaPayload, outputFuncPtr, void *, L7_uint32);
L7_RC_t ospfv3UnknownLsaDecode(L7_char8 *, L7_char8 *lsaPayload, outputFuncPtr , void * , L7_uint32 );
