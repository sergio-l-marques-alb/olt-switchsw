<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM "object.dtd">
<FastPathXML>
<objectxml name="Switching" version="1.0" >

<module name="DhcpL2RelayGlobalConfig">

<object name="AdminMode">
<core  type="enum" xtype="enable_t" access="read-write" descr="This object indicates whether DHCP L2 Relay is enabled globally or not."/>
<ui  name="Admin Mode" style="choiceNselect1" >
</ui>
</object>

</module>

<module name="DhcpL2RelayIntfConfig">

<object name="Interface" compile_if="IF_STACKING_PACKAGE_PRESENT">
<core  type="user" xtype="usp_t" key="yes" access="read-write" descr="This row instance contains the configuration for DHCP L2 Relay at each physical interface capable of this feature."/>
<ui  name="Unit/Slot/Port" style="inputLine" >
</ui>
</object>

<object name="Interface" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT">
<core  type="user" xtype="usp_t" key="yes" access="read-write" descr="This  row instance contains the configuration for DHCP L2 Relay at each physical interface capable of this feature."/>
<ui  name="Slot/Port" style="inputLine" >
</ui>
</object>

<object name="L2RelayMode" depends="Interface">
<core  type="enum" xtype="enable_t" access="read-write" descr="This object indicates whether the DHCP L2Relay is enabled or not"/>
<ui  name="L2 Relay Mode" style="choiceNselect1">
</ui>
</object>

<object name="L2RelayTrustMode" depends="Interface">
<core  type="enum" xtype="enable_t" access="read-write" descr="This object indicates whether the interface is trusted for DHCP L2 Relay purpose."/>
<ui  name="L2 Relay Trust Mode" style="choiceNselect1" >
</ui>
</object>

</module>

<module name="DhcpL2RelayVlanConfig"  >

<object name="VlanID"  >
<core  type="uint" key="yes" access="read-write" descr= "This object indicates the VLAN number on which DHCP L2 Relay feature is configured."/>
<ui  name="Vlan ID" style="inputLine" >
</ui>
</object>

<object name="L2RelayMode" depends="VlanID" >
<core  type="enum" xtype="enable_t" access="read-write" descr="This object indicates whether DHCP L2 Relay is enabled on this VLAN."/>
<ui  name="L2 Relay Mode" style="choiceNselect1" >
</ui>
</object>

<object name="CircuitIdMode" depends="VlanID" >
<core  type="enum" xtype="enable_t" access="read-write" descr="This object indicates whether DHCP Circuit Id is enabled for L2 relaying on this VLAN."/>
<ui  name="CircuitId Mode" style="choiceNselect1" >
</ui>
</object>

<object name="L2RelayRemoteId" depends="VlanID" >
<core  type = "user" xtype="string_t" access="read-write" descr="When this object is set with a non-empty string, DHCP RemoteId is enabled for L2 relaying on this VLAN."/>
<ui  name="L2 Relay Remote Id" style="inputLine" >
</ui>
</object>

</module>

<module name="DhcpL2RelayStats"  >

<object name="Interface" compile_if="IF_STACKING_PACKAGE_PRESENT" >
<core  type="user" xtype="usp_t" key="yes" access="read-write" descr="This row instance contains the DHCP L2Relay statistics per interface."/>
<ui  name="Unit/Slot/Port" style="inputLine" >
</ui>
</object>

<object name="Interface" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT" >
<core  type="user" xtype="usp_t" key="yes" access="read-write" descr="This row instance contains the DHCP L2Relay statistics per interface."/>
<ui  name="Slot/Port" style="inputLine" >
</ui>
</object>

<object name= "UntrustedSrvrMsgsWithOptn82" depends="Interface" >
<core  type="uint"  access="read-only" descr="This object indicates the number of DHCP server messages received with Option-82 field on the untrusted interface."/>
<ui  name="Untrusted Server Msgs With OPT-82" style="plainText" >
</ui>
</object>

<object name= "UntrustedClntMsgsWithOptn82" depends="Interface" >
<core  type="uint"  access="read-only" descr="This object indicates the number of DHCP client messages received with Option-82 field on the untrusted interface."/>
<ui  name="Untrusted Client Msgs With OPT-82" style="plainText" >
</ui>
</object>

<object name= "TrustedSrvrMsgsWithoutOptn82" depends="Interface" >
<core  type="uint"  access="read-only" descr= "This object indicates the number of DHCP server messages received without Option-82 field on the trusted interface."/>
<ui  name="Trusted Server Msgs With OPT-82" style="plainText" >
</ui>
</object>

<object name= "TrustedClntMsgsWithoutOptn82" depends="Interface" >
<core  type="uint"  access="read-only" descr="This object indicates the number of DHCP client messages received without Option-82 field on the trusted interface."/>
<ui  name="Trusted Client Msgs without OPT-82" style="plainText" >
</ui>
</object>

<object name= "IntfStatsClear" depends="Interface" >
<core  type="enum" xtype="enable_t"  access="write-only" descr="Clear the DHCP L2Relay statistics on ports."/>
<ui  name="Clear Interface Statistics" style="inputLine" >
</ui>
</object>

<object name= "IntfStatsClearAll" >
<core  type="enum" xtype="enable_t"  access="write-only" descr="Clear the DHCP L2Relay statistics on ALL ports."/>
<ui  name="Clear Interface Statistics All" style="inputLine" >
</ui>
</object>

</module> 
<module name="DhcpL2RelaySubscConfig"  >
<key-combo list="Interface,SubscriptionName" />
<object name="Interface" compile_if="IF_DOT1AD_AND_STACKING_PACKAGES_PRESENT">
<core  type="user" xtype="usp_t" key="yes" access="read-write" descr="This row instance contains  the DHCP L2 Relay subscription configuration at each physical interface capable of this feature."/>
<ui  name="Unit/Slot/Port" style="inputLine" >
</ui>
</object>

<object name="Interface" compile_if="IF_DOT1AD_AND_NOT_STACKING_PACKAGES_PRESENT">
<core  type="user" xtype="usp_t" key="yes" access="read-write" descr="This row instance contains the configuration for DHCP L2 Relay at each physical interface capable of this feature."/>
<ui  name="Slot/Port" style="inputLine" >
</ui>
</object>

<object name="SubscriptionName" depends="Interface" compile_if="IF_DOT1AD_PACKAGE_PRESENT">
<core  type="user" xtype="string_t" key="yes" access="read-write" descr="This row instance contains the configuration for DHCP L2 Relay at each subscription capable of this feature."/>
<ui  name="Subscription Name" style="inputLine" >
</ui>
</object>

<object name="L2RelayMode" depends="Interface,SubscriptionName" compile_if="IF_DOT1AD_PACKAGE_PRESENT">
<core  type="enum" xtype="enable_t" access="read-write" descr="This object indicates whether DHCP L2 Relay is enabled on this VLAN."/>
<ui  name="L2 Relay Mode" style="choiceNselect1" >
</ui>
</object>

<object name="CircuitIdMode" depends="Interface,SubscriptionName" compile_if="IF_DOT1AD_PACKAGE_PRESENT">
<core  type="enum" xtype="enable_t" access="read-write" descr="This object indicates whether DHCP CircuitId is enabled for L2 relaying on this VLAN."/>
<ui  name="CircuitId Mode" style="choiceNselect1" >
</ui>
</object>

<object name="L2RelayRemoteId" depends="Interface,SubscriptionName" compile_if="IF_DOT1AD_PACKAGE_PRESENT">
<core  type = "user" xtype="string_t" access="read-write" descr="When this object is set with a non-empty string, DHCP RemoteId is enabled for L2 relaying on this VLAN."/>
<ui  name="L2 Relay RemoteId" style="inputLine" >
</ui>
</object>

</module>


<module name="DhcpL2RelaySubscConfigSummary" >
<key-combo list="Interface,SubscriptionName" />
<object name="Interface"  compile_if="IF_DOT1AD_AND_STACKING_PACKAGES_PRESENT">
<core  type="user" xtype="usp_t" key="yes" access="read-write" descr="This row instance contains the DHCP L2 Relay subscription configuration per interface."/>
<ui  name="Unit/Slot/Port" style="inputLine" >
</ui>
</object>

<object name="Interface"  compile_if="IF_DOT1AD_AND_NOT_STACKING_PACKAGES_PRESENT">
<core  type="user" xtype="usp_t" key="yes" access="read-write" descr="This row instance contains the DHCP L2Relay subscription configuration per interface."/>
<ui  name="Slot/Port" style="inputLine" >
</ui>
</object>

<object name="SubscriptionName" depends="Interface" compile_if="IF_DOT1AD_PACKAGE_PRESENT">
<core  type="user" xtype="string_t" key="yes" access="read-write" descr="This row instance contains the configuration for DHCP L2 Relay at each subscription capable of this feature."/>
<ui  name="Subscription Name" style="inputLine" >
</ui>
</object>

<object name="L2RelayMode" depends="Interface,SubscriptionName" compile_if="IF_DOT1AD_PACKAGE_PRESENT">
<core  type="enum" xtype="enable_t" access="read-only" descr="This object indicates whether DHCP L2 Relay is enabled on this VLAN."/>
<ui  name="L2 Relay Mode" style="plainText" >
</ui>
</object>

<object name="CircuitIdMode" depends="Interface,SubscriptionName" compile_if="IF_DOT1AD_PACKAGE_PRESENT">
<core  type="enum" xtype="enable_t" access="read-only" descr="This object indicates whether DHCP CircuitId is enabled for L2 relaying on this subscription."/>
<ui  name="CircuitId Mode" style="plainText" >
</ui>
</object>

<object name="L2RelayRemoteId" depends="Interface,SubscriptionName" compile_if="IF_DOT1AD_PACKAGE_PRESENT">
<core  type = "user" xtype="string_t" access="read-only" descr="This object indicates the current configured remoteId String for L2 Relaying on this subscription."/>
<ui  name="L2 Relay RemoteId" style="plainText" >
</ui>
</object>

</module>



    </objectxml>
</FastPathXML>

