<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM "object.dtd">
<FastPathXML>
<objectxml name="Switching" version="1.0" >
<module name="MacVlan" key-method="individual">
<key-combo list="MacAddress,VlanId" />
<object name="MacAddress"  >
<core  type="mac" key="yes" access="read-write" descr="This instance specifies Mac address"/>
<ui  name="MAC VLAN MACAddress" style="inputLine">
 <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.17.1.1.1"/> 
</ui></object>
<object name="VlanId">
<core  type="uint" key="yes" min="L7_DOT1Q_MIN_VLAN_ID" max="L7_DOT1Q_MAX_VLAN_ID" access="read-write" descr="This instance specifies VLAN, the Mac address assigned to"/>
<ui  name="MAC VLAN VLANId" style="inputLine">
 <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.17.1.1.2"/> 
</ui></object>
<object name="RowStatus" depends="MacAddress,VlanId" >
<core  type="row-status" xtype="L7_ROW_STATUS_t" access="read-write" descr="This instance specifies Mac to VLAN association row status. Supported values:Add - used to create a new entry delete- removes the entry"/>
<ui  name="MAC VLAN Row Status" style="inputLine">
 <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.17.1.1.3"/> 
</ui></object>

<!-- These objects are added to provide edit support for key object's in summary pages-->
<object name="VlanIdEdit">
<core  type="uint" min="L7_DOT1Q_MIN_VLAN_ID" max="L7_DOT1Q_MAX_VLAN_ID" access="read-write" descr="This instance specifies VLAN the Mac address assigned to"/>
<ui  name="MAC VLAN VlanId Edit" style="inputLine">
</ui></object>
<object name="RowStatusEdit" depends="MacAddress" >
<core  type="row-status" xtype="L7_ROW_STATUS_t" access="read-write" descr="This instance specifies Mac to VLAN association row status. Supported values:Add - used to create a new entry delete- removes the entry"/>
<ui  name="MAC VLAN Row Status Edit" style="inputLine">
</ui></object>
</module>
</objectxml>
</FastPathXML>
