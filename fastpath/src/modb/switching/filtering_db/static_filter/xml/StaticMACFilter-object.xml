<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM  "object.dtd" >


<FastPathXML>
    <objectxml version="1.0" name="Switching">
        <module name="StaticMACFilterConfig">
		<key-combo list="StaticMacFilteringVlanId,StaticMacFilteringAddress"/>
            <object name="StaticMacFilteringVlanId">
                <core descr="This ojbect specifies the Static MAC Filter Vlan Id" max="L7_DOT1Q_MAX_VLAN_ID" min="L7_DOT1Q_MIN_VLAN_ID" key="yes" access="read-write" type="uint" properties="dynamic"/>
                <ui style="choiceNselect1" name="Static Mac Filter VlanId">
                    <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.5.1.1"/>
                </ui>
            </object>
            <object name="StaticMacFilteringAddress" depends="StaticMacFilteringVlanId">
                <core descr="This object specifies the Static MAC Filter MAC address" key="yes" access="read-write" type="mac"/>
                <ui style="inputLine" name="Static Mac Filter Address">
                    <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.5.1.2"/>
                </ui>
            </object>
			<object depends="StaticMacFilteringVlanId,StaticMacFilteringAddress" name="StaticMacFilteringSourcePortMask">
                <core descr="This object specifies the Static MAC Filter source port mask." access="read-write" type="user" xtype="uspRange_t" properties="range,dynamic"/>
                <ui style="choiceMselectN" name="Static Mac Filter Source Port Mask">
                    <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.5.1.3"/>
                </ui>
            </object>
			<object depends="StaticMacFilteringVlanId,StaticMacFilteringAddress" name="StaticMacFilteringDestPortMask">
                <core descr="This object specifies the Static MAC Filter destination port mask." access="read-write" type="user" xtype="uspRange_t" properties="range,dynamic"/>
                <ui style="choiceMselectN" name="Static Mac Filter Dest Por tMask">
                    <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.5.1.4"/>
                </ui>
            </object>
			<object depends="StaticMacFilteringVlanId,StaticMacFilteringAddress" name="StaticMacFilteringStatus">
                <core descr="This object specifies the Static MAC Filter status. Supported values: Remove - removes the entry" xtype="L7_ROW_STATUS_t" access="read-write" type="row-status"/>
                <ui style="inputLine" name="Static Mac Filter Status">
                    <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.5.1.5"/>
                </ui>
            </object>
			<object name="StaticMacFilteringDeleteAll">
                <core descr="This object deletes all Static MAC Filtering Entries" access="write-only" type="uint"/>
                <ui style="inputLine" name="Static Mac Filter Delete All"/>                    
            </object>
          <object name="VidMac">
          <core access="read-write" descr="This object displays MAC Address with VLAN ID as the prefix which is an entry in filter database." key="yes" type="user" xtype="vidMac"/>
         <ui name="Static Mac Filter MAC Address" style="inputLine"/>
         </object>
	<object name="StaticMacFilteringAdminMode">
                <core descr="This ojbect deletes all Static MAC Filtering Entries" access="read-write" type="enum" xtype="enable_t"/>
                <ui style="choiceNselect1" name="Static Mac Filter Admin Mode"/>                    
            </object>
 	<object depends="StaticMacFilteringVlanId" name="vlanForwardingMode">
                <core descr="This object sets Static MAC address filtering configuration" access="read-write" type="enum" xtype="L7_VLAN_FILTERING_MODE_t"/>
                <ui style="choiceNselect1" name="Static Mac Filter VLAN Forwarding Mode"/>                    
            </object>
        </module>
<!--
These objects are written to meet the requirements of smartpath.
These objects may not be required if object combination feature is supported
-->
<module name="StaticMACFilterInterfaceConfig">
 <object name="VidMac">
          <core access="read-write" descr="This object displays MAC Address with VLAN ID as the prefix which is an entry in filter database." key="yes" type="user" xtype="vidMac"/>
         <ui name="Static Mac Filter Interface MAC Address" style="inputLine"/>
         </object>
        <object depends="VidMac" name="StaticMacFilteringSourcePortMask">
                <core descr="This object specifies the Static MAC Filter source port mask." access="read-write" type="user" xtype="uspRange_t" properties="range,dynamic"/>
                <ui style="choiceMselectN" name="Static Mac Filter Interface Source Port Mask">
                </ui>
            </object>
      <object depends="VidMac" name="StaticMacFilteringDestPortMask">
                <core descr="This object specifies the Static MAC Filter destination port mask." access="read-write" type="user" xtype="uspRange_t" properties="range,dynamic"/>
                <ui style="choiceMselectN" name="Static Mac Filter Interface Dest Port Mask">
                </ui>
            </object>
      <object depends="VidMac" name="StaticMacFilteringStatus">
                <core descr="This object specifies the Static MAC Filter status. Supported values: Remove - removes the entry" xtype="L7_ROW_STATUS_t" access="read-write" type="row-status"/>
                <ui style="inputLine" name="Static Mac Filter Interface Status">
                </ui>
      </object>
 
</module>

<module name="BridgeMulticastGroup">    
    <object name="VlanIndex">
        <core descr="This object specifies the VLAN-ID in integer format" max="L7_DOT1Q_MAX_VLAN_ID" min="L7_DOT1Q_MIN_VLAN_ID" key="yes" access="read-write" type="uint"/>
        <ui style="inputLine" name="Bridge Mcast Group VLAN ID"/>
    </object>
    <object depends="VlanIndex" name="VlanType">
         <core descr="This object determines the VLAN type" xtype="L7_VLAN_TYPE_t" access="read-only" type="enum"/>
         <ui style="plainText" name="Bridge Mcast Group Vlan Type"/>
    </object>
    <object name="MacAddress">
        <core descr="This object specifies the static multicast MAC address" access="read-write" type="mac" key="yes"/>
        <ui style="inputLine" name="Bridge Mcast Group MAC Address">
            <snmp asn-id=""/>
        </ui>
    </object> 
    <object name="MulticastAddress" depends="MacAddress">
        <core descr="This object displays Multicast IP Address range" access="read-write" type="string" properties="pseudo-key"/>
        <ui style="inputLine" name="Bridge Multicast Address">
            <snmp asn-id=""/>
        </ui>
    </object> 
    <object name="ForwardingMask" depends="VlanIndex,MacAddress">
        <core properties="range,dynamic" descr="This object specifies the interface number of this instance" xtype="uspRangeFP_t" access="read-write" type="user"/>
        <ui style="choiceMselectN" name="Bridge Mcast Forwarding Mask">
            <snmp asn-id=""/>
        </ui>
    </object>
    <object name="FilteringMask" depends="VlanIndex,MacAddress">
        <core properties="range,dynamic" descr="This object specifies the interface number of this instance" xtype="uspRangeFP_t" access="read-write" type="user"/>
        <ui style="choiceMselectN" name="Bridge Mcast Filtering Mask">
            <snmp asn-id=""/>
        </ui>
    </object>   
    <object depends="VlanIndex,MacAddress" name="RowStatus">
        <core descr="This object is used for creation or deletion of a static multicast MAC address group." xtype="L7_ROW_STATUS_t" access="read-write" type="row-status"/>
        <ui style="inputLine" name="Bridge Mcast Row Status">
          <snmp asn-id=""/>
        </ui>
    </object>
</module>

 </objectxml>
</FastPathXML>
