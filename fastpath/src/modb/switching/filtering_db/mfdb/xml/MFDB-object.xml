<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM "object.dtd">
<FastPathXML>
<objectxml name="Switching" version="1.0">
<module name="MFDB">
<key-combo list="ProtocolType,MFDBVIDMACAddress"/>

<object name="CurrentEntries">
<core type="uint" access="read-only" descr="This object specifies the number of current entries in the MFDB table"/>
<ui name="MFDB Current Entries" style="plainText"/>
</object>
<object name="MaxTableEntries">
<core type="uint" access="read-only" descr="This object specifies the maximum number of entries that the MFDB table can hold"/>
<ui name="MFDB Max Table Entries" style="plainText"/>
</object>
<object name="MostEntries">
<core type="uint" access="read-only" descr="This object specifies the most number of entries ever in the MFDB table"/>
<ui name="MFDB Most Entries" style="plainText"/>
</object>

<object name="ProtocolType">
<core type="enum" xtype="L7_MFDB_PROTOCOL_TYPE_t" properties="key,dynamic" access="read-write" descr="This object specifies the MFDB Protocol Type"/>
<ui name="MFDB Protocol Type" style="choiceNselect1"/>
</object>

<object name="MFDBVIDMACAddress" depends="ProtocolType">
<core type="user" xtype="vidMac" key="yes" access="read-only" descr="This object specifies the MFDB VID MAC Address combination"/>
<ui name="MFDB VID MAC Address" style="plainText"/>
</object>

<object name="MFDBType" depends="MFDBVIDMACAddress,ProtocolType">
<core type="enum" xtype="L7_MFDB_ENTRY_TYPE_t" access="read-only" descr="This object specifies the MFDB Type"/>
<ui name="MFDB Type" style="plainText"/>
</object>
<object name="MFDBCompId" depends="MFDBVIDMACAddress,ProtocolType">
<core type="enum" xtype="L7_MFDB_PROTOCOL_TYPE_t" access="read-only" descr="This object specifies the MFDB Component Id"/>
<ui name="MFDB Component Id" style="plainText"/>
</object>
<object name="MFDBDescription" depends="MFDBVIDMACAddress,ProtocolType">
<core type="string" access="read-only" descr="This object specifies the MFDB Description"/>
<ui name="MFDB Description" style="plainText"/>
</object>
<object name="MFDBForwardingPortMask" depends="MFDBVIDMACAddress,ProtocolType" compile_if="IF_STACKING_PACKAGE_PRESENT">
<core type="user" xtype="usplist_t" access="read-only" descr="This object specifies the forwarding ports mask"/>
<ui name="Forwarding Unit/Slot/Port(s)" style="plainText"/>
</object>

<object name="MFDBForwardingPortMask" depends="MFDBVIDMACAddress,ProtocolType" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT">
<core type="user" xtype="usplist_t" access="read-only" descr="This object specifies the forwarding ports mask"/>
<ui name="Forwarding Slot/Port(s)" style="plainText"/>
</object>

<object name="MFDBFilteringPortMask" depends="MFDBVIDMACAddress,ProtocolType">
<core type="user" xtype="usplist_t" access="read-only" descr="This object specifies the filtering ports mask"/>
<ui name="MFDB Filtering Port Mask" style="plainText"/>
</object>

<object name="MFDBFwdPortList" depends="MFDBVIDMACAddress,ProtocolType" compile_if="IF_STACKING_PACKAGE_PRESENT">
<core type="user" xtype="usplist_t" access="read-only" descr="This object specifies the list of interfaces that are designated for forwarding (Fwd) and filtering (Flt) for the selected address."/>
<ui name="Unit/Slot/Port(s)" style="plainText"/>
</object>

<object name="MFDBFwdPortList" depends="MFDBVIDMACAddress,ProtocolType" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT">
<core type="user" xtype="usplist_t" access="read-only" descr="This object specifies the list of interfaces that are designated for forwarding (Fwd) and filtering (Flt) for the selected address."/>
<ui name="Slot/Port(s)" style="plainText"/>
</object>

<object name="MFDBFwdIntf" depends="MFDBVIDMACAddress,ProtocolType">
<core type="user" xtype="usplist_t" access="read-only" descr="This object specifies the resultant forwarding list is derived from combining all the forwarding interfaces and removing the interfaces that are listed as the static filtering interfaces."/>
<ui name="MFDB Forward Interface" style="plainText"/>
</object>
<object name="MACAddress" depends="MFDBVIDMACAddress,ProtocolType">
<core type="string" access="read-only" descr="This object specifies the MFDB MAC Address"/>
<ui name="MFDB MAC Address" style="plainText"/>
</object>

<object name="VlanID" depends="MFDBVIDMACAddress,ProtocolType">
<core type="string" access="read-only" descr="This object specifies the MFDB VLAN ID"/>
<ui name="MFDB VLAN" style="plainText"/>
</object>
</module>
</objectxml>
</FastPathXML>
