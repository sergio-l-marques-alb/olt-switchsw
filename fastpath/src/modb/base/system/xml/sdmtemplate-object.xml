<?xml version="1.0"?> <!DOCTYPE FastPathXML SYSTEM "object.dtd"> 
<FastPathXML> 
<objectxml name="sdmTemplate" version="1.0"> 

<module name="Configuration" purpose="To configure the Switch Database Management Template"> 
  <object name="current">
   <core type="enum" xtype="L7_SDM_TEMPLATE_ID_TYPE_t"  access="read-only"  descr="To Display the current SDM Template ID."/> 
   <ui name="Current SDM Template " style="plainText">
   </ui>
  </object>
  <object name="next">
   <core type="enum" xtype="L7_SDM_TEMPLATE_ID_TYPE_t"  access="read-write"   descr="To Change the Next SDM Template ID."/> 
   <ui name="Next SDM Template " style="choiceNselect1">
   </ui>
  </object>
  <object name="ID" > 
   <core type="enum" xtype="L7_SDM_TEMPLATE_ID_TYPE_t" key="yes" access="read-write" descr="To Display the Summary of SDM Template ID."/>
   <ui name="SDM Template ID" style="plainText">
   </ui> 
  </object> 
  <object name="ARPEntries" depends="ID">
   <core type="uint" key="no" access="read-only" descr="The maximum number of entries in the IPv4 Address Resolution Protocol (ARP) cache for routing interfaces."/> 
   <ui name="ARP Entries" style="plainText">
   </ui>
  </object>
  <object name="IPv4UnicastRoutes" depends="ID">
   <core type="uint" key="no" access="read-only" descr="The maximum number of IPv4 unicast forwarding table entries."/> 
   <ui name="IPv4 Unicast Routes" style="plainText">
   </ui>
  </object>

  <object name="IPv6NDPEntries" depends="ID">
   <core type="uint" key="no" access="read-only" descr="The maximum number of IPv6 Neighbor Discovery Protocol (NDP) cache entries."/> 
   <ui name="IPv6 NDP Entries" style="plainText">
   </ui>
  </object>

  <object name="IPv6UnicastRoutes" depends="ID">
   <core type="uint" key="no" access="read-only" descr="The maximum number of IPv6 unicast forwarding table entries."/>
   <ui name="IPv6 Unicast Routes" style="plainText">
   </ui>
  </object>

  <object name="ECMPNextHops" depends="ID">
   <core type="uint" key="no" access="read-only" descr="The maximum number of equal-cost next hops that can be installed in the IPv4 and IPv6 unicast forwarding tables."/> 
   <ui name="ECMP Next Hops" style="plainText">
   </ui>
  </object>

  <object name="IPv4MulticastRoutes" depends="ID">
   <core type="uint" key="no" access="read-only" descr="The maximum number of IPv4 multicast forwarding table entries."/> 
   <ui name="IPv4 Multicast Routes" style="plainText">
   </ui>
  </object>

  <object name="IPv6MulticastRoutes" depends="ID">
   <core type="uint" key="no" access="read-only" descr="The maximum number of IPv6 multicast forwarding table entries."/>
   <ui name="IPv6 Multicast Routes" style="plainText">
   </ui>
  </object>
</module><!-- END : sdmTemplate Module -->
</objectxml> 
</FastPathXML> 
