<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "object.dtd" >


<FastPathXML>
    <objectxml version="1.0" name="base">
        <module key-method="individual" name="AutoInstall">
            <object name="AutoInstallStartStop">
                <core descr="Start/stop autoinstall procedure." xtype="StartStop_t" access="read-write" type="enum"/>
                <ui style="choiceNselect1" name="Auto Install"/>
            </object>
            <object name="AutoInstallAutoSave">
                <core descr="Enable or disable auto saving the installed configuration." xtype="enable_t" access="read-write" type="enum"/>
                <ui style="choiceNselect1" name="Auto Install Auto Save"/>
            </object>
             <object name="AutoInstallAutoImage">
                <core descr="Enable or disable auto firmware download." xtype="enable_t" access="read-write" type="enum"/>
                <ui style="choiceNselect1" name="Auto Install Auto Image"/>
            </object>
              <object name="AutoInstallAutoReboot">
                <core descr="Enable or disable rebooting the device after successful." xtype="enable_t" access="read-write" type="enum"/>
                <ui style="choiceNselect1" name="Auto Install Auto Reboot"/>
            </object>
         <object name="AutoInstallCurrentStatus">
                <core descr="Get the status(like Waiting for boot options, Processing DHCP options,etc...) of auto-install." access="read-only" type="string"/>
                <ui style="plainText" name="Auto Install Current Status"/>
            </object>
            <object name="AutoInstallFileDownloadRetryCount">
                <core min="FD_AUTO_INSTALL_UNICAST_RETRY_COUNT_MIN" max="FD_AUTO_INSTALL_UNICAST_RETRY_COUNT_MAX" default="FD_AUTO_INSTALL_UNICAST_RETRY_COUNT_DEFAULT"
                      descr="Number of times the unicast TFTP tries should be made for the DHCP specified file before falling back for broadcast TFTP tries." access="read-write" type="uint"/>
                <ui style="inputLine" name="Auto Install File Download Retry Count"/>
            </object>
        </module>
    </objectxml>
</FastPathXML>
