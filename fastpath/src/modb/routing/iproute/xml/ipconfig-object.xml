<module name="IpVlanConfig" purpose="To Enable/Disable Routing on a configured VLAN." >

 <object name="vlanId" >
 <core  type="uint" key = "yes" access="read-write" descr="The Vlan's Id on which routing is enabled ."/>
 <ui  name="VLAN ID" style="inputLine">
 </ui></object>

 <object name="vlanIfIndex" depends = "vlanId" >
 <core  type="uint" access="read-only" descr="The IfIndex associated with this VLAN."/>
 <ui  name="IfIndex" style="plainText">
 </ui></object>

 <object name="ipAddress" depends="vlanId" >
 <core  type="ipv4" access="read-only" descr="The IpAddress assigned to this interface. When setting this value, the value of agentSwitchIpInterfaceNetMask must be set at the same time."/>
 <ui  name="IP Address" style="inputLine">
 </ui></object>

 <object name="netMask" depends="vlanId" >
 <core  type="ipv4" access="read-only" descr="The NetMask assigned to this interface. When setting this value, the value of agentSwitchIpInterfaceIpAddress must be set at the same time."/>
 <ui  name="Subnet Mask" style="inputLine">
 </ui></object>

 <object name="ifPhysAddress" depends="vlanId" >
 <core  type="mac" access="read-only" descr=" The interface's address at its protocol sub-layer. For example, for an 802.x interface, this object normally contains a MAC address. The interface's media-specific MIB must define the bit and byte ordering and the fo"/>
 <ui  name="Physical Address" style="plainText">
 </ui></object>

 <object name="USPFormat" depends="vlanId" >
 <core  type="string" access="read-only" descr="This object returns the vlan number in Unit/Slot/Port Format."/>
 <ui  name="Unit/Slot/Port" style="plainText">
 </ui></object>

 <object name="vlanRoutingStatus" depends = "vlanId">
 <core  type="row-status" xtype="L7_ROW_STATUS_t" access="read-write" descr="Status of Routing mode per Vlan. active(1) - Vlan is enabled for routing; createAndGo(4) - Adds Vlan entry for routing; destroy(6) - Removes Vlan from routing"/>
 <ui  name="Routing Mode" style="inputLine">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.5.1.3"/>
 </ui></object>

 <object name="vlanRoutingCreate" depends = "vlanId">
 <core  type="enum" xtype="enable_t" default="FD_RTR_DEFAULT_ADMIN_MODE" access="read-write" descr="Enables/disables routing on the VLAN."/>
 <ui  name="Routing Mode" style="choiceNselect1">
 </ui></object>
<object name="vlanRoutingCreateAndFillIfIndex" depends = "vlanId">
<core  type="row-status" xtype="L7_ROW_STATUS_t" access="read-write" descr="Administratively enables/disables routing on the switch."/>
<ui  name="routingMode" style="inputLine">
</ui></object>
</module><!-- END:IpVlanConfig Module -->


<module name="VlanInternalUsage" purpose="To set the VLAN Allocation policy for the port based Routing Interfaces." >
  <object name="baseVlanId">
  <core type="uint" access="read-write" max="L7_DOT1Q_MAX_VLAN_ID" min="L7_DOT1Q_MIN_VLAN_ID" descr="This is the Base VLAN ID for Internal allocation of port-based routing interface"/>
  <ui name="VLAN ID" style="inputLine"/>
  </object>
  <object name="AllocationPolicy">
  <core type="enum" xtype="AllocationPolicy_t"  access="read-write" descr="Allocation Policy for VALN ID in ascending/ descending order of port-based routing interface"/>
  <ui name="Allocation Policy" style="choiceNselect1"/>
  </object>
  <object name="VlanId">
  <core type="uint" key="yes" access="read-only" descr="This is the Used Internal VLAN ID for Interface"/>
  <ui name="VLAN ID" style="plainText"/>
  </object>
  <object name="Interface"  depends="VlanId" compile_if="IF_STACKING_PACKAGE_PRESENT">
  <core type="user" xtype="usp_t" access="read-only" descr="This is the interface number assigned with VLAN ID"/>
  <ui style="plainText" name="Unit/Slot/Port"/>
  </object>
  <object name="Interface" depends="VlanId" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT">
  <core type="user" xtype="usp_t" access="read-only" descr="This is the interface number assigned with VLAN ID"/>
  <ui style="plainText" name="Slot/Port"/>
  </object>
</module>

