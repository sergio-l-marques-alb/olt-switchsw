  <snmp asn-id=""/>
  </ui></object>
  <object name="NumOfDhcpSpoofedGiaddrPktsRx">
  <core type="uint" access="read-only" descr="Number of DHCP client messages received with the giaddr set to local address."/>
  <ui name="No Of DHCP Spoofed Giaddr Pkts Rx" style="plainText">
  <snmp asn-id=""/>
  </ui></object>
  <object name="NumOfUdpTtlExpiredPktsRx">
  <core type="uint" access="read-only" descr="Number of UDP packets received with TTL value less than or equal to one."/>
  <ui name="No Of UDP TTL Expired Pkts Rx" style="plainText">
  <snmp asn-id=""/>
  </ui></object>
  <object name="NumOfUdpDisardPkts">
  <core type="uint" access="read-only" descr="Number of UDP packets dropped due to match against a discard entry."/>
  <ui name="No Of UDP Disard Pkts" style="plainText">
  <snmp asn-id=""/>
  </ui></object>
  <object name="ResetHelperStats">
  <core type="uint" access="write-only" descr="Clears the IP Helper Statistics."/>
  <ui name="Reset Helper Stats" style="inputLine">
  <snmp asn-id=""/>
  </ui></object>
</module><!-- END: switchHelperStatistics Module -->

<!-- BEGIN: switchIpIcmpControlGroup Module -->
<module name="switchIpIcmpControlGroup" purpose="To Confiure the ICMP Throttling parameters." >
  <object name="switchIpIcmpEchoReplyMode" >
  <core  type="enum" xtype="enable_t" access="read-write" descr="Sets the administrative Echo Reply Mode."/>
  <ui  name="ICMP Echo Reply Mode" style="choiceNselect1">
  <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.8.1"/> 
  </ui></object>
  <object name="switchIpIcmpRedirectsMode" >
  <core  type="enum" xtype="enable_t" access="read-write" descr="Sets the administrative Mode for sending Redirects."/>
  <ui  name="ICMP Redirects Mode" style="choiceNselect1">
  <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.8.2"/> 
  </ui></object>
  <object name="switchIpIcmpRateLimitInterval" >
  <core  type="uint" access="read-write" default="FD_RTR_DEFAULT_ICMP_RATE_LIMIT_INTERVAL" min="L7_L3_ICMP_RATE_LIMIT_MIN_INTERVAL" max="L7_L3_ICMP_RATE_LIMIT_MAX_INTERVAL" descr="Specifies the time interval between tokens being placed in the bucket for ICMP Ratelimit."/>
  <ui  name="ICMP Rate Limit Interval" style="inputLine">
