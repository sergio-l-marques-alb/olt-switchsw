<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM "object.dtd">
<FastPathXML>
<objectxml name="routing" version="1.0" >
<!-- BEGIN : switchIPConfig Module -->
<module name="switchIPConfig" purpose="To add/delete Secondary IP Address on a Routing Supported Interfaces" >
  <object name="IfIndex" compile_if="IF_STACKING_PACKAGE_PRESENT" >
  <core  type="user" xtype="usp_t" key="yes" access="read-write" descr="The IfIndex associated with this instance"/>
  <ui  name="Unit/Slot/Port" style="inputLine">
  <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.1"/> 
  </ui></object>
  <object name="IfIndex" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT" >
  <core  type="user" xtype="usp_t" key="yes" access="read-write" descr="The IfIndex associated with this instance"/>
  <ui  name="Slot/Port" style="inputLine">
  <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.1"/> 
  </ui></object>
  <object name="secondaryIpAddress" depends="IfIndex" >
  <core  type="ipv4" key="yes" access="read-write" descr="The Secondary IP Address assigned to this interface."/>
  <ui  name="Secondary IP Address" style="inputLine">
  <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.6.1.1"/> 
  </ui></object>
  <object name="secondaryNetMask" depends="IfIndex,secondaryIpAddress" >
  <core  type="user" xtype="rtoipv4_t" access="read-write" descr="The Secondary Net Mask assigned to this interface. This value must be set during creation, and can not be changed."/>
  <ui  name="Secondary Subnet Mask" style="inputLine">
  <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.6.1.2"/> 
  </ui></object>
  <object name="secondaryStatus" depends="IfIndex,secondaryIpAddress" >
  <core  type="row-status" xtype="L7_ROW_STATUS_t" access="read-write" descr="Creates a new entry in the Secondary Address table. Allowed values are: createAndGo(4) - Creates an entry in this table, destroy(6) - Deletes an entry in this table"/>
  <ui  name="Secondary Status" style="inputLine">
  <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.6.1.3"/> 
  </ui></object>
  <object depends="IfIndex" name="MaxSecAddressConfigured">
  <core validation="validate_uint" descr="&lt;HTML&gt;To check whether Max Secondary Addresses Configured" xtype="TrueFalse_t" access="read-only" type="enum"/>
  <ui style="choiceNselect1" name="Are Max Secondary Addresses Configured">
  <cli validation="validate_uint"/>
  <web validation="validate_uint"/>
  <snmp asn-id=""/>
  </ui>
  </object>
</module><!-- END : switchIPConfig Module -->

<!-- BEGIN: switchHelperGlobal Module -->
<module name="switchHelperGlobal" purpose="To Configure the Helper IP Global Parameters." >
  <object name="ipHelperMode">
  <core  type="enum" xtype="enable_t" access="read-write" descr="Administratively enables/disables IP Helper mode on the switch."/>
  <ui  name="Helper Mode" style="choiceNselect1">
  <snmp asn-id=""/> 
  </ui></object>
</module><!-- END: switchHelperGlobal Module -->

<!-- BEGIN: switchHelperTableGlobal Module -->
<module name="switchHelperTableGlobal" purpose="To Add Helper IP Entries Globally." >
  <key-combo list="helperIpDestUDPPort,helperIpAddress"/>
  <object name="helperIpAddress">
  <core  type="ipv4" key="yes" access="read-write" descr="The Helper IP Address assigned to this interface."/>
  <ui  name="Server Address" style="inputLine">
  </ui></object>
  <object name="helperIpDestUDPPort">
  <core type="uint" key="yes" min="0" max="L7_IP_HELPER_MAX_PORT" access="read-write" descr="Destination port of the UDP packets to be relayed."/>
  <ui name="Destination UDP Port" style="inputLine">
  <snmp asn-id=""/>
  </ui></object>
  <object name="helperIpHitCount" depends="helperIpAddress,helperIpDestUDPPort">
  <core type="uint" access="read-only" descr="The number of times a packet has been forwarded or discarded according to this entry."/>
  <ui name="Hit Count" style="plainText">
  <snmp asn-id=""/>
  </ui></object>
  <object name="helperStatus" depends="helperIpAddress,helperIpDestUDPPort" >
  <core  type="row-status" xtype="L7_ROW_STATUS_t" access="read-write" descr="Creates a new entry in the Helper Address table. Allowed values are: createAndGo(4), destroy(6)."/>
  <ui  name="Helper Entry" style="inputLine">
  </ui></object>
</module><!-- END: switchHelperTableGlobal Module -->

<!-- BEGIN: switchHelperTable Module -->
<module name="switchHelperTable" purpose="To Add Helper IP Entries per interface." >
  <key-combo list="IfIndex,helperIpDestUDPPort,helperIpAddress"/>
  <object name="IfIndex" compile_if="IF_STACKING_PACKAGE_PRESENT" >
  <core  type="user" xtype="usp_t" key="yes" access="read-write" descr="The IfIndex associated with this instance"/>
  <ui  name="Unit/Slot/Port" style="inputLine">
  <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.1"/> 
  </ui></object>
  <object name="IfIndex" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT" >
  <core  type="user" xtype="usp_t" key="yes" access="read-write" descr="The IfIndex associated with this instance"/>
  <ui  name="Slot/Port" style="inputLine">
  <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.1"/> 
  </ui></object>
  <object name="helperIpDestUDPPort" depends="IfIndex">
  <core type="uint" key="yes" access="read-write" min="0" max="L7_IP_HELPER_MAX_PORT" depends="IfIndex" descr="Destination port of the UDP packets to be relayed."/>
  <ui name="Destination UDP Port" style="inputLine">
  <snmp asn-id=""/>
  </ui></object>
  <object name="helperIpAddress" depends="IfIndex, helperIpDestUDPPort" >
  <core  type="user" xtype="formatipv4_t" key="yes" access="read-write" descr="The Helper IP Address assigned to this interface."/>
  <ui  name="Server Address" style="inputLine">
  <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.7.1.1"/> 
  </ui></object>
  <object name="helperIpDiscard" depends="IfIndex,helperIpDestUDPPort">
  <core type="enum" xtype="L7_FalseTrue" access="read-write" descr="Discard the packets matching the specified UDP port."/>
  <ui name="Discard" style="choiceNselect1">
  <snmp asn-id=""/>
  </ui></object>
  <object name="helperIpHitCount" depends="IfIndex,helperIpDestUDPPort,helperIpAddress">
  <core type="uint" access="read-only" descr="This gives the number of packets relayed to the given helper IP received on the given interface."/>
  <ui name="Hit Count" style="plainText">
  <snmp asn-id=""/>
  </ui></object>
  <object name="helperStatus" depends="IfIndex,helperIpDestUDPPort,helperIpAddress" >
  <core  type="row-status" xtype="L7_ROW_STATUS_t" access="read-write" descr="Creates a new entry in the Helper Address table. Allowed values are: createAndGo(4), destroy(6)."/>
  <ui  name="Helper Entry" style="inputLine">
  <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.7.1.2"/> 
  </ui></object>
  <object depends="IfIndex" name="MaxHelperAddressConfigured">
  <core validation="validate_uint" descr="&lt;HTML&gt;To Check whether Max Helper Addresses configured or not" xtype="TrueFalse_t" access="read-only" type="enum"/>
  <ui style="choiceNselect1" name="Are Max Helper Addresses Configured">
  <cli validation="validate_uint"/>
  <web validation="validate_uint"/>
  <snmp asn-id=""/>
  </ui>
  </object>
</module><!-- END: switchHelperTable Module -->

<!-- BEGIN: switchHelperStatistics Module -->
<module name="switchHelperStatistics" purpose="Helper Statistics" >
  <object name="NumOfDhcpClientPktsRx">
  <core type="uint" access="read-only" descr="Number of DHCP client packets received on the switch."/>
  <ui name="No Of DHCP Client Pkts Rx" style="plainText">
  <snmp asn-id=""/>
  </ui></object>
  <object name="NumOfDhcpClientPktsRelayed">
  <core type="uint" access="read-only" descr="Number of DHCP client packets forwared to the server."/>
  <ui name="No Of DHCP Client Pkts Relayed" style="plainText">
  <snmp asn-id=""/>
  </ui></object>
  <object name="NumOfDhcpServerPktsRx">
  <core type="uint" access="read-only" descr="Number of DHCP client received on the switch."/>
  <ui name="No Of DHCP Server Pkts Rx" style="plainText">
  <snmp asn-id=""/>
  </ui></object>
  <object name="NumOfDhcpServerPktsRelayed">
  <core type="uint" access="read-only" descr="Number of DHCP server packets forwared to the server."/>
  <ui name="No Of DHCP Server Pkts Relayed" style="plainText">
  <snmp asn-id=""/>
  </ui></object>
  <object name="NumOfUdpClientPktsRx">
  <core type="uint" access="read-only" descr="Number of UDP client packets received on the switch."/>
  <ui name="No Of UDP Client Pkts Rx" style="plainText">
  <snmp asn-id=""/>
  </ui></object>
  <object name="NumOfUdpClientPktsRelayed">
  <core type="uint" access="read-only" descr="Number of UDP client packets forwarded to the server."/>
  <ui name="No Of UDP Client Pkts Relayed" style="plainText">
  <snmp asn-id=""/>
  </ui></object>
  <object name="NumOfDhcpMaxHopCountPktsRx">
  <core type="uint" access="read-only" descr="Number of DHCP packets received with maximum hop count on the switch."/>
  <ui name="No Of DHCP Max HopCount Pkts Rx" style="plainText">
  <snmp asn-id=""/>
  </ui></object>
  <object name="NumOfDhcpTooEarlyPktsRx">
  <core type="uint" access="read-only" descr="Number of DHCP packets received with secs field set to value less than minimum allowed value."/>
