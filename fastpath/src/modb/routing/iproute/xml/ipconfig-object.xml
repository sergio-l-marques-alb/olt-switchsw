 <object name="preferenceLevel" depends="IfIndex" >
 <core  type="int" min="L7_RTR_DISC_PREFERENCE_MIN" max="L7_RTR_DISC_PREFERENCE_MAX" default="FD_RTR_DEFAULT_PREFERENCE_LEVEL" access="read-write" descr="Preferability of the address as a default router address, related to other addresses on the same subnet. This is defined as the larger the number, the higher the preference."/>
 <ui  name="Preference Level" style="inputLine">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.4.1.6"/> 
 </ui></object>
 <object name="advertisementAddress" depends="IfIndex" >
 <core  type="user" xtype="formatipv4_t" default="FD_RTR_DEFAULT_ADV_ADDR" access="read-write" descr="Address used when sending router advertisements from the interface."/>
 <ui  name="Advertisement Address" style="inputLine">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.4.1.7"/> 
 </ui></object>
</module><!-- END: IpRouterDiscovery Module -->

