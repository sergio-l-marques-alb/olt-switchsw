<!-- BEGIN: switchIPInterfaceConfig Module -->
<module name="switchIPInterfaceConfig" purpose="To Configure Routing Parameters on an Interface" >
 <object name="IfIndex" compile_if="IF_STACKING_PACKAGE_PRESENT" >
 <core  type="user" xtype="usp_t" key="yes" access="read-write" descr="The IfIndex associated with this instance."/>
 <ui  name="Unit/Slot/Port" style="inputLine">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.1"/> 
 </ui></object>
 <object name="IfIndex" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT" >
 <core  type="user" xtype="usp_t" key="yes" access="read-write" descr="The IfIndex associated with this instance."/>
 <ui  name="Slot/Port" style="inputLine">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.1"/> 
 </ui></object>
 <object name="ipAddress" depends="IfIndex" >
 <core  type="ipv4" access="read-write" descr="The IpAddress assigned to this interface. When setting this value, the value of agentSwitchIpInterfaceNetMask must be set at the same time."/>
 <ui  name="IP Address" style="inputLine">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.2"/> 
 </ui></object>
 <object name="netMask" depends="IfIndex" >
 <core  type="ipv4" access="read-write" descr="The NetMask assigned to this interface. When setting this value, the value of agentSwitchIpInterfaceIpAddress must be set at the same time."/>
 <ui  name="Subnet Mask" style="inputLine">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.3"/> 
 </ui></object>
 <object name="clearIp" depends="IfIndex" >
 <core  type="enum" xtype="enable_t" access="write-only" descr="Sets the interfaces IpAddress and NetMask back to 0.0.0.0"/>
 <ui  name="Remove Primary IP" style="choiceNselect1">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.4"/> 
 </ui></object>
 <object name="routingMode" depends="IfIndex" >
 <core  type="enum" xtype="enable_t" default="L7_DISABLE" access="read-write" descr="Enables or disables routing for this interface."/>
 <ui  name="Routing Mode" style="choiceNselect1">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.5"/> 
 </ui></object>
 <object name="proxyARPMode" depends="IfIndex" >
 <core  type="enum" xtype="enable_t" default="FD_PROXY_ARP_ADMIN_MODE" access="read-write" descr="Enables or disables Proxy ARP for this interface."/>
 <ui  name="Proxy ARP Mode" style="choiceNselect1">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.6"/> 
 </ui></object>
 <object name="mtuValue" depends="IfIndex" >
<core  type="uint" access="read-write" descr="Configures the MTU value for this interface. If the value returned is 0 this interface is not enabled for routing. It is not valid to set this value to 0 if routing is enabled."/>
 <ui  name="IP MTU" style="inputLine">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.7"/> 
 </ui></object>
 <object name="bandwidth" depends="IfIndex" >
 <core  type="uint" min="L7_L3_MIN_BW" max="L7_L3_MAX_BW" default="FD_IP_DEFAULT_BW" access="read-write" descr="Configures the Bandwidth value for this interface. If the value returned is 0 bandwidth for this interface is not Configured."/>
 <ui  name="Bandwidth" style="inputLine">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.8"/> 
 </ui></object>
 <object name="unnumberedIfIndex" depends="IfIndex" >
 <core  type="enum" xtype="enable_t" access="read-write" descr="If this object is non-zero, it indicates that the interface is unnumbered, and specifies which interface the address is borrowed from."/>
 <ui  name="Unnumbered Index" style="choiceNselect1">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.9"/> 
 </ui></object>
 <object name="IcmpUnreachables" depends="IfIndex" >
 <core  type="enum" xtype="enable_t" access="read-write" descr="If this object is enabled, it indicates that ICMP unreachables can be sent on this interface."/>
 <ui  name="ICMP Unreachables" style="choiceNselect1">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.10"/> 
 </ui></object>
 <object name="IcmpRedirects" depends="IfIndex" >
 <core  type="enum" xtype="enable_t" access="read-write" descr="If this object is enabled and agentSwitchIpIcmpRedirectsMode is also enabled, ICMP Redirects can be sent on this interface."/>
 <ui  name="ICMP Redirects" style="choiceNselect1">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.3.1.11"/> 
 </ui></object>
 <object name="netDirectBroadcasts" depends="IfIndex" >
 <core  type="enum" xtype="enable_t" default="FD_RTR_DEFAULT_FWD_NET_DIRECT_BCAST" access="read-write" descr="Configures Ip regarding forwarding net directed broadcasts"/>
 <ui  name="Net Direct Broadcasts" style="choiceNselect1" />
 </object>
 <object name="irdpHoldTime" depends="IfIndex" >
 <core  type="uint" max="L7_RTR_DISC_LIFETIME_MAX" access="read-write" descr="Sets the value of lifetime field of router advertisement sent from the interface"/>
 <ui  name="HoldTime" style="inputLine" />
 </object>
 <object name="IsPrimaryConfigured" depends="IfIndex">
 <core validation="validate_uint" descr="&lt;HTML&gt;Returns True if Primary Address is Configured" xtype="L7_INTF_IP_ADDR_METHOD_t" access="read-only" type="enum"/>
 <ui style="choiceNselect1" name="Is Primary Configured">
 </ui>
 </object>
 <object name="IPAddressConfigMethod">
 <core type="enum" xtype="L7_INTF_IP_ADDR_METHOD_t" default="L7_INTF_IP_ADDR_METHOD_CONFIG" access="read-write" descr="The source of the IP Address"/>
 <ui name="IP Address Configuration Method" style="inputLine"/>
 </object>

 <object name="renewDHCP">
 <core type="uint" default="0" access="write-only" descr="Renew the IP Address learned through DHCP"/>
 <ui name="Renew DHCP IP Address" style="inputLine"/>
 </object>

 <object name="releaseDHCP">
 <core type="uint" default="0" access="write-only" descr="Release the IP Address learned through DHCP"/>
 <ui name="Release DHCP IP Address" style="inputLine"/>
 </object>
</module><!-- END: switchIPInterfaceConfig Module -->

<!-- BEGIN: IpRouterDiscovery Module -->
