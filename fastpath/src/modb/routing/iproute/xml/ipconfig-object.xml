 <ui  name="Unit/Slot/Port" style="inputLine">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.4.1.1"/> 
 </ui></object>
 <object name="IfIndex" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT" >
 <core  type="user" xtype="usp_t" key="yes" access="read-write" descr="Interface Number to configure Router Discovery on."/>
 <ui  name="Slot/Port" style="inputLine">
 <snmp asn-id=".1.3.6.1.4.1.4413.1.1.2.2.4.1.1"/> 
 </ui></object>
 <object name="advertiseMode" depends="IfIndex" >
