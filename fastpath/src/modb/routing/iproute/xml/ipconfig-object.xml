 <object name="advertisementLifetime" depends="IfIndex" >
 <core  type="uint" max="L7_RTR_DISC_LIFETIME_MAX" default="FD_RTR_DEFAULT_ADV_LIFETIME" access="read-write" descr="Value of lifetime field of router advertsisement sent from the interface. This value must be greater than or equal to agentSwitchIpRouterDiscoveryMaxAdvertisementInterval."/>
