<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM "object.dtd">
<FastPathXML>
<objectxml name="dhcp4Client" version="1.0" >
<!-- BEGIN:dhcp4ClientLeaseParameters module -->
<module name="LeaseParameters" purpose="DHCP Client Lease Information" >

  <object name="dhcp4ClientInterfaceIndex" compile_if="IF_STACKING_PACKAGE_PRESENT" >
  <core  type="user" xtype="usp_t" key="yes" access="read-write" descr="The Interface index on which the IP address was leased by the DHCP Server."/>
  <ui  name="Interface" style="plainText">
  </ui></object>

  <object name="dhcp4ClientInterfaceIndex" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT" >
  <core  type="user" xtype="usp_t" key="yes" access="read-write" descr="The Interface index on which the IP address was leased by the DHCP Server."/>
  <ui  name="Interface" style="plainText">
  </ui></object>

  <object name="dhcp4ClientIpAddress" depends="dhcp4ClientInterfaceIndex" >
  <core  type="ipv4" key="no" access="read-only" descr="The IP Address leased by the DHCP Server."/>
  <ui  name="IP Address" style="plainText">
  </ui></object>

  <object name="dhcp4ClientSubnetMask" depends="dhcp4ClientInterfaceIndex"  >
  <core  type="ipv4" key="no" access="read-only" descr="The Subnet mask of the IP Address leased by the DHCP Server."/>
  <ui  name="Subnet Mask" style="plainText">
  </ui></object>

  <object name="dhcp4ClientDhcpServerAddress" depends="dhcp4ClientInterfaceIndex"  >
  <core  type="ipv4" key="no" access="read-only" descr="The IP Address of the DHCP Server that leased the IP Address."/>
  <ui  name="DHCP Server Address" style="plainText">
  </ui></object>

  <object name="dhcp4ClientState" depends="dhcp4ClientInterfaceIndex" >
  <core  type="enum" xtype="L7_DHCP_CLIENT_STATE_t" key="no" access="read-only" descr="The State of the DHCP Client on this interface."/>
  <ui  name="Client State" style="plainText">
  </ui></object>

  <object name="dhcp4ClientTransactionID" depends="dhcp4ClientInterfaceIndex" >
  <core  type="user" xtype="hex_uint_t" key="no" access="read-only" descr="The transaction ID of the DHCP Client."/>
  <ui  name="Transaction ID" style="plainText">
  </ui></object>

  <object name="dhcp4ClientLeaseTime" depends="dhcp4ClientInterfaceIndex" >
  <core  type="user" xtype="elapsedTime_t" key="no" access="read-only" descr="The time (in seconds) that the IP address was leased by the DHCP Server."/>
  <ui  name="Lease Time" style="plainText">
  </ui></object>

  <object name="dhcp4ClientRenewTime" depends="dhcp4ClientInterfaceIndex" >
  <core  type="user" xtype="elapsedTime_t" key="no" access="read-only" descr="The time (in seconds) when the next DHCP Renew Request is sent by DHCP Client to renew the leased IP address."/>
  <ui  name="Renewal Time" style="plainText">
  </ui></object>

  <object name="dhcp4ClientRebindTime" depends="dhcp4ClientInterfaceIndex" >
  <core  type="user" xtype="elapsedTime_t" key="no" access="read-only" descr="The time (in seconds) when the DHCP Rebind process starts."/>
  <ui  name="Rebind Time" style="plainText">
  </ui></object>

  <object name="dhcp4ClientRetryCount" depends="dhcp4ClientInterfaceIndex" >
  <core  type="uint" key="no" access="read-only" descr="The number of times the DHCP Client sends a DHCP REQUEST message before the server responds."/>
  <ui  name="Retry Count" style="plainText">
  </ui></object>

  <object name="dhcp4ClientConfigured" depends="dhcp4ClientInterfaceIndex" >
  <core  type="enum" xtype="True1False2_t" access="read-only" descr="Returns True if client is configured."/>
  <ui  name="Client Configured" style="choiceNselect1">
  </ui></object> 

</module> <!-- END:dhcp4ClientLeaseParameters module -->
</objectxml>
</FastPathXML>
