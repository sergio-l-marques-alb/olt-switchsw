<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM "object.dtd">
<FastPathXML>
<objectxml name="routing" version="1.0" >
<module name="ospfVirtNbr" >
<key-combo list="Area,RtrId"/>
<object name="Area"  >
<core  type="user" xtype="ipv4_uint" key="yes" access="read-write" descr="The Transit Area Identifier."/>
<ui  name="Area Id" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.14.11.1.1"/> 
</ui></object>
<object name="RtrId"  >
<core  type="user" xtype="formatipv4_t" key="yes" access="read-write" descr="A 32-bit integer uniquely identifying the neighboring router in the Autonomous System."/>
<ui  name="Router Id" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.14.11.1.2"/> 
</ui></object>
<object name="IpAddr" depends="Area,RtrId" >
<core  type="ipv4" access="read-only" descr="The IP address this Virtual Neighbor is using."/>
<ui  name="IP Address" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.14.11.1.3"/> 
</ui></object>
<object name="Options" depends="Area,RtrId" >
<core  type="uint" access="read-only" descr="A Bit Mask corresponding to the neighbor's options field. Bit 1, if set, indicates that the system will operate on Type of Service metrics other than TOS 0. If zero, the neighbor will ignore all metrics except the TOS 0 metric. Bit 2, if set, indicates that the system is Network Multicast capable; ie, that it implements OSPF Multicast Routing."/>
<ui  name="Options" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.14.11.1.4"/> 
</ui></object>
<object name="State" depends="Area,RtrId" >
<core  type="enum" xtype="XUI_L7_OSPF_STATE_t" access="read-only" descr="The state of the Virtual Neighbor Relationship."/>
<ui  name="State" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.14.11.1.5"/> 
</ui></object>
<object name="Events" depends="Area,RtrId" >
<core  type="uint" access="read-only" descr="The number of times this virtual link has changed its state, or an error has occurred."/>
<ui  name="Events" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.14.11.1.6"/> 
</ui></object>
<object name="LsRetransQLen" depends="Area,RtrId" >
<core  type="uint" access="read-only" descr="The current length of the LSA retransmission queue."/>
<ui  name="LSA Retransmission Queue Length" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.14.11.1.7"/> 
</ui></object>
<object name="HelloSuppressed" depends="Area,RtrId" >
<core  type="uint" access="read-only" descr="Indicates whether Hellos are being suppressed to the neighbor"/>
<ui  name="Hellos Suppressed" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.14.11.1.8"/> 
</ui></object>
</module>
</objectxml>
</FastPathXML>
