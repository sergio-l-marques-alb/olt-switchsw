<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM "object.dtd">
<FastPathXML>
<objectxml name="routing" version="1.0" >
<module name="ospfHost">
<key-combo list="IpAddress,TOS"/>
<object name="IpAddress"  >
<core  type="ipv4" key="yes" access="read-write" descr="The IP Address of the Host."/>
<ui  name="IP Address" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.14.6.1.1"/> 
</ui></object>
<object name="TOS"  >
<core  type="uint" key="yes" access="read-write" descr="The Type of Service of the route being configured."/>
<ui  name="Type Of Service" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.14.6.1.2"/> 
</ui></object>
<object name="Metric" depends="IpAddress,TOS" >
<core  type="uint" access="read-write" descr="The Metric to be advertised."/>
<ui  name="Metric" style="inputLine">
 <snmp asn-id=".1.3.6.1.2.1.14.6.1.3"/> 
</ui></object>
<object name="Status" depends="IpAddress,TOS" >
<core  type="uint" access="read-write" descr="This variable displays the status of the entry. Setting it to 'invalid' has the effect of rendering it inoperative. The internal effect (row removal) is implementation dependent."/>
<ui  name="Status" style="inputLine">
 <snmp asn-id=".1.3.6.1.2.1.14.6.1.4"/> 
</ui></object>
<object name="AreaID" depends="IpAddress,TOS" >
<core  type="ipv4" access="read-only" descr="The Area the Host Entry is to be found within. By default, the area that a subsuming OSPF interface is in, or 0.0.0.0"/>
<ui  name="Area ID" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.14.6.1.5"/> 
</ui></object>
</module>
</objectxml>
</FastPathXML>
