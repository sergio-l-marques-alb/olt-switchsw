                   <action type="enum" enable="2_3_1|2_4_1"/>
                   <action type="enum" disable="2_3_1|2_4_1"/>
                </element>
            </node>
            <node id="3">
                <element control="combo" obj-name="baseUsrMgrUserConfig_UserEncryptType" id="1" name="Encryption Protocol"/>
            </node>
            <node id="4">
                <element control="textbox-passwd" maxLength="64" obj-name="baseUsrMgrUserConfig_UserEncryptPassword" id="1" name="Encryption Key" arghelp="Password" value=""/>
           </node>
		       <node id="6">
                <element control="textbox-passwd" value="" obj-name="baseUsrMgrUserConfig_UserPassword" id="1" name=""/>
