            <node id="4">
                <element control="textbox-passwd" arghelp="Password" value="" maxLength="64" obj-name="xLibObjects_dummy" id="1" name="Password" validation="validate_passwd"/>
            </node>
            <node id="8">
                <element control="textbox-passwd" arghelp="Password" value="" maxLength="64" obj-name="xLibObjects_dummy" id="1" name="Confirm Password" validation="validate_passwd"/>
            </node>
            <node id="5">
                <element control="combo" obj-name="baseUsrMgrUserConfig_UserAccessLevel" id="1" name="Access Level" >
                    <action type="enum"/>
                    <action type="enum" web_name="Read-Only"/>
                    <action type="enum" web_name="Read-Write"/>
                </element>
            </node>
            <node id="6">
                <element control="label" properties="read-only" obj-name="baseUsrMgrUserConfig_UserLockoutStatus" id="1" name="Lockout Status">
                    <action type="enum" web_name="True"/>
