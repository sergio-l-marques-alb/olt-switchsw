        <group id="2" heading="SNMP v3 User Configuration" level="ReadWriteUser">
            <node id="10" compile_if="IF_SNMP_PACKAGE_PRESENT">
                <element obj-name="baseSNMPV3UserSecurity_UserIndex" control="combo" id="1" cpval="baseUsrMgrUserConfig_UserIndex" properties="hidden"/>
             </node> 
            <node id="1" compile_if="IF_SNMP_PACKAGE_PRESENT">
                <element control="combo" obj-name="baseSNMPV3UserSecurity_AccessMode" id="1" name="SNMP v3 Access Mode">
                    <action type="enum" web_name="Read-Only"/>
                    <action type="enum" web_name="Read-Write"/>
                </element>
                <element control="label" obj-name="baseSNMPV3UserSecurity_AccessMode" id="2" name="SNMP v3 Access Mode">
                    <action type="enum" web_name="Read-Only"/>
                    <action type="enum" web_name="Read-Write"/>
                </element>
            </node>
            <node id="2">
                <element control="combo" obj-name="baseUsrMgrUserConfig_UserAuthType" id="1" name="Authentication Protocol">
