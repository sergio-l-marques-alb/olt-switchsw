            <node id="7">
                <element control="label" properties="read-only" obj-name="baseUsrMgrUserConfig_UserPasswordExpireTime" id="1" name="Password Expiration Date"/>
            </node>
            <node id="9">
                <element control="textbox-passwd" value="" obj-name="baseUsrMgrUserConfig_UserPassword" id="1" name=""/>
            </node>
            <node id="10">
                <element control="label" properties="read-only,hidden" obj-name="baseUsrMgrGlobal_PasswordManagementMinLength" id="1" name=""/>
            </node>
            <node id="11">
                 <element control="checkbox" uienum="TrueFalse_t" properties="noname,hidden" obj-name="xLibObjects_dummy" id="1" name="" value="False" />
            </node>
