srcdir = .
LIB     = $(srcdir)/..
ROOT    = $(LIB)/..

CONFIG	= CONFIG_MAKE_VARIABLE_MUST_BE_SET

include	$(ROOT)/config.$(CONFIG)/config.mak

OBJECTS     += ew_md5c
SOURCES     += ew_md5c.c
SOURCES	    += md5.c
SOURCES	    += digest.c

TARGETA = $(OD)/ew_md5.a
LIBS = $(TARGETA)

default: _objdir md5_lib

md5_lib: $(TARGETA)

digest: _objdir $(BD)/digest

all: digest $(BD)/md5

$(BD)/md5: $(OD)/md5.o $(LIBS)
	$(CC) -o $@ $(OD)/md5.o \
		$(LDINITFLAGS) \
		$(LDFLAGS) \
		$(LIBS) $(XLIBS) $(CLIBS)

$(BD)/digest: $(OD)/digest.o $(LIBS)
	$(CC) -o $@ $(OD)/digest.o \
		$(LDINITFLAGS) \
		$(LDFLAGS) \
		$(LIBS) $(XLIBS) $(CLIBS)

include	$(LIB)/rules.mak

include depend.mak
