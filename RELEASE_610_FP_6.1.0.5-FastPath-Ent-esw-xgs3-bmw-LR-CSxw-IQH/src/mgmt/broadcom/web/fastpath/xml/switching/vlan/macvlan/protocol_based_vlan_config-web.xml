<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" java-script="range_type" name="protocol_based_vlan_config" title="Protocol-based VLAN Configuration ">
        <group id="1" level="ReadWriteUser" heading=" ">
            <node id="1">
                <element control="combo" properties="filter,iterate" obj-name="SwitchingPbvlanGroupConfig_GroupId" arghelp="HELP_FORMAT_GROUP128" id="1" name="Group ID">
                    <action type="default" enable="1_3_1|1_5_1|1_6_1|1_7_1" disable="1_2_1|1_4_1|1_8_1" hide="1_2_1|1_8_1" show="1_3_1|1_4_1|1_5_1|1_6_1|1_7_1"/>
                    <action type="mandate" disable="1_3_1" invoke="onChange" />
                    <action type="extn" value="Create New Group" enable="1_2_1|1_8_1" disable="1_3_1|1_5_1|1_6_1|1_7_1|1_9_1|1_9_2" hide="1_3_1|1_4_1|1_5_1|1_6_1|1_7_1|1_9_1|1_9_2" show="1_2_1|1_8_1"/>
                </element>
            </node>
            <node id="2">
                <element control="textbox" maxLength="16" value="" obj-name="SwitchingPbvlanGroupConfig_GroupName" arghelp="HELP_FORMAT_CHAR" id="1" name="Group Name"/>
            </node>
            <node id="3">
                <element control="label"  obj-name="SwitchingPbvlanGroupConfig_GroupName" id="1" name="Group Name"/>
            </node>

	    <node id="4">
	        <element control="label" properties="filter,iterate" obj-name="SwitchingPbvlanGroupConfig_GroupId" id="1" name="Group ID"/>
	    </node>
  
            <node id="5">
                <element control="combo-multiselect" properties="edit,collate,list" obj-name="SwitchingPbvlanGroupConfig_GroupProtocol" id="1" name="Protocols"/>
	    </node>
	 
            <node id="6">
                <element arghelp="HELP_FORMAT_FP"  maxLength="10" obj-name="SwitchingPbvlanGroupConfig_GroupVlanId" id="1" name="VLAN"/>
            </node>
            <node id="7">
                <element control="combo-multiselect" properties="edit,collate,list" obj-name="SwitchingPbvlanGroupConfig_GroupInterface" id="1" name="Unit/Slot/Port(s)" compile_if="IF_STACKING_PACKAGE_PRESENT"/>
            <element control="combo-multiselect" properties="edit,collate,list" obj-name="SwitchingPbvlanGroupConfig_GroupInterface" id="1" name="Slot/Port(s)" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT"/>
            </node>
            <node id="8">
                <button scope="page" type="buttonSubmit" value="Add" obj-name="SwitchingPbvlanGroupConfig_GroupStatus" help="Submits the form on this page to the Switch" id="1" name="Submit"/>
		    
	    </node>
	    <node id="9">
	        <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="1" name="Submit">
		        <action type="button" disable="1_9_2"/>
		</button>
		<button scope="page" type="buttonSubmit" value="Delete" obj-name="SwitchingPbvlanGroupConfig_GroupStatus" help="Deletes the entry of respective group" id="2" name="Delete">
		        <action type="button" disable="1_5_1|1_6_1|1_7_1|1_9_1"/>
	        </button>
            </node>
        </group>
   </webxml>
</FastPathXML>
