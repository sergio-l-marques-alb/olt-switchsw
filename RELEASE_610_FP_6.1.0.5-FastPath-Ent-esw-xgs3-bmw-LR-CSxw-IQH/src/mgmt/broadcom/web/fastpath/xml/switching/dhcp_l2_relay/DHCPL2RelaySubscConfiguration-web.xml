            <node id="3">
                <element properties="read-only" obj-name="SwitchingDhcpL2RelaySubscConfig_L2RelayMode" id="1" name="DHCP L2 Relay Mode"/>
            </node>
            <node id="4">
                <element properties="read-only" obj-name="SwitchingDhcpL2RelaySubscConfig_CircuitIdMode" id="1" name="DHCP L2 Relay Circuit-Id"/>
            </node>
            <node id="5">
                <element properties="read-only" obj-name="SwitchingDhcpL2RelaySubscConfig_L2RelayRemoteId" id="1" name="DHCP L2 Relay Remote-Id"/>
            </node>
            <node id="6">
