            </node>
            <node id="2">
                <element control="combo" properties="filter,iterate" obj-name="SwitchingDhcpL2RelaySubscConfig_SubscriptionName" id="1" name="Subscription Name" >
                  <action type="default" show="1_4|1_5|1_6|1_7_1"  hide="1_3" disable="1_3" enable="1_4|1_5|1_6|1_7_1" />
