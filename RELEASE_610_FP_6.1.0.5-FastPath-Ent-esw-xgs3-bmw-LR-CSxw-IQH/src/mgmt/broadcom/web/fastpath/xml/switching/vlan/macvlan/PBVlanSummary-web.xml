                <element control="label" properties="collate" obj-name="SwitchingPbvlanGroupConfig_GroupInterface" id="7" run_if="nostacking" name="Slot/Port(s)" />
            </repeat>
            <node id="2">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>
            </node>
        </group>
    </webxml>
