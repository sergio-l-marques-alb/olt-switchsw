<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
  <webxml version="1.0" name="class_ethertype" title="DiffServ Class Rule Match Ethertype">
    <group id="1" heading="Ethertype">
      <node id="1">
        <element properties="hidden" obj-name="qosDiffServClassRule_ClassIndex"  id="1" name="ClassIndex" />
