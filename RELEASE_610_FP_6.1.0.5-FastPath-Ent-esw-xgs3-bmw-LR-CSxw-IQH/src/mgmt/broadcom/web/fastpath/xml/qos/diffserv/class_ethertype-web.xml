        <element control="checkbox" uienum="TrueFalse_t" obj-name="xLibObjects_dummy" id="3" name="Exclude" run_if="DIFFSERV_CLASS_MATCH_EXCLUDE_FEATURE">
          <action type="enum" discard="no" setval_args="TRUTH_VALUE_TRUE" setval_ids="1_11_2"/>
          <action type="enum" discard="no" setval_args="TRUTH_VALUE_FALSE" setval_ids="1_11_2"/>
