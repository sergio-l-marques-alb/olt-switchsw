      <node id="5">
        <element obj-name="qosDiffServClassRule_ProtoType" id="1" name="Class Layer 3 Protocol" control="label"/>
      </node>
      <node id="11" >
        <element control="combo" obj-name="qosDiffServClassRule_MatchEntryType" id="3" name="Class Match Selector" properties="hidden,write-only" value="Ethertype" /> 
