          <action type="enum"   discard="no" hide="1_9_1" disable="1_9_1" /><!--apple talk-->
          <action type="enum"  discard="no" hide="1_9_1" disable="1_9_1" /><!--arp-->
          <action type="enum" discard="no" hide="1_9_1" disable="1_9_1" /><!--ibmsna-->
          <action type="enum" discard="no" hide="1_9_1" disable="1_9_1" /><!--ipv4-->
          <action type="enum" discard="no" hide="1_9_1" disable="1_9_1" /><!--ipv6-->
          <action type="enum" discard="no" hide="1_9_1" disable="1_9_1" /><!--ipx-->
          <action type="enum" discard="no" hide="1_9_1" disable="1_9_1" /><!--mpls mcast-->
          <action type="enum" discard="no" hide="1_9_1" disable="1_9_1" /><!--mpls ucast-->
