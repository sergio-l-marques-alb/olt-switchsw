<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="IPACLConfig" java-script="acltable" title="IP ACL Configuration">
        <group level="ReadWriteUser" id="1" heading=" ">
          <node id="1">
              <element properties="read-only,hidden" obj-name="fastPathQOSACLQOSACL_aclIndex" id="1" name="aclID" />
          </node>
            <node id="2">
                <element control="combo" properties="filter" obj-name="fastPathQOSACLQOSACL_aclIPNamedIndex" id="1" name="IP ACL">
                    <action type="extn" value="Create New Named IP ACL" disable="1_7_2|1_7_3|1_4|1_5" hide="1_3_1|1_7_2|1_7_3|1_4|1_5" enable="1_7_1|1_6" show="1_7_1|1_6" setval_args="valMapEmptyString" setval_ids="1_6" position="0"/>
                    <action type="extn" value="Create New Extended IP ACL" disable="1_7_2|1_7_3|1_4|1_6"  hide="1_3_1|1_7_2|1_7_3|1_4|1_6"  enable="1_7_1|1_5" show="1_7_1|1_5" setval_args="valMapEmptyString" setval_ids="1_5" position="0"/>
                    <action type="extn" value="Create New Standard IP ACL" disable="1_7_2|1_7_3|1_5|1_6" hide="1_3_1|1_7_2|1_7_3|1_5|1_6" enable="1_7_1|1_4" show="1_7_1|1_4"  setval_args="valMapEmptyString" setval_ids="1_4" position="0"/>
                    <action type="default" hide="1_3_1|1_7_1" disable="1_7_1"/>
                </element>
            </node>
            <node id="3">
              <element properties="filter" control="combo" obj-name="fastPathQOSACLQOSACL_aclIPNamedType" id="1" name="aclIPType">
                  <action type="enum" show="1_7_3" hide="1_7_2|1_4|1_5|1_6" disable="1_7_2|1_4|1_5|1_6" enable="1_7_3"/>
                  <action type="enum" show="1_7_3" hide="1_7_2|1_4|1_5|1_6" disable="1_7_2|1_4|1_5|1_6" enable="1_7_3"/>
                  <action type="enum" show="1_7_2|1_7_3|1_6" hide="1_4|1_5" disable="1_4|1_5"/>
                  <action type="enum" show="" hide="" />
             </element>
           </node>
            <node id="4">
                <element control="textbox"  obj-name="fastPathQOSACLQOSACL_aclIPNamedIndex" id="1" name="IP ACL ID" maxLength="10" arghelp= "HELP_FORMAT_STD_IP_ACL_ID" validation="standardIpAclIdRangeValidation"/>
            </node>
            <node id="5">
                <element control="textbox"  obj-name="fastPathQOSACLQOSACL_aclIPNamedIndex" id="1" name="IP ACL ID" maxLength="10" arghelp= "HELP_FORMAT_EXT_IP_ACL_ID" validation="extendedIpAclIdRangeValidation"/>
            </node>
            <node id="6">
                <element maxLength="32" control="textbox"  obj-name="fastPathQOSACLQOSACL_aclIPNamedIndex" id="1" name="IP ACL Name" arghelp= "HELP_FORMAT_NAME31" validation="validate_name31"/>
            </node>
            <node id="7">
                <button scope="group" clazz="ipAclConfigButton" type="buttonSubmit" level="ReadWriteUser" value="Add" obj-name="fastPathQOSACLQOSACL_aclStatus" help="Submits the form on this page to the Switch" id="1" name="Submit"/>
                <button scope="group" clazz="ipAclConfigButton" type="buttonSubmit" level="ReadWriteUser" help="Submits the form on this page to the Switch" id="2" name="Rename"/>
                 <action type="button" disable="1_7_3" enable="1_7_2" show="1_7_2" hide="1_7_3" />
                <button scope="group" clazz="ipAclConfigButton" type="buttonSubmit" level="ReadWriteUser" value="Delete" obj-name="fastPathQOSACLQOSACL_aclStatus" help="Submits the form on this page to the Switch" id="3" name="Delete"/>
                 <action type="button" disable="1_7_2" enable="1_7_3" show="1_7_3" hide="1_7_2" />
            </node>           
        </group>

        <group id="2" div-name="summary" heading=" ">
           <node id="1">
              <element properties="hidden" obj-name="fastPathQOSACLQOSACLTABLE_aclsConfigCurrent" id="1" />
              <element properties="hidden" obj-name="fastPathQOSACLQOSACLTABLE_aclMaxNum" id="2" />
           </node>
           <node id="2">
              <element control="label" clazz="ipAclConfig" value="Table" properties="noname,write-only" obj-name="xLibObjects_dummy" id="1" name="Table"/>
              <element control="label" clazz="ipAclConfig" value="Current Size / Max Size" properties="noname,write-only" obj-name="xLibObjects_dummy" id="2" name="Current Size / Max Size"/>
           </node>
           <node id="3">
              <element control="label" value="ACL" properties="noname,write-only" obj-name="xLibObjects_dummy" id="1" name="ACL"/>
              <element control="label" properties="noname" obj-name="xLibObjects_dummy" id="2"/>
           </node>
        </group>
    </webxml>
</FastPathXML>
