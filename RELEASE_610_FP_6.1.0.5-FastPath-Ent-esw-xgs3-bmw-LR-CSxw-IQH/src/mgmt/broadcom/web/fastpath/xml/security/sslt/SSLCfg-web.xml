<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="SSLCfg" title="Secure HTTP Configuration">
        <group id="1" heading=" " level="ReadWriteUser">
            <node id="1">
                <element control="combo" obj-name="securitySSLConfig_AdminMode" id="1" name="HTTPS Admin Mode">
                    <action type="enum" discard="no" hide="1_8_3|1_9_2|1_10_1" setval_args="L7_ENABLE" setval_ids="1_10_1"/>
                    <action type="enum" discard="no" hide="1_8_3|1_9_2|1_10_1" setval_args="L7_DISABLE" setval_ids="1_10_1"/>
                </element>
            </node>
            <node id="10">
                <element control="combo" uienum="enable_t" obj-name="xLibObjects_dummy" id="1" name="Enable/Disable Items">
                    <action type="enum" hide="2_1_1|2_1_2" disable="2_1_1|2_1_2" invoke="onLoad"/>
                    <action type="enum" show="2_1_1|2_1_2" enable="2_1_1|2_1_2" invoke="onLoad"/>
                </element>
            </node>
            <node id="2">
                <element obj-name="securitySSLConfig_TLSVersion" id="1" name="TLS Version 1"/>
            </node>
            <node id="3">
                <element obj-name="securitySSLConfig_SSLVersion" id="1" name="SSL Version 3"/>
            </node>
            <node id="4">
                <element maxLength="10" arghelp="HELP_FORMAT_FP" obj-name="securitySSLConfig_SecurePort" id="1" name="HTTPS Port"/>
            </node>
            <node id="5">
                <element maxLength="10" arghelp="HELP_FORMAT_FP" obj-name="securitySSLConfig_SoftTimeout" id="1" name="HTTPS Session Soft Timeout (Minutes)"/>
            </node>
            <node id="6">
                <element maxLength="10" arghelp="HELP_FORMAT_FP" obj-name="securitySSLConfig_HardTimeout" id="1" name="HTTPS Session Hard Timeout (Hours)"/>
            </node>
            <node id="7">
                <element maxLength="10" arghelp="HELP_FORMAT_FP" obj-name="securitySSLConfig_MaxSessions" id="1" name="Maximum Number of HTTPS Sessions"/>
            </node>
            <node id="8">
                <element obj-name="securitySSLConfig_CertificatePresent" id="1" name="Certificate Present? "/>
                <button scope="page" type="buttonSubmit" value="Delete" obj-name="securitySSLConfig_CertificateControl" help="Submits the form on this page to the Switch" id="2" name="Delete">
                    <action type="button" disable="2_1_1|2_1_2|2_1_3"/>
                </button>
                <element control="combo" obj-name="securitySSLConfig_CertificatePresent" id="3" name="CertificatePresent">
                    <action type="enum" discard="no" show="1_8_2"/>
                    <action type="enum" discard="no" disable="1_8_2" hide="1_8_2"/>
                </element>
            </node>
            <node id="9">
                <element obj-name="securitySSLConfig_CertificateGenerationStatus" id="1" name="Certificate Generation Status "/>
                <element control="combo" obj-name="securitySSLConfig_CertificateGenerationStatus" id="2" name="CertificateGenerationStatus">
                    <action type="enum" hide="2_1_1|2_1_2" show="2_1_3"/>
                    <action type="enum" show="2_1_3"/>
                </element>
            </node>
        </group>
        <group id="2" heading=" " level="ReadWriteUser">
            <node id="1">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="4" name="Refresh"/>
                <button scope="page" type="buttonSubmit" target-page="DownloadFile.html" help="Submits the form on this page to the Switch" id="1" name="Download Certificates">
                    <action type="button" disable="2_1_3|2_1_2|1_8_2"/>
                </button>
                <button scope="page" type="buttonSubmit" value="Generate" obj-name="securitySSLConfig_CertificateControl" help="Submits the form on this page to the Switch" id="2" name="Generate Certificate">
                    <action type="button" disable="1_8_2|2_1_1|2_1_3"/>
                 </button>
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="3" name="Submit">
                    <action type="button" disable="2_1_1|2_1_2|1_8_2"/>
                </button>
            </node>
        </group>
         <group id="3" heading=" " level="ReadOnlyUser">
            <node id="1">
                <element control="label" properties="read-only" obj-name="securitySSLConfig_AdminMode" id="1" name="HTTPS Admin Mode" />
            </node>
            <node id="2">
                <element control="label" properties="read-only" obj-name="securitySSLConfig_TLSVersion" id="1" name="TLS Version 1"/>
            </node>
            <node id="3">
                <element control="label" properties="read-only" obj-name="securitySSLConfig_SSLVersion" id="1" name="SSL Version 3"/>
            </node>
            <node id="4">
                <element control="label" properties="read-only" obj-name="securitySSLConfig_SecurePort" id="1" name="HTTPS Port"/>
            </node>
            <node id="5">
                <element  control="label" properties="read-only" obj-name="securitySSLConfig_SoftTimeout" id="1" name="HTTPS Session Soft Timeout (Minutes)"/>
            </node>
            <node id="6">
                <element  control="label" properties="read-only" obj-name="securitySSLConfig_HardTimeout" id="1" name="HTTPS Session Hard Timeout (Hours)"/>
            </node>
            <node id="7">
                <element  control="label" properties="read-only" obj-name="securitySSLConfig_MaxSessions" id="1" name="Maximum Number of HTTPS Sessions"/>
            </node>
			<node id="8">
                <element control="label" obj-name="securitySSLConfig_CertificatePresent" id="1" name="Certificate Present? "/>
                
            </node>
            <node id="9">
                <element control="label" obj-name="securitySSLConfig_CertificateGenerationStatus" id="1" name="Certificate Generation Status "/>
                
            </node>
            <node id="10">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>
            </node>
			
        </group>
    </webxml>
</FastPathXML>
