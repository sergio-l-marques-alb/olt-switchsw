<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="DVLANConfig" title="DVLAN Configuration">
        <group level="ReadWriteUser" id="1" heading=" ">
            <node id="1">
                <element run_if="DVLANTAG_MULTI_TPID_FEATURE" control="checkbox" uienum="TrueFalse_t" obj-name="xLibObjects_dummy" id="1" name="Configure Default TPID">
                    <action type="enum" discard="no" enable="1_7_1"  hide="1_7_1|1_2"/>
                    <action type="enum" discard="no" disable="1_7_1" hide="1_7_1|1_2"/>
                </element>

            </node>
            <node id="2">
                <element run_if="DVLANTAG_MULTI_TPID_FEATURE" control="label" obj-name="xLibObjects_dummy" id="1">
                    <action type="mandate" invoke="onLoad" setval_ids="1_1_1" setval_args="L7_FALSE"/>
                </element>
            </node>
            <node id="4">
               <element run_if="DVLANTAG_ETHERTYPE_FEATURE" control="combo" uienum="dvlanEthertype_t" obj-name="xLibObjects_dummy" id="1" name="Global EtherType">
	                   <action type="enum" hide="1_5_1" setval_ids="1_5_1" setval_args="DVLAN_Ethertype_802Q"/>
                     <action type="enum" hide="1_5_1" setval_ids="1_5_1" setval_args="DVLAN_Ethertype_vman"/>
                     <action type="enum" show="1_5_1" setval_ids="1_5_1" setval_args="valMapEmptyString"/> 
               </element>
            </node>   
            <node id="5">
                <element control="textbox" arghelp="HELP_FORMAT_ETHERTYPE" obj-name="baseDVLANTunneling_GlobalEtherType" id="1" run_if="DVLANTAG_ETHERTYPE_FEATURE" name="Custom Value"/>
            </node>   
            <node id="7">
                <element control="textbox" arghelp="HELP_FORMAT_ETHERTYPE" obj-name="baseDVLANTunneling_GlobalDefaultTpid" id="1" run_if="DVLANTAG_MULTI_TPID_FEATURE" name="Default TPID"/>
            </node>   
            <node id="8">
                <button scope="page" type="buttonSubmit" value="Add" obj-name="baseDVLANTunneling_GlobalDVlanTagRowStatus" help="Submits the form on this page to the Switch" id="1" name="Submit">
                   <action type="button" copy_from="1_5_1" copy_to="1_7_1"/>
                </button>
                <button scope="page" type="buttonSubmit" value="Delete" obj-name="baseDVLANTunneling_GlobalDVlanTagRowStatus" help="Submits the form on this page to the Switch" id="2" name="Remove">
                    <action type="button" disable="1_7_1"/>
                </button>
            </node>
        </group>
    </webxml>
</FastPathXML>

