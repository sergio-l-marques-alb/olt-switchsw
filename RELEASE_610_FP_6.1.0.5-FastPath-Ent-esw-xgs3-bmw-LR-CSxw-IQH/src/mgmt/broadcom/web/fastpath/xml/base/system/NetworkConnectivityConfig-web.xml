                    <action type="enum" enable="2_7_1|3_3_1" show="2_7_1|3_3_1" disable="2_5_1|2_6_1|3_1_1|3_2_1" hide="2_5_1|2_6_1|3_1_1|3_2_1"/>
                </element>
            </node>
            <node id="5">
                <element control="textbox" obj-name="basesysNetworkIPv6Mgmt_sysIPV6AddressPrefix" id="1" name="New IPv6 Address" maxLength="39"/>
            </node>
            <node id="6">
                <element control="combo" value="FALSE" obj-name="basesysNetworkIPv6Mgmt_sysIPV6AddressEUIFlag" id="1" name="EUI Flag"/>
            </node>
            <node id="7">
                <element control="textbox" obj-name="basesysNetworkIPv6Mgmt_sysIPV6AddressPrefix" id="1" name="IPv6 Address to Delete"/>
            </node>
            <node id="2">
                <element control="checkbox" uienum="disable_t" obj-name="xLibObjects_dummy" id="1" name="Change IPv6 Gateway">
                    <action type="enum" enable="2_3_1"/>
                    <action type="enum" disable="2_3_1"/>
                </element>
            </node>
            <node id="3">
                <element control="textbox" obj-name="basesysNetworkIPv6Mgmt_sysIPV6GateWay" id="1" name="IPv6 Gateway" maxLength="39"/>
            </node>
            <repeat present="vtable" edit="no" heading=" " id="8">
                <element properties="filter,iterate,hidden" obj-name="basesysNetworkIPv6Mgmt_sysIPV6AddressPrefixIndex" id="1" name="IPv6 Address Index"/>
                <element properties="read-only" obj-name="basesysNetworkIPv6Mgmt_sysIPV6AddressPrefix" id="2" name="IPv6 Addresses"/>
            </repeat>
            <repeat present="vtable" edit="no" heading=" " id="9">
                <element properties="hidden,iterate" obj-name="basesysNetworkIPv6Mgmt_sysIPV6DefaultRouterIndex" id="1" name="Router Index"/>
                <element properties="read-only" obj-name="basesysNetworkIPv6Mgmt_sysIPV6DefaultRouter" id="2" name="Default IPv6 Routers"/>
            </repeat>
        </group>
        <group id="3" level="ReadWriteUser" heading=" ">
            <node id="1">
                <button scope="page" type="buttonSubmit" help="Submits the form on this page to the Switch" id="1" name="Submit"/>
            </node>
            <node id="2" compile_if="IF_IPV6_MGMT_PACKAGE_PRESENT">
                <button scope="page" type="buttonSubmit" value="Add" obj-name="basesysNetworkIPv6Mgmt_sysIPV6AddressPrefixRowStatus" help="Submits the form on this page to the Switch" id="1" name="Submit"/>
            </node>
            <node id="3" compile_if="IF_IPV6_MGMT_PACKAGE_PRESENT">
                <button scope="page" type="buttonSubmit" value="Delete" obj-name="basesysNetworkIPv6Mgmt_sysIPV6AddressPrefixRowStatus" help="Submits the form on this page to the Switch" id="1" name="Submit"/>
            </node>
        </group>
        <group id="4" level="ReadOnlyUser" heading=" ">
