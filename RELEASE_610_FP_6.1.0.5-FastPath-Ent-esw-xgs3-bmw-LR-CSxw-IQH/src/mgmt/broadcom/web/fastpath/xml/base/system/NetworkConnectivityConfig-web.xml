<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="NetworkConnectivityConfig" title="Network Connectivity Configuration">
        <group id="1" level="ReadWriteUser" heading="IPv4">
            <node id="1">
                <element control="combo" obj-name="baseSystemInfo_sysAgentBasicConfigNetworkConfigProtocol" id="1" name="Network Configuration Protocol">
                    <action type="enum" enable="1_2_1|1_3_1|1_4_1"/>
                    <action type="enum" disable="1_2_1|1_3_1|1_4_1"/>
                    <action type="enum" disable="1_2_1|1_3_1|1_4_1"/>
                </element>
            </node>
            <node id="2">
                <element obj-name="baseSystemInfo_sysIpAddr" id="1" name="IP Address" maxLength="15"/>
            </node>
            <node id="3">
                <element obj-name="baseSystemInfo_sysIpNetMask" id="1" name="Subnet Mask" maxLength="15"/>
            </node>
            <node id="4">
                <element obj-name="baseSystemInfo_sysAgentIpIfDefaultRouter" id="1" name="Default Gateway" maxLength="15"/>
            </node>
            <node id="5">
                <element obj-name="basesysHardwareInfo_sysInventoryBurnedInMacAddr" id="1" name="Burned In MAC Address"/>
            </node>
            <node id="6">
                <element obj-name="basesysHardwareInfo_sysLocalAdminMacAddr" id="1" name="Locally Administered MAC Address" run_if="SIM_ALLOW_LOCAL_MAC_USAGE_FEATURE" maxLength="17"/>
            </node>
            <node id="7">
                <element control="combo" obj-name="basesysHardwareInfo_sysMacAddrType" id="1" name="MAC Address Type" run_if="SIM_ALLOW_LOCAL_MAC_USAGE_FEATURE">
                  <action type="enum" disable="1_6_1"/>
                  <action type="enum" disable="1_6_1"/>
                  <action type="enum" enable="1_6_1" />
                </element>  
            </node>
            <node id="8">
                <element obj-name="baseSystemInfo_sysMgmtVlanId" id="1" name="Management VLAN ID" maxLength="10"/>
            </node>
            <node id="9">
                <element control="combo" obj-name="baseSysWebConnection_sysWebMgmtMode" id="1" name="Web Mode"/>
            </node>
            <node id="10">
                <element control="combo" obj-name="baseSysWebConnection_sysWebJavaMode" id="1" name="Java Mode"/>
            </node>
        </group>
        <group id="2" level="ReadWriteUser" heading="IPv6" compile_if="IF_IPV6_MGMT_PACKAGE_PRESENT">
            <node id="1">
                <element control="combo" obj-name="basesysNetworkIPv6Mgmt_sysIPV6AdminMode" id="1" name="IPv6 Mode"/>
            </node>
            <node id="11">
                <element control="combo" obj-name="basesysNetworkIPv6Mgmt_sysIPv6ConfigProtocol" id="1" name="Network Configuration Protocol">
                  <action type="enum" discard="no" enable="2_12_1" hide="2_13_1"/>
