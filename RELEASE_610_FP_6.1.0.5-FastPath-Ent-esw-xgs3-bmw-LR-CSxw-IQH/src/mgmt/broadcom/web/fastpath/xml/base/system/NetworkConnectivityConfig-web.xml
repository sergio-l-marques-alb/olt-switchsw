            </repeat>
            <repeat present="vtable" edit="no" heading=" " id="15" compile_if="IF_IPV6_MGMT_PACKAGE_PRESENT">
                <element properties="hidden,iterate" obj-name="basesysNetworkIPv6Mgmt_sysIPV6DefaultRouterIndex" id="1" name="Router Index"/>
