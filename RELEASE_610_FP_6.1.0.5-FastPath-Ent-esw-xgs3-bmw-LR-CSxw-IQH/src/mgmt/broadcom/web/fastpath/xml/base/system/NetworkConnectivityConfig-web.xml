                <element control="label" obj-name="baseSystemInfo_sysAgentIpIfDefaultRouter" id="1" name="Default Gateway"/>
            </node>
            <node id="5">
                <element control="label" obj-name="basesysHardwareInfo_sysInventoryBurnedInMacAddr" id="1" name="Burned In MAC Address"/>
            </node>
            <node id="6">
                <element control="label" obj-name="basesysHardwareInfo_sysLocalAdminMacAddr" id="1" name="Locally Administered MAC Address" run_if="SIM_ALLOW_LOCAL_MAC_USAGE_FEATURE"/>
            </node>
            <node id="7">
                <element control="label" obj-name="basesysHardwareInfo_sysMacAddrType" id="1" name="MAC Address Type" run_if="SIM_ALLOW_LOCAL_MAC_USAGE_FEATURE"/>
            </node>
            <node id="8">
                <element control="label" obj-name="baseSystemInfo_sysMgmtVlanId" id="1" name="Management VLAN ID"/>
            </node>
            <node id="9">
                <element control="label" obj-name="baseSysWebConnection_sysWebMgmtMode" id="1" name="Web Mode"/>
            </node>
            <node id="10">
                <element control="label" obj-name="baseSysWebConnection_sysWebJavaMode" id="1" name="Java Mode"/>
            </node>
            <node id="11" compile_if="IF_IPV6_MGMT_PACKAGE_PRESENT">
                <element control="label" obj-name="basesysNetworkIPv6Mgmt_sysIPV6AdminMode" id="1" name="IPv6 Mode"/>
            </node>
            <node id="12" compile_if="IF_IPV6_MGMT_PACKAGE_PRESENT">
                <element control="label" obj-name="basesysNetworkIPv6Mgmt_sysIPV6GateWay" id="1" name="IPv6 Gateway"/>
            </node>
            <node id="13" compile_if="IF_IPV6_MGMT_PACKAGE_PRESENT">
                <element control="label" obj-name="basesysNetworkIPv6Mgmt_sysIPv6ConfigProtocol" id="1" name="Network Configuration Protocol"/>
                <element control="combo" properties="read-only, hidden" obj-name="basesysNetworkIPv6Mgmt_sysIPv6ConfigProtocol" id="2" name="IPv6 Network Configuration Protocol">
                  <action type="enum" hide="4_16_1"/>
