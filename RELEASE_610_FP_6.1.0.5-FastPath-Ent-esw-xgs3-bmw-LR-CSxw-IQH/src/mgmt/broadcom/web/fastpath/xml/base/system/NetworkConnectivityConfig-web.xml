                  <action type="enum" discard="no" disable="2_12_1" show="2_13_1"/>
                </element>
            </node>
            <node id="12">
                <element control="combo" obj-name="basesysNetworkIPv6Mgmt_sysIPv6StatelessAutoConfMode" id="1" name="IPv6 Stateless Address AutoConfig Mode"/>
            </node>
            <node id="13">
                <element control="label" obj-name="basedhcpv6Client_dhcp6ClientDuid" id="1" name="DHCPv6 Client DUID"/>
            </node>
            <node id="4">
                <element control="combo" uienum="noneAddRemove_t" value="None" obj-name="xLibObjects_dummy" id="1" name="Add/Delete IPv6 Address">
                    <action type="enum" disable="2_5_1|2_6_1|2_7_1|3_2_1|3_3_1" hide="2_5_1|2_6_1|2_7_1|3_2_1|3_3_1" enable="3_1_1" show="3_1_1"/>
