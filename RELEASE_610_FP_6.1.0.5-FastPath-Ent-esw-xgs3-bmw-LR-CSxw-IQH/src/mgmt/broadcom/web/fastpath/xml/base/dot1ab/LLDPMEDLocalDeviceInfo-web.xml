<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "web.dtd" >
 


<FastPathXML>
    <webxml version="1.0" name="LLDPMEDLocalDeviceInfo" title="LLDP-MED Local Device Information">
        <group id="1" heading=" ">
            <node id="1">
                <element control="combo" properties="filter,iterate" obj-name="SwitchingLLDPXMEDLocalData_LocalPortNum" id="1" name="Interface">
                   <action type="value" value="valMapEmptyString" hide="1_1|1_2|1_3|1_4|1_5|2_1" show="1_6"/>
                   <action type="default" hide="1_6"/>
                </element>
                <element properties="hidden" obj-name="SwitchingLLDPXMEDLocalData_LldpXMedInterface" cpval="SwitchingLLDPXMEDLocalData_LocalPortNum" id="2" name=""/>
            </node>
            <node id="2">
                <message>Network Policy Information</message>
            </node>
            <repeat present="vtable" edit="no" heading=" " id="3">
                <element properties="filter,iterate" obj-name="SwitchingLLDPXMEDLocalData_MediaPolicyAppType" id="1" name="Media Application Type"/>
                <element obj-name="SwitchingLLDPXMEDLocalData_MediaPolicyVlanID" id="2" name="VLAN ID"/>
                <element obj-name="SwitchingLLDPXMEDLocalData_MediaPolicyPriority" id="3" name="Priority"/>
                <element obj-name="SwitchingLLDPXMEDLocalData_MediaPolicyDscp" id="4" name="DSCP"/>
                <element obj-name="SwitchingLLDPXMEDLocalData_MediaPolicyUnknown" id="5" name="Unknown Bit Status"/>
                <element obj-name="SwitchingLLDPXMEDLocalData_MediaPolicyTagged" id="6" name="Tagged Bit Status"/>
            </repeat>
            <node id="4">
                <message>Location Information</message>
            </node>
            <repeat present="vtable" edit="no" heading=" " id="5">
                <element properties="iterate" obj-name="SwitchingLLDPXMEDLocalData_LldpXMedLocLocationSubType" id="1" name="Sub Type"/>
                <element obj-name="SwitchingLLDPXMEDLocalData_lldpXMedLocLocationInfo" id="2" name="Info"/>
            </repeat>
            <node id="6" clazz="unavailableattributes">
                 <message> Local interfaces are not enabled to transmit LLDP-MED data</message>
            </node>
        </group>
        <group id="2" heading=" ">
            <node id="1">
                <button scope="page" type="buttonClear" help="Refreshes the form on this page from the Switch" id="1" name="Refresh"/>
            </node>
        </group>
    </webxml>
</FastPathXML>
