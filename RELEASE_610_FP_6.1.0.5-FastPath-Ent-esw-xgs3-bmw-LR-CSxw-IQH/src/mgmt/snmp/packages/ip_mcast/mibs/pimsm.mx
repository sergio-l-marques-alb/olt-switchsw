PIM-STD-MIB DEFINITIONS ::= BEGIN

IMPORTS
MODULE-IDENTITY, OBJECT-TYPE, mib-2,
  NOTIFICATION-TYPE,
  Unsigned32, TimeTicks            FROM SNMPv2-SMI
  TEXTUAL-CONVENTION,
  RowStatus, TruthValue            FROM SNMPv2-TC
  MODULE-COMPLIANCE, OBJECT-GROUP,
  NOTIFICATION-GROUP               FROM SNMPv2-CONF
  InterfaceIndexOrZero,
  InterfaceIndex                   FROM IF-MIB
  InetAddressType,
  InetAddressPrefixLength,
  InetAddress, InetVersion         FROM INET-ADDRESS-MIB
  IANAipRouteProtocol              FROM IANA-RTPROTO-MIB;

  pimStdMIB MODULE-IDENTITY
  LAST-UPDATED "200507150000Z" -- July 15, 2005
  ORGANIZATION "IETF PIM Working Group"
  CONTACT-INFO
  "Email: pim@ietf.org"
  DESCRIPTION
  "The MIB module for management of PIM routers.
  Copyright (C) The Internet Society (2005).  This version of
  this MIB module is part of RFC yyyy; see the RFC itself for
  full legal notices."
  -- RFC Ed.: replace yyyy with actual RFC number & remove this note
  REVISION     "200507150000Z" -- July 15, 2005
  DESCRIPTION  "Initial version, published as RFC yyyy."
  -- RFC Ed.: replace yyyy with actual RFC number & remove this note
  ::= { mib-2 203 }
  -- RFC Ed.: replace XXX with IANA-assigned number & remove this note

  pimSmMIBObjects OBJECT IDENTIFIER ::= { pimStdMIB 1 }
  pimSmTraps    OBJECT IDENTIFIER ::= { pimSmMIBObjects 0 }
  pimSm         OBJECT IDENTIFIER ::= { pimSmMIBObjects 1 }

  --
  -- Textual Conventions
  --

  PimSmMode ::= TEXTUAL-CONVENTION
  STATUS     current
  DESCRIPTION
  "The PIMSM mode in which a group is operating.

  none(1)      The group is not using PIM, which may be the
  case if, for example, it is a link-local or
  unroutable group address.
  ssm(2)       Source-Specific Multicast (SSM), with PIM
  Sparse Mode.
  asm(3)       Any Source Multicast (ASM), with PIM Sparse
  Mode.
  bidir(4)     Bi-directional PIM.
  other(5)     Any other PIM mode."
  SYNTAX     INTEGER {
    none(1),
      ssm(2),
      asm(3),
      bidir(4),
      other(5)
  }

PimSmGroupMappingOriginType ::= TEXTUAL-CONVENTION
STATUS     current
DESCRIPTION
"The mechanism by which a PIM group mapping was learned.

fixed(1)     Statically created link-local or unroutable
group mappings.
config(2)    Local configuration.
bsr(3)       The PIM Bootstrap Router (BSR) mechanism
[I-D.ietf-pim-sm-bsr].
autoRP(4)    Cisco's Auto-RP mechanism.
embedded(5)  The Embedded-RP mechanism [RFC3956] where the
RP address is embedded in the multicast group
address.
other(6)     Any other mechanism."
  SYNTAX     INTEGER {
    fixed(1),
      config(2),
      bsr(3),
      autoRP(4),
      embedded(5),
      other(6)
  }

--
-- The PIMSM Interface Table
--

pimSmInterfaceTable OBJECT-TYPE
SYNTAX     SEQUENCE OF PimSmInterfaceEntry
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"The (conceptual) table listing the router's PIMSm interfaces.
PIM is enabled on all interfaces listed in this table."
::={pimSm 1}

pimSmInterfaceEntry OBJECT-TYPE
SYNTAX     PimSmInterfaceEntry
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"An entry (conceptual row) in the pimSmInterfaceTable."
INDEX      { pimSmInterfaceIfIndex,
  pimSmInterfaceIPVersion }
  ::= { pimSmInterfaceTable 1 }

  PimSmInterfaceEntry ::= SEQUENCE {

      pimSmInterfaceIfIndex              InterfaceIndex,
      pimSmInterfaceIPVersion            InetVersion,
      pimSmInterfaceAddressType          InetAddressType,
      pimSmInterfaceAddress              InetAddress,
      pimSmInterfaceNetMaskLength        InetAddressPrefixLength,
      pimSmInterfaceDR                   InetAddress,
      pimSmInterfaceHelloInterval        Unsigned32,
      pimSmInterfaceTrigHelloInterval    Unsigned32,
      pimSmInterfaceJoinPruneInterval    Unsigned32,
      pimSmInterfaceDFElectionRobustness Unsigned32,
      pimSmInterfaceHelloHoldtime        Unsigned32,
      pimSmInterfaceJoinPruneHoldtime    Unsigned32,
      pimSmInterfaceUseLanPruneDelay     TruthValue,
      pimSmInterfacePropagationDelay     Unsigned32,
      pimSmInterfaceOverrideInterval     Unsigned32,
      pimSmInterfaceUseGenerationID      TruthValue,
      pimSmInterfaceGenerationIDValue    Unsigned32,
      pimSmInterfaceUseDRPriority        TruthValue,
      pimSmInterfaceDRPriority           Unsigned32,
      pimSmInterfaceLanDelayEnabled      TruthValue,
      pimSmInterfaceEffectPropagDelay    Unsigned32,
      pimSmInterfaceEffectOverrideIvl    Unsigned32,
      pimSmInterfaceSuppressionEnabled   TruthValue,
      pimSmInterfaceBidirCapable         TruthValue,
      pimSmInterfaceDRPriorityEnabled    TruthValue,
      pimSmInterfaceBSRBorder            TruthValue,
      pimSmInterfaceStatus               RowStatus
  }

pimSmInterfaceIfIndex OBJECT-TYPE
SYNTAX     InterfaceIndex
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"The ifIndex value of this PIMSm interface."
::= { pimSmInterfaceEntry 1 }

pimSmInterfaceIPVersion OBJECT-TYPE
SYNTAX     InetVersion
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"The IP version of this PIM interface.  A physical interface
may be configured in multiple modes concurrently, e.g. IPv4
and IPv6, however the traffic is considered to be logically
separate."
::= { pimSmInterfaceEntry 2 }

pimSmInterfaceAddressType OBJECT-TYPE
SYNTAX     InetAddressType
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The address type of this PIM interface."
::= { pimSmInterfaceEntry 3 }

pimSmInterfaceAddress OBJECT-TYPE
SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The primary IP address of this router on this PIM
  interface.  The InetAddressType is given by the
  pimSmInterfaceAddressType object."
  ::= { pimSmInterfaceEntry 4 }

  pimSmInterfaceNetMaskLength OBJECT-TYPE
  SYNTAX     InetAddressPrefixLength
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The network mask length for the primary IP address of this
  router on this PIM interface.  The InetAddressType is given
  by the pimSmInterfaceAddressType object.  A value of 0
  indicates that the network mask length is unknown."
  ::= { pimSmInterfaceEntry 5 }

  pimSmInterfaceDR OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The primary IP address of the Designated Router on this PIM
  interface.  The InetAddressType is given by the
  pimSmInterfaceAddressType object."
  ::= { pimSmInterfaceEntry 6 }

  pimSmInterfaceHelloInterval OBJECT-TYPE
  SYNTAX     Unsigned32 (0..18000)
  UNITS      "seconds"
  MAX-ACCESS read-create
  STATUS     current
  DESCRIPTION
  "The frequency at which PIM Hello messages are transmitted
  on this interface.  This object corresponds to the
  Hello_Period timer value defined in the PIM-SM
  specification [I-D.ietf-pim-sm-v2-new].  A value of 0
  represents an 'infinite' interval, and indicates that
  periodic PIM Hello messages should not be sent on this
  interface."
  DEFVAL { 30 }
  ::= { pimSmInterfaceEntry 7 }

  pimSmInterfaceTrigHelloInterval OBJECT-TYPE
  SYNTAX     Unsigned32 (0..60)
  UNITS      "seconds"
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The maximum time before this router sends a triggered PIM
  Hello message on this interface.  This object corresponds to
  the 'Trigered_Hello_Delay' timer value defined in the PIM-SM
  specification [I-D.ietf-pim-sm-v2-new].  A value of 0 has
  no special meaning and indicates that triggered PIM Hello
  messages should always be sent immediately."
  DEFVAL { 5 }
  ::= { pimSmInterfaceEntry 8 }

  pimSmInterfaceJoinPruneInterval OBJECT-TYPE
  SYNTAX     Unsigned32 (0..18000)
  UNITS      "seconds"
  MAX-ACCESS read-create
  STATUS     current
  DESCRIPTION
  "The frequency at which this router sends PIM Join/Prune
  messages on this PIM interface.  This object corresponds to
  the 't_periodic' timer value defined in the PIM-SM
  specification [I-D.ietf-pim-sm-v2-new].  A value of 0
  represents an 'infinite' interval, and indicates that
  periodic PIM Join/Prune messages should not be sent on this
  interface."
  DEFVAL { 60 }
  ::= { pimSmInterfaceEntry 9 }

  pimSmInterfaceDFElectionRobustness OBJECT-TYPE
  SYNTAX     Unsigned32
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The minimum number of PIM DF-Election messages that must be
  lost in order for DF election on this interface to fail."
  DEFVAL { 3 }
  ::= { pimSmInterfaceEntry 10 }


  pimSmInterfaceHelloHoldtime OBJECT-TYPE
  SYNTAX     Unsigned32 (0..65535)
  UNITS      "seconds"
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The value set in the Holdtime field of PIM Hello messages
  transmitted on this interface.  A value of 65535 represents
  an 'infinite' holdtime.  Implementations are recommended
  to use a holdtime that is 3.5 times the value of
  pimSmInterfaceHelloInterval, or 65535 if
  pimSmInterfaceHelloInterval is set to 0."
  DEFVAL { 105 }
  ::= { pimSmInterfaceEntry 11 }

  pimSmInterfaceJoinPruneHoldtime OBJECT-TYPE
  SYNTAX     Unsigned32 (0..65535)
  UNITS      "seconds"
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The value inserted into the Holdtime field of a PIM
  Join/Prune message sent on this interface.  A value of 65535
  represents an 'infinite' holdtime.  Implementations are
  recommended to use a holdtime that is 3.5 times the value of
  pimSmInterfaceJoinPruneInterval, or 65535 if
  pimSmInterfaceJoinPruneInterval is set to 0."
  DEFVAL { 210 }
  ::= { pimSmInterfaceEntry 12 }

  pimSmInterfaceUseLanPruneDelay OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Whether or not this router includes the LAN Prune Delay
  option in the PIM Hello messages it sends on this
  interface."
  DEFVAL { true }
  ::= { pimSmInterfaceEntry 13 }

  pimSmInterfacePropagationDelay OBJECT-TYPE
SYNTAX     Unsigned32 (0..32767)
  UNITS      "milliseconds"
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The value this router inserts into the Propagation_Delay
  field of the LAN Prune Delay option in the PIM Hello
  messages it sends on this interface.  This object is only
  used if pimSmInterfaceUseLanPruneDelay is set to TRUE.
  Implementations should enforce a lower bound on the
  permitted values for this object to allow for scheduling and
  processing delays within the local router."
  DEFVAL { 500 }
  ::= { pimSmInterfaceEntry 14 }

  pimSmInterfaceOverrideInterval OBJECT-TYPE
  SYNTAX     Unsigned32 (0..65535)
  UNITS      "milliseconds"
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The value this router inserts into the Override_Interval
  field of the LAN Prune Delay option in the PIM Hello
  messages it sends on this interface.  This object is only
  used if pimSmInterfaceUseLanPruneDelay is set to TRUE."
  DEFVAL { 2500 }
  ::= { pimSmInterfaceEntry 15 }

  pimSmInterfaceUseGenerationID OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Whether or not this router includes the Generation ID
  option in the PIM Hello messages it sends on this
  interface."
  DEFVAL { true }
  ::= { pimSmInterfaceEntry 16 }

  pimSmInterfaceGenerationIDValue OBJECT-TYPE
  SYNTAX     Unsigned32
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The value of the Generation ID this router inserted in the
  last PIM Hello message it sent on this interface.  This
  object is 0 if pimSmInterfaceUseGenerationID is set to FALSE."
  ::= { pimSmInterfaceEntry 17 }

  pimSmInterfaceUseDRPriority OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Whether or not this router includes the DR Priority option
  in the PIM Hello messages it sends on this interface."
  DEFVAL { true }
  ::= { pimSmInterfaceEntry 18 }

  pimSmInterfaceDRPriority OBJECT-TYPE
  SYNTAX     Unsigned32
  MAX-ACCESS read-create
  STATUS     current
  DESCRIPTION
  "The Designated Router Priority value inserted into the DR
  Priority option on this interface.  Numerically higher
  values for this object indicate higher priorities.  This
  object is only used if pimSmInterfaceUseDRPriority is set to
  TRUE."
  DEFVAL { 1 }
  ::= { pimSmInterfaceEntry 19 }

  pimSmInterfaceLanDelayEnabled OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Evaluates to TRUE if all routers on this interface are
  using the LAN Prune Delay option."
  ::= { pimSmInterfaceEntry 20 }

  pimSmInterfaceEffectPropagDelay OBJECT-TYPE
  SYNTAX     Unsigned32 (0..32767)
  UNITS      "milliseconds"
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The Effective Propagation Delay on this interface.  This
  object is always 500 if pimSmInterfaceLanDelayEnabled is
  FALSE."
  ::= { pimSmInterfaceEntry 21 }

  pimSmInterfaceEffectOverrideIvl OBJECT-TYPE
  SYNTAX     Unsigned32 (0..65535)
  UNITS      "milliseconds"
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The Effective Override Interval on this interface.  This
  object is always 2500 if pimSmInterfaceLanDelayEnabled is
  FALSE."
  ::= { pimSmInterfaceEntry 22 }

  pimSmInterfaceSuppressionEnabled OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Whether join suppression is enabled on this interface.
  This object is always TRUE if pimSmInterfaceLanDelayEnabled is
  FALSE."
  ::= { pimSmInterfaceEntry 23 }

  pimSmInterfaceBidirCapable OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Evaluates to TRUE if all routers on this interface are
  using the Bidirectional-PIM Capable option."
  ::= { pimSmInterfaceEntry 24 }

  pimSmInterfaceDRPriorityEnabled OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Evaluates to TRUE if all routers on this interface are
  using the DR Priority option."
  ::= { pimSmInterfaceEntry 25 }

  pimSmInterfaceBSRBorder OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-create
  STATUS     current
  DESCRIPTION
  "Whether or not this interface acts as a border for all PIM
  Bootstrap messages."
  DEFVAL { false }
  ::= { pimSmInterfaceEntry 26 }

  pimSmInterfaceStatus OBJECT-TYPE
  SYNTAX     RowStatus
  MAX-ACCESS read-create
  STATUS     current
  DESCRIPTION
  "The status of this entry.  Creating the entry enables PIM
  on the interface; destroying the entry disables PIM on the
  interface."
  ::= { pimSmInterfaceEntry 27 }

  --
  -- The PIM Neighbor Table
  --

  pimSmNeighborTable OBJECT-TYPE
  SYNTAX     SEQUENCE OF PimSmNeighborEntry
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "The (conceptual) table listing the router's PIM neighbors."
  ::= { pimSm 2 }

  pimSmNeighborEntry OBJECT-TYPE
  SYNTAX     PimSmNeighborEntry
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "An entry (conceptual row) in the pimSmNeighborTable."
  INDEX      { pimSmNeighborIfIndex,
    pimSmNeighborAddressType,
    pimSmNeighborAddress }
    ::= { pimSmNeighborTable 1 }

  PimSmNeighborEntry ::= SEQUENCE {
      pimSmNeighborIfIndex              InterfaceIndex,
      pimSmNeighborAddressType          InetAddressType,
      pimSmNeighborAddress              InetAddress,
      pimSmNeighborUpTime               TimeTicks,
      pimSmNeighborExpiryTime           TimeTicks,
      pimSmNeighborLanPruneDelayPresent TruthValue,
      pimSmNeighborPropagationDelay     Unsigned32,
      pimSmNeighborOverrideInterval     Unsigned32,
      pimSmNeighborTBit                 TruthValue,
      pimSmNeighborGenerationIDPresent  TruthValue,
      pimSmNeighborGenerationIDValue    Unsigned32,
      pimSmNeighborBidirCapable         TruthValue,
      pimSmNeighborDRPriorityPresent    TruthValue,
      pimSmNeighborDRPriority           Unsigned32
    }

pimSmNeighborIfIndex OBJECT-TYPE
SYNTAX     InterfaceIndex
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"The value of ifIndex for the interface used to reach this
PIM neighbor."
::= { pimSmNeighborEntry 1 }

pimSmNeighborAddressType OBJECT-TYPE
SYNTAX     InetAddressType
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"The address type of this PIM neighbor."
::= { pimSmNeighborEntry 2 }

  pimSmNeighborAddress OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "The primary IP address of this PIM neighbor.  The
  InetAddressType is given by the pimSmNeighborAddressType
  object."
  ::= { pimSmNeighborEntry 3 }

  pimSmNeighborUpTime OBJECT-TYPE
  SYNTAX     TimeTicks
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The time since this PIM neighbor (last) became a neighbor
  of the local router."
  ::= { pimSmNeighborEntry 4 }

  pimSmNeighborExpiryTime OBJECT-TYPE
  SYNTAX     TimeTicks
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The minimum time remaining before this PIM neighbor will
  be aged out.  The value zero indicates that this PIM
  neighbor will never be aged out."
  ::= { pimSmNeighborEntry 5 }

  pimSmNeighborLanPruneDelayPresent OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Evaluates to TRUE if this neighbor is using the LAN Prune
  Delay option."
  ::= { pimSmNeighborEntry 6 }

  pimSmNeighborPropagationDelay OBJECT-TYPE
  SYNTAX     Unsigned32 (0..32767)
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The value of the Propagation_Delay field of the LAN Prune
  Delay option received from this neighbor.  This object is
  always 0 if pimSmNeighborLanPruneDelayPresent is FALSE."
  ::= { pimSmNeighborEntry 7 }

  pimSmNeighborOverrideInterval OBJECT-TYPE
  SYNTAX     Unsigned32 (0..65535)
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The value of the Override_Interval field of the LAN Prune
  Delay option received from this neighbor.  This object is
  always 0 if pimSmNeighborLanPruneDelayPresent is FALSE."
  ::= { pimSmNeighborEntry 8 }

  pimSmNeighborTBit OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Whether the T bit was set in the LAN Prune Delay option
  received from this neighbor.  The T bit specifies the
  ability of the neighbor to disable join suppression.  This
  object is always TRUE if pimSmNeighborLanPruneDelayPresent is
  FALSE."
  ::= { pimSmNeighborEntry 9 }

  pimSmNeighborGenerationIDPresent OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Evaluates to TRUE if this neighbor is using the Generation
  ID option."
  ::= { pimSmNeighborEntry 10 }

  pimSmNeighborGenerationIDValue OBJECT-TYPE
  SYNTAX     Unsigned32
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The value of the Generation ID from the last PIM Hello
  message received from this neighbor.  This object is always
  0 if pimSmNeighborGenerationIDPresent is FALSE."
  ::= { pimSmNeighborEntry 11 }

  pimSmNeighborBidirCapable OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Evaluates to TRUE if this neighbor is using the
  Bidirectional-PIM Capable option."
  ::= { pimSmNeighborEntry 12 }

  pimSmNeighborDRPriorityPresent OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Evaluates to TRUE if this neighbor is using the DR Priority
  option."
  ::= { pimSmNeighborEntry 13 }

  pimSmNeighborDRPriority OBJECT-TYPE
  SYNTAX     Unsigned32
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The value of the Designated Router Priority from the last
  PIM Hello message received from this neighbor.  This object
  is always 0 if pimSmNeighborDRPriorityPresent is FALSE."
  ::= { pimSmNeighborEntry 14 }

  --
  -- The PIM Neighbor Secondary Address Table
  --

  pimSmNbrSecAddressTable OBJECT-TYPE
  SYNTAX     SEQUENCE OF PimSmNbrSecAddressEntry
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "The (conceptual) table listing the secondary addresses
  advertised by each PIM neighbor (on a subset of the rows of
      the pimSmNeighborTable defined above)."
  ::= { pimSm 3 }

  pimSmNbrSecAddressEntry OBJECT-TYPE
  SYNTAX     PimSmNbrSecAddressEntry
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "An entry (conceptual row) in the pimSmNbrSecAddressTable."
  INDEX      { pimSmNbrSecAddressIfIndex,
    pimSmNbrSecAddressType,
    pimSmNbrSecAddressPrimary,
    pimSmNbrSecAddress }
    ::= { pimSmNbrSecAddressTable 1 }

    PimSmNbrSecAddressEntry ::= SEQUENCE {
      pimSmNbrSecAddressIfIndex InterfaceIndex,
      pimSmNbrSecAddressType    InetAddressType,
      pimSmNbrSecAddressPrimary InetAddress,
      pimSmNbrSecAddress        InetAddress
    }

pimSmNbrSecAddressIfIndex OBJECT-TYPE
SYNTAX     InterfaceIndex
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"The value of ifIndex for the interface used to reach this
PIM neighbor."
::= { pimSmNbrSecAddressEntry 1 }

pimSmNbrSecAddressType OBJECT-TYPE
SYNTAX     InetAddressType
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"The address type of this PIM neighbor."
::= { pimSmNbrSecAddressEntry 2 }

  pimSmNbrSecAddressPrimary OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "The primary IP address of this PIM neighbor.  The
  InetAddressType is given by the pimSmNbrSecAddressType
  object."
  ::= { pimSmNbrSecAddressEntry 3 }

  pimSmNbrSecAddress OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The secondary IP address of this PIM neighbor.  The
  InetAddressType is given by the pimSmNbrSecAddressType
  object."
  ::= { pimSmNbrSecAddressEntry 4 }

  --
  -- The PIM (*,G) State Table
  --

  pimSmStarGTable OBJECT-TYPE
  SYNTAX     SEQUENCE OF PimSmStarGEntry
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "The (conceptual) table listing the non-interface specific
  (*,G) state that PIM has."
  ::= { pimSm 4 }

  pimSmStarGEntry OBJECT-TYPE
  SYNTAX     PimSmStarGEntry
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "An entry (conceptual row) in the pimSmStarGTable."
  INDEX      { pimSmStarGAddressType,
    pimSmStarGGrpAddress }
    ::= { pimSmStarGTable 1 }

    PimSmStarGEntry ::= SEQUENCE {
      pimSmStarGAddressType             InetAddressType,
      pimSmStarGGrpAddress              InetAddress,
      pimSmStarGUpTime                  TimeTicks,
      pimSmStarGPimSmMode               PimSmMode,
      pimSmStarGRPAddress               InetAddress,
      pimSmStarGRPOrigin                PimSmGroupMappingOriginType,
      pimSmStarGRPIsLocal               TruthValue,
      pimSmStarGUpstreamJoinState       INTEGER,
      pimSmStarGUpstreamJoinTimer       TimeTicks,
      pimSmStarGUpstreamNeighbor        InetAddress,
      pimSmStarGRPFIfIndex              InterfaceIndexOrZero,
      pimSmStarGRPFNextHop              InetAddress,
      pimSmStarGRPFRouteProtocol        IANAipRouteProtocol,
      pimSmStarGRPFRouteAddress         InetAddress,
      pimSmStarGRPFRoutePrefixLength    InetAddressPrefixLength,
      pimSmStarGRPFRouteMetricPref      Unsigned32,
      pimSmStarGRPFRouteMetric          Unsigned32
    }

pimSmStarGAddressType OBJECT-TYPE
SYNTAX     InetAddressType
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"The address type of this multicast group."
::= { pimSmStarGEntry 1 }

  pimSmStarGGrpAddress OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "The multicast group address.  The InetAddressType is given
  by the pimSmStarGAddressType object."
  ::= { pimSmStarGEntry 2 }

  pimSmStarGUpTime OBJECT-TYPE
  SYNTAX     TimeTicks
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The time since this entry was created by the local router."
  ::= { pimSmStarGEntry 3 }

  pimSmStarGPimSmMode OBJECT-TYPE
  SYNTAX     INTEGER { asm(3), bidir(4) }
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Whether this entry represents an ASM (Any Source Multicast,
  used with PIM-SM) or BIDIR-PIM group."
  ::= { pimSmStarGEntry 4 }

  pimSmStarGRPAddress OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The address of the Rendezvous Point (RP) for the group.
  The InetAddressType is given by the pimSmStarGAddressType
  object.  This object is zero if the RP address is unknown."
  ::= { pimSmStarGEntry 5 }

  pimSmStarGRPOrigin OBJECT-TYPE
  SYNTAX     PimSmGroupMappingOriginType
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The mechanism by which the PIM mode and RP for the group
  were learned."
  ::= { pimSmStarGEntry 6 }

  pimSmStarGRPIsLocal OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Whether the local router is the RP for the group."
  ::= { pimSmStarGEntry 7 }

  pimSmStarGUpstreamJoinState OBJECT-TYPE
  SYNTAX     INTEGER {
    notJoined (1),
              joined (2)
  }
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Whether the local router should join the RP tree for the
  group.  This corresponds to the state of the upstream (*,G)
  state machine in the PIM-SM specification
  [I-D.ietf-pim-sm-v2-new]."
  ::= { pimSmStarGEntry 8 }

  pimSmStarGUpstreamJoinTimer OBJECT-TYPE
  SYNTAX     TimeTicks
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The time remaining before the local router next sends a
  periodic (*,G) Join message on pimSmStarGRPFIfIndex.  This
  timer is called the (*,G) Upstream Join Timer in the PIM-SM
  specification [I-D.ietf-pim-sm-v2-new].  This object is zero
  if the timer is not running."
  ::= { pimSmStarGEntry 9 }

  pimSmStarGUpstreamNeighbor OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The primary address of the neighbor on pimSmStarGRPFIfIndex
  that the local router is sending periodic (*,G) Join
  messages to.  This is zero if the the RPF next hop is
  unknown or is not a PIM neighbor.  The InetAddressType is
  given by the pimSmStarGAddressType object.  This address is
  called RPF'(*,G) in the PIM-SM specification
  [I-D.ietf-pim-sm-v2-new]."
  ::= { pimSmStarGEntry 10 }

  pimSmStarGRPFIfIndex OBJECT-TYPE
  SYNTAX     InterfaceIndexOrZero
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The value of ifIndex for the RPF interface towards the RP,
  or zero if the RPF interface is unknown."
  ::= { pimSmStarGEntry 11 }

  pimSmStarGRPFNextHop OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The address of the RPF next hop towards the RP, or zero if
  the the RPF next hop is unknown.  The InetAddressType is
  given by the pimSmStarGAddressType object.  This address is
  called MRIB.next_hop(RP(G)) in the PIM-SM specification
  [I-D.ietf-pim-sm-v2-new]."
  ::= { pimSmStarGEntry 12 }

  pimSmStarGRPFRouteProtocol OBJECT-TYPE
  SYNTAX     IANAipRouteProtocol
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The routing mechanism via which the route used to find the
  RPF interface towards the RP was learned."
  ::= { pimSmStarGEntry 13 }

  pimSmStarGRPFRouteAddress OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The IP address which when combined with the corresponding
  value of pimSmStarGRPFRoutePrefixLength identifies the route
  used to find the RPF interface towards the RP.  The
  InetAddressType is given by the pimSmStarGAddressType
  object."
  ::= { pimSmStarGEntry 14 }

  pimSmStarGRPFRoutePrefixLength OBJECT-TYPE
  SYNTAX     InetAddressPrefixLength
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The prefix length which when combined with the
  corresponding value of pimSmStarGRPFRouteAddress identifies
  the route used to find the RPF interface towards the RP.
  The InetAddressType is given by the pimSmStarGAddressType
  object."
  ::= { pimSmStarGEntry 15 }

  pimSmStarGRPFRouteMetricPref OBJECT-TYPE
  SYNTAX     Unsigned32 (0..2147483647)
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The metric preference of the route used to find the RPF
  interface towards the RP."
  ::= { pimSmStarGEntry 16 }

  pimSmStarGRPFRouteMetric OBJECT-TYPE
  SYNTAX     Unsigned32
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The routing metric of the route used to find the RPF
  interface towards the RP."
  ::= { pimSmStarGEntry 17 }

  --
  -- The PIM (*,G,I) State Table
  --

  pimSmStarGITable OBJECT-TYPE
  SYNTAX     SEQUENCE OF PimSmStarGIEntry
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "The (conceptual) table listing the interface-specific (*,G)
  state that PIM has."
 ::={pimSm 5}

  pimSmStarGIEntry OBJECT-TYPE
  SYNTAX     PimSmStarGIEntry
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "An entry (conceptual row) in the pimSmStarGITable."
  INDEX      { pimSmStarGAddressType,
    pimSmStarGGrpAddress,
    pimSmStarGIIfIndex }
  ::= { pimSmStarGITable 1 }

    PimSmStarGIEntry ::= SEQUENCE {
      pimSmStarGIIfIndex                 InterfaceIndex,
      pimSmStarGIUpTime                  TimeTicks,
      pimSmStarGILocalMembership         TruthValue,
      pimSmStarGIJoinPruneState          INTEGER,
      pimSmStarGIPrunePendingTimer       TimeTicks,
      pimSmStarGIJoinExpiryTimer         TimeTicks,
      pimSmStarGIAssertState             INTEGER,
      pimSmStarGIAssertTimer             TimeTicks,
      pimSmStarGIAssertWinnerAddress     InetAddress,
      pimSmStarGIAssertWinnerMetricPref  Unsigned32,
      pimSmStarGIAssertWinnerMetric      Unsigned32
    }

pimSmStarGIIfIndex OBJECT-TYPE
SYNTAX     InterfaceIndex
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"The ifIndex of the interface that this entry corresponds
to."
::= { pimSmStarGIEntry 1 }

pimSmStarGIUpTime OBJECT-TYPE
SYNTAX     TimeTicks
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The time since this entry was created by the local router."
::= { pimSmStarGIEntry 2 }

pimSmStarGILocalMembership OBJECT-TYPE
SYNTAX     TruthValue
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"Whether the local router has (*,G) local membership on this
interface (resulting from a mechanism such as IGMP or MLD).
This corresponds to local_receiver_include(*,G,I) in the
PIM-SM specification [I-D.ietf-pim-sm-v2-new]."
::= { pimSmStarGIEntry 3 }

pimSmStarGIJoinPruneState OBJECT-TYPE
SYNTAX     INTEGER {
         noInfo (1),
         join (2),
         prunePending (3)
}
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The state resulting from (*,G) Join/Prune messages
received on this interface.  This corresponds to the state
of the downstream per-interface (*,G) state machine in the
PIM-SM specification [I-D.ietf-pim-sm-v2-new]."
::= { pimSmStarGIEntry 4 }

pimSmStarGIPrunePendingTimer OBJECT-TYPE
SYNTAX     TimeTicks
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The time remaining before the local router acts on a (*,G)
Prune message received on this interface, during which the
router is waiting to see whether another downstream router
will override the Prune message.  This timer is called the
(*,G) Prune-Pending Timer in the PIM-SM specification
[I-D.ietf-pim-sm-v2-new].  This object is zero if the timer
is not running."
::= { pimSmStarGIEntry 5 }

pimSmStarGIJoinExpiryTimer OBJECT-TYPE
SYNTAX     TimeTicks
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The time remaining before (*,G) Join state for this
interface expires.  This timer is called the (*,G) Join
Expiry Timer in the PIM-SM specification
[I-D.ietf-pim-sm-v2-new].  This object is zero if the timer
is not running.  A value of 'FFFFFFFF'h indicates an
infinite expiry time."
::= { pimSmStarGIEntry 6 }

pimSmStarGIAssertState OBJECT-TYPE
  SYNTAX     INTEGER {
    noInfo (1),
           iAmAssertWinner (2),
           iAmAssertLoser (3)
  }
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The (*,G) Assert state for this interface.  This
corresponds to the state of the per-interface (*,G) Assert
state machine in the PIM-SM specification
[I-D.ietf-pim-sm-v2-new].  If pimSmStarGPimSmMode is 'bidir',
  this object must be 'noInfo'."
  ::= { pimSmStarGIEntry 7 }

  pimSmStarGIAssertTimer OBJECT-TYPE
  SYNTAX     TimeTicks
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "If pimSmStarGIAssertState is 'iAmAssertWinner', this is the
  time remaining before the local router next sends a (*,G)
  Assert message on this interface.  If pimSmStarGIAssertState
  is 'iAmAssertLoser', this is the time remaining before the
  (*,G) Assert state expires.  If pimSmStarGIAssertState is
  'noInfo', this is zero.  This timer is called the (*,G)
  Assert Timer in the PIM-SM specification
  [I-D.ietf-pim-sm-v2-new]."
  ::= { pimSmStarGIEntry 8 }

  pimSmStarGIAssertWinnerAddress OBJECT-TYPE
  SYNTAX     InetAddress
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "If pimSmStarGIAssertState is 'iAmAssertLoser', this is the
  address of the assert winner; otherwise, this object is
  zero.  The InetAddressType is given by the
  pimSmStarGAddressType object."
  ::= { pimSmStarGIEntry 9 }

  pimSmStarGIAssertWinnerMetricPref OBJECT-TYPE
  SYNTAX     Unsigned32 (0..2147483647)
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "If pimSmStarGIAssertState is 'iAmAssertLoser', this is the
  metric preference of the route to the RP advertised by the
  assert winner; otherwise, this object is zero."
  ::= { pimSmStarGIEntry 11 }

  pimSmStarGIAssertWinnerMetric OBJECT-TYPE
  SYNTAX     Unsigned32
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "If pimSmStarGIAssertState is 'iAmAssertLoser', this is the
  routing metric of the route to the RP advertised by the
  assert winner; otherwise, this object is zero."
  ::= { pimSmStarGIEntry 12 }

  --
  -- The PIMSM (S,G) State Table
  --

  pimSmSGTable OBJECT-TYPE
  SYNTAX     SEQUENCE OF PimSmSGEntry
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "The (conceptual) table listing the non-interface specific
  (S,G) state that PIM has."
  ::= { pimSm 6 }

  pimSmSGEntry OBJECT-TYPE
  SYNTAX     PimSmSGEntry
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "An entry (conceptual row) in the pimSmSGTable."
  INDEX      { pimSmSGAddressType,
    pimSmSGGrpAddress,
    pimSmSGSrcAddress }
    ::= { pimSmSGTable 1 }

    PimSmSGEntry ::= SEQUENCE {
      pimSmSGAddressType             InetAddressType,
      pimSmSGGrpAddress              InetAddress,
      pimSmSGSrcAddress              InetAddress,
      pimSmSGUpTime                  TimeTicks,
      pimSmSGPimSmMode               PimSmMode,
      pimSmSGUpstreamJoinState       INTEGER,
      pimSmSGUpstreamJoinTimer       TimeTicks,
      pimSmSGUpstreamNeighbor        InetAddress,
      pimSmSGRPFIfIndex              InterfaceIndexOrZero,
      pimSmSGRPFNextHop              InetAddress,
      pimSmSGRPFRouteProtocol        IANAipRouteProtocol,
      pimSmSGRPFRouteAddress         InetAddress,
      pimSmSGRPFRoutePrefixLength    InetAddressPrefixLength,
      pimSmSGRPFRouteMetricPref      Unsigned32,
      pimSmSGRPFRouteMetric          Unsigned32,
      pimSmSGSPTBit                  TruthValue,
      pimSmSGKeepaliveTimer          TimeTicks,
      pimSmSGDRRegisterState         INTEGER,
      pimSmSGDRRegisterStopTimer     TimeTicks,
      pimSmSGRPRegisterPMBRAddress   InetAddress
    }

pimSmSGAddressType OBJECT-TYPE
SYNTAX     InetAddressType
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"The address type of the source and multicast group for this
entry."
::= { pimSmSGEntry 1 }

  pimSmSGGrpAddress OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "The multicast group address for this entry.  The
  InetAddressType is given by the pimSmSGAddressType object."
  ::= { pimSmSGEntry 2 }

  pimSmSGSrcAddress OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS not-accessible
  STATUS     current
  DESCRIPTION
  "The source address for this entry.  The InetAddressType is
  given by the pimSmSGAddressType object."
  ::= { pimSmSGEntry 3 }

  pimSmSGUpTime OBJECT-TYPE
  SYNTAX     TimeTicks
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The time since this entry was created by the local router."
  ::= { pimSmSGEntry 4 }

  pimSmSGPimSmMode OBJECT-TYPE
  SYNTAX     INTEGER { ssm(2), asm(3) }
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Whether pimSmSGGrpAddress is an SSM (Source Specific
  Multicast, used with PIM-SM) or ASM (Any Source Multicast,
    used with PIM-SM) group."
  ::= { pimSmSGEntry 5 }

  pimSmSGUpstreamJoinState OBJECT-TYPE
  SYNTAX     INTEGER {
    notJoined (1),
              joined (2)
  }
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"Whether the local router should join the shortest-path tree
for the source and group represented by this entry.  This
corresponds to the state of the upstream (S,G) state machine
in the PIM-SM specification [I-D.ietf-pim-sm-v2-new]."
::= { pimSmSGEntry 6 }

pimSmSGUpstreamJoinTimer OBJECT-TYPE
SYNTAX     TimeTicks
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The time remaining before the local router next sends a
periodic (S,G) Join message on pimSmSGRPFIfIndex.  This timer
is called the (S,G) Upstream Join Timer in the PIM-SM
specification [I-D.ietf-pim-sm-v2-new].  This object is zero
if the timer is not running."
::= { pimSmSGEntry 7 }

  pimSmSGUpstreamNeighbor OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The primary address of the neighbor on pimSmSGRPFIfIndex that
  the local router is sending periodic (S,G) Join messages to.
  This is zero if the the RPF next hop is unknown or is not a
  PIM neighbor.  The InetAddressType is given by the
  pimSmSGAddressType object.  This address is called RPF'(S,G)
  in the PIM-SM specification [I-D.ietf-pim-sm-v2-new]."
  ::= { pimSmSGEntry 8 }

  pimSmSGRPFIfIndex OBJECT-TYPE
  SYNTAX     InterfaceIndexOrZero
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The value of ifIndex for the RPF interface towards the
  source, or zero if the RPF interface is unknown."
  ::= { pimSmSGEntry 9 }

  pimSmSGRPFNextHop OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The address of the RPF next hop towards the source, or zero
  if the the RPF next hop is unknown.  The InetAddressType is
  given by the pimSmSGAddressType object.  This address is
  called MRIB.next_hop(S) in the PIM-SM specification
  [I-D.ietf-pim-sm-v2-new]."
  ::= { pimSmSGEntry 10 }

  pimSmSGRPFRouteProtocol OBJECT-TYPE
  SYNTAX     IANAipRouteProtocol
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The routing mechanism via which the route used to find the
  RPF interface towards the source was learned."
  ::= { pimSmSGEntry 11 }

  pimSmSGRPFRouteAddress OBJECT-TYPE
  SYNTAX     InetAddress (SIZE (4|16|20))
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The IP address which when combined with the corresponding
  value of pimSmSGRPFRoutePrefixLength identifies the route used
  to find the RPF interface towards the source.  The
  InetAddressType is given by the pimSmSGAddressType object."
  ::= { pimSmSGEntry 12 }

  pimSmSGRPFRoutePrefixLength OBJECT-TYPE
  SYNTAX     InetAddressPrefixLength
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The prefix length which when combined with the
  corresponding value of pimSmSGRPFRouteAddress identifies the
  route used to find the RPF interface towards the source.
  The InetAddressType is given by the pimSmSGAddressType
  object."
  ::= { pimSmSGEntry 13 }

  pimSmSGRPFRouteMetricPref OBJECT-TYPE
  SYNTAX     Unsigned32 (0..2147483647)
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The metric preference of the route used to find the RPF
  interface towards the source."
  ::= { pimSmSGEntry 14 }

  pimSmSGRPFRouteMetric OBJECT-TYPE
  SYNTAX     Unsigned32
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The routing metric of the route used to find the RPF
  interface towards the source."
  ::= { pimSmSGEntry 15 }

  pimSmSGSPTBit OBJECT-TYPE
  SYNTAX     TruthValue
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "Whether the SPT bit is set; and therefore whether
  forwarding is taking place on the shortest-path tree."
  ::= { pimSmSGEntry 16 }

  pimSmSGKeepaliveTimer OBJECT-TYPE
  SYNTAX     TimeTicks
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
  "The time remaining before, in the absence of explicit (S,G)
  local membership or (S,G) Join messages received to maintain
  it, this (S,G) state expires.  This timer is called the
  (S,G) Keepalive Timer in the PIM-SM specification
  [I-D.ietf-pim-sm-v2-new]."
  ::= { pimSmSGEntry 17 }

  pimSmSGDRRegisterState OBJECT-TYPE
  SYNTAX     INTEGER {
    noInfo (1),
           join (2),
           joinPending (3),
           prune (4)
  }
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"Whether the local router should encapsulate (S,G) data
packets in Register messages and send them to the RP.  This
corresponds to the state of the per-(S,G) Register state
machine in the PIM-SM specification
[I-D.ietf-pim-sm-v2-new].  This object is always 'noInfo'
unless pimSmSGPimSmMode is 'asm'."
::= { pimSmSGEntry 18 }

pimSmSGDRRegisterStopTimer OBJECT-TYPE
SYNTAX     TimeTicks
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"If pimSmSGDRRegisterState is 'prune', this is the time
remaining before the local router sends a Null-Register
message to the RP.  If pimSmSGDRRegisterState is
'joinPending', this is the time remaining before the local
router resumes encapsulating data packets and sending them
to the RP.  Otherwise, this is zero.  This timer is called
the Register-Stop Timer in the PIM-SM specification
[I-D.ietf-pim-sm-v2-new]."
::= { pimSmSGEntry 19 }

pimSmSGRPRegisterPMBRAddress OBJECT-TYPE
SYNTAX     InetAddress
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The IP address of the first PIM Multicast Border Router to
send a Register message with the Border bit set.  This
object is zero if the local router is not the RP for the
group.  The InetAddressType is given by the pimSmSGAddressType
object."
::= { pimSmSGEntry 20 }

--
-- The PIM (S,G,I) State Table
--

pimSmSGITable OBJECT-TYPE
SYNTAX     SEQUENCE OF PimSmSGIEntry
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"The (conceptual) table listing the interface-specific (S,G)
state that PIM has."
::= { pimSm 7 }

pimSmSGIEntry OBJECT-TYPE
SYNTAX     PimSmSGIEntry
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"An entry (conceptual row) in the pimSmSGITable."
INDEX      { pimSmSGAddressType,
  pimSmSGGrpAddress,
  pimSmSGSrcAddress,
  pimSmSGIIfIndex }
  ::= { pimSmSGITable 1 }

  PimSmSGIEntry ::= SEQUENCE {
    pimSmSGIIfIndex                 InterfaceIndex,
    pimSmSGIUpTime                  TimeTicks,
    pimSmSGILocalMembership         TruthValue,
    pimSmSGIJoinPruneState          INTEGER,
    pimSmSGIPrunePendingTimer       TimeTicks,
    pimSmSGIJoinExpiryTimer         TimeTicks,
    pimSmSGIAssertState             INTEGER,
    pimSmSGIAssertTimer             TimeTicks,
    pimSmSGIAssertWinnerAddress     InetAddress,
    pimSmSGIAssertWinnerMetricPref  Unsigned32,
    pimSmSGIAssertWinnerMetric      Unsigned32
  }

pimSmSGIIfIndex OBJECT-TYPE
SYNTAX     InterfaceIndex
MAX-ACCESS not-accessible
STATUS     current
DESCRIPTION
"The ifIndex of the interface that this entry corresponds
to."
::= { pimSmSGIEntry 1 }

pimSmSGIUpTime OBJECT-TYPE
SYNTAX     TimeTicks
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The time since this entry was created by the local router."
::= { pimSmSGIEntry 2 }

pimSmSGILocalMembership OBJECT-TYPE
SYNTAX     TruthValue
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"Whether the local router has (S,G) local membership on this
  interface (resulting from a mechanism such as IGMPv3 or
      MLDv2).  This corresponds to local_receiver_include(S,G,I)
  in the PIM-SM specification [I-D.ietf-pim-sm-v2-new]."
  ::= { pimSmSGIEntry 3 }

  pimSmSGIJoinPruneState OBJECT-TYPE
  SYNTAX     INTEGER {
    noInfo (1),
           join (2),
           prunePending (3)
  }
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The state resulting from (S,G) Join/Prune messages
received on this interface.  This corresponds to the state
of the downstream per-interface (S,G) state machine in the
PIM-SM specification [I-D.ietf-pim-sm-v2-new]."
::= { pimSmSGIEntry 4 }

pimSmSGIPrunePendingTimer OBJECT-TYPE
SYNTAX     TimeTicks
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The time remaining before the local router acts on an (S,G)
Prune message received on this interface, during which the
router is waiting to see whether another downstream router
will override the Prune message.  This timer is called the
(S,G) Prune-Pending Timer in the PIM-SM specification
[I-D.ietf-pim-sm-v2-new].  This object is zero if the timer
is not running."
::= { pimSmSGIEntry 5 }

pimSmSGIJoinExpiryTimer OBJECT-TYPE
SYNTAX     TimeTicks
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The time remaining before (S,G) Join state for this
interface expires.  This timer is called the (S,G) Join
Expiry Timer in the PIM-SM specification
[I-D.ietf-pim-sm-v2-new].  This object is zero if the timer
is not running.  A value of 'FFFFFFFF'h indicates an
infinite expiry time."
::= { pimSmSGIEntry 6 }

pimSmSGIAssertState OBJECT-TYPE
SYNTAX     INTEGER {
  noInfo (1),
           iAmAssertWinner (2),
         iAmAssertLoser (3)
}
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"The (S,G) Assert state for this interface.  This
corresponds to the state of the per-interface (S,G) Assert
state machine in the PIM-SM specification
[I-D.ietf-pim-sm-v2-new]."
::= { pimSmSGIEntry 7 }

pimSmSGIAssertTimer OBJECT-TYPE
SYNTAX     TimeTicks
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
"If pimSmSGIAssertState is 'iAmAssertWinner', this is the time
remaining before the local router next sends a (S,G) Assert
message on this interface.  If pimSmSGIAssertState is
'iAmAssertLoser', this is the time remaining before the
(S,G) Assert state expires.  If pimSmSGIAssertState is
'noInfo', this is zero.  This timer is called the (S,G)
Assert Timer in the PIM-SM specification
[I-D.ietf-pim-sm-v2-new]."
::= { pimSmSGIEntry 8 }

pimSmSGIAssertWinnerAddress OBJECT-TYPE
SYNTAX     InetAddress
MAX-ACCESS read-only
STATUS     current
DESCRIPTION
