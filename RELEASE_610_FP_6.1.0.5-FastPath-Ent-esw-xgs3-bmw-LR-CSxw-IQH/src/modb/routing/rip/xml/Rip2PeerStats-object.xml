<?xml version="1.0"?>
<!DOCTYPE FastPathXML SYSTEM "object.dtd">
<FastPathXML>
<objectxml name="routing" version="1.0" >
<module name="Rip2Peer" >
<key-combo list="Address,Domain"/>
<object name="Address"  >
<core  type="ipv4" key="yes" access="read-write" descr="The IP Address that the peer is using as its source address. Note that on an unnumbered link, this may not be a member of any subnet on the system."/>
<ui  name="Peer IP Address" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.23.4.1.1"/> 
</ui></object>
<object name="Domain"  >
<core  type="string" key="yes" access="read-write" descr="The value in the Routing Domain field in RIP packets received from the peer. As domain suuport is deprecated, this must be zero."/>
<ui  name="Peer Routing Domain" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.23.4.1.2"/> 
</ui></object>
<object name="LastUpdate" depends="Address,Domain" >
<core  type="uint" access="read-only" descr="The value of sysUpTime when the most recent RIP update was received from this system."/>
<ui  name="Last RIP Update Time " style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.23.4.1.3"/> 
</ui></object>
<object name="Version" depends="Address,Domain" >
<core  type="uint" access="read-only" descr="The RIP version number in the header of the last RIP packet received."/>
<ui  name="Last RIP packet's version" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.23.4.1.4"/> 
</ui></object>
<object name="RcvBadPackets" depends="Address,Domain" >
<core  type="uint" access="read-only" descr="The number of RIP response packets from this peer discarded as invalid."/>
<ui  name="RIP responses discarded" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.23.4.1.5"/> 
</ui></object>
<object name="RcvBadRoutes" depends="Address,Domain" >
<core  type="uint" access="read-only" descr="The number of routes from this peer that were ignored because the entry format was invalid."/>
<ui  name="RIP routes ignored" style="plainText">
 <snmp asn-id=".1.3.6.1.2.1.23.4.1.6"/> 
</ui></object>
</module>
</objectxml>
</FastPathXML>
