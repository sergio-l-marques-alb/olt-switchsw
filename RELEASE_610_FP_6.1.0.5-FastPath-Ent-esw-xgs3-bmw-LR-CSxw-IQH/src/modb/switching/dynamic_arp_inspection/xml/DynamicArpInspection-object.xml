<?xml version="1.0"?> 
<!DOCTYPE FastPathXML SYSTEM "object.dtd"> 
<FastPathXML>
<objectxml name="Switching" version="1.0" >
<module name="DAI"  >
<object name="SRCMACVerify"  >
<core  type="enum" xtype="boolean_t" access="read-write" descr="This object specifies dynamic ARP Inspection to verify Source MAC"/>
<ui  name="DAI Source MAC Verify" style="choiceNselect1"  >
<snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.21.1" />
</ui>
</object>
<object name="DSTMACVerify"  >
<core  type="enum" xtype="boolean_t" access="read-write" descr="This object specifies dynamic ARP Inspection to verify Destination MAC"/>
<ui  name="DAI Destination MAC Verify" style="choiceNselect1"  >
 <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.21.2" />
</ui>
</object>
<object name="IPVerify"  >
<core  type="enum" xtype="boolean_t" access="read-write" descr="This object specifies dynamic ARP Inspection to verify IP"/>
<ui  name="DAI IP Verify" style="choiceNselect1"  >
  <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.21.3" />
</ui>
</object>
<object name="Interface" compile_if="IF_STACKING_PACKAGE_PRESENT" >
<core  type="user" xtype="usp_t" key="yes" access="read-write" descr="This object specifies all configurable interfaces for Dynamic ARP Inspection"/>
<ui  name="Unit/Slot/Port" style="inputLine" />
</object>
<object name="Interface" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT" >
<core  type="user" xtype="usp_t" key="yes" access="read-write" descr="This object specifies all configurable interfaces for Dynamic ARP Inspection"/>
<ui  name="Slot/Port" style="inputLine" />
</object>
<!--
<object name="IsValidDAIInterface" depends="Interface" >
<core  type="uint" access="read-only" descr="Specifies if the given interface is a Valid DAI Interface"/>
<ui  name="IsValidDAIInterface" style="plainText" />
</object> -->
<object name="IntfTrust" depends="Interface" >
<core  type="enum" xtype="boolean_t" access="read-write" descr="This object specifies Trust setting for DAI on the interface"/>
<ui  name="DAI Interface Trust" style="choiceNselect1"  >
<snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.21.7.1.1" />
</ui>
</object>
<object name="DAIEnable" depends="Interface" >
<core  type="enum" xtype="boolean_t" access="read-only" descr="This object enables DAI on this Interface"/>
<ui  name="DAI Enable" style="plainText" />
</object>
<object name="IntfRateLimit" depends="Interface" >
<core  type="int" min="L7_DAI_RATE_LIMIT_NONE" max="L7_DAI_RATE_LIMIT_MAX" access="read-write" descr="This object specifies the rate limit value of incoming ARP packets on untrusted interfaces"/>
<ui  name="ARP untrusted Packets Interface Rate Limit" style="inputLine"  >
<snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.21.7.1.2" />
</ui>
</object>

<object name="NoLimit" depends="Interface" >
<core  type="int" default="L7_DAI_RATE_LIMIT_NONE" access="read-write" descr="This object specifies the rate limit value of incoming ARP packets on untrusted interfaces"/>
<ui  name="ARP untrusted Packets Interface No Limit" style="inputLine"  >
<snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.21.7.1.4" />
</ui>
</object>

<!--
<object name="IntfRateLimitEnable" depends="Interface" >
<core  type="enum" xtype="enable_t" access="read-write" descr="This object specifies the enable/disable rate limit of incoming ARP packets on untrusted interfaces"/>
<ui  name="IntfRateLimitEnable" style="inputLine" />
</object>-->
<object name="IntfBurstInterval" depends="Interface" >
<core  type="uint" min="L7_DAI_BURST_INTERVAL_MIN" max="L7_DAI_BURST_INTERVAL_MAX" access="read-write" descr="This object specifies the burst interval value of incoming ARP packets on untrusted interfaces"/>
<ui  name="ARP Packets Interface Burst Interval" style="inputLine"  >
<snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.21.7.1.3" />
</ui>
</object>
<object name="VlanIndex"  >
<core  type="uint" key="yes" min="L7_DOT1Q_MIN_VLAN_ID" max="L7_DOT1Q_MAX_VLAN_ID" access="read-write" descr="This object specifies the VLAN-ID or other identifier refering to this VLAN."/>
<ui  name="DAI Vlan Index" style="inputLine" />
</object>
<object name="VLANEnable" depends="VlanIndex" >
<core  type="enum" xtype="boolean_t" access="read-write" descr="This object specifies whether dynamic ARP Inspection is enabled or disabled on a VLAN"/>
<ui  name="DAI VLAN Enable" style="choiceNselect1" />
</object>
<object name="VlanLoggingEnable" depends="VlanIndex" >
<core  type="enum" xtype="enable_t" access="read-write" descr="This object specifies whether DAI logging is enabled or disabled on a VLAN"/>
<ui  name="VLAN Logging Enable" style="choiceNselect1" >
  <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.21.4.1.3" />
</ui>
</object>
<object name="VlanArpAcl" depends="VlanIndex" >
<core  type="string" access="read-write" descr="This object specifies ARP ACL Name for a vlan" validation="valiadate_arpAcl"/>
<ui  name="VLAN ARP ACL" style="inputLine" >
 <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.21.4.1.4" />
</ui>
</object>
<object name="VlanArpAclStaticFlag" depends="VlanIndex" >
<core  type="enum" xtype="enable_t" access="read-write" descr="This object specifies the ARP ACL static flag for a vlan"/>
<ui  name="VLAN ARP ACL Static Flag" style="choiceNselect1" >
  <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.21.4.1.5" />
</ui>
</object>
<!--
<object name="VLANRangeEnable"  >
<core  type="enum" xtype="enable_t" access="write-only" descr="Dynamic ARP Inspection is enabled or disabled on a VLAN"/>
<ui  name="VLANRangeEnable" style="inputLine" />
</object> -->
<object name="VlanStatsClear"  >
<core  type="uint" access="write-only" descr="This object clears DAI statistics on all vlans"/>
<ui  name="Clear DAI Vlan Statistics" style="inputLine"  >
<snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.21.5" />
</ui>
</object>
</module>

<module name = "DAISTATS"> 

<object name="VlanIndex"  >
<core  type="uint" key="yes" min="L7_DOT1Q_MIN_VLAN_ID" max="L7_DOT1Q_MAX_VLAN_ID" access="read-write" descr="This object specifies the VLAN-ID or other identifier refering to this VLAN."/>
<ui  name="DAI Stats VLAN Index" style="inputLine" />
</object>

<object name="Forwarded" depends="VlanIndex" >
<core  type="uint" access="read-only" descr="This object specifies the number of packets forwarded through DAI"/>
<ui  name="DAI Packets Forwarded" style="plainText" />
</object>

<object name="Dropped" depends="VlanIndex" >
<core  type="uint" access="read-only" descr="This object specifies the number of packets dropped through DAI"/>
<ui  name="DAI Packets Dropped" style="plainText" />
</object>

<object name="dhcpDrops" depends="VlanIndex" >
<core  type="uint" access="read-only" descr="This object specifies the number of packets dropped through DHCP snooping"/>
<ui  name=" DHCP Packets Dropped" style="plainText" />
</object>

<object name="dhcpPermits" depends="VlanIndex" >
<core  type="uint" access="read-only" descr="This object specifies the number of packets permited through DHCP snooping"/>
<ui  name="DHCP Packets Permitted" style="plainText" />
</object>

<object name="aclDrops" depends="VlanIndex" >
<core  type="uint" access="read-only" descr="This object specifies the number of packets dropped through DAI acl"/>
<ui  name="DAI ACL Packets Dropped" style="plainText" />
</object>


<object name="aclPermits" depends="VlanIndex" >
<core  type="uint" access="read-only" descr="This object specifies the number of packets permitted through DAI acl"/>
<ui  name="DAI ACL Packets Permitted" style="plainText" />
</object>

<object name="sMacFailures" depends="VlanIndex" >
<core  type="uint" access="read-only" descr="This object specifies the number of packets dropped  through DAI sMacFailure"/>
<ui  name="DAI sMacFailure Dropped Packets" style="plainText" />
</object>

<object name="dMacFailures" depends="VlanIndex" >
<core  type="uint" access="read-only" descr="This object specifies the number of packets dropped through DAI dMacFailures"/>
<ui  name="DAI dMacFailure Dropped Packets" style="plainText" />
</object>

<object name="ipValidFailures" depends="VlanIndex" >
<core  type="uint" access="read-only" descr="This object specifies the number of packets dropped through DAI ipValidFailures"/>
<ui  name="DAI ipValidFailures Dropped Packets" style="plainText" />
</object>
</module>

<module name="ARPACL"  >

<object name="ARPACL"  >
<core  type="string" key="yes" access="read-write" descr="ARP ACL entry"/>
<ui  name="ARPACL" style="inputLine" >
<snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.22.1.1.1"/>
</ui>
</object>

<object name="ROWSTATUS" depends="ARPACL" >
<core  type="row-status" xtype = "L7_ROW_STATUS_t" access="read-write" descr="This object creates and Deletes an ARP ACL"/>
<ui  name="ARP ACL Row Status" style="inputLine" />
</object>

</module>

<module name="ARPAclRule"  >
<key-combo list="ARPACL,RuleIP,RuleMAC" />

<object name="ARPACL"  >
<core  type="string" key="yes" access="read-write" descr="This object specifies ARP ACL entry"/>
<ui  name="ARP ACL Entry" style="inputLine" />
</object>

<object name="RuleIP" >
<core  type="ipv4" key="yes" access="read-write" descr="This object adds IP of the Rule to ARP ACL"/>
<ui  name="Rule IP ADD" style="inputLine" >
<snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.22.2.1.1"/>
</ui>
</object>

<object name="RuleMAC" >
<core  type="string" key="yes" access="read-write" descr="This object adds MAC of the Rule to ARP ACL"/>
<ui  name="Rule MAC ADD" style="inputLine" >
<snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.22.2.1.2"/>
</ui>
</object>
