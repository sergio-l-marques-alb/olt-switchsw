            <object depends="Interface,SnoopingProtocol,SnoopingIntfVlanID" name="intfMulticastRouterVlanMode">
                <core descr="This enables or disables Multicast Routing on the selected interface and vlan" xtype="enable_t" access="read-write" type="enum"/>
                <ui style="choiceNselect1" name="Interface Mcast Router Vlan Mode">
                </ui>
            </object>
            <!-- I dont think this object is required 
            &lt;object depends=&quot;Interface,SnoopingProtocol&quot; name=&quot;SnoopingIntfIndex&quot;&gt;
                &lt;core descr=&quot;The port number to be selected. Each port maps to an interface&quot; access=&quot;read-write&quot; type=&quot;uint&quot;/&gt;
                &lt;ui style=&quot;inputLine&quot; name=&quot;SnoopingIntfIndex&quot;&gt;
                    &lt;snmp asn-id=&quot;1.3.6.1.2.1.2.2.1.1&quot;/&gt;
                &lt;/ui&gt;
            &lt;/object&gt; -->
            <object depends="Interface,SnoopingProtocol" name="SnoopingIntfAdminMode">
                <core descr="This enables or disables IGMP Snooping on a selected interface" default="FD_IGMP_SNOOPING_INTF_MODE" xtype="enable_t" access="read-write" type="enum"/>
                <ui style="choiceNselect1" name="Snooping Interface Admin Mode">
                    <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.6.1.1.1"/>
                </ui>
            </object>
            <object depends="SnoopingProtocol" name="SnoopingGlobalAdminMode">
                <core descr="This enables or disables IGMP Snooping on all interfaces" default="FD_IGMP_SNOOPING_ADMIN_MODE" xtype="enable_t" access="write-only" type="enum"/>
                <ui style="inputLine" name="Snooping Global Admin Mode">
                    <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.6.1.1.1"/>
                </ui>
            </object>
           <object depends="Interface,SnoopingProtocol" name="SnoopingIntfGroupMembershipInterval">
                <core descr="This object specifies the amount of time in seconds that a switch will wait for a report from a particular group on the selected interface before deleting the interface from the entry. This value must be greater than SnoopingIntfMaxResponseTime." access="read-write" type="uint"/>
                <ui style="inputLine" name="Snooping Interface Group Membership Interval">
                    <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.7.1.1.1"/>
                </ui>
            </object>
            <object depends="SnoopingProtocol" name="SnoopingGlobalGroupMembershipInterval">
                <core descr="This object specifies the amount of time in seconds that a switch will wait for a report from a particular group on the selected interface before deleting the interface from the entry. This value must be greater than SnoopingIntfMaxResponseTime." default="FD_IGMP_SNOOPING_GROUP_MEMBERSHIP_INTERVAL" max="L7_IGMP_SNOOPING_MAX_GROUP_MEMBERSHIP_INTERVAL" min="L7_IGMP_SNOOPING_MIN_GROUP_MEMBERSHIP_INTERVAL" access="write-only" type="uint"/>
                <ui style="inputLine" name="Snooping Global Group Membership Interval">
                    <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.2.8.7.1.1.1"/>
                </ui>
