<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "object.dtd" >


<FastPathXML>
    <objectxml version="1.0" name="Switching">
        <module key-method="individual" name="dot1dBase">
            <object name="TrafficClassesEnabled">
                <core descr="The value true(1) indicates that Traffic Classes are enabled on this bridge. When false(2), the bridge operates with a single priority level for all traffic" xtype="boolean_t" access="read-write" type="enum"/>
                <ui style="choiceNselect1" name="Traffic Classes Enabled">
                    <snmp asn-id="1.3.6.1.2.1.17.6.1.1.2"/>
                </ui>
            </object>
            <object name="GmrpStatus" compile_if="IF_GVRP_PACKAGE_PRESENT">
                <core descr="This object specifies the administrative status requested by management for GMRP. The value enabled(1) indicates that GMRP should be enabled on this device, in all VLANs, on all ports for which it has not been specifically disabled." xtype="enable_t" access="read-write" type="enum"/>
                <ui style="choiceNselect1" name="GMRP Status">
                    <snmp asn-id="1.3.6.1.2.1.17.6.1.1.3"/>
                </ui>
            </object>
            <object name="GvrpStatus" compile_if="IF_GVRP_PACKAGE_PRESENT">
                <core descr="This object specifies the state of GVRP operation. The value enabled(1) indicates that GVRP is enabled on this port,as long as Global GvrpStatus is also enabled for this device.When disabled(2) but global GVRP Status is still enabled for the device" xtype="enable_t" access="read-write" type="enum"/>
                <ui style="choiceNselect1" name="GVRP Status">
                </ui>
            </object>  
            <object name="NumPorts">
                <core descr="This object specifies the number of ports controlled by this bridging entity." access="read-only" type="uint"/>
                <ui style="plainText" name="Number of Ports">
                    <snmp asn-id=".1.3.6.1.2.1.17.1.2"/>
                </ui>
            </object>
            <object name="dot1dBaseType">
                <core descr="This object indicates what type of bridging this bridge can perform. If a bridge is actually performing a certain type of bridging this will be indicated by entries in the port table for the given type" access="read-only" type="uint"/>
                <ui style="plainText" name="dot1d Base Type">
                    <snmp asn-id=".1.3.6.1.2.1.17.1.3"/>
                </ui>
            </object>
            <object name="BridgeAddress">
                <core descr="This object specifes the MAC address used by this bridge when it must referred to in a unique fashion. It is recommended that this be the numerically smallest MAC address of all ports that belong to this bridge. However it is only required to be unique. When concatenated w" access="read-only" type="mac"/>
                <ui style="plainText" name="Bridge Address">
                    <snmp asn-id=".1.3.6.1.2.1.17.1.1"/>
                </ui>
            </object>

            <object name="Dot1dDeviceCapabilities">
                <core descr="This object is used to get device capabilities" access="read-write" type="string"/>
                <ui style="inputLine" name="Dot1d Device Capabilities">
                <snmp asn-id="1.3.6.1.2.1.17.6.1.1.4.1.1"/>
</ui>
            </object>
            <!-- This is duplicate object of first object.Actually We have taken this object from next module 
            <object name="Dot1dGmrpStatus">
                <core descr="the administrative status requested by management for GMRP" xtype="enable_t" access="read-write" type="enum"/>
                <ui style="inputLine" name="Dot1dGmrpStatus"/>
            </object> -->
            <!-- We dont have support for this object.that means application call not yet implimented.WE have only dummy usmdb  
            <object name="Dot1dTpLearnedEntryDiscards"> 
                <core descr="Learned entry discards" access="read-write" type="uint"/>
                <ui style="inputLine" name="Dot1dTpLearnedEntryDiscards">
                <snmp asn-id="1.3.6.1.2.1.17.4.1"/>
</ui>
            </object>-->
            <object name="Dot1dTpAgingTime">
                <core descr="This object specifies the Aging time" access="read-write" min="L7_FDB_MIN_AGING_TIMEOUT" max="L7_FDB_MAX_AGING_TIMEOUT" default="FD_FDB_DEFAULT_AGING_TIMEOUT" type="uint"/>
                <ui style="inputLine" name="Dot1d Tp Aging Time">
                <snmp asn-id=".1.3.6.1.2.1.17.4.2"/>
</ui>
            </object>

        </module>
        <module key-method="individual" name="Dot1d">
            <object name="Dot1dBasePort" compile_if="IF_STACKING_PACKAGE_PRESENT">
                <core descr="This object gives list of information for each port of bridge." xtype="usp_t" key="yes" access="read-write" type="user"/>
                <ui style="inputLine" name="Unit/Slot/Port">
                <snmp asn-id=".1.3.6.1.2.1.17.1.4.1.1"/>
                </ui>
            </object>
            <object name="Dot1dBasePort" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT">
                <core descr="This instance gives the list of information for each port of bridge." xtype="usp_t" key="yes" access="read-write" type="user"/>
                <ui style="inputLine" name="Slot/Port">
                <snmp asn-id=".1.3.6.1.2.1.17.1.4.1.1"/>
                </ui>
            </object>
            <object depends="Dot1dBasePort" name="Dot1dBasePortIfIndex">
                <core descr="This instance gives the ifIndex associated with the internal interface number." access="read-only" type="uint"/>
                <ui style="plainText" name="Dot1d Base Port Interface Index">
                <snmp asn-id=".1.3.6.1.2.1.17.1.4.1.2"/>
                </ui>
            </object>
            <object depends="Dot1dBasePort" name="Dot1dBasePortCircuit">
                <core descr="This instacne specifies the name of an object instance unique to this port." access="read-only" type="string"/>
                <ui style="plainText" name="Dot1d Base Port Circuit"/>
            </object>
            <object depends="Dot1dBasePort" name="Dot1dBasePortDelayExceededDiscards">
                <core descr="This instance gives the number of frames discarded by this port due to delay through the bridge." access="read-only" type="uint"/>
                <ui style="plainText" name="Dot1d Base Port Delay Exceeded Discards"/>
            </object>
            <object depends="Dot1dBasePort" name="Dot1dBasePortMtuExceededDiscards">
                <core descr="This instance gives the number of frames discarded by this port due to excessive size" access="read-only" type="uint"/>
                <ui style="plainText" name="Dot1d Base Port Mtu Exceeded Discards"/>
            </object>
            <object depends="Dot1dBasePort" name="Dot1dPortCapabilities">
                <core descr="This object specifies the optional parts of IEEE 802.1D that are implemented and are manageable" access="read-only" type="string"/>
                <ui style="plainText" name="Dot1d Port Capabilities"/>
            </object>
            <object depends="Dot1dBasePort" name="Dot1dPortDefaultUserPriority">
                <core descr="This instance gives the default ingress User Priority" access="read-write" type="uint" min="L7_DOT1P_MIN_PRIORITY" max="L7_DOT1P_MAX_PRIORITY" default="L7_DOT1P_MIN_PRIORITY"/>
                <ui style="inputLine" name="Dot1d Port Default User Priority"/>
            </object>
            <object depends="Dot1dBasePort" name="Dot1dPortGarpJoinTime" compile_if="IF_GVRP_PACKAGE_PRESENT">
                <core descr="This object gives the GARP Join time, in centiseconds" access="read-only" type="uint"/>
                <ui style="plainText" name="Dot1d Port GARP Join Time"/>
            </object>
            <object depends="Dot1dBasePort" name="Dot1dPortGarpLeaveAllTime" compile_if="IF_GVRP_PACKAGE_PRESENT">
                <core descr="This object specifies the GARP LeaveAll time" access="read-only" type="uint"/>
                <ui style="plainText" name="Dot1d Port GARP Leave All Time"/>
            </object>
            <object depends="Dot1dBasePort" name="Dot1dPortGarpLeaveTime" compile_if="IF_GVRP_PACKAGE_PRESENT">
                <core descr="This instance specifies the GARP Leave time" access="read-only" type="uint"/>
                <ui style="plainText" name="Dot1d Port GARP Leave Time"/>
            </object>
            <object depends="Dot1dBasePort" name="Dot1dPortGmrpStatus" compile_if="IF_GVRP_PACKAGE_PRESENT">
                <core descr="This object specifies the Gmrp status" xtype="enable_t" access="read-only" type="enum"/>
                <ui style="plainText" name="Dot1d Port GMRP Status"/>
            </object>
            <object depends="Dot1dBasePort" name="Dot1dPortNumTrafficClasses">
                <!-- This is not read-write.Fastpath does not support this filed at run time --> 
                <core descr="This object specifies the number of traffic classes" max="8" min="1" access="read-only" type="uint"/>
                <ui style="plainText" name="Dot1d Port Nummber of  Traffic Classes"/>
            </object>
      <object name="Dot1dTpPort">
        <core access="read-only" descr=" This object specifies the port number of the port for which this entry contains Transparent bridging management information." key="yes" type="uint"/>
        <ui name="Dot1d Tp Port" style="plainText">
          <snmp asn-id=".1.3.6.1.2.1.17.4.4.1.1"/>
        </ui>
      </object>

            <object depends="Dot1dTpPort" name="Dot1dTpPortInFrames">
                <core descr="This object specifies the, port In frames" access="read-only" type="uint"/>
                <ui style="plainText" name="Dot1d Tp Port In Frames">
                <snmp asn-id=".1.3.6.1.2.1.17.4.4.1.3"/>
</ui>
            </object>
            <object depends="Dot1dTpPort" name="Dot1dTpPortMaxInfo">
                <core descr="This object specifies the Max info of port" access="read-only" type="uint"/>
                <ui style="plainText" name="Dot1d Tp Port Max Info">
                <snmp asn-id=".1.3.6.1.2.1.17.4.4.1.2"/>
</ui>
            </object>
            <object depends="Dot1dTpPort" name="Dot1dTpPortOutFrames">
                <core descr="This object specifies the,Port out frames" access="read-only" type="uint"/>
                <ui style="plainText" name="Dot1d Tp Port Out Frames">
                <snmp asn-id=".1.3.6.1.2.1.17.4.4.1.4"/>
</ui>
            </object>
      <object depends="Dot1dTpPort" name="Dot1dTpPortInDiscards">
        <core access="read-only" descr="This object specifies the count of valid frames received which were  discarded (i.e., filtered) by the Forwarding Process." type="uint"/>
        <ui name="Dot1d Tp Port In Discards" style="plainText">
          <snmp asn-id=".1.3.6.1.2.1.17.4.4.1.5"/>
        </ui>
      </object>

            <object depends="Dot1dBasePort" name="timeSinceCountersLastCleared">
                <core descr="This object gives the time elapsed since the counters were cleared" access="read-only" type="string"/>
                <ui style="plainText" name="time Since Counters Last Cleared"/>
            </object>
            <object depends="Dot1dBasePort" name="IntfStatsClear">
                <core descr="This object clears stats on a given interface" xtype="enable_t" access="write-only" type="enum"/>
                <ui style="inputLine" name="Clear Intferface Statistics ">
            <snmp asn-id="1.3.6.1.4.1.4413.1.1.1.3.6" />
            </ui>
            </object>
            <!-- Right now we are not handling stats  for all case. They are adding support for ALL CASE in TOOL 
            <object name="StatsAllClear">
                <core descr="Clear all statistics" xtype="enable_t" access="write-only" type="enum"/>
                <ui style="plainText" name="StatsAllClear"/> 
            </object>-->
        </module>

        <!-- This module usmdbs are not supported.But still we generate the code for MIB compliance -->    
        <module name = "dot1dPortOutboundAccessPriorityEntry">
        <key-combo list="Dot1dBasePort,dot1dOutboundRegenUserPriority" />

            <object name="Dot1dBasePort" compile_if="IF_STACKING_PACKAGE_PRESENT">
                <core descr="This object lists  information for each port of bridge." xtype="usp_t" key="yes" access="read-write" type="user"/>
                <ui style="inputLine" name="Unit/Slot/Port"/>
            </object>

            <object name="Dot1dBasePort" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT">
                <core descr="This object lists  information for each port of bridge." xtype="usp_t" key="yes" access="read-write" type="user"/>
                <ui style="inputLine" name="Slot/Port"/>
            </object>

            <object  name="dot1dOutboundRegenUserPriority">
                <core descr="This object specifies the outbound user priority" key="yes" access="read-write" type="uint"/>
                <ui style="inputLine" name="dot1d Out Bound Regen User Priority"/>
            </object>

            <object depends="Dot1dBasePort,dot1dOutboundRegenUserPriority" name="dot1dPortOutboundAccessPriority">
                <core descr="This instances specifies the access priority" access="read-only" type="uint"/>
                <ui style="plainText" name="dot1d Port Out Bound Access Priority">
                <snmp asn-id="1.3.6.1.2.1.17.6.1.2.2.1.2"/>
</ui>
            </object>

        </module>
        <!-- This module usmdbs are not supported.But still we generate the code for MIB compliance -->    
        <module name = "dot1dUserPriorityRegenEntry">
        <key-combo list="Dot1dBasePort,dot1dUserPriority" />

            <object name="Dot1dBasePort" compile_if="IF_STACKING_PACKAGE_PRESENT">
                <core descr="This object lists information for each port of bridge." xtype="usp_t" key="yes" access="read-write" type="user"/>
                <ui style="inputLine" name="Unit/Slot/Port"/>
            </object>

            <object name="Dot1dBasePort" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT">
                <core descr="This object lists information for each port of bridge." xtype="usp_t" key="yes" access="read-write" type="user"/>
                <ui style="inputLine" name="Slot/Port"/>
            </object>

            <object  name="dot1dUserPriority">
                <core descr="This object specifies the user priority" key="yes" access="read-write" type="uint"/>
                <ui style="inputLine" name="dot1d User Priority"/>
            </object>

            <object depends="Dot1dBasePort,dot1dUserPriority" name="Dot1dRegenUserPriority">
                <core descr="This instance specifies the regen user priority" access="read-write" type="uint"/>
                <ui style="inputLine" name="Dot1d Regen User Priority">
                <snmp asn-id="1.3.6.1.2.1.17.6.1.2.2.1.2"/>
</ui>
            </object>

        </module>
        <!-- This module usmdbs are not supported.But still we generate the code for MIB compliance -->    
        <module key-method="individual" name="Dot1dStatic">
        <key-combo list="dot1dStaticAddress,dot1dStaticReceivePort" />
            <object name="dot1dStaticAddress">
                <core descr="This object gives the static address" key="yes" access="read-write" type="string"/>
                <ui style="inputLine" name="dot1d Static Address"/>
            </object>
            <object name="dot1dStaticReceivePort" compile_if="IF_STACKING_PACKAGE_PRESENT">
                <core descr="This object specifies the receive static port" xtype="usp_t" key="yes" access="read-write" type="user"/>
                <ui style="inputLine" name="Unit/Slot/Port"/>
            </object>
            <object name="dot1dStaticReceivePort" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT">
                <core descr="This object specifies the receive static port" xtype="usp_t" key="yes" access="read-write" type="user"/>
                <ui style="inputLine" name="Slot/Port"/>
            </object>
            <object depends="dot1dStaticAddress,dot1dStaticReceivePort" name="Dot1dStaticAllowedToGo">
                <core descr="This instance specifies the  set of ports to which frames received from a specific port and destined for a specific MAC Address, are allowed to be forwarded.
" access="read-write" type="string"/>
                <ui style="inputLine" name="Dot1d Static Allowed To Go">
                <snmp asn-id="1.3.6.1.2.1.17.5.1.1.3"/>
</ui>
            </object>
            <object depends="dot1dStaticAddress,dot1dStaticReceivePort" name="Dot1dStaticStatus">
                <core descr="This instance gets static status" access="read-write" type="uint"/>
                <ui style="inputLine" name="Dot1d Static Status">
                <snmp asn-id="dot1dStaticStatus"/>
</ui>
            </object>
        </module>
        <module key-method="individual" name="Dot1dTpFdb">
            <object name="dot1dTpFdbAddress">
                <core descr="This instacne specifies Fdb address" key="yes" access="read-write" type="mac"/>
                <ui style="inputLine" name="dot1d Tp Fdb Address">
                <snmp asn-id=".1.3.6.1.2.1.17.4.3.1.1"/>
</ui>
            </object>
            <object depends="dot1dTpFdbAddress" name="Dot1dTpFdbPort" compile_if="IF_STACKING_PACKAGE_PRESENT">
                <core descr="This instacne specifies the Fdb port" xtype="usp_t" access="read-only" type="user"/>
                <ui style="plainText" name="Unit/Slot/Port">
                <snmp asn-id="1.3.6.1.2.1.17.4.3.1.2"/>
</ui>
            </object>
            <object depends="dot1dTpFdbAddress" name="Dot1dTpFdbPort" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT">
                <core descr="This instacne specifies the Fdb port" xtype="usp_t" access="read-only" type="user"/>
                <ui style="plainText" name="Slot/Port">
                <snmp asn-id="1.3.6.1.2.1.17.4.3.1.2"/>
</ui>
            </object>
            <object depends="dot1dTpFdbAddress" name="Dot1dTpFdbStatus">
                <core descr="This object gives the Fdb status" access="read-only" type="uint"/>
                <ui style="plainText" name="Dot1d Tp Fdb Status">
                <snmp asn-id=".1.3.6.1.2.1.17.4.3.1.3"/>
</ui>
            </object>
        </module>
        <module key-method="individual" name="Dot1dTrafficClass">
        <key-combo list="dot1dTrafficClassPort,dot1dTrafficClassPriority" />

            <object name="dot1dTrafficClassPort" compile_if="IF_STACKING_PACKAGE_PRESENT">
                <core descr="This object specifies the traffic class port" xtype="usp_t" key="yes" access="read-write" type="user"/>
                <ui style="inputLine" name="Unit/Slot/Port"/>
            </object>

            <object name="dot1dTrafficClassPort" compile_if="IF_STACKING_PACKAGE_NOT_PRESENT">
                <core descr="This object specfies the traffic class port" xtype="usp_t" key="yes" access="read-write" type="user"/>
                <ui style="inputLine" name="Slot/Port"/>
            </object>

            <object name="dot1dTrafficClassPriority">
                <core descr="This object specfies the traffic class priority" key="yes" access="read-write" type="uint"/>
                <ui style="inputLine" name="dot1d Traffic Class Priority"/>
            </object>

            <object depends="dot1dTrafficClassPort,dot1dTrafficClassPriority" name="Dot1dTrafficClass">
                <core descr="This object specfies the traffic class" access="read-write" dynamic="yes" type="uint" min="L7_DOT1P_MIN_TRAFFIC_CLASS" max="L7_DOT1P_MAX_TRAFFIC_CLASS" />
                <ui style="choiceNselect1" name="Dot1d Traffic Class">
                <snmp asn-id="1.3.6.1.2.1.17.6.1.2.3.1.2"/>
</ui>
            </object>
            <object name="dot1dTrafficClassRestoreDefaults" depends="dot1dTrafficClassPort">
                <core descr="This object restores the trust Mode for specified interface to default" access="write-only" type="uint"/>
                <ui style="inputLine" name="Dot1d Traffic Class Restore Defaults" >
                </ui>
            </object>

        </module>
         <module name="Dot1dTrafficClassGlobal">
            <object name="dot1dTrafficClassPriority">
                <core descr="This object specfies the traffic class priority" key="yes" access="read-write" type="uint"/>
                <ui style="inputLine" name="Dot1d Traffic Class Priority"/>
            </object>

            <object name="Dot1dTrafficClass" depends="dot1dTrafficClassPriority">
                <core descr="This object specfies the traffic class" access="read-write" dynamic="yes" type="uint" min="L7_DOT1P_MIN_TRAFFIC_CLASS" max="L7_DOT1P_MAX_TRAFFIC_CLASS" />
                <ui style="choiceNselect1" name="Dot1d Traffic Class">
                <snmp asn-id="1.3.6.1.2.1.17.6.1.2.3.1.2"/>
</ui>
            </object>
            <object name="dot1dTrafficClassRestoreDefaults">
                <core descr="This object restores teh  trust Mode for specified interface to default" access="write-only" type="uint"/>
                <ui style="inputLine" name="dot1d Traffic Class Restore Defaults" >
                </ui>
            </object>
        </module>
    </objectxml>
</FastPathXML>
