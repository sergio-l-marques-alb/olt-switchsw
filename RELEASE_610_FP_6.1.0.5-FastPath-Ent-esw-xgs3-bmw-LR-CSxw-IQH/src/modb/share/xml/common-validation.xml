<?xml version='1.0'?>
<!DOCTYPE FastPathXML SYSTEM  "misc.dtd" >

<FastPathXML>
    <misc version="1.0">
    
       <validation name="validate_uint">
            <and>
                <regexpand error="INVALID_UINT">^([0-9]{1,9}|[1-3][0-9]{9}|4([01][0-9]{8}|2([0-8][0-9]{7}|9([0-3][0-9]{6}|4([0-8][0-9]{5}|9([0-5][0-9]{4}|6([0-6][0-9]{3}|7([01][0-9]{2}|2([0-8][0-9]|9[0-5])))))))))$</regexpand>
            </and>
        </validation>

    <!--
        <validation name="validate_uint">
            <and>
                <regexpand error="INVALID_UINT">^[0-9]{1,10}$</regexpand>
            </and>
        </validation>
        -->
        <validation name="validate_int">
            <and>
                <regexpand error="INVALID_INT">^.{1,11}$</regexpand>
                <regexpand error="INVALID_INT">^(([-+]?0*([0-9]{1,9}|1[0-9]{9}|2(0[0-9]{8}|1([0-3][0-9]{7}|4([0-6][0-9]{6}|7([0-3][0-9]{5}|4([0-7][0-9]{4}|8([0-2][0-9]{3}|3([0-5][0-9]{2}|6([0-3][0-9]|4[0-7]))))))))))|([-]0*(2147483648)))$</regexpand>
            </and>
        </validation>
        <validation name="validate_alpha_num">
            <and>
                 <regexpand error="INVALID_STRING_ALPHA_NUM">^[a-zA-Z0-9]+$</regexpand> 
            </and>
        </validation>
        <validation name="validate_string">
            <and>
                <regexpand error="INVALID_STRING">^.+$</regexpand>
            </and>
        </validation>
        <validation name="validate_string_t">
            <and>
               <regexpand error="INVALID_STRING">.*</regexpand>
            </and>
        </validation>
        <validation name="validate_ipv4">
            <and>
                <regexpand error="NOT_DECIMAL_DOTTED_FORMAT">^([0]{0,2}[1-9]|[0]?[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(\\.([01]?[0-9][0-9]?|2[0-4][0-9]|25[0-5])){3}$</regexpand>                
            </and>
        </validation>
        
        <validation name="validate_newipv4">
            <and>
                <regexpand error="NOT_DECIMAL_DOTTED_FORMAT">^([01]?[0-9][0-9]?|2[0-4][0-9]|25[0-5])(\\.([01]?[0-9][0-9]?|2[0-4][0-9]|25[0-5])){3}$</regexpand>
            </and>
        </validation>
        
        <validation name="validate_rtoipv4_t">
            <and>
                <regexpand error="NOT_DECIMAL_DOTTED_FORMAT">^((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$</regexpand>
            </and>
        </validation>
        <validation name="validate_formatipv4_t">
            <and>
                <regexpand error="NOT_DECIMAL_DOTTED_FORMAT">^((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$</regexpand>
            </and>
        </validation>
        <validation name="validate_ipv6">
            <and>
            
             <or error="INVALID_IPv6_ADDRESS">

                 <!-- x:x:x:x:x:x:x:x -->
                 <regexpor>^([0-9a-fA-F]{1,4})(\\:([0-9a-fA-F]{1,4})){7}$</regexpor>
                 <!-- x::x/ x::x:x/ x::x:x:x/ x::x:x:x:x/ x::x:x:x:x:x / x::x:x:x:x:x:x -->
                 <regexpor>^(([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,6})$</regexpor>
                 <!--  x:x::x/ x:x::x:x/x:x::x:x:x/x:x::x:x:x:x/ x:x::x:x:x:x:x   -->
                 <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,5}$</regexpor>
                 <!--  x:x:x::x/ x:x:x::x:x/x:x:x::x:x:x/x:x:x::x:x:x:x  -->
                 <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,4}$</regexpor>
                 <!--  x:x:x:x::x/ x:x:x:x::x:x/x:x:x:x::x:x:x -->
                 <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,3}$</regexpor>
                 <!--  x:x:x:x:x::x/ x:x:x:x:x::x:x -->
                 <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,2}$</regexpor>
                  <!-- x:x:x:x:x:x::x -->
                 <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4}))$</regexpor>

                 <!-- x:: / x:x:: / x:x:x:: / x:x:x:x:: / x:x:x:x:x:: / x:x:x:x:x:x:: / x:x:x:x:x:x:x:: -->
                 <regexpor>^([0-9a-fA-F]{1,4})(\\:([0-9a-fA-F]{1,4})){0,6}(\\:\\:)$</regexpor>
            </or>
            <regexpand error="INVALID_IPv6_ADDRESS">[^0000:0000:0000:0000:0000:0000]</regexpand>
            </and>
</validation>
                  
        <validation name="validate_mac">
            <and>
                <regexpand error="INVALID_MAC_ADDRESS">^([0-9a-fA-F][0-9a-fA-F]:){5}([0-9a-fA-F][0-9a-fA-F])$</regexpand>
            </and>
        </validation>
        <validation name="validate_local_mac">
            <and>
                <regexpand error="INVALID_LOCAL_MAC_ADDRESS">^([0-9a-fA-F][26aAeE]:)([0-9a-fA-F][0-9a-fA-F]:){4}([0-9a-fA-F][0-9a-fA-F])$</regexpand>
            </and>
        </validation> 
         <validation name="validate_tr069PeriodicTime">
            <and>
                <regexpand error="INVALID_TR069_PERIODIC_TIME">^([0-9]{4}-)([0-9][0-9]-)([0-9][0-9][T])([0-9][0-9]:)([0-9][0-9]:)([0-9][0-9])$</regexpand>
            </and>
        </validation>
        <validation name="validate_ipv4_ipv6">
            <or error="INVALID_IP4_IPV6">
                <and>
                    <regexpand error="NOT_DECIMAL_DOTTED_FORMAT">^((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$</regexpand>
                    <regexpand error="INVALID_IPv4_ADDRESS_ALL_ZEROS">[^0.0.0.0]</regexpand>
                </and>
                <and>
                     <or error="INVALID_IPv6_ADDRESS">
                            <!-- x:x:x:x:x:x:x:x -->
                            <regexpor>^([0-9a-fA-F]{1,4})(\\:([0-9a-fA-F]{1,4})){7}$</regexpor>
                            <!-- x::x/ x::x:x/ x::x:x:x/ x::x:x:x:x/ x::x:x:x:x:x / x::x:x:x:x:x:x -->
                            <regexpor>^(([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,6})$</regexpor>
                            <!--  x:x::x/ x:x::x:x/x:x::x:x:x/x:x::x:x:x:x/ x:x::x:x:x:x:x   -->
                            <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,5}$</regexpor>
                            <!--  x:x:x::x/ x:x:x::x:x/x:x:x::x:x:x/x:x:x::x:x:x:x  -->
                            <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,4}$</regexpor>
                            <!--  x:x:x:x::x/ x:x:x:x::x:x/x:x:x:x::x:x:x -->
                            <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,3}$</regexpor>
                            <!--  x:x:x:x:x::x/ x:x:x:x:x::x:x -->
                            <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,2}$</regexpor>
                            <!-- x:: / x:x:: / x:x:x:: / x:x:x:x:: / x:x:x:x:x:: / x:x:x:x:x:x:: / x:x:x:x:x:x:x:: -->
                            <regexpor>^([0-9a-fA-F]{1,4})(\\:([0-9a-fA-F]{1,4})){0,6}(\\:\\:)$</regexpor>       <!-- x:x:x:x:x:x::x -->
                            <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4}))</regexpor>
                     </or>

                    <regexpand error="INVALID_IPv6_ADDRESS">[^0000:0000:0000:0000:0000:0000]</regexpand>
                </and>
            </or>
        </validation>
        <validation name="validate_ipv4_ipv6_with_zeros">
            <or error="INVALID_IP4_IPV6">
                <and>
                    <regexpand error="NOT_DECIMAL_DOTTED_FORMAT">^((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$</regexpand>
                </and>
                <and>
                     <or error="INVALID_IPv6_ADDRESS">

                            <!-- x:x:x:x:x:x:x:x -->
                            <regexpor>^([0-9a-fA-F]{1,4})(\\:([0-9a-fA-F]{1,4})){7}$</regexpor>
                            <!-- x::x/ x::x:x/ x::x:x:x/ x::x:x:x:x/ x::x:x:x:x:x / x::x:x:x:x:x:x -->
                            <regexpor>^(([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,6})$</regexpor>
                            <!--  x:x::x/ x:x::x:x/x:x::x:x:x/x:x::x:x:x:x/ x:x::x:x:x:x:x   -->
                            <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,5}$</regexpor>
                            <!--  x:x:x::x/ x:x:x::x:x/x:x:x::x:x:x/x:x:x::x:x:x:x  -->
                           <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,4}$</regexpor>
                            <!--  x:x:x:x::x/ x:x:x:x::x:x/x:x:x:x::x:x:x -->
                            <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,3}$</regexpor>
                            <!--  x:x:x:x:x::x/ x:x:x:x:x::x:x -->
                            <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4})){1,2}$</regexpor>

                            <!-- x:: / x:x:: / x:x:x:: / x:x:x:x:: / x:x:x:x:x:: / x:x:x:x:x:x:: / x:x:x:x:x:x:x:: -->
                            <regexpor>^([0-9a-fA-F]{1,4})(\\:([0-9a-fA-F]{1,4})){0,6}(\\:\\:)$</regexpor>         <!-- x:x:x:x:x:x::x -->
                            <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4}))</regexpor>
                            <regexpor>^([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)([0-9a-fA-F]{1,4}\\:)(\\:([0-9a-fA-F]{1,4}))</regexpor>
                     </or>
                     <regexpand error="INVALID_IPv6_ADDRESS">[^0000:0000:0000:0000:0000:0000]</regexpand>
                </and>
            </or>
        </validation>
        <validation name="validate_link_local_ipv6">
          <and>
                 <regexpand error="INVALID_LINK_LOCAL_IPv6_ADDRESS">^(fe80|FE80)</regexpand>
          </and>
        </validation>
        <validation name="validate_usp_range">
            <and>
                <regexpand error="INVALID_USP_RANGE">^((((([0-9]/)?[0-9]+/[0-9]+)-(([0-9]/)?[0-9]+/[0-9]+)|(([0-9]/)?[0-9]+/[0-9]+)),)*((([0-9]/)?[0-9]+/[0-9]+)-(([0-9]/)?[0-9]+/[0-9]+)|(([0-9]/)?[0-9]+/[0-9]+))|((([0-9]/)?[0-9]+/[0-9]+)-(([0-9]/)?[0-9]+/[0-9]+)|(([0-9]/)?[0-9]+/[0-9]+)))$</regexpand>  
            </and>
        </validation>
        <validation name="validate_int_range">
            <and>
                <regexpand error="INVALID_INT_RANGE">^(((([0-9]+)-{1}([0-9]+)|([0-9]+)),)*(([0-9]+)-{1}([0-9]+)|([0-9]+))|(([0-9]+)-([0-9]+)|([0-9]+)))$</regexpand>
            </and>
        </validation>
