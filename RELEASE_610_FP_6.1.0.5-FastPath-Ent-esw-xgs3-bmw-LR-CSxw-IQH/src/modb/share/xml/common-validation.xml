        <validation name="validate_sysName">
            <and>
                 <regexpand error="INVALID_STRING">.*</regexpand>
            </and>
        </validation>
        
        <validation name="valiadate_arpAcl">
          <and>
                 <regexpand error="INVALID_ACL">^([a-zA-Z0-9]{1,31})$</regexpand>
          </and>
        </validation>
        <validation name="validate_alphanum16">
          <and>
                <regexpand error="INVALID_alphanum16">^([a-zA-Z0-9]{1,16})$</regexpand>
          </and>
        </validation>
        <validation name="validate_alphanum33">
          <and>
                <regexpand error="INVALID_alphanum33">^([a-zA-Z0-9]{1,33})$</regexpand>
          </and>
        </validation>
        <validation name="validate_radiusSecret">
          <and>
                <regexpand error="INVALID_radiusSecret">^[^ ~]{1,16}$</regexpand>
          </and>
        </validation>
        <validation name="validate_alphanum32">
          <and>
                <regexpand error="INVALID_alphanum32">^([a-zA-Z0-9]{1,32})$</regexpand>
          </and>
        </validation>
        <validation name="validate_alphanum32_allow_blank">
            <and>
