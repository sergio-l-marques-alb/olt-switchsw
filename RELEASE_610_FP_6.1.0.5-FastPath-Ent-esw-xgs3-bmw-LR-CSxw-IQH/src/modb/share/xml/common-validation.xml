            </and>
        </validation>
        <validation name="validate_alphanum_underscore32">
          <and>
                <regexpand error="INVALID_alphanum_underscore32">^([a-zA-Z0-9_-]{1,32})$</regexpand>
          </and>
        </validation>
        <validation name="validate_alphanum_underscore31">
          <and>
                <regexpand error="INVALID_alphanum_underscore31">^([a-zA-Z0-9_-]{1,31})$</regexpand>
          </and>
        </validation>
        <validation name="validate_alphanum_underscore_dash_space32">
          <and>
                <regexpand error="INVALID_alphanum_underscore_dash_space32">^([ a-zA-Z0-9_-]{0,32})$</regexpand>
          </and>
        </validation>
        <validation name="validate_alphanum128">
          <and>
                <regexpand error="INVALID_alphanum128">^([a-zA-Z0-9]{1,128})$</regexpand>
          </and>
        </validation>
        <validation name="validate_alphanum128_withblank">
          <and>
                <regexpand error="INVALID_alphanum_underscore32">^([a-zA-Z0-9]{0,128})$</regexpand>
          </and>
        </validation>
         <validation name="validate_alphanum8">
          <and>
                <regexpand error="INVALID_alphanum8">^([a-zA-Z0-9]{1,8})$</regexpand>
          </and>
        </validation>
        <validation name="validate_alphanum256">
          <and>
                <regexpand error="INVALID_alphanum256">^([a-zA-Z0-9]{1,256})$</regexpand>
          </and>
        </validation>
        <validation name="validate_alphanum96">
          <and>
                <regexpand error="INVALID_alphanum96">^([a-zA-Z0-9]{1,96})$</regexpand>
          </and>
        </validation>
        <validation name="validate_alphanum64">
          <and>
                <regexpand error="INVALID_alphanum64">^([a-zA-Z0-9]{1,64})$</regexpand>
          </and>
        </validation>

        <validation name="validate_alphanum_underscore_dash_space43">
          <and>
                <regexpand error="INVALID_alphanum_underscore_dash_space43">^([ a-zA-Z0-9_-]{1,43})$</regexpand>
          </and>
        </validation>
        <validation name="validate_alphanum_underscore_dash_space45">
          <and>
                <regexpand error="INVALID_alphanum_underscore_dash_space45">^([ a-zA-Z0-9_-]{1,45})$</regexpand>
