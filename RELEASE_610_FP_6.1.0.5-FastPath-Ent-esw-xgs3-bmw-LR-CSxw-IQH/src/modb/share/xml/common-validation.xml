        </validation>

        <validation name="validate_AciiHexData">
          <and>
                <regexpand error="INVALID_AciiHexData">^[a-zA-Z]{1,441}$</regexpand>
          </and>
        </validation>
        <validation name="validate_hex_uint_t">
        <or error="INVALID_hex_uint">
          <and>
                <regexpand error="INVALID_INTEGER">^^([0-9])+$</regexpand>
          </and>
          <and>
                <regexpand error="INVALID_HEX_DECIMAL_NUMBER">^(0x)[0-9a-fA-F]+$</regexpand>
          </and>
            <and>
                <regexpand error="INVALID_HEX_DECIMAL_NUMBER">^(0X)[0-9a-fA-F]+$</regexpand>
          </and>
        </or>
        </validation>
        <validation name="validate_ipv4_uint">
        <or error="INVALID_ipv4_uint">
          <and>
                <regexpand error="INVALID_INTEGER">^^([0-9])+$</regexpand>
          </and>
          <and> 
                <regexpand error="INVALID_IPv4_ADDRESS">^((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$</regexpand>
          </and>
        </or>
        </validation>
        <validation name="validate_castSrvAdr">
        <or error="INVALID_castSrvAdr">
          <and>
                <regexpand error="NOT_DECIMAL_DOTTED_FORMAT">^((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$</regexpand>
          </and>
          <and>
                <regexpand error="INVALID_alphanum64">^([a-zA-Z0-9][a-zA-Z0-9.-]{0,63})$</regexpand>
          </and>
        </or>
        </validation>

        <validation name="validate_hostname">
        <or error="INVALID_VALUE_GENERIC">
           <!-- This is to validate host names should start with the letter and end with the letter or digit and the
             Max length should not be greater than the 255 -->
            <regexpor error="INVALID_VALUE_GENERIC">^([a-zA-Z])([a-zA-Z0-9\\.\\-]{1,253})([a-zA-Z0-9])$</regexpor>
            <regexpor error="INVALID_VALUE_GENERIC">^([a-zA-Z])([a-zA-Z0-9])$</regexpor>
            <regexpor error="INVALID_VALUE_GENERIC">^([a-zA-Z])$</regexpor>
        </or>
        </validation>

        <validation name="validate_hostname_allownull">
        <or error="INVALID_VALUE_GENERIC">
           <!-- This is to validate host names should start with the letter and end with the letter or digit and the
             Max length should not be greater than the 255 -->
            <regexpor error="INVALID_VALUE_GENERIC">^([a-zA-Z])([a-zA-Z0-9\\.\\-]{0,253})([a-zA-Z0-9])*$</regexpor>
            <regexpor error="INVALID_VALUE_GENERIC">^([a-zA-Z])([a-zA-Z0-9])*$</regexpor>
            <regexpor error="INVALID_VALUE_GENERIC">^([a-zA-Z])*$</regexpor>
        </or>
        </validation>
        
        <validation name="cosqueueValidation">
        <and>
          <regexpand error="INVALID_COSQUEUEID">^[0-6]{1}$</regexpand>
        </and>
        </validation>

       <validation name="portpriorityvalidation">
        <and>
          <regexpand error="INVALID_alphanum8">^[0-6]{1}$</regexpand>
